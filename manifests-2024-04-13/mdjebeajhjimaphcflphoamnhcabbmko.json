{
"update_url": "https://clients2.google.com/service/update2/crx",

    "name" : "Askeeio Screen share",
    "author": "linearhub",
    "version" : "1.0.0",
    "manifest_version" : 2,
    "minimum_chrome_version": "34",
    "description" : "Share the screen from your computer in the Roundee video conference.",
    "homepage_url": "https://www.roundee.io",
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "content_scripts": [ {
       "js": [ "content.js" ],
       "all_frames": true,
       "run_at": "document_end",
       "matches": ["https://localhost:4200/*", "https://askee.io/*" ]
    }],

    "externally_connectable": {
        "matches": ["https://localhost:4200/*", "https://askee.io/*"]
    },
    "icons" : {
        "16" : "16.png",
        "19" : "19.png",
        "38" : "38.png",
        "128": "128.png"
    },
    "permissions": [
        "desktopCapture", "*://localhost:4200/*", "*://askee.io/*", "*://*.askee.io/*"
    ],
    "web_accessible_resources": [
        "38.png"
    ]
}
