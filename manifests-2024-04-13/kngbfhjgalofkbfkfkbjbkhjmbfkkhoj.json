  "manifest_version": 3,
  "description": "Access the Sourcescrub profile from a company's website",
  "homepage_url": "https://webextension.sourcescrub.com",
  "key": "",
  "icons": {
    "16": "icons/icon16.png",
    "40": "icons/icon40.png",
