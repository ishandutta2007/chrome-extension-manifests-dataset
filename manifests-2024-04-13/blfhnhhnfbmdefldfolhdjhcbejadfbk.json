      }
    ]
  },
  "permissions": [
    "declarativeNetRequest",
    "storage"
  ],
  "options_page": "options.html",
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*/*"
