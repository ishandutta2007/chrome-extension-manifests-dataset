{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "RC Video Companion",
  "description": "Use RC Video with ease",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "serviceWorker.js"
  },
  "permissions": ["tabs"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/icon16.png",
      "32": "/images/icon32.png",
      "48": "/images/icon48.png",
      "128": "/images/icon128.png"
    }
  },
  "icons": {
    "16": "/images/icon16.png",
    "32": "/images/icon32.png",
    "48": "/images/icon48.png",
    "128": "/images/icon128.png"
  },
  "commands": {
    "switch-to-rcv-tab": {
      "suggested_key": {
        "default": "Ctrl+Shift+1",
        "mac": "Command+Shift+1"
      },
      "description": "Switch to RCV tab",
      "global": true
    },
    "toggle-rcv-mute": {
      "suggested_key": {
        "default": "Alt+Shift+M",
        "mac": "Command+MacCtrl+M"
      },
      "description": "Toggle RCV microphone mute",
      "global": false
    },
    "toggle-rcv-camera": {
      "suggested_key": {
        "default": "Alt+Shift+V",
        "mac": "Command+MacCtrl+V"
      },
      "description": "Toggle RCV camera on/off",
      "global": false
    },
    "toggle-rcv-sharing": {
      "suggested_key": {
        "default": "Alt+Shift+S",
        "mac": "Command+MacCtrl+S"
      },
      "description": "Start/stop RCV screen sharing",
      "global": false
    }
  },
  "content_scripts": [
    {
      "matches": ["https://v.ringcentral.com/*"],
      "js": ["contentScript.js"]
    }
  ]
}
