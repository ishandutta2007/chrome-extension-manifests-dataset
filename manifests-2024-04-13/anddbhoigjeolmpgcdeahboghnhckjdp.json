  "description": "Saves YouTube thumbnails to NailedThumbs mood boards",
  "permissions": [
      "storage"
  ],
  "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
      }
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "content_scripts": [
      {
          "matches": ["https://www.youtube.com/"],
          "css": ["styles.css"],
          "js": ["content.js"],
          "run_at": "document_idle"
      }
  ]
