{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "URL Redirect and Login",
  "version": "1.2",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "declarativeNetRequestWithHostAccess",
    "declarativeNetRequest",
    "declarativeNetRequestFeedback",
    "cookies"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://merchants.ubereats.com/*",
        "https://auth.uber.com/*",
        "https://www.doordash.com/*",
        "https://identity.doordash.com/*",
        "https://restaurant.grubhub.com/*"
      ],
      "js": ["content.js","jquery.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "http://debug.loopapplication.xyz/*",
    "https://debug.loopapplication.xyz/*",
    "https://internal.loopapplication.xyz/*",
    "https://merchants.ubereats.com/*",
    "https://auth.uber.com/*",
    "https://*.doordash.com/*",
    "https://identity.doordash.com/*",
    "https://restaurant.grubhub.com/*"
  ],
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "ruleset_1",
        "enabled": true,
        "path": "rules.json"
      }
    ]
  }
}
