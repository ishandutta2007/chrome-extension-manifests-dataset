{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,

  "name": "Code Injector",
  "description": "Injects CSS and JS on any page",
  "version": "1.0",

  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },

  "browser_action": {
    "default_icon": "icon-512.png"
  },

  "content_scripts": [
      {
          "matches": ["<all_urls>"],
          "run_at": "document_end",
          "js": ["js/vendor.js", "js/content_script.js"]
      }
  ],

  "background": {
    "scripts": ["js/vendor.js", "js/background.js"]
  },

  "permissions": [
    "storage",
    "<all_urls>"
  ]
}
