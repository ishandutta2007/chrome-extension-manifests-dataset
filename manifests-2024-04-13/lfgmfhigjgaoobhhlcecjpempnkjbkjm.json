{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,

  "name": "instantAha highlighter",
  "short_name" : "ahalight",
  "description": "instantAha highlighter shows visual representations for your highlighted words.",
  "version": "1.2.2",

  "browser_action": {
    "default_icon": {
     "19" : "/favicons/fav19.png",
     "38" : "/favicons/fav38.png"
    },
    "default_popup" : "popup.html",
    "default_title": "instantAha"
  },

  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "exclude_matches":[
        "*://192.168.0.55/*",
        "*://127.0.0.1/*",
        "*://localhost/*",
        "*://instantaha.com/*"
      ],
      "all_frames": true,
      "js": [
        "jquery-2.0.3.min.js",
        "jquery.highlighter.js",
        "/video-js-5.0.2/video.js",
        "ahaplayer-js.js",
        "content_script.js"
      ],
      "css" :[
        "app.css",
        "/video-js-5.0.2/video-js.min.css"
      ]
    }
  ],

  "background":{
    "scripts":["background.js"],
    "persistent": false
  },

  "permissions": [
    "storage",
    "*://instantAha.com/"
  ]
}