{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "curview",
  "version": "2.3",
  "description": "This extension allows you to share free videos from any website with a curview room.",
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ],
      "all_frames": true
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "48": "publishIcon.png"
  },
  "action": {
    "default_icon": {
      "32": "publishIcon.png"
    },
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "images/curviewIcon.svg",
        "images/play.svg",
        "images/pause.svg"
      ],
      "matches": ["<all_urls>"],
      "extension_ids": []
    }
  ]
}
