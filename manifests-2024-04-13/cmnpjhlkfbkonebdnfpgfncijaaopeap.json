    "service_worker": "scripts/background.js"
  },
  "description": "__MSG_extensionDescription__",
  "icons": {
    "128": "icon.png"
  },
  "default_locale": "en",
  "content_security_policy": {
    "script-src": "self",
    "object-src": "self"
 }
