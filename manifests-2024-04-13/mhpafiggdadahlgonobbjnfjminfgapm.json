  "manifest_version": 2,
  "name": "ServiceTick Survey Toggle",
  "version": "1.2",
  "browser_action": {
   "default_icon": "iconon.png"
  },
  "icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" },
  "description": "This extension toggles between saving / not saving responses on ServiceTick surveys.",
  "background": {
     "scripts": ["background.js"]
  },
  "permissions": [
    "tabs",
    "webRequest",
    "*://*.servicetick.com/SurveyPortal/*",
    "*://*.servicetick.com/surveyportal/*",
    "webRequestBlocking",
    "storage"
  ]
}
