{
   "name": "N.C. State Library GOBI Linky",
   "version": "2.3",
   "description": "This extension will search titles on a Gobi page and display the number of matches found in NC State University Libraries.",
   
   "background": {
     "service_worker": "background.js"
   },
   "content_scripts": [
     {
       "all_frames": false,
       "js": ["gobi3.js"],
       "matches": ["https://*.gobi3.com/*"],
       "run_at": "document_idle"
     }
   ],

   "host_permissions": [
     "https://catalog.lib.ncsu.edu/"
   ],

   "icons": {
    "16": "/images/icon16.png",
    "32": "/images/icon32.png",
    "48": "/images/icon48.png",
    "128": "/images/icon128.png"
 },

   "action": {
    "default_icon": {
       "16": "/images/icon16.png",
       "32": "/images/icon32.png",
       "48": "/images/icon48.png",
       "128": "/images/icon128.png"
   }
 },
   
   "update_url": "https://clients2.google.com/service/update2/crx",
   "manifest_version": 3
 }