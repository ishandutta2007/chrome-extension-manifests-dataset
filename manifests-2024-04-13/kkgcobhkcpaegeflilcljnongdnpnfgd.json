    "http://*/*",
    "https://*/*",
    "tabs",
    "browsingData",
    "storage",
    "cookies",
    "unlimitedStorage",
    "clipboardWrite",
    "bookmarks"
  ],
  "optional_permissions": [
    "contextMenus",
    "privacy"
  ],
  "browser_action": {
    "default_icon": {
      "128": "src/images/icons/icon-128.png"
