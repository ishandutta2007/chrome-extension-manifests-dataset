        "16": "icons/16.png",
        "24": "icons/24.png",
        "32": "icons/32.png",
        "48": "icons/48.png",
        "128": "icons/128.png"
    },
    "background": {
        "scripts": ["src/background.js"],
        "persistent": true
    },
    "content_scripts": [{
        "matches": [
            "*://*.fb.com/*",
            "*://*.facebook.com/*",
            "*://*.m.me/*",
