{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Cosmo AI",
  "version": "1.27",
  "description": "Your all-in-one AI assistant. Level up your browsing experience with Cosmo. Access AI everywhere on the web.",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "host_permissions": ["<all_urls>"],
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "scripting",
    "webNavigation"
  ],
  "background": {
    "service_worker": "backgroundScript.js"
  },
  "content_scripts": [
    {
      "matches": ["https://extensionpay.com/*"],
      "js": ["ExtPay.js"],
      "run_at": "document_start"
  },
    {
      "matches": [
        "https://*/*",
        "http://*/*"
      ],
      "js": ["ExtPay.js","contentScript.js"],
      "css": ["prompts.css"]

    },
    {
      "matches": ["https://twitter.com/*", "http://twitter.com/*"],
      "js": ["ExtPay.js","twitter-content.js"],
      "css": ["prompts.css"]
    },
    {
      "matches": ["https://mail.google.com/*", "http://mail.google.com/*"],
      "js": ["ExtPay.js","gmail-content.js"],
      "css": ["prompts.css"]

    },
    {
      "matches": ["https://linkedin.com/*", "http://linkedin.com/*", "https://www.linkedin.com/*", "http://www.linkedin.com/*"],
      "js": ["ExtPay.js","linkedin-content.js"],
      "css": ["prompts.css"]

    },
    {
      "matches": ["https://www.google.com/*", "http://www.google.com/*", "https://google.com/*", "http://google.com/*"],
      "js": ["ExtPay.js","google-content.js"],
      "css": ["prompts.css"]

    },
    {
      "matches": ["https://www.reddit.com/*", "http://www.reddit.com/*", "https://reddit.com/*", "http://reddit.com/*"],
      "js": ["ExtPay.js","reddit-content.js"],
      "css": ["prompts.css"]

    },
    {
      "matches": ["https://www.wikipedia.org/*", "http://www.wikipedia.org/*", "https://wikipedia.org/*", "http://wikipedia.org/*", "https://*.wikipedia.org/*", "http://*.wikipedia.org/*"],
      "js": ["ExtPay.js","wikipedia-content.js"],
      "css": ["prompts.css"]

    }
   
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png"
    }
  },
  "web_accessible_resources": [
    {
      "resources": [
        "fonts/Satoshi-Bold.woff2",
        "fonts/Satoshi-Medium.woff2",
        "fonts/Satoshi-Regular.ttf",
        "prompts.css"


      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+M",
        "mac": "Command+M"
      },
      "description": "Open Cosmo"
    }
  }
  
  
}
