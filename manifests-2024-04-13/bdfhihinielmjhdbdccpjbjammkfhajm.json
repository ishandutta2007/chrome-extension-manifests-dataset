        "48": "icon-48.png",
        "32": "icon-32.png",
        "16": "icon-16.png"
    },
    "action": {
        "default_icon": {
            "16": "icon-16.png",
            "24": "icon-24.png",
            "32": "icon-32.png"
        },
        "default_popup": "popup.html"
    },
    "default_locale": "en",
    "background": {
        "service_worker": "js/background.js",
        "type": "module"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "/js/save-png.js"
            ],
            "all_frames": true,
            "match_about_blank": true
        }
    ],
    "permissions": [
        "contextMenus",
        "downloads"
    ],
    "offline_enabled": true,
    "container": [
        "GOOGLE_DRIVE"
    ],
