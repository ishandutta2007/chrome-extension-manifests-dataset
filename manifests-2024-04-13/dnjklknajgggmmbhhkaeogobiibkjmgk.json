  "name": "TradingNotes",
  "description": "Attach floating notes to your TradingView's stock lists",
  "version": "3.6",
  "manifest_version": 3,
  "permissions": [
    "storage"
  ],
  "host_permissions": [
      "https://www.tradingview/*",
      "https://finviz.com/*",
      "https://elite.finviz.com/*",
      "https://finance.yahoo.com/*",
      "https://guce.yahoo.com/*",
      "https://*.yahoo.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://*.tradingview.com/*","https://tradingview.com/*"],
      "js": ["content.js"],
      "css": ["fix.css"],
      "run_at": "document_end"
    }
  ],
    "web_accessible_resources": [
    {
      "resources": ["icon16.png","icon48.png","icon128.png"],
      "matches": ["https://www.tradingview/*","https://tradingview.com/*"]
    }
  ],
  "background": { "service_worker": "./background.js", "type": "module"},
  "action": {
    "default_title": "TradingNotes",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": { "16": "icon16.png",
             "48": "icon48.png",
            "128": "icon128.png" }
}
