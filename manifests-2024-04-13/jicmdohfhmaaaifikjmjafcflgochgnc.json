{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "超级助理",
    "description": "基于文心一言的超级助理，提供伴随式大模型服务，可解析复杂问题、执行快捷指令、辅助知识应用，无所不知、无所不能、无所不在",
    "version": "2.0.2",
    "icons": {
        "16": "icon.png",
        "32": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "action": {
        "default_icon": {
            "16": "16.png",
            "32": "32.png",
            "48": "48.png",
            "128": "128.png"
        },
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "all_frames": true,
            "js": [
                "js/enter.js"
            ],
            "run_at": "document_end"
        }
    ],
    "background": {
        "service_worker": "js/background.js",
        "type": "module"
    },
    "web_accessible_resources": [
        {
            "matches": [
                "<all_urls>"
            ],
            "resources": [
                "js/content_script.js",
                "js/newtab.js",
                "redirect.html"
            ]
        }
    ],
    "options_ui": {
        "open_in_tab": true,
        "page": "setting.html"
    },
    "sandbox": {
        "pages": [
            "test.html"
        ]
    },
    "permissions": [
        "storage",
        "tabs",
        "clipboardRead",
        "clipboardWrite",
        "activeTab"
    ],
    "host_permissions": [
        "https://*.baidu.com/",
        "https://*.baidu-int.com/",
        "http://*.baidu.com/",
        "http://*.baidu-int.com/",
        "<all_urls>"
    ],
    "commands": {
        "sidebar": {
            "suggested_key": {
                "default": "Ctrl+I",
                "mac": "Command+I",
                "linux": "Ctrl+I"
            },
            "description": "sidebar"
        },
        "capture": {
            "suggested_key": {
                "default": "Ctrl+Shift+Z",
                "mac": "Command+Shift+Z",
                "linux": "Ctrl+Shift+Z"
            },
            "description": "网页截图"
        }
    },
    "content_security_policy": {
        "sandbox": "sandbox allow-scripts allow-forms allow-popups allow-modals; script-src 'self' https://developer.baidu-int.com; child-src 'self';"
    }
}
