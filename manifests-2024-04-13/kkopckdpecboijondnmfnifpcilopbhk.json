{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "QlikVerify Browser Plugin",
  "description": "QlikVerify Browser extension to manage Intelligent Web Apps",
  "version": "1.0.3",
  "manifest_version": 3,
  "permissions": [
    "privacy",
    "cookies",
    "storage"
  ],
  "minimum_chrome_version": "88",
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "extension.css"
      ],
      "js": [
        "jquery-1.9.1.js",
        "extension.js"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "icons/*",
        "extension.css"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "content_security_policy": {
    "extension_pages": "object-src 'self' ;script-src 'self';"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "host_permissions": [
    "*://*/*"
  ],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Alt+V",
        "windows": "Alt+V",
        "mac": "Command+Shift+V"
      }
    }
  }
}