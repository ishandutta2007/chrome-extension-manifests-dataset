{
"update_url": "https://clients2.google.com/service/update2/crx",

 "author": "SSTS",
 "manifest_version": 3,
 "name": "ObserveIQ Extension",
 "permissions": [
  "http://*/*",
  "https://*/*",
  "<all_urls>",
  "offscreen"
 ],
 "host_permissions": [
        "*://*/*",
        "<all_urls>"
    ],
 "externally_connectable": {
  "matches": [
   "*://localhost/*",
   "*://*.stg.smartopkey.com/*",
   "*://*.myopkey.com/*",
   "*://*.smartopkey.com/*",
   "*://*.lightning.force.com/*",
   "*://*.opkeyone.com/*"
  ]
 },
 "content_security_policy": {
  "extension_pages": "script-src 'self'; object-src 'self'"
   },

 "version": "1.0.0.4",
 "background": {
  "service_worker": "background.js"
 },
 "description": "This Chrome extension facilitates user to seamlessly capture the user journeys for whitelisted ERP URLs in form of recordings.",
 "icons": {
  "16": "icons/addon128.png",
  "32": "icons/addon128.png",
  "48": "icons/addon128.png",
  "128": "icons/addon128.png"
 },
 "action": {
  "default_icon": {
   "32": "icons/addon128.png",
   "16": "icons/addon128.png",
   "14": "icons/addon128.png",
   "12": "icons/addon128.png"
  },
  "default_title": "ObserveIQ",
  "default_popup": "index.html"
 },
 "content_scripts": [
  {
   "matches": [
    "*://*/*"
   ],
   "js": ["assets/analytics/js/apps/genericapptracker.js","assets/analytics/js/apps/oraclefusiontracker.js","assets/analytics/js/mainapptracker.js"],
   "css": ["assets/analytics/css/main.css"],
   "all_frames": true,
   "run_at": "document_end"
  }
 ],
 "web_accessible_resources": [{
  "resources": ["assets/analytics/js/apps/tools/genericappinterceptor.js"],
  "matches": ["<all_urls>"]
   },{
  "resources": ["assets/analytics/js/apps/tools/oracelfusionappinterceptor.js"],
  "matches": ["<all_urls>"]
   }]
}