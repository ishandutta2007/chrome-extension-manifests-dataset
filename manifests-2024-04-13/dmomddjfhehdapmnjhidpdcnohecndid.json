      "js": [
        "js/jquery.min.js",
        "js/message.min.js",
        "js/wordpress.min.js"
      ],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "js/*",
        "css/*",
        "fonts/*",
        "images/*"
      ],
      "matches": [
        "https://*.taobao.com/*",
        "https://*.tmall.com/*",
        "https://*.tmall.hk/*"
      ],
      "use_dynamic_url": true
    }
  ],
  "externally_connectable": {
    "matches": [
      "*://*.taobao.com/*",
      "*://*.tmall.com/*",
      "*://*.tmall.hk/*"
    ]
