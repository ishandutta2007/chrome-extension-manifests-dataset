{
    "update_url":"http://clients2.google.com/service/update2/crx",
    "name": "__MSG_name__",
    "version" : "1.1",
    "manifest_version": 3,
    "minimum_chrome_version": "88.0",
    "default_locale": "en",
    "description": "__MSG_description__",
    "homepage_url": "http://genuinereader.com",
    "icons": {
      "16": "src/assets/icons/app/icon-16.png",
