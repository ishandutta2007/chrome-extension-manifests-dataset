    {
      "matches": [
        "https://app.reachdesk.com/*"
      ],
      "css": [],
      "js": [
        "dist/presence.js"
      ],
      "run_at": "document_start"
    },
    {
      "matches": [
        "https://*.lightning.force.com/*"
      ],
      "css": [
        "dist/salesforce.css",
        "popup.css"
      ],
      "js": [
        "dist/salesforce.js"
      ]
    },
    {
      "matches": [
        "https://app2a.outreach.io/*"
      ],
      "js": [
        "dist/outreach.js"
      ]
    },
    {
      "matches": [
        "https://app.salesloft.com/*"
      ],
      "js": [
        "dist/salesloft.js"
      ]
    },
    {
      "matches": [
        "https://app.hubspot.com/*"
      ],
      "js": [
        "dist/hubspot.js"
      ]
    },
    {
      "matches": [
        "https://*.pardot.com/*"
      ],
      "js": [
        "dist/pardot.js"
      ]
    },
    {
      "matches": [
        "https://*.eloqua.com/*"
      ],
      "js": [
        "dist/eloqua.js"
      ]
    },
    {
      "matches": [
        "https://*.dynamics.com/*"
      ],
      "js": [
        "dist/dynamics.js"
      ]
    },
    {
      "matches": [
        "https://*.marketo.com/*"
      ],
      "exclude_matches": [
        "https://www.marketo.com/*"
      ],
      "js": [
        "dist/marketo.js"
      ]
    },
    {
      "matches": [
        "https://mail.google.com/*"
      ],
      "css": [
        "dist/gmail.css",
        "popup.css"
      ],
      "js": [
        "dist/gmail.js"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "https://*.lightning.force.com/*",
        "https://mail.google.com/*"
      ],
      "resources": [
        "dist/images/*",
        "pageWorld.js"
      ]
    }
  ],
  "background": {
    "service_worker": "dist/authBackground.js"
  }
}
