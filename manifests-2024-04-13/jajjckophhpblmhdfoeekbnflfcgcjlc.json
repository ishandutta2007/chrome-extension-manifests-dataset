"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "IETF RFC Reader",
  "version": "0.1.0",
  "description": "This extension makes it easy to read IETF RFCs.",
  "icons": {
    "16": "icons/icon_16x16.png",
