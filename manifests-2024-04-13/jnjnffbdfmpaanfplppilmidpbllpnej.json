    "default_title": "Search Larder",
    "default_panel": "search.html",
    "default_icon": "64.png"
  },
  
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "windows": "Ctrl+Shift+L",
        "mac": "Ctrl+Shift+L",
        "chromeos": "Ctrl+Shift+L",
        "linux": "Ctrl+Shift+L"
      }
    }
  },
  
  "applications": {
    "gecko": {
      "id": "addon@larder.io",
      "strict_min_version": "55.0"
    }
  },
  
  "permissions": [
    "activeTab",
    "cookies",
    "storage",
