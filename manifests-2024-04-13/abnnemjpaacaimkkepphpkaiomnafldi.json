            "all_frames": true
        },
        {
            "matches": ["<all_urls>"],
            "js": ["common/browser-polyfill.js", "content/prompt-injecter.js"],
            "match_about_blank": true,
            "all_frames": true,
            "run_at": "document_end"
        }
