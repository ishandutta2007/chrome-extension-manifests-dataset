{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "HTML5 Video controller",
  "version": "1.1",
  "description": "Simple extension to control HTML5 video by keyboard numbers",
  "permissions": [
    "<all_urls>"
  ],
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png",
    "512": "icon512.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "clientControlVideo.js"
      ],
      "run_at": "document_end",
      "all_frames": true
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "/injected/*"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}
