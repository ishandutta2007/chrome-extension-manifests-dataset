  "version": "0.1.16",
  "name": "Pinterest Tag Helper",
  "manifest_version": 3,
  "description": "A tool to help you build, troubleshoot and test your Pinterest Tags.",
  "action": {
    "default_title": "Pinterest Tag Helper",
