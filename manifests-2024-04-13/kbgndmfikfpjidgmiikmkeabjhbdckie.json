{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "PRMDB - Prompts Database for ChatGPT",
    "description": "Create public and private custom prompts for OpenAI ChatGPT",

    "version": "1.1.6",
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "background": {
        "service_worker": "background.js",
        "type": "module"
    },
    "action": {
        "default_title": "PRMDB - Open ChatGPT"
    },
    "options_ui": {
        "page": "options.html",
        "open_in_tab": true
    },
    "permissions": [
        "activeTab",
        "storage",
        "declarativeNetRequestWithHostAccess"
    ],
    "declarative_net_request": {
        "rule_resources": [
            {
                "id": "ruleset_1",
                "enabled": true,
                "path": "rules.json"
            }
        ]
    },
    "host_permissions": [
        "https://prmdb.com/*",
        "https://chat.openai.com/*"
    ],
    "web_accessible_resources" : [{
        "resources": ["config.js", "main.js","inactive.js", "httpService.js", "utils.js", "vars.js", "style.css", "options.html"],
        "matches": ["https://chat.openai.com/*"],
        "use_dynamic_url": true
    }], 
    "content_scripts": [{
        "js": ["content_script.js"],
        "css": ["style.css"],
        "matches": ["https://chat.openai.com/*"]
    }]
}
