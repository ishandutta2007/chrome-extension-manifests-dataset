{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "iZiTest",
  "description": "iZiTest is a Chrome extension that making tests your website easier.",
  "version": "0.1.0",
  "manifest_version": 3,
  "default_locale": "en",
  "background": {
    "service_worker": "serviceWorker.js"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>",
        "*://*/*"
      ],
      "match_origin_as_fallback": true,
      "css": [
        "styles/contentScript.css"
      ],
      "js": [
        "contentScript.js"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "contextMenus",
    "webNavigation",
    "activeTab",
    "debugger",
    "scripting",
    "background",
    "tabs",
    "storage"
  ],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "action": {
    "default_title": "iZiTest",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/icons/16x.png",
      "32": "/icons/32x.png",
      "48": "/icons/48x.png",
      "128": "/icons/128x.png"
    }
  },
  "icons": {
    "16": "/icons/16x.png",
    "32": "/icons/32x.png",
    "48": "/icons/48x.png",
    "128": "/icons/128x.png"
  },
  "externally_connectable": {
    "matches": [
      "http://localhost:3000/*",
      "https://app.izitest.io/*",
      "https://www.app.izitest.io/*"
    ]
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self';"
  },
  "host_permissions": [
    "*://*/*",
    "https://*/*",
    "<all_urls>"
  ]
}