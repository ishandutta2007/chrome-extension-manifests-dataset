   "page_action": {
          "default_icon": {
            "19": "icons/icon-19.png",
            "38": "icons/icon-38.png"
          },
          "default_title": "JK-Navigator-Too" },

  "permissions": [
     "storage",
     "tabs",
     "webNavigation"
  ]
