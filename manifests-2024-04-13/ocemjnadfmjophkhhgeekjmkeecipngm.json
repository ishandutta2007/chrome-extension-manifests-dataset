{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,

  "name": "Track post code",
  "description": "Tracking post codes from page",
  "version": "1.2.1",

  "browser_action": {
   "default_icon": "icon48.png",
   "default_popup": "popup.html"
  },

  "icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" },

  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "css": ["contentscript.css"],
      "js": ["contentscript.js"],
      "run_at": "document_end",
      "all_frames": true
    }
  ],

  "web_accessible_resources": [
    "images/*.png",
    "images/*.jpg"
  ],

  "permissions": [
   "activeTab"
   ]
}