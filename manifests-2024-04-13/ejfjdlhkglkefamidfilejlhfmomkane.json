  "name": "Avito CRM Connector",
  "description": "Отправка данных из avito.ru в CRM",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "activeTab",
    "storage"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
