        "js": [ "jquery.min.js", "content.js" ],
        "css": [ "styles.css" ],
        "run_at": "document_end"
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "logo.png",
    "icons": {
      "16": "logo.png",
      "48": "logo.png",
      "128": "logo.png"
    }
  }
