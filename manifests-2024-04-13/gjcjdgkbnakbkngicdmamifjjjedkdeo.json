  "name": "Xcelero Extension",
  "version": "5.3.2",
  "manifest_version": 2,
  "description": "NetSuite Test Automation",
  "icons": {
    
 "16": "icons/icon16.png",
     "48": "icons/icon48.png",
  "64": "icons/icon.png",
   "96": "icons/icon96.png",
     "128": "icons/icon128.png"
