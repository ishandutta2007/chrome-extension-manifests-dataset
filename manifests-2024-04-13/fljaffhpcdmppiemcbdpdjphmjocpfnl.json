{
"update_url": "https://clients2.google.com/service/update2/crx",

    "name": "WebPT Extension",
    "version": "1.1.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "enter.html",
        "default_title": "Welcome To WebPT Extension"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
