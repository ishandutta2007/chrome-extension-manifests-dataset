{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "AniLog",
  "description": "Automatically track your anime progress in AniList.",
  "version": "0.0.2",
  "manifest_version": 3,
  "permissions": [
    "tabs",
    "identity",
    "storage"
  ],
  "icons": {
    "16": "/images/logo-16.png",
    "32": "/images/logo-32.png",
    "48": "/images/logo-48.png",
    "128": "/images/logo-128.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "build/background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*/*"
      ],
      "include_globs": [
        "https://*gogoanime*/*"
      ],
      "js": [
        "build/gogoanime.js"
      ]
    },
    {
      "matches": [
        "https://*/*"
      ],
      "include_globs": [
        "https://*aniwave*/*"
      ],
      "js": [
        "build/aniwave.js"
      ]
    },
    {
      "matches": [
        "https://*/*"
      ],
      "include_globs": [
        "https://*kumo*/*"
      ],
      "js": [
        "build/kumo.js"
      ]
    },
    {
      "matches": [
        "*://*/*"
      ],
      "all_frames": true,
      "run_at": "document_end",
      "js": [
        "build/videotracking.js"
      ]
    }
  ],
  "key": ""
}
