{
"update_url": "https://clients2.google.com/service/update2/crx",

    "name": "Get Focused - Avoid distracting websites",
    "version": "0.5.0",
    "description": "Helps you avoid distracting websites and stay focused on your goals",
    "permissions": [
        "storage",
        "activeTab",
        "alarms",
        "tabs"
    ],
    "background": {
        "scripts": [
            "psl.js",
            "background.js",
            "backgroundReminderPopup.js",
            "backgroundSnoozeCounterPopup.js"
        ],
        "persistent": false
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>",
                "http://*/*",
                "https://*/*"
            ],
            "css": [
                "reminderPopup.css"
            ],
            "js": [
                "reminderPopup.js",
                "snoozeCounterPopup.js"
            ],
            "run_at": "document_end"
        }
    ],
    "options_page": "option.html",
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/icon-16.png",
            "32": "images/icon-32.png",
            "48": "images/icon-48.png",
            "128": "images/icon-128.png"
        }
    },
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    },
    "manifest_version": 2,
    "web_accessible_resources": [
        "*.woff2",
        "reminder.html",
        "snooze-counter-popup.html"
    ]
}