    "identity",
    "storage",
    "contextMenus",
    "tabs",
    "activeTab",
    "desktopCapture",
    "scripting",
    "notifications"
  ],

  "host_permissions": [
    "<all_urls>"
  ],

  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },

