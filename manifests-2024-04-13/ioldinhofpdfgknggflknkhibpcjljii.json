    {
      "matches": ["*://*.linkedin.com/in/*"],
      "js": ["linkedinPerson.js"]
    },
    {
      "matches": ["*://*.linkedin.com/company/*"],
      "js": ["linkedinCompany.js"]
    },
    {
      "matches": ["*://*.linkedin.com/in/*/recent-activity/all/"],
      "js": ["linkedinActivity.js"]
    },
    {
      "matches": ["*://*.linkedin.com/in/*/recent-activity/reactions/"],
      "js": ["linkedinReactions.js"]
    }
  ],
