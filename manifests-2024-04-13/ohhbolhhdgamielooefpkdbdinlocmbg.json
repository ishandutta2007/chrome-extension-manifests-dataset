"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "Sassique Snip Button",
  "description": "Snip your favorite looks and products",
  "version": "1.5",

  "browser_action": {
   "default_icon": "icon-snip.png"
