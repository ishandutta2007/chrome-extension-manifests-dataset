size: 84.76KiB
languages:
  - English
description: >-
  This extension automatically inserts Swift drafts into your ticketing system's
  textbox. Works in Zendesk and Trengo.
publisher_account: admin
privacy_policy: https://getswift.ai/privacy-policy
---

{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Swift",
  "version": "1.1",
  "icons": {
    "16": "/swift16.png",
    "48": "/swift48.png",
    "128": "/swift128.png"
  },
  "permissions": ["activeTab", "scripting"],
  "host_permissions": [
    "*://hook.eu1.make.com/*",
    "https://*.zendesk.com/*"
  ],
  "web_accessible_resources": [
    {
      "resources": ["swift1000.png"],
      "matches": ["<all_urls>"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {},
  "content_scripts": [
    {
      "matches": ["https://app.trengo.com/*"],
      "js": ["trengo.js"]
    },
    {
      "matches": ["https://*.zendesk.com/*"],
      "js": ["zendesk.js"]
    }
  ]
}
