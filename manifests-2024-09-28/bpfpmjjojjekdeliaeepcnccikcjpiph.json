 {
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "JustLabelMe",
  "description": "Provides the ability to transform & print FBA shipping and product labels while you are on Amazon Seller Central with a single click",
  "version": "1.4.1",
  
  "icons": {
    "16": "/icons/icon16.png",
    "24": "/icons/icon24.png",
    "36": "/icons/icon32.png",
    "64": "/icons/icon64.png",
    "128": "/icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [ "*://*.amazon.com/*",  "*://*.amazon.com/*", "*://just-label.me/*", 
        "file://*/*"],
      "css": ["/css/main.css"],
      "js": [ "/js/main.js", "/js/shortcut.js"]
    }
  ],
  "background": {
    "scripts": ["/js/background.js"],
    "persistent": true
  },
  "browser_action": {
    "default_title": "JustLabelMe",
    "default_icon": "/icons/icon32.png"
  },
  "permissions": [
    "<all_urls>",
    "background",
    "*://*.amazon.com/*",
    "file:///*",
    "*://just-label.me/*",
    "*://*.amazon.com/*",
    "activeTab",
    "tabs",
    "storage",
    "file://*/*"
  ]
 
}