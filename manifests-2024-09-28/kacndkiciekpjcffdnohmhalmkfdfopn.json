{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "Extract-Hor",
  "version": "1.1",
  "author": "André Duchesne",
  "description": "Extension pour extraire l'horaire enseignant à partir de Tosca.net",


    "options_ui": {
      "page": "/options/options.html"
    },

  "permissions": [
        "tabs"
  ],
  
  "background": {
    "scripts": ["/background-scripts/background-script.js"]
  },
  
  
  "content_scripts": [
  {
    "matches": ["*://*/*horaire_enseignant*"],   
    "run_at": "document_idle",            
    "js": ["/content_scripts/content-script.js"]
  }
],
    
  "icons": {
    "16": "icons/horaire-16.png",        
    "32": "icons/horaire-32.png",
    "48": "icons/horaire-48.png",
    "128": "icons/horaire-128.png"
  },


  "browser_action": {
    "default_icon": "icons/horaire-32.png",
    "default_title": "Extract-Hor",
    "default_popup": "popup/popup.html"
  },


  "web_accessible_resources": [
    "images/config-tosca.png",
    "images/ecran1.PNG",
    "images/ecran2.PNG",
    "policy/policy.html"
  ]
}