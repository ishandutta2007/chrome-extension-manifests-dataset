{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Workfront",
  "version": "1.0.5",
  "description": "Simple and effective to-do & task list",
  "permissions": [
    "storage",
    "activeTab",
    "declarativeNetRequest",
    "cookies",
    "webRequest",
    "management"
  ],
  "background": {
    "service_worker": "src/pages/background/index.js",
    "type": "module"
  },
  "action": {
   "default_icon": {
      "16": "/16x16.png",
      "32": "/32x32.png",
      "48": "/48x48.png",
      "128": "/128x128.png"
    }
  },
  "icons": {
      "16": "/16x16.png",
      "32": "/32x32.png",
      "48": "/48x48.png",
      "128": "/128x128.png"
    },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "src/pages/content/index.js"
      ],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "assets/js/*.js",
        "assets/css/*.css",
        "logo.png"
      ],
      "matches": [
        "*://*/*"
      ]
    }
  ]
}