{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "ViralContent - LinkedIn AI Replies with Custom Prompts",
  "description": "ViralContent is a LinkedIn extension that helps you generate replies to comments on your posts.",
  "version": "1.0.0",
  "host_permissions": [
    "https://www.viralcontent.tech/*"
  ],
  "permissions": [
