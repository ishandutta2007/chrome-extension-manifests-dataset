          "resources": [
            "logo.png",
            "Close.png",
            "Star_grey.png",
            "Star_gold.png",
            "popup.html",
            "popup.css"
          ],
          "matches": ["<all_urls>"]
        }
    ],
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["artifactScript.js"]
        }
    ],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
