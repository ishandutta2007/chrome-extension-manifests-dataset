{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,

  "name": "Pingry Today",
  "short_name": "Pingry Today",
  "description":"A layout for your new tab page that fills you in on Pingry news and information.",
  "version": "5.5",

  "browser_action": {
