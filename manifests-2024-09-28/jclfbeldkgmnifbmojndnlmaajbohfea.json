{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Мои Звонки Click to Call",
  "description": "Свяжите Android смартфоны сотрудников с бизнес-процессами вашей компании.",
  "version": "2.0.1",
  "manifest_version": 3,
  "icons": {
    "16": "icon/logo_tiny.png",
    "128": "icon/logo.png"
  },

  "background": {
    "service_worker": "js/background.js"
  },
  
  "content_scripts" : [
    {
      "matches" : ["http://*/*", "https://*/*"],
      "js" : ["js/jquery-1.11.1.min.js", "js/bootstrap.js", "js/contentscript.js"],
   "css" : ["css/bootstrap.css"],
      "run_at" : "document_idle",
      "all_frames" : false
    }
  ],
  "permissions" : [
    "contextMenus", "storage"
  ],
  "host_permissions": [
    "http://*/*", "https://*/*"
  ],
  
   "action": {
        "default_popup": "index.html",
        "default_icon": {
            "16": "icon/logo.png",
            "32": "icon/logo.png",
            "48": "icon/logo.png",
            "128": "icon/logo.png"
        }
    },
 
   "web_accessible_resources": [{
  "resources": ["/icon/call.png"],
  "matches": ["<all_urls>"]
 }]
}