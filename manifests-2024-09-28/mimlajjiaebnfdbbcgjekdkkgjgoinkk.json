  },
  "action": {
    "default_icon": "images/icon.png"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "matches": ["https://*.google.com/*", "https://groups.google.com/", "http://*/*", "https://*/*"],
      "run_at": "document_idle",
      "js": ["content.js", "js/readability.js"]
    }
  ]
