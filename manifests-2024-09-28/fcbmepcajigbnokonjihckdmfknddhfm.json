{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Google Lens Translate Online",
  "description": "Select any text on the webpage, right-click to translate it to any language with Google Lens Translate.",
  "version": "1.1",
  "permissions": ["contextMenus", "activeTab", "scripting"],
  "background": {
    "service_worker": "background.bundle.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.bundle.js"]
    }
  ],
  "action": {
    "default_popup": "index.html",
    "default_title": "Translate selected text"
    
  },
  "icons": {
    "16": "icon (1).png",
    "48": "icon (3).png",
    "128": "icon (2).png"
  },
  "host_permissions": [
    "<all_urls>"
  ]
}
