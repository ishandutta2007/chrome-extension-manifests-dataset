{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Profitl ASIN Grabber",
  "description": "Grab and copy pages of Amazon ASINs with a single click",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "storage",
    "tabs"
  ],
  "host_permissions": [
    "*://*.amazon.co.uk/*",
    "*://*.amazon.com/*",
    "*://*.amazon.ca/*",
    "*://*.amazon.in/*"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.amazon.com/*", "*://*.amazon.co.uk/*"],
      "js": ["content.js"],
      "all_frames": true
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Show ASINs",
    "default_popup": "popup.html",
    "default_icon": "icon48.png"
  }
}
