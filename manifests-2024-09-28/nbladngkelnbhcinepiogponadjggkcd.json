{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Blue Arca PhishGuard - Phishing Protection for WebMail",
  "version": "1.1.9.15",
  "manifest_version": 3,
  "description": "100% Phishing protection for webmail. Completely private. No registration required.",
  "permissions": [
    "storage",
    "history",
    "activeTab"
  ],

  "host_permissions": [
    "*://mail.google.com/*",
    "*://outlook.office.com/*",
    "*://outlook.office365.com/*",
    "*://outlook.live.com/*",
    "https://console.bluearca.com/*",
    "http://api.bluearca.com:3000/*"
  ],

  "optional_host_permissions": [
    "*://*/*"
  ],

  "background": {
    "service_worker": "background.js",
    "type": "module"
  },

  "web_accessible_resources": [{
    "resources": ["announcement.html","icon128.png","libs/uuid.min.js"],
    "matches": ["*://mail.google.com/*", "*://outlook.office.com/*", "*://outlook.office365.com/*", "*://outlook.live.com/*"]
  }],
  
  "content_scripts": [
    {
      "js": [
        "jquery.min.js",
        "content_script.js"
      ],
      "matches": [
        "*://mail.google.com/*",
        "*://outlook.office.com/*",
        "*://outlook.office365.com/*",
        "*://outlook.live.com/*"
      ],
      "run_at": "document_end"
    }
  ],
    "icons":
    {
      "32": "img/logo.png",
      "128": "img/icon128.png"
    },
    "action":
    {
      "default_icon": "img/logo.png",
      "default_popup": "popup.html",
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"

    }
}
