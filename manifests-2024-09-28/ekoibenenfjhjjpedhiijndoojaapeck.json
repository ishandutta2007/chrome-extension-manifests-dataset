  "manifest_version": 3,
  "name": "JRVS ChatGPT",
  "version": "3.0.20",
  "description": "ChatGPT Messages",
  "permissions": [
    "webRequest",
    "storage",
    "activeTab",
    "scripting",
    "clipboardRead"
  ],
  "background": {
    "service_worker": "service-worker.js"
