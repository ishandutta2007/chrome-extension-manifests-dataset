{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Webpage Doodler",
  "version": "1.1",
  "permissions": ["activeTab", "scripting"],
  "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
