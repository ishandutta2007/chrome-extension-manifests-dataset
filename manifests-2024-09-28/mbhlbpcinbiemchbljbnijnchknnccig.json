{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "YBL Connect ",
  "version": "1.2.1",
  "description": "Browser extension that transcribes audio using YBL service.",
  "permissions": [
    "storage",
    "tabs",
    "webRequest"
  ],
  "host_permissions": [
    "https://meet.google.com/*",
    "https://app.ybl.ai/*"
  ],
  "background": {
    "service_worker": "pages/background/index.js",
    "type": "module"
  },
  "action": {
    "default_icon": "images/logo-48x48.png"
  },
  "icons": {
    "16": "images/logo-16x16.png",
    "48": "images/logo-48x48.png",
    "128": "images/logo-128x128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://meet.google.com/*"
      ],
      "js": [
        "./pages/content/index.js"
      ],
      "css": [
        "assets/css/contentStyle1726627655530.chunk.css"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "assets/js/*.js",
        "assets/css/*.css",
        "images/logo-16x16.png",
        "images/logo-48x48.png",
        "images/logo-128x128.png"
      ],
      "matches": [
        "*://*/*"
      ]
    }
  ]
}