   "suggested_key": {
    "default": "Alt+Q",
    "mac": "MacCtrl+Q"
   },
   "description": "Open alt-tab-style popup"
  },
  "020-focus-search": {
   "suggested_key": {
    "default": "Alt+W",
    "mac": "MacCtrl+W"
   },
   "description": "Search in alt-tab-style popup"
  },
  "1-previous-tab": {
   "description": "Switch to the previous tab"
  },
  "2-next-tab": {
   "description": "Switch to the next tab"
  },
  "30-toggle-recent-tabs": {
   "suggested_key": {
    "default": "Alt+Z",
    "mac": "MacCtrl+S"
   },
   "description": "Switch instantly between the two most recent tabs"
  }
 },
 "offline_enabled": true
