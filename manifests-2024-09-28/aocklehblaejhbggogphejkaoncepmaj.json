 "manifest_version": 3,
 "author": "CRM Together",
    "background": {
        "service_worker": "service-worker.min.js"
    }, 
 "action": {
  "default_popup": "dist/invalidsite.html",
  "default_icon": "dist/crmtogether.png",
  "default_title": "Open Gmail Accelerator for Sage 300"
 },
 "web_accessible_resources": [{
  "resources": ["iframe.js",
   "dist/gmailJsLoader.js",
            "dist/extension.js",
            "dist/extensionInjector.js",
            "dist/gmailJsLoader.js.map",
            "dist/extension.js.map"],
  "matches": ["<all_urls>"]
 }],
    "externally_connectable": {
        "matches": ["*://mail.google.com/*" ]
    },    
 "icons": {
  "16": "dist/AC S300-16.png",
  "32": "dist/AC S300-32.png",
  "48": "dist/AC S300-48.png",
    "64": "dist/AC S300-64.png",
    "80": "dist/AC S300-80.png",
  "128": "dist/AC S300-128.png",
    "192": "dist/AC S300-192.png",
    "512": "dist/AC S300-512.png"    
 },
  "side_panel": {
    "default_path": "dist/popup.html"
  }, 
  "permissions": ["sidePanel", "tabs"],
    "host_permissions": [
        "https://*/*"
    ],
    "commands": {
        "_execute_action": {
          "suggested_key": {
            "default": "Ctrl+B",
            "mac": "Command+B"
          }
        }
      },    
    "content_scripts": [
        {
            "matches": [ "*://mail.google.com/*" ],
            "js": [
                "dist/extensionInjector.js"
            ],
            "run_at": "document_start"
        }
    ]
