{
"update_url": "https://clients2.google.com/service/update2/crx",

 "manifest_version": 3,
 "name": "Colorboard",
 "description": "Extract colors from your screen and save them to your library.",
 "version": "2.8.4",
 "icons": {
  "16": "/images/logo16.png",
  "32": "/images/logo32.png",
  "64": "/images/logo64.png",
  "128": "/images/logo128.png"
 },
 "action": {
  "default_popup": "index.html",
  "default_icon": {
   "16": "/images/logo16.png",
   "32": "/images/logo32.png"
  }
 },
 "background": {
  "service_worker": "background.js"
 },
 "content_scripts": [
  {
   "matches": ["<all_urls>"],
   "css": ["styles.css"],
   "js": ["content.js"]
  }
 ],
 "web_accessible_resources": [
  {
   "resources": ["/images/*"],
   "matches": ["<all_urls>"]
  }
 ],
 "permissions": ["storage", "contextMenus", "notifications", "tabs"],
 "externally_connectable": {
  "matches": ["https://www.colorboard.co/*", "http://localhost:3020/*"]
 }
}
