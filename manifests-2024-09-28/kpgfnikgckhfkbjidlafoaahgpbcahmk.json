{
"update_url": "https://clients2.google.com/service/update2/crx",

    "chrome_url_overrides": {
      "newtab": "ui/homepage.html"
    },
    "manifest_version": 3,
    "default_locale": "en",
    "name": "__MSG_homepage_chrome_extension_name__",
    "description": "__MSG_homepage_chrome_extension_description__",
    "version": "3.1.2",
    "permissions": [
      "tabs",
      "storage",
      "cookies",
      "alarms",
      "declarativeNetRequestWithHostAccess"
    ],
    "host_permissions": [
      "*://*.yahoo.com/*"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
      "128": "icons/icon-128.png"
    },
    "chrome_settings_overrides": {
        "search_provider": {
            "name": "Yahoo",
            "keyword": "Yahoo",
            "search_url": "https://search.yahoo.com/yhs/search?p={searchTerms}&fr=yhs-ytest-amplify&type=yhs_pc_chr_ext_omnibar&hsimp=yhs-amplify&hspart=ytest",
            "favicon_url": "https://www.yahoo.com/favicon.ico",
            "suggest_url": "https://search.yahoo.com/sugg/ie?output=fxjson&command={searchTerms}&nResults=10",
            "encoding": "UTF-8",
            "is_default": true
        }
    },
    "web_accessible_resources": [{
      "resources": [
        "components/front/open-cta@3x.*.png",
        "components/front/open-cta-hover@3x.*.png",
        "components/front/close@3x.*.png",
        "components/front/close-over@3x.*.png",
        "components/front/yahoo-icon@3x.*.png",
        "components/front/mlb.*.png",
        "components/front/nfl.*.png",
        "components/front/nhl.*.png",
        "components/front/nba.*.png"
      ],
      "matches": ["*://www.yahoo.com/*", "*://us.yahoo.com/*"]
  
    }]
  }
  