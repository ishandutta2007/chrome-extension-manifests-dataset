        }
    ],
    "background": {
        "service_worker": "js_and_css/background.js"
    },
    "action": {
        "default_icon": {
            "16": "icons/16.png",
            "48": "icons/48.png",
            "128": "icons/128.png"
        },
        "default_title": "__MSG_PopupTitle__",
        "default_popup": "options/options.html"
    },
    "options_ui": {
        "page": "options/options.html"
    },
    "web_accessible_resources": [
        {
            "resources": ["assets/share.png"],
            "matches": ["<all_urls>"]
        }
