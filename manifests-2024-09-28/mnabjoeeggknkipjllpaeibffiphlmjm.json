{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "YT Customizer - YouTube Feed Filter",
    "description": "Seamlessly filter out YT recommendations with Keywords. Remove unwanted Thumbnails & Shorts for an undistracted viewing paradise.",
    "version": "0.0.3",
    "permissions": [
        "scripting",
        "storage",
        "activeTab"
    ],
    "host_permissions": [
        "https://*.youtube.com/*"
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "assets/images/*.png",
                "css/youtube-styles.css"
            ],
            "matches": [
                "https://*.youtube.com/*"
            ]
        }
    ],
    "content_scripts": [
        {
            "matches": [
                "https://*.youtube.com/*"
            ],
            "js": [
                "contentScript.js"
            ],
            "run_at": "document_end"
        }
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_title": "Youtube Customizer - Thumbnail Blocker",
        "default_popup": "popup.html",
        "default_icon": "icon-48.png"
    },
    "icons": {
        "16": "icon-16.png",
        "48": "icon-48.png",
        "128": "icon-128.png"
    }
}