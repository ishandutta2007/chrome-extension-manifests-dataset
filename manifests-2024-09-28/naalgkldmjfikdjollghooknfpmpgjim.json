{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "BubbleAI Extension",
  "description": "BubbleAI is a crypto off-chain data analysis platform with the fastest news capture and top-notch AI sentiment analysis tool",
  "version": "1.0.6",
  "manifest_version": 3,
  "action": {
    "default_icon": "assets/imgs/icon128x128.png",
    "default_title": "Bubble AI"
  },
  "background": {
    "service_worker": "./src/background/index.js",
    "type": "module"
  },
  "icons": {
    "16": "assets/imgs/icon16x16.png",
    "48": "assets/imgs/icon48x48.png",
    "128": "assets/imgs/icon128x128.png"
  },
  "options_page": "bubbleAiOptions.html",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "exclude_matches": [
        "https://test-bba.bba10000u.xyz/*",
        "https://bubbleai.xyz/*",
        "http://localhost:3000/*"
      ],
      "js": ["src/content/index.js"],
      "css": ["assets/css/index.css"],

      "run_at": "document_end",
      "all_frames": false
    }
  ],
  "externally_connectable": {
    "matches": [
      "*://localhost/*",
      "*://*.bubbleai.xyz/*",
      "*://*.bba10000u.xyz/*"
    ]
  },
  "host_permissions": [
    "https://www.binance.com/",
    "https://bubbleai.xyz/",
    "https://test-bba.bba10000u.xyz/",
    "http://localhost:3000/"
  ],
  "permissions": ["contextMenus", "storage", "cookies"],
  "omnibox": { "keyword": "Bubble AI" },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+Y",
        "mac": "Command+Shift+Y"
      },
      "description": "Opens bubbleAiOptions.html"
    }
  }
}
