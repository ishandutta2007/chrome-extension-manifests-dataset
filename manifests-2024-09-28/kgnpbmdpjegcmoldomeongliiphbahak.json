{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Layup Extension",
  "description": "Layup Attribution Extension",
  "version": "0.1",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "oauth2": {
    "client_id": "layup-web-extension",
    "scopes": ["openid", "profile", "email"]
  },
  "permissions": ["identity", "storage", "activeTab", "tabs"],
  "host_permissions": [
    "https://www.linkedin.com/*",
    "https://*.twitter.com/*",
    "https://www.threads.net/*"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.linkedin.com/*",
        "https://*.twitter.com/*",
        "https://www.threads.net/*"
      ],
      "js": ["content_entry.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["*"],
      "matches": [
        "https://www.linkedin.com/*",
        "https://*.twitter.com/*",
        "https://www.threads.net/*"
      ]
    }
  ],
  "icons": {
    "16": "layupicon16x16.png",
    "48": "layupicon48x48.png",
    "128": "layupicon128x128.png"
  }
}
