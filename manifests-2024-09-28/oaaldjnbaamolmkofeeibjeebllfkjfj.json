    "open-custom-shortcut-1": {
      "suggested_key": {
        "default": "Ctrl+Shift+1",
        "mac": "Command+Shift+1"
      },
      "description": "Open custom shortcut for the second slot"
    },
    "open-custom-shortcut-2": {
      "suggested_key": {
        "default": "Ctrl+Shift+2",
        "mac": "Command+Shift+2"
      },
      "description": "Open custom shortcut for the third slot"
    }
  },
  "icons": {
    "16": "images/icon16.png",
