{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "__MSG_EXT_NAME__",
  "description": "__MSG_EXT_DESCRIPTION__",
  "version": "1.0.0",
  "manifest_version": 3,
  "default_locale": "en",
  "permissions": [
    "tabs",
    "declarativeNetRequest",
    "storage",
    "contextMenus"
  ],
  "host_permissions": [
    "\u003Call_urls\u003E"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content_script.js"
      ],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "24": "icons/icon24.png",
