{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "AuthX Password Manager",
  "version": "5.32",
  "manifest_version": 3,
  "description": "AuthX Protected Applications",
  "action": {
    "default_icon": "./assets/authx.png",
    "default_popup": "./IndexPage/portalapps.html"
  },
  "background": {
    "service_worker": "./background.js"
  },

  "icons": {
    "16": "./assets/authx.png",
    "32": "./assets/authx.png",
    "48": "./assets/authx.png",
    "128": "./assets/authx.png"
  },

  "web_accessible_resources": [
    {
      "resources": [
        "/Login/LoginScreen.html",
        "/Login/LoginScreen.js",
        "/SavePasswordPopUp/Savepassword.html",
        "/assets/images/logo.svg",
        "/assets/authx.png"
      ],
      "matches": [ "<all_urls>" ]
    }
  ],

  "content_scripts": [
    {
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at": "document_idle",
      "js": [
        "/assets/js/jquery.min.js",
        "/assets/js/common/browserutilities.js",
        "/assets/js/common/AuthxLogManager.js",
        "/assets/js/common/settings.js",
        "/assets/js/common/ApiService.js",
        "autofill.js",
        "/assets/js/cryptoutil.js",
        "/assets/js/FillPassword.js"
      ],
      "type": "module"
    }
  ],
  "permissions": [
    "tabs",
    "scripting",
    "activeTab",
    "storage",
    "nativeMessaging",
    "cookies",
    "webNavigation",
    "identity"
  ]
}
