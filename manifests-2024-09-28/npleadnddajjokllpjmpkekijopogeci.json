  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["scripting", "activeTab", "tabs"],
  "host_permissions": [
    "https://github.com/*?tab=repositories",
    "https://github.com/*/settings"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*?tab=repositories",
        "https://github.com/*/settings"
      ],
      "run_at": "document_idle",
      "js": ["contentScript.js"]
    }
  ]
