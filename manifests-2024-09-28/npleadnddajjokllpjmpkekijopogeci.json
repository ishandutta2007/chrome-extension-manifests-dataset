  "manifest_version": 3,
  "name": "Github Bulk Delete",
  "version": "0.1.0",
  "description": "Chrome Extension to bulk delete github repositories",
  "icons": {
    "16": "icons/icon_16.png",
    "32": "icons/icon_32.png",
    "48": "icons/icon_48.png",
