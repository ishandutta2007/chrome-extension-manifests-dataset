name: X Automation
version: 2.2.0
category_slug: lifestyle/social
rating: null
rating_count: null
user_count: 15
release_date: '2024-05-22T10:09:00.000Z'
size: 1.29MiB
languages:
  - English
description: X/Twitter Automation, it send dm to followers, list.
privacy_policy: https://robustemail.com/privacy-policy-2
---

{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 3,
  "name": "__MSG_extName__",
  "description": "__MSG_extDescription__",
  "default_locale": "en",
  "version": "2.2.0",
  "action": {
    "default_icon": "./assets/TWEEKSOCIAL ICON.png",
    "default_popup": "./dist/popup/index.html"
  },
  "icons": {
    "16": "./assets/TWEEKSOCIAL ICON.png",
    "48": "./assets/TWEEKSOCIAL ICON.png",
    "128": "./assets/TWEEKSOCIAL ICON.png"
  },
  "permissions": [
    "cookies",
    "storage",
    "tabs",
    "unlimitedStorage"
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "host_permissions": [
    "*://*.twitter.com/*",
    "*://*.x.com/*"
  ],
  "background": {
    "service_worker": "./dist/background/index.mjs"
  },
  "content_scripts": [
    {
      "run_at": "document_start",
      "matches": [
        "https://*.twitter.com/*",
        "https://*.x.com/*"
      ],
      "js": [
        "./dist/contentScripts/index.global.js"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "dist/contentScripts/style.css",
        "assets/*"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}