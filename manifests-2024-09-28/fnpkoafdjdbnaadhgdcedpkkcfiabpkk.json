{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "Ejudge+",
  "version": "2.2.0",

  "icons": {
    "16": "img/main16.png",
    "32": "img/main32.png",
    "48": "img/main48.png",
