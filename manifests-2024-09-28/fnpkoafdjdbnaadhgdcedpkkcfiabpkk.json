      "matches": ["http://caos.ejudge.ru/*", "https://caos.ejudge.ru/*"],
      "css": ["css/style.css"],
      "run_at": "document_idle",
      "js": ["js/snowflakes.js", "js/userscript.js"]
    }
  ],

  "web_accessible_resources": [
    "img/logo.png",
    "img/newyear.png"
  ],

