  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "default.html"
  },
  "background": {
    "service_worker": "scripts/background.js"
  },
  "host_permissions": [
    "https://monorepo-info.app.twosigma.com/api/v1/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://sourcegraph.app.twosigma.com/*",
        "https://sourcegraphqa.app.twosigma.com/*"
      ],
      "run_at": "document_idle",
      "js": [
        "scripts/display-commit-cr.js"
      ],
      "css": [
        "styles/button.css"
      ]
    },
    {
      "js": [
        "scripts/swmi_link.js"
      ],
      "matches": [
        "https://sourcegraph.app.twosigma.com/*",
        "https://sourcegraphqa.app.twosigma.com/*"
      ],
      "css": [
        "styles/sourcegraph_link.css"
      ]
    }
  ],
  "icons": {
    "16": "images/sg16.png",
    "32": "images/sg32.png",
    "48": "images/sg48.png",
    "128": "images/sg128.png"
