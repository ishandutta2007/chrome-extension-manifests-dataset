{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Expert AdBlocker",
  "description": "Block ads on YouTube, HBO Max, Disney Plus, and more with OTT Ad Blocker. Enjoy uninterrupted, ad-free entertainment!",
  "version": "1.2.0",
  "manifest_version": 3,
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "TrueAdBlocker Expert AdBlocker - free adblocker",
    "default_icon": "logo16.png"
  },
  "permissions": ["storage", "tabs"],
  "host_permissions": ["<all_urls>"],
  "options_page": "options.html",
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js", "sponsered.js"]
    },
    {
      "matches": ["https://open.spotify.com/*"],
      "js": ["contentScriptSpotify.js"],
      "run_at": "document_start"
    },
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["Youtube.js"]
    },
    {
      "matches": ["https://www.peacocktv.com/*"],
      "js": ["peacock.js"]
    },
    {
      "matches": ["https://www.peacocktv.com/*"],
      "js": ["peacock.js"]
    },
    { "matches": ["https://play.max.com/*"], "js": ["hbomax.js"] },
    {
      "matches": ["*://*.crunchyroll.com/*"],
      "js": ["crunchyroll.js"],
      "match_origin_as_fallback": true,
      "all_frames": true
    },
    {
      "matches": ["https://www.hulu.com/*"],
      "js": ["hulu.js"]
    },
    {
      "matches": ["https://*.twitch.tv/*"],
      "js": ["adRemove.js", "removeTwitchAds.js"],
      "run_at": "document_start",
      "match_origin_as_fallback": true,
      "all_frames": true
    },
    {
      "matches": ["https://www.jiocinema.com/*"],
      "js": ["jioCinema.js"],
      "match_origin_as_fallback": true,
      "all_frames": true
    },
    {
      "matches": ["https://www.sonyliv.com/*"],
      "js": ["sonyliv.js"],
      "match_origin_as_fallback": true,
      "all_frames": true
    },
    {
      "matches": ["https://www.hotstar.com/*"],
      "js": ["hotstar.js"],
      "match_origin_as_fallback": true,
      "all_frames": true
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["adRemove.js", "removeTwitchAds.js"],
      "matches": ["https://*.twitch.tv/*"]
    },
    {
      "resources": ["crunchyroll.js"],
      "matches": ["https://www.crunchyroll.com/*"]
    },
    {
      "resources": [
        "contentScriptSpotify.js",
        "adsSweetalertSpotify.js",
        "adsAdsRemoveSpotify.js",
        "adswsHooksSpotify.js"
      ],
      "matches": ["https://open.spotify.com/*"]
    }
  ]
}
