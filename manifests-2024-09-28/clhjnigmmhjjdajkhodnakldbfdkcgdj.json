  "action": {
    "default_icon": {
      "16": "icon16.png",
      "32": "icon32.png",
      "48": "icon48.png"
    }
  },
  "web_accessible_resources": [
    {
      "resources": ["/static/css/content_script.css"],
      "matches": ["<all_urls>"]
    }
  ]
}
