{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "default_locale": "en",
  "name": "__MSG_extensionName__",
  "version": "0.3.3",
  "description": "__MSG_extensionDescription__",
  "permissions": [
    "history",
    "bookmarks",
