{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Automatic YouTube Ad Placer | TurboPlacer",
  "version": "2.1.0",
  "description": "Automate YouTube Video Ad Placing, Maximize your revenue potential with TurboPlacer!",
  "permissions": [
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
    "background": {
        "service_worker": "background.js"
    },
  "content_scripts": [
    {
      "matches": ["https://studio.youtube.com/*"],
      "js": ["ExtPay.js","content.js"],
      "run_at": "document_idle"
    },
    {
      "matches": ["https://extensionpay.com/*"],
      "js": ["ExtPay.js"],
      "run_at": "document_start"
  }
],

  "web_accessible_resources": [
    {
      "resources": ["styles.css", "ExtPay.js","popup.js","popup.html","content.js","background.js","icons/icon16.png","icons/icon48.png","icons/icon64.png","icons/icon128.png","icons/bolt.png","icons/wordmark.png"],
      "matches": ["https://studio.youtube.com/*", "https://extensionpay.com/*"]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },

  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'",
    "content_scripts": "connect-src https://extensionpay.com"
  }
  
}