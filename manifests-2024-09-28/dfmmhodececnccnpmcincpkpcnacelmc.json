{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "MMK.LOCAL Jitsi Desktop Streamer",
  "description": "A simple extension that allows you to stream your desktop into MMK.LOCAL meetings with Jitsi Meet and Jitsi Videobridge.",
  "version": "0.1.6",
  "minimum_chrome_version": "34",
  "icons": {
    "16": "jitsi-logo-16x16.png",
    "48": "jitsi-logo-48x48.png",
    "128": "jitsi-logo-128x128.png"
  },
  "background": {
        "scripts": [ "background.js" ],
        "persistent": true
  },
  "permissions": [
      "desktopCapture"
  ],
  "externally_connectable": {
      "matches": [
          "*://meet.mmk.local/*"
      ]
  }
}
