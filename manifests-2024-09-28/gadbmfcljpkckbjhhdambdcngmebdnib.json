  "content_scripts": [
    {
      "match_about_blank": true,
      "all_frames": false,
      "js": [
        "./js/app.js"
      ],
      "run_at": "document_idle",
      "matches": [
        "*://*/*"
      ]
    }
  ],
  "background": {
    "service_worker": "./js/bg.js"
  }
