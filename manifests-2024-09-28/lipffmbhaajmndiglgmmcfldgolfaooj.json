  "background": {
    "scripts": ["dist/background.js"],
    "persistent": false
  },
  "permissions": ["tabs", "https://*.github.com/*", "storage"],
  "content_scripts": [
    {
      "matches": ["https://*.github.com/*"],
      "js": ["dist/inject/inject.js"],
      "css": ["css/styles.css"]
    }
  ]
