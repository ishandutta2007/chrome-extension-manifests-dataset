{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Netflix Tab Manager",
  "version": "1.0",
  "description": "Replaces existing Netflix tab with a new one if a second Netflix page is opened.",
  "permissions": ["tabs"],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "http://*.netflix.com/*",
    "https://*.netflix.com/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
}

