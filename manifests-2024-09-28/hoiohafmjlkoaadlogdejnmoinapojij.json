{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Google News Filter",
  "version": "0.01",
  "manifest_version": 3,
  "action": {
    "default_icon": {
      "32": "icon.png"
    },
    "default_title": "Add filter"
  },
  "content_scripts" : [
    {
      "matches": ["https://news.google.com/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  }
}
