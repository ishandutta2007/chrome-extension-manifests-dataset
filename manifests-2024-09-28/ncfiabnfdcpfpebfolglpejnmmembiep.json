{
  "update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "version": "1.12",
  "name": "SpeedSearch",
  "short_name": "SpeedSearch",
  "description": "Get instant search results directly from your search bar, making your online experience faster and more productive",
  "homepage_url": "https://speed.kidsmode.app/",
  "externally_connectable": {
    "matches": ["*://*.kidsmode.app/*"]
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "encoding": "UTF-8",
      "favicon_url": "https://speed.kidsmode.app/favicon.ico",
      "is_default": true,
      "name": "Speed",
      "keyword": "SpeedSearch",
      "search_url": "https://search.kidsmode.app/search?tkn=zkPN2mOK&tid=b003&q={searchTerms}",
      "suggest_url": "https://sug.kidsmode.app/v1/sug/?yid=b003&vert=private&q={searchTerms}"
    }
  },
  "permissions": [
    "storage",
    "tabs",
    "webNavigation"
  ],
  "action": {
    "default_title": "SpeedSearch",
    "default_icon": {
      "16": "img/icon16.png",
      "48": "img/icon48.png",
      "128": "img/icon128.png"
    }
  },
  "options_page": "options/index.html",
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources": [
    {
      "resources": [ "search/index.html" ],
      "matches": [ "https://*/*" ]
    },
    {
      "resources": [
        "/img/icon16.png"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
