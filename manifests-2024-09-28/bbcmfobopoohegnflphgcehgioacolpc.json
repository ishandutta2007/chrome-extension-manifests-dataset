    "content_scripts": [
        {
            "matches": ["https://amazon.in/*", "http://amazon.in/*", "https://www.amazon.in/*", "http://www.amazon.in/*"],
            "js": ["content.js"],
            "run_at": "document_end"
        }
    ],
    "background": {
        "scripts":["background.js"]
    },
    "content_security_policy": "script-src 'self' https://apis.google.com/; object-src 'self'"
