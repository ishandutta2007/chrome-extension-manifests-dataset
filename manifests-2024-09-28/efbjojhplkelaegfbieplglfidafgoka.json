{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "VT4Browsers",
  "version": "4.0.10",
  "default_locale": "en",
  "description": "VirusTotal Browser Extension.",


  "icons": {
    "16": "icons/vt-16.png",
    "32": "icons/vt-32.png"
  },

  "action": {
    "default_icon": "icons/vt_logo.png",
    "default_popup": "src/options.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "exclude_matches": [
        "*://*.virustotal.com/*"
      ],
      "css": ["src/styles.css"],
      "js": [
        "src/vt-augment.min.js",
        "src/jquery.min.js",
        "src/virustotal.js"]
    }
  ],
  "background": {
    "service_worker": "src/background.js"
  },
  "options_ui": {
    "page": "src/options.html"
  },
  "permissions": [
    "webRequest",
    "contextMenus",
    "downloads",
    "storage",
    "tabs"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "commands": {
    "vthighlight": {
      "description": "One-off highlighting on current page"
    },
    "vtenrich": {
      "description": "One-off enrichment on current page"
    }
  },
  "web_accessible_resources": [
    { "resources" : ["icons/vt-logo.svg"],
      "matches": ["<all_urls>"]
    }
  ]
}
