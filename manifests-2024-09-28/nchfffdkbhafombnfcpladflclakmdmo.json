    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    },
    "background": {
      "service_worker": "serviceWorker.js"
    },
    "content_scripts": [
      {
        "matches": ["https://www.skool.com/*"],
        "js": [
          "browser-polyfill.min.js",
          "content.js"
        ]
      }
    ],
    "action": {
      "default_popup": "popup.html"
    }    
  }
  