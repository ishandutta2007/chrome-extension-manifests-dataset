{
"update_url": "https://clients2.google.com/service/update2/crx",

  "background": {
    "service_worker":"backend/background.js",
    "type": "module"
  },
  "minimum_chrome_version": "92",
  "default_locale": "en",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "assets/img/16.png",
      "32": "assets/img/32.png",
      "48": "assets/img/48.png",
      "128": "assets/img/128.png"
    }
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self';",
    "sandbox": "sandbox allow-scripts allow-forms allow-popups allow-modals; script-src 'self' 'unsafe-inline' 'unsafe-eval'; child-src 'self';"
  },
  "content_scripts": [
    {
      "all_frames": false,
      "js": [
        "content.js"
      ],
      "matches": [ "https://www.tellershive.com/*",
            "http://www.tellershive.com/*",
            "https://www.facebook.com/*",
            "https://www.linkedin.com/*"],
      "run_at": "document_start"
    }
  ],
  "description": "TellersHive",
  "homepage_url": "https://www.tellershive.com",
  "icons": {
    "128": "assets/img/128.png"
  },
  "manifest_version": 3,
  "name": "TellersHive - Build your brand with personalized content.",
  "permissions": [
    "identity",
    "activeTab",
    "webNavigation",
    "tabs"
  ],
  "host_permissions": [ "https://www.tellershive.com/*",
            "http://www.tellershive.com/*",
            "https://www.facebook.com/*",
            "https://www.linkedin.com/*"],
  "web_accessible_resources": [
    {
      "resources": [ "assets/*", "fonts/*" ,"templates/*"],
      "matches": [ "https://www.tellershive.com/*",
            "http://www.tellershive.com/*",
            "https://www.facebook.com/*",
            "https://www.linkedin.com/*"]
    }
  ],
  "version": "37",
  "short_name": "TellersHive Extension"
