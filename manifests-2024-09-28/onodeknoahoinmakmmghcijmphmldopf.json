{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Files Connect",
  "description": "Integrate Google Docs, Sheets, Slides and Drive folders with Salesforce in minutes.",
  "version": "1.0.6",
  "manifest_version": 3,
  "action": {
    "default_title": "Files Connect App"
  },
  "background": {
    "service_worker": "background/background.js",
    "type": "module",
    "minimum_chrome_version": 92
  },
  "options_page": "pages/options/options.html",
  "permissions": [
    "background",
    "declarativeContent",
    "storage",
    "identity"
  ],
  "oauth2": {
    "client_id": "910252102107-g6af3qhnf40c65i67femhtdsia1165ud.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/drive.metadata"
    ]
  },
  "host_permissions": [
    "https://docs.google.com/*",
    "https://drive.google.com/*",
    "https://www.googleapis.com/*",
    "https://*.salesforce.com/*"
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "pages/options/options.html",
        "pages/popup/popup.html"
      ],
      "matches": [
        "*://*/*",
        "*://*/*"
      ]
    }
  ],
  "content_scripts": [
    {
      "matches": [
        "https://docs.google.com/*",
        "https://drive.google.com/*"
      ],
      "js": ["background/inject-html.js"],
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "16": "images/n16.png",
    "32": "images/n32.png",
    "48": "images/n48.png",
    "64": "images/n64.png",
    "128": "images/n128.png"
  }
}
