{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Top plugin",
  "version": "1.1",
  "description": "Connect with people on LinkedIn",
  "permissions": [
    "activeTab",
    "https://www.linkedin.com/*"
  ],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "logo16.png",
      "48": "logo48.png",
      "128": "logo128.png"
    }
  },
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.linkedin.com/in/*"],
      "js": ["contentScript.js"],
      "run_at": "document_end"
    }
  ]
}