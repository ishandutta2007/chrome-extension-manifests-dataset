{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "4ccessible",
  "description": "Empower your browsing experience with voice commands! Our tool makes the web accessible and navigable for everyone",
  "version": "1.0.3",
  "permissions": ["storage"],
  "commands": {
    "trigger_voice_prompt": {
      "suggested_key": {
        "default": "Ctrl+Shift+Y"
      },
      "description": "Start/Stop the voice prompt"
    }
  },
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*", "http://*/*"],
      "js": ["contentScript.js"]
    }
  ],
  "host_permissions": ["<all_urls>"],
  "web_accessible_resources": [
    {
      "resources": ["assets/*"],
      "matches": ["<all_urls>"]
    }
  ],
  "icons": {
    "16": "assets/images/icon-16.png",
    "32": "assets/images/icon-32.png",
    "48": "assets/images/icon-48.png",
    "128": "assets/images/icon-128.png"
  }
}
