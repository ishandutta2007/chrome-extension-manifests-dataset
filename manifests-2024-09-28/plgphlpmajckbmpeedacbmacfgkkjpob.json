{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "Playlist Extension for Chrome",
    "version": "0.0.1",
    "permissions": ["notifications"],
    "content_scripts": [
      {
        "matches": ["http://*/*", "https://*/*"],
        "js": [
          "scripts/custom-elements.js",
          "scripts/polyfills.js",
          "scripts/main.js"
        ]
      },
      {
        "matches": ["http://*/*", "https://*/*"],
        "js": ["app.js"]
      }
    ],
    "background": {
        "service_worker": "background.js"
    },
    "web_accessible_resources": [
      {
        "resources": [
          "scripts/custom-elements.js",
          "scripts/polyfills.js",
          "scripts/main.js",
          "scripts/config.json"
        ],
        "matches": ["http://*/*", "https://*/*"]
      }
    ],
    "icons": {
      "12": "assets/Icon.png",
      "48": "assets/Icon.png",
      "128": "assets/Icon.png"
    }
  }
  