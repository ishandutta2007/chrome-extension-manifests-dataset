"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "Focus Lock",
    "version": "1.0",
    "description": "Block distracting websites until you meet your productivity goals.",
    "permissions": [
      "storage",
      "webNavigation",
      "webRequest",
      "webRequestBlocking"
