{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Simple Refresh",
  "version": "1.0",
  "description": "A simple web page refresh extension.",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "assets/icon16.png",
    "48": "assets/icon48.png",
    "128": "assets/icon128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "assets/icon16.png",
      "48": "assets/icon48.png",
      "128": "assets/icon128.png"
    }
  },
  "web_accessible_resources": [
    {
      "resources": [
        "popup.html",
        "popup.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "default_locale": "en"
}