{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Image Saver",
  "description": "Save your assets to Playbook",
  "version": "4.9",
  "icons": {
    "16": "src/icons/16x16.png",
    "48": "src/icons/48x48.png",
    "128": "src/icons/128x128.png"
  },
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "js": [
        "src/Notification.tsx-loader.js",
        "src/saveAllImagesToPreview.ts-loader.js",
        "src/captureArea.ts-loader.js",
        "src/getSharedBoardSlugs.ts-loader.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "css": [
        "src/Notification.css",
        "src/captureArea.css"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "resources": [
        "src/icons/alert.svg",
        "src/icons/tick.svg",
        "src/icons/32x32.png"
      ],
      "use_dynamic_url": false
    },
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "resources": [
        "src/AkkuratLLSub-RegularSubset.woff2",
        "src/AkkuratLLSub-RegularSubset.woff",
        "src/AkkuratLLSub-BoldSubset.woff2",
        "src/AkkuratLLSub-BoldSubset.woff",
        "src/client.js",
        "src/Notification.tsx.js",
        "src/saveAllImagesToPreview.ts.js",
        "src/captureArea.ts.js",
        "src/getSharedBoardSlugs.ts.js"
      ],
      "use_dynamic_url": true
    }
  ],
  "host_permissions": [
    "*://*.playbook.com/*"
  ],
  "permissions": [
    "storage",
    "identity",
    "contextMenus",
    "activeTab",
    "scripting",
    "tabs",
    "cookies",
    "downloads"
  ],
  "action": {
    "default_popup": "src/components/Popup/Popup.html"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'; connect-src https://api.segment.io https://*.sentry.io https://cdn.segment.com https://be.playbook.com https://storage.googleapis.com blob: data:"
  }
