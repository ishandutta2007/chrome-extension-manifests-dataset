{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "Fonts Try - FTS",
    "description": "Fonts Try là một extension cho phép người dùng có thể thay đổi font chữ của các trang web.",
    "version": "1.0.1",
    "permissions": ["activeTab", "scripting", "storage"],
    "default_locale": "en",
    "host_permissions": ["*://*/*"],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup/popup.html"
    },
    "content_scripts": [
        {
          "matches": ["http://*/*", "https://*/*"],
          "js": [
            "libs/swiper/swiper-bundle.min.js",
            "content.js"
          ],
          "css": [
            "libs/swiper/swiper-bundle.min.css",
            "styles.css"
          ]
        }
    ],
    "web_accessible_resources": [
        {
            "resources": ["images/*"],
            "matches": ["*://*/*"]
        },
        {
            "resources": [
                "libs/swiper/swiper-bundle.min.css",
                "libs/swiper/swiper-bundle.min.js"
            ],
            "matches": ["*://*/*"]
        }
    ]
}
