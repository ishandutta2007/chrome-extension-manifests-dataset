{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "DarkModeExtension",
  "version": "1.0",
  "description": "Chrome extension for instant dark mode/light mode switching on websites!",
  "permissions": ["activeTab", "scripting", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "commands": {
    "toggle_dark_mode": {
      "suggested_key": {
        "default": "Ctrl+Shift+D"
      },
      "description": "Toggle Dark Mode"
    },
    "toggle_light_mode": {
      "suggested_key": {
        "default": "Ctrl+Shift+L"
      },
      "description": "Toggle Light Mode"
    },
    "toggle_night_light": {
      "suggested_key": {
        "default": "Ctrl+Shift+T"
      },
      "description": "Toggle Night Light"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
