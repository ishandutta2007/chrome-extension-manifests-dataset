{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Block Grabify",
  "version": "1.0",
  "description": "Blocks any requests to grabify.link.",
  "permissions": [
    "declarativeNetRequest"
  ],
  "host_permissions": [
    "*://*/*"
  ],
  "declarative_net_request": {
    "rule_resources": [{
      "id": "redirect_grabify",
      "enabled": true,
      "path": "rules.json"
    }]
  },
  "icons": {
    "16": "icons/16x16.png",
    "32": "icons/32x32.png",
    "48": "icons/icon.png",
    "128": "icons/icon.png"
  }
}
