{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "ScreenSheet - Screenshots to xlsx, csv and more",
  "version": "1.0.2",
  "description": "Transform screenshots into spreadsheets in one click",
  "permissions": [
    "activeTab",
    "storage",
    "downloads",
    "identity",
    "identity.email"
  ],
  "action": {
    "default_title": "ScreenSheet - Screenshots to xlsx, csv and more",
    "default_popup": "index.html"
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "background": {
    "service_worker": "static/js/background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["static/css/content.css"],
      "js": ["static/js/content.js"]
    }
  ]
}