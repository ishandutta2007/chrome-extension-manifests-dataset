{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "GitHub PR Template Picker",
  "version": "0.1.0",
  "description": "A browser extension that allows you to select GitHub PR templates",
  "permissions": [
    "activeTab",
    "identity",
    "tabs",
    "storage"
  ],
  "browser_action": {
    "default_title": "GitHub PR Template Picker",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/key16.png",
      "32": "images/key32.png",
      "128": "images/key128.png"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "*://github.com/*"
      ],
      "run_at": "document_end",
      "js": [
        "content.js"
      ],
      "css": [
        "global.css"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "icons": {
    "16": "images/key16.png",
    "32": "images/key32.png",
    "128": "images/key128.png"
  },
  "manifest_version": 2
}
