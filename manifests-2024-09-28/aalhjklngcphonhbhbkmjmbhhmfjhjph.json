{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Navigation Timer",
  "version": "1.0",
  "description": "Track the time you spent in each website",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
