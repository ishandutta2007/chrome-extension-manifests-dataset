{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "Pokémon Trading Card Game(PTCG) printer",
    "version": "1.0.5",
    "description": "Create your handmade PTCG card in one click",
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "ptcg.html",
      "default_icon": {
        "16": "icons/v2/icon16.png",
        "48": "icons/v2/icon48.png",
        "128": "icons/v2/icon128.png"
      }
    },
    "icons": {
      "16": "icons/v2/icon16.png",
      "48": "icons/v2/icon48.png",
      "128": "icons/v2/icon128.png"
    },
    "permissions": [
      "activeTab",
      "contextMenus",
      "storage"
    ],
    "host_permissions": [
      "https://www.pokemon.com/*",
      "https://asia.pokemon-card.com/*",
      "https://www.pokemon-card.com/*"
    ],
    "content_scripts": [
      {
        "matches": ["https://www.pokemon.com/*", "https://asia.pokemon-card.com/*", "https://www.pokemon-card.com/*"],
        "js": ["content.js"]
      }
    ],
    "default_locale": "en"
  }
