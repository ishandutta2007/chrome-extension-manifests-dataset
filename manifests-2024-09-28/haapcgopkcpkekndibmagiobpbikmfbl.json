{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Side Panel for Gemini",
  "version": "0.5.1",
  "manifest_version": 3,
  "description": "Opens Gemini AI in Google Chrome's side panel",
  "host_permissions": ["*://*.google.com/*", "*://*.googleusercontent.com/*"],
  "action": {
      "default_icon": {
        "16": "images/icon-16-1.png",
        "128": "images/icon-128-1.png"
      }
    },
  "commands": {
  "_execute_action": {
    "suggested_key": {
        "default": "Alt+G"
      },
    "description" : "Start the extension"
    }
  },
  "background": {
    "service_worker": "service-worker.js"
  },
  "icons": {
    "128": "images/icon-128-1.png",
    "16": "images/icon-16-1.png"
  },
  "side_panel": {
    "default_path": "sidepanel.html"
  },
  "permissions": [
    "sidePanel",
    "declarativeNetRequest",
    "storage"
  ],
  "web_accessible_resources": [
    {
      "resources": ["sidepanel.html","service-worker.js"],
      "matches": ["<all_urls>"],
      "use_dynamic_url": true
    }
  ],
  "options_page": "options.html"
}
