{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Maps Hub Search",
  "version": "2.0.9",
  "manifest_version": 3,
  "description": "Get accurate maps & directions while ensuring web searches lead you there with geo-optimized Yahoo as your default search.",
  "homepage_url": "https://www.newtabtools.com/",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_icon": "icons/icon48.png"
  },
  "background": {
    "service_worker": "js/bg/mh_background.js"
  },
  "permissions": [
    "alarms",
    "tabs",
    "storage",
    "topSites",
    "history",
    "search"
  ],
  "host_permissions": [
    "*://*.newtabtools.com/*",
    "*://*.yahoo.com/*"
  ],
  "content_scripts": [{
  "matches": ["http://*.newtabtools.com/*", "https://*.newtabtools.com/*"],
      "js": ["js/bg/mh_ext.js"]
  },{
    "matches": ["http://*.yahoo.com/*","https://*.yahoo.com/*","http://*.newtabtools.com/*", "https://*.newtabtools.com/*"],
    "all_frames": true,
    "js": ["js/mh_events.js"]
  }],
  "chrome_url_overrides": {
    "newtab": "mh_new_tab.html"
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "encoding": "UTF-8",
      "favicon_url": "https://www.newtabtools.com/assets/images/yahoo-favicon.ico",
      "is_default": true,
      "keyword": "Yahoo",
      "name": "Yahoo Partner",
      "search_url": "https://www.newtabtools.com/search?p={searchTerms}&aid=25",
      "suggest_url": "https://www.newtabtools.com/search/suggest?q={searchTerms}"
    }
  },
  "web_accessible_resources": [
    {
      "resources": [
        "images/*"
      ],
      "matches": [
        "*://*/*"
      ]
    }
  ]
}