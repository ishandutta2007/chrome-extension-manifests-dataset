{
"update_url": "https://clients2.google.com/service/update2/crx",

    "name": "Skipp",
    "version": "1.0.0.1",
    "description": "Skips Netflix, Prime, Hotstar intro scene so you dont have to",
    "manifest_version": 3,
    "author": "Edson V Reginold",
    "icons": {
        "16": "white-logo.png",
        "32": "white-logo.png",
        "48": "white-logo.png",
        "128": "white-logo.png"
      },
    "action": {
        "default_popup": "index.html",
        "default_title": "Skipp"
    },
    "content_scripts": [
        {
            "matches": [
                "https://*.netflix.com/*",
                "https://netflix.com/*",
                "https://*.primevideo.com/*",
                "https://*.hotstar.com/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "permissions": [
        "storage"
      ],
    "web_accessible_resources": [
        {
            "resources": [
                "inject.js"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ]
