{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "Watch With Me",
    "version": "0.21",
    "description": "Watch videos with friends and discuss them in real-time.",
    "permissions": [
        "activeTab",
        "scripting",
        "storage"
    ],
    "content_scripts": [
        {
            "js": [
                "src/content.tsx-loader.js"
            ],
            "matches": [
                "<all_urls>"
            ]
        },
        {
            "js": [
                "src/iframe.ts-loader.js"
            ],
            "all_frames": true,
            "matches": [
                "<all_urls>"
            ]
        }
    ],
    "background": {
        "service_worker": "service-worker-loader.js",
        "type": "module"
    },
    "action": {},
    "commands": {
        "_execute_action": {
            "suggested_key": {
                "default": "Ctrl+E",
                "mac": "Command+E"
            },
            "description": "Toggle watch with me overlay"
        }
    },
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "web_accessible_resources": [
        {
            "matches": [
                "<all_urls>"
            ],
            "resources": [
                "src/error.mp3",
                "src/join.mp3",
                "src/message.mp3",
                "chunks/utils.js",
                "chunks/video.js",
                "chunks/content.tsx.js",
                "chunks/netflix.ts.js",
                "chunks/iframe.ts.js"
            ]
        }
    ]
}