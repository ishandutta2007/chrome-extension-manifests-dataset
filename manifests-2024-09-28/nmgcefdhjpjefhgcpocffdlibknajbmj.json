{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "mymind — An extension for your mind",
  "description": "Add images, bookmarks, notes, quotes or text highlights to your new mind.",
  "short_name": "mymind",
  "version": "3.0.0",
  "author": "mymind, Inc",

  "manifest_version": 3,

  "permissions": [
    "cookies",
    "storage",
    "scripting",
    "background",
    "contextMenus",
    "activeTab"
  ],

  "host_permissions": [
    "https://access.mymind.com/*"
  ],

  "optional_permissions": [
    "webNavigation"
  ],

  "optional_host_permissions": [
    "https://www.instagram.com/*"
  ],

  "icons": {
    "16": "images/icon_16.png",
    "48": "images/icon_48.png",
    "128": "images/icon_128.png",
    "256": "images/icon_256.png",
    "512": "images/icon_512.png"
  },

  "background": {
    "service_worker": "worker.js",
    "type": "module"
  },

  "content_scripts": [
    {
      "matches": [ "*://*.mymind.com/extension*" ],
      "js": [ "utils/safari-authentication.js" ]
    }
  ],

  "web_accessible_resources": [
    {
      "resources": [
        "icons/*",
        "images/*",
        "components/*.css",
        "fonts/*"
      ],
      "matches": [
        "*://*/*"
        ]
    }  
  ],

  "action": {
    "default_title": "Save to my mind",
    "default_icon": {
      "256": "images/icon_256.png"
    }
  }
