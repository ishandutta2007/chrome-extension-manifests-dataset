{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "天星账号保管箱",
  "description": "轻松登录网站、生成密码和保存账号信息。给账号一个安全的家。",
  "default_locale": "zh_CN",
  "version": "5.2.3",
  "icons": {
    "16": "images/icon.png",
    "48": "images/icon.png",
    "128": "images/icon.png"
  },
  "background": {
    "type": "module",
    "service_worker": "js/serviceWorker.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/contentScriptsLogin.js"
      ],
      "run_at": "document_end",
      "all_frames": true
    }
  ],
  "permissions": [
    "privacy",
    "contextMenus",
    "declarativeNetRequest",
    "declarativeNetRequestFeedback",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "/components/selectorUserInfo.html",
        "/components/waitSaveUserInfo.html"
      ],
      "matches": [
        "*://*/*"
      ]
    }
  ],
  "externally_connectable": {
    "matches": [
      "*://*.weteoes.cn/*"
    ]
  }
}