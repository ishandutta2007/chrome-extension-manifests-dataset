{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "__MSG_extName__",
  "short_name": "ViinyxAI",
  "description": "__MSG_extDescription__",
  "version": "1.4.0",
  "icons": {
    "128": "assets/logo.png"
  },
  "action": {
    "default_popup": "popup/index.html"
  },
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "css": ["main.css", "fw-markdown.css"],
      "all_frames": true,
      "match_origin_as_fallback": true
    },
    {
      "matches": ["https://*/*"],
      "js": ["cs.bundle.js"],
      "css": ["assets/ReactToastify.css"]
    },
    {
      "matches": ["https://docs.google.com/document/*"],
      "run_at": "document_start",
      "js": ["gdocs/script.bundle.js"],
      "world": "MAIN"
    },
    {
      "matches": ["https://firefly.adobe.com/*"],
      "js": ["firefly-at.js"],
      "world": "MAIN"
    },
    {
      "matches": ["https://poe.com/*"],
      "js": ["poe-formKey.js"],
      "world": "MAIN"
    },
    {
      "matches": ["https://x.com/*"],
      "js": ["twitter-x.js"],
      "run_at": "document_start"
    },
    {
      "matches": ["https://*/*"],
      "js": ["cs-main.js"],
      "world": "MAIN",
      "runAt": "document_start"
    }
  ],
  "sandbox": {
    "pages": ["twitter-iframe.html"]
  },
  "web_accessible_resources": [
    {
      "resources": [
        "style.css",
        "assets/*",
        "docs-manager/*",
        "chatbox/*",
        "workers/*",
        "or-auth/*",
        "new-prompt/*",
        "twitter-iframe.html"
      ],
      "matches": ["https://*/*"]
    }
  ],
  "background": {
    "service_worker": "sw.bundle.js",
    "type": "module"
  },
  "options_page": "options/index.html",
  "permissions": [
    "contextMenus",
    "activeTab",
    "storage",
    "unlimitedStorage",
    "webRequest",
    "declarativeNetRequest",
    "clipboardWrite",
    "clipboardRead"
  ],
  "commands": {
    "open_chat": {
      "suggested_key": {
        "default": "Alt+V",
        "mac": "Alt+V"
      },
      "description": "Open the chat box modal inside the current active tab."
    }
  },
  "host_permissions": ["<all_urls>"],
  "content_security_policy": {
    "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self';",
    "sandbox": "sandbox allow-scripts allow-forms allow-popups allow-modals; script-src 'self' 'unsafe-inline' 'unsafe-eval'; child-src 'self';"
  },
  "externally_connectable": {
    "matches": ["https://*.viinyx.com/*"]
  },
  "default_locale": "en",
  "homepage_url": "https://viinyx.com",
  "author": {
    "email": "support@viinyx.com"
  }
}
