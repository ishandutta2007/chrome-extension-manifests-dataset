        "service_worker": "background.js",
        "type": "module"
    },
    "web_accessible_resources": [{
        "resources": ["imgs/icon.png"],
        "matches": [ "https://*.instagram.com/*" ]
    }],
    "icons": {
        "128": "imgs/icon.png"
