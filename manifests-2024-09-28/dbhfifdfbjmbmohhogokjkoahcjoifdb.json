{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "LinkedIn Job Search Language Filter",
  "description": "This extension filters job posts on the LinkedIn job search page regarding their content language.",
  "permissions": [],
  "version": "1.0.0.3",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": ["*://*.linkedin.com/jobs/search/*"]
    }
  ],
  "host_permissions": ["*://*.linkedin.com/jobs/search/*"]

}
