{
  "manifest_version": 3,
  "name": "Minecraft Wiki Redirector",
  "version": "1.02",
  "permissions": ["declarativeNetRequest"],
  "host_permissions": ["*://minecraft.fandom.com/*"],
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "ruleset_1",
        "enabled": true,
        "path": "rules.json"
      }
    ]
  },
  "action": {
    "default_icon": {
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "update_url": "https://clients2.google.com/service/update2/crx"
}
