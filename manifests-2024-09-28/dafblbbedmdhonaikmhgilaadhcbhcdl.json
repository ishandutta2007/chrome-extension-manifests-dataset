{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "LinkedIn Tech Recruiter SuperAssistant",
  "version": "0.0.4",
  "description": "An AI-powered helper to personalize your LinkedIn outreach to tech candidates and build your talent pipeline",
  "background": {
    "service_worker": "src/pages/background/index.js",
    "type": "module"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+Y",
        "mac": "Command+Shift+Y"
      }
    }
  },
  "action": {
    "default_popup": "src/pages/popup/index.html",
    "default_icon": "blueWave.jpg"
  },
  "icons": {
    "128": "blueWave.jpg"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.linkedin.com/*"
      ],
      "js": [
        "src/pages/content/index.js"
      ],
      "css": [
        "contentStyle.css"
      ]
    }
  ],
  "permissions": [
    "activeTab",
    "clipboardWrite",
    "clipboardRead",
    "notifications",
    "storage",
    "tabs"
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "contentStyle.css",
        "assets/img/blueWave.jpg"
      ],
      "matches": []
    }
  ]
}