{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Bookie Data Sharer",
  "version": "1.0.2",
  "description": "Shares data between betting sites and a tracking application",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "https://www.tab.co.nz/*",
    "https://www.sportsbet.com.au/*",
    "http://localhost:3000/*",
    "https://www.onthepunt.app/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.tab.co.nz/*",
        "https://www.sportsbet.com.au/*"
      ],
      "js": [
        "tab_content.js"
      ]
    },
    {
      "matches": [
        "http://localhost:3000/*",
        "https://www.onthepunt.app/*"
      ],
      "js": [
        "app_content.js"
      ]
    }
  ],
  "background": {
    "service_worker": "background.js"
  }
}