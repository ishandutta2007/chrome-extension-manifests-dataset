{
  "manifest_version": 3,
  "name": "__MSG_appTitle__",
  "description": "__MSG_appDesc__",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.4",
  "default_locale": "en",
  "action": {
    "default_icon": "icon16.png",
    "default_popup": "options.html?popup=true"
  },
  "options_page": "options.html",
  "background": {
    "service_worker": "goodchatbackground.js"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "css": [
        "goodchat.css",
        "sharedCss.css"
      ],
      "js": [
        "goodchat.js",
        "Tipped/jquery-3.5.1.min.js",
        "Tipped/tipped.min.js"
      ],
      "matches": [
        "https://www.youtube.com/live_chat*"
      ]
    },
    {
      "js": [
        "goodchatGeneral.js"
      ],
      "matches": [
        "https://www.youtube.com/*"
      ]
    }
  ],
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png",
    "16": "icon16.png"
  },
  "permissions": [
    "storage",
    "tabs",
    "unlimitedStorage"
  ],
  "host_permissions": [
    "https://www.youtube.com/*",
    "https://www.youtube.com/live_chat*"
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "pause.svg"
      ],
      "matches": [
        "https://www.youtube.com/*"
      ],
      "use_dynamic_url": true
    }
  ],
  "commands": {
    "enable_disable": {
      "suggested_key": {
        "default": "Ctrl+Shift+E",
        "mac": "Command+Shift+E"
      },
      "description": "Turn the extension on/off in a window."
    }
  }
}
