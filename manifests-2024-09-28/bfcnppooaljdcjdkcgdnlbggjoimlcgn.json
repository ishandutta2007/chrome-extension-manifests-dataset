{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "Kita Browser",
    "version": "3.0",
    "description": "Simple immersion tracker",
    "action": {
        "default_popup": "popup.html",
        "default_title": "Kita Browser",
        "default_icon": {
            "16": "/icons/enabled/icon16.png",
            "32": "/icons/enabled/icon32.png",
            "128": "/icons/enabled/icon128.png",
            "192": "/icons/enabled/icon192.png",
            "512": "/icons/enabled/icon512.png"
        }
    },
    "icons": {
        "16": "/icons/enabled/icon16.png",
        "32": "/icons/enabled/icon32.png",
        "128": "/icons/enabled/icon128.png",
        "192": "/icons/enabled/icon192.png",
        "512": "/icons/enabled/icon512.png"
    },
    "options_ui": {
        "page": "settings.html",
        "open_in_tab": true
    },
    "permissions": [
        "storage",
        "unlimitedStorage",
        "identity"
    ],
    "content_scripts": [
        {
            "matches": [
                "*://*.youtube.com/*",
                "*://*.crunchyroll.com/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "/icons/enabled/icon128.png",
                "/icons/saved/icon128.png"
            ],
            "matches": [
                "*://*.youtube.com/*",
                "*://*.crunchyroll.com/*"
            ]
        }
    ],
    "background": {
        "service_worker": "background.js"
    }
}