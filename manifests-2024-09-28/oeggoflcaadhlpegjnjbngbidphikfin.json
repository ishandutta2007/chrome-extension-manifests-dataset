{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 2,
    "name": "Diaspora Prime",
    "description": "The Diaspora extension enables you to checkout on Amazon and get your item shipped to your doorstep in Africa",
    "version": "1.1.5",
    "permissions": [
        "https://www.amazon.com/*",
        "https://www.amazon.co.uk/*",
        "https://www.amazon.fr/*",
        "https://api.sellerapp.com/*"
    ],
    "background": {
        "scripts": [
            "axios.min.js",
            "background.js"
        ]
    },
    "content_scripts": [
        {
            "matches": [
                "https://www.amazon.com/*",
                "https://www.amazon.co.uk/*",
                "https://www.amazon.fr/*"
            ],
            "js": [
                "jquery.min.js",
                "contentscript.js"
            ],
            "css": [
                "style.css",
                "modal.css"
            ]
        }
    ],
    "browser_action": {
        "default_icon": "icon.png"
    },
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "web_accessible_resources": [
        "banner.html",
        "modal.html",
        "loader.svg",
        "cart.svg"
    ]
}