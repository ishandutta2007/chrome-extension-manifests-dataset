  "action": {
    "default_icon": "img/icon48.png",
    "default_title": "popup",
    "default_popup": "popup.html"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self' ; object-src 'self'; frame-src 'self'; child-src 'self'",
