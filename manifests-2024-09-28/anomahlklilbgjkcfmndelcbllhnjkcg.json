{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "QoolLab",
  "homepage_url": "http://localhost:8080/",
  "description": "Useful GitLab -/issues/boards page",
  "default_locale": "en",
  "options_page": "options.html",
  "permissions": [
    "webRequest",
    "*://*/*/boards*",
    "tabs"
  ],
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "__MSG_extName__",
    "default_icon": {
      "19": "icons/19.png",
      "38": "icons/38.png"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*/boards*"
      ],
      "run_at": "document_idle",
      "js": [
        "js/gitlab-script.js",
        "js/restruct-card.js",
        "js/main.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "img/*"
  ],
  "version": "1.1.1",
  "content_security_policy": "script-src 'self' ; object-src 'self'"
}