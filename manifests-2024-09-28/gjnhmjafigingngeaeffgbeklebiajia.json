   "js": ["js/jquery.min.js", "js/content-script.js"],
   "run_at": "document_start"
  }
 ],
 "permissions": [
  "http://*/*",
  "https://*/*"
 ],
 "web_accessible_resources": ["js/jquery.min.js", "js/jquery.base64.js", "js/crawler.js"],
