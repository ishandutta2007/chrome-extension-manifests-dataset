{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Reglocs",
  "version": "1.1",
  "description": "Extension Reglocs, permettant de visualiser des zones à location court terme sur Google Maps",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  
  "content_scripts": [
    {
      "matches": [
        "https://www.centris.ca/*",
        "https://reglocs.com/*"
      ],
      "js": [
        "content-script.js"
      ]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Ajouter un cercle sur Google Maps"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "injected-script.js",
        "icons/icon16.png",
        "icons/icon32.png",
        "icons/icon48.png",
        "icons/icon128.png",
        "icons/icon16_active.png",
        "icons/icon32_active.png",
        "icons/icon48_active.png",
        "icons/icon128_active.png"
      ],
      "matches": [
        "https://www.centris.ca/*",
        "https://reglocs.com/*"
      ]
    }
  ]
}