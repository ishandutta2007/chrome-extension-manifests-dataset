        "54": "/img/icon_colored_54.png",
        "104": "/img/icon_colored_104.png",
        "128": "/img/icon_colored_128.png"
    },
    "options_page": "/html/options.html",
    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "windows": "Ctrl+Shift+P",
