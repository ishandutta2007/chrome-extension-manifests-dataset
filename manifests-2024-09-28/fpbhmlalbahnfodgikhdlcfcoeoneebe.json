{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Cosafe",
  "version": "2.0",
  "description": "Cosmetic Safety Chatbot",
  "permissions": [
    "contextMenus",
    "activeTab",
    "scripting",
    "sidePanel",
    "storage"
  ],

  "icons": {
    "16": "icons/cosafe16.png",
    "32": "icons/cosafe32.png",
    "48": "icons/cosafe48.png",
    "128": "icons/cosafe128.png"
  },

  "action": {
    "default_title": "Click to chat with Cosafe",
    "default_icon": "icons/cosafe128.png",
    "default_popup": "index.html"
  },
  "side_panel": {
    "default_path": "index.html"
  },
  "content_scripts": [
    {
      "js": [
        "scripts/content.js",
        "scripts/services/passive_search.js"
      ],
      "css": ["css/content.css"],
      "matches": ["<all_urls>"]
    }
  ],
  "background": {
    "service_worker": "scripts/services/background.js"
  },
  "commands": {
    "trigger_search": {
      "description": "Trigger search based on selected text",
      "suggested_key": {
        "default": "Ctrl+K",
        "mac": "Command+K"
      }
    }
  }
}
