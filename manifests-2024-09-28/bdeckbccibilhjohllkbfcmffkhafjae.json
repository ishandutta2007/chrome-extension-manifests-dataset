{
"update_url": "https://clients2.google.com/service/update2/crx",

    "name": "Page qrcode",
    "manifest_version": 2,
    "version": "0.1.0",
    "description": "Create QRCode of current page.",
    "permissions": [
        "<all_urls>"
    ],
    "icons": {
        "20": "icons/20.png",
        "24": "icons/24.png",
        "32": "icons/32.png",
        "48": "icons/48.png",
        "64": "icons/64.png",
        "128": "icons/128.png",
        "256": "icons/256.png",
        "512": "icons/512.png"
    },
    "browser_action": {
        "default_title": "QRCode",
        "default_icon": "icons/24.png"
    },
    "background": {
        "scripts": [
            "index.js"
        ]
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "css": ["style.css"],
        "js": ["lib/jquery-2.0.3.min.js", "contentScript.js", "lib/qrcode.min.js"]
    }]
}
