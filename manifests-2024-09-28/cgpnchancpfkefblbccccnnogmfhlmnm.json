"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "skyrpa",
  "version": "1.0.0.3",
  "description": "Support extension for SkymericRPA",
  "permissions": [
    "tabs",
    "nativeMessaging",
    "webNavigation",
    "downloads"
  ],
  "host_permissions": [ "<all_urls>" ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [ "content.js" ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_start"
    }
  ],
  "applications": {
    "gecko": {
      "id": "info@skymeric.com",
      "strict_min_version": "78.0"
    }
  },
  "content_security_policy": { "extension_pages": "script-src 'self'; object-src 'self';" },
  "icons": {
    "16": "OpenRPA16.png",
    "48": "OpenRPA48.png",
    "128": "OpenRPA128.png"
  }
}
