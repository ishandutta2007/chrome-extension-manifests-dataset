{
"update_url": "https://clients2.google.com/service/update2/crx",


  "name": "Shortcuts for basic navigation",
  "description": "Allows to natively specify shortcuts for basic navigation in Chrome",
  "version": "1.2",
  "manifest_version": 2,

  "icons": {
    "16":  "images/basic-navigation-16px.png",
    "32":  "images/basic-navigation-32px.png",
    "64":  "images/basic-navigation-64px.png",
    "128": "images/basic-navigation-128px.png",
    "256": "images/basic-navigation-256px.png",
    "512": "images/basic-navigation-512px.png"
  },

  "background": {
    "scripts": [ "background.js" ]
  },

  "browser_action": {
    "default_title": "Open shortcuts bindings"
  },

  "permissions":[ "tabs"] ,

  "commands": {

    "next-tab": {
      "suggested_key": {
        "default": "Alt+Right"
      },
      "description": "Next tab"
    },
    "previous-tab": {
      "suggested_key": {
        "default": "Alt+Left"
      },
      "description": "Previous tab"
    },

    "go-forward": {
      "suggested_key": {
        "default": "Alt+Shift+Delete"
      },
      "description": "Go forward"
    },
    "go-back": {
      "suggested_key": {
        "default": "Alt+Delete"
      },
      "description": "Go back"
    }
  }

}
