{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "AtyeS AI - OneClick Resume Enhancer",
  "version": "1.2",
  "description": "AI-powered resume enhancer for LinkedIn and Indeed job pages—create tailored resumes directly within your browser.",
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "downloads"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://www.linkedin.com/*", "https://*.indeed.com/*"],
      "js": ["content.js"]
    },
    {
      "matches": ["*://atyes.co/*"],
      "js": ["content_atyes.js"],
      "run_at": "document_end"
    }
  ],
  "action": {
    "default_icon": "icons/icon.png",
    "default_popup": "drawer.html"
  },
  "web_accessible_resources": [
    {
      "resources": ["drawer.html", "styles.css", "images/resume-ext.jpg", "images/basic-resume1.png", "images/downloadIcon.svg", "images/edit.svg"],
      "matches": ["<all_urls>"]
    },
    {
      "resources": [ "images/basic-resume1.png", "images/resume-ext.jpg", "images/downloadIcon.svg", "images/edit.svg", "images/logo.png", "images/write-ai.svg", "images/right-arrow.svg", "images/new-resume.svg", "images/new-page.png"],
      "matches": [ "https://www.linkedin.com/*", "https://*.indeed.com/*"]
    }
  ]
}
