    "manifest_version": 3,
    "description": "Kibela Custom Template",
    "content_scripts": [
        {
            "matches": [
                "https://*.kibe.la/note_templates/new",
                "https://*.kibe.la/note_templates/*/edit"
            ],
            "js": [
                "entry/edit.js"
            ]
        },
        {
            "matches": [
                "https://*.kibe.la/note_templates/*/notes/*"
            ],
            "js": [
                "entry/notes.js"
            ]
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "pages/*",
                "tokenizer.*.js",
                "vendor.*.js"
            ],
            "matches": [
                "https://*.kibe.la/*"
            ]
        }
    ]
}