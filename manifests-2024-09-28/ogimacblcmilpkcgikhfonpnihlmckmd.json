{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,

  "name": "Webmiu",
  "description": "This extension gathers what you want to miu on the web on a single page.",
  "version": "1.5",
  "background": {
 "scripts": ["miulib.js", "context_menu.js"]
  },
  "browser_action": {
    "default_icon": "miu.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "css": [],
      "js": ["jquery-1.11.3.min.js","miulib.js", "injected.js"]
    }
  ],
  "permissions": [
    "storage",
 "contextMenus",
 "activeTab",
 "http://*/*",
 "https://*/*"
  ],
  "icons": {
    "48": "ico48.png",
    "128": "ico128.png"
  },
  "web_accessible_resources": [ "yes_no.png",
        "miu.png",
        "croquette.png",
        "tincan.png",
        "tuna.png",
        "tuto/*",
        "play.png",
        "openInTab.png",
        "webmiutab.html"],
 "content_security_policy": "script-src 'self'; object-src 'self'"
}
