{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "AutoJoin",
  "version": "3.0",
  "description": "",
  "permissions": [
    "storage"
  ],
  "background": {
    "service_worker": "./dist/background.js"
  },
