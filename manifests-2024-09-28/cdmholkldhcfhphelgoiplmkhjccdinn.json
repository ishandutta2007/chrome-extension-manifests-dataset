{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "Austender Insights",
    "version": "1.1",
    "description": "A simple extension that adds additional details to tenders.gov.au",
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": [
          "https://tenders.gov.au/*",
          "https://www.tenders.gov.au/*"
          
        ],
        "js": ["content.js",
              "lib/chart.js",
              "lib/chartjs-plugin-datalabels.js",
              "lib/dhg.js",
              "lib/sorttable.js"
              ],
        
        "css":["dhg-styles.css"]
      }
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon48.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
      }
    },
    "icons": {
      "16": "images/icon48.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  }