---
name: RetweetsExtract - Export X/Twitter Retweets
version: 1.1.0
category_slug: productivity/workflow
rating: 1
rating_count: 1
user_count: 1000
release_date: '2024-05-20T11:05:29.000Z'
size: 1.1MiB
languages:
  - English
description: One click to export retweets list of any x/twitter tweets to CSV.
publisher_account: mini shark
privacy_policy: https://twfollowexport.extensionsfox.com/privacy
---

{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "__MSG_extName__",
  "description": "__MSG_extDescription__",
  "default_locale": "en",
  "version": "1.1.0",
  "homepage_url": "https://retweetsexport.extensionsfox.com/",
  "action": {
    "default_icon": "./assets/icon-512.png",
    "default_popup": "./dist/popup/index.html"
  },
  "icons": {
    "16": "./assets/icon-512.png",
    "48": "./assets/icon-512.png",
    "128": "./assets/icon-512.png"
  },
  "permissions": [
    "storage",
    "webRequest"
  ],
  "host_permissions": [
    "*://*.twitter.com/*",
    "*://*.x.com/*"
  ],
  "background": {
    "service_worker": "./dist/background/index.mjs"
  },
  "content_scripts": [
    {
      "run_at": "document_end",
      "matches": [
        "https://*.twitter.com/*",
        "https://*.x.com/*"
      ],
      "js": [
        "./dist/contentScripts/index.global.js"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "dist/contentScripts/style.css",
        "assets/*"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}
