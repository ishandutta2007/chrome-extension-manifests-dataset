{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Linkedin Profile Scraper",
  "version": "2.4.1",
  "description": "A chrome extension to scrape linkedin profiles",
  "background": {
    "service_worker": "src/pages/background/index.js",
    "type": "module"
  },
  "action": {},
  "icons": {
    "128": "scrapaba.png"
  },
  "permissions": [
    "tabs",
    "cookies",
    "identity"
  ],
  "host_permissions": [
    "https://www.linkedin.com/"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*",
        "<all_urls>"
      ],
      "js": [
        "src/pages/content/index.js"
      ],
      "css": [
        "contentStyle.css"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "contentStyle.css",
        "scrapaba.png",
        "right-arrow.png",
        "left-arrow.png",
        "assets/*.png",
        "close.png",
        "linkedin-button.png",
        "power-off.png",
        "fonts/*"
      ],
      "matches": [
        "https://*/*",
        "http://*/*",
        "<all_urls>"
      ]
    }
  ]
}