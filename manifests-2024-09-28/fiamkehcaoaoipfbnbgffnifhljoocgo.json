"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 2,

    "name": "RPic",
    "description": "View pictures from your favourite subreddit directly in your browser",
    "version": "0.1",

    "permissions": [
        "http://www.reddit.com/*",
        "http://reddit.com/*",
        "http://imgur.com/*",
