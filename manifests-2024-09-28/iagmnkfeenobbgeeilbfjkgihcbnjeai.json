{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,

  "name": "Astro Empires Observer",
  "description": "Keep up to date with the most recent guild messages and review your account statistics.",
  "version": "0.8.7",

  "homepage_url": "https://github.com/delphian/astro-empires-chrome-extension/wiki",

  "permissions": [
    "https://*.astroempires.com/",
    "http://*.astroempires.com/",
    "notifications"
  ],

  "options_page": "options/options.html",

  "browser_action": {
    "default_icon": {
      "19": "images/ae-icon-19.png",
      "38": "images/ae-icon-38.png"
    },
    "default_title": "Astro Empires Observer",
    "default_popup": "popup.html"
  },

  "icons": { 
    "16": "images/ae-icon-16.png",
    "19": "images/ae-icon-19.png",
    "38": "images/ae-icon-38.png",
    "48": "images/ae-icon-48.png",
    "128": "images/ae-icon-128.png"
  },

  "background": {
    "page": "background.html"
  },

  "content_scripts": [
    {
      "matches": ["http://*.astroempires.com/*"],
      "js": [
        "libraries/external/jquery/jquery-1.8.3.min.js",
        "libraries/ae-js-lib/astro-empires.js",
        "content/watcher.js"
      ]
    }
  ]

