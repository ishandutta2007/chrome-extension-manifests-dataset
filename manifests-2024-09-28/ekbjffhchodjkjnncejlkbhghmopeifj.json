{
"update_url": "https://clients2.google.com/service/update2/crx",

  "short_name": "Mood Based Tab Manager",
  "name": "Mood Based Tab Manager",
  "manifest_version": 3,
  "version": "2.0.2",
  "description": "A tab manager that organizes tabs based on your mood",
  "action": {
    "default_popup": "index.html",
    "default_title": "Mood Based Tab Manager"
  },
  "side_panel": {
    "default_path": "sidePanel.html",
    "default_title": "View Site Mood Settings"
  },
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "static/js/content.js"
      ]
    }
  ],
  "background": {
    "service_worker": "static/js/background.js"
  },
  "commands": {
    "switch_to_DND": {
      "suggested_key": {
        "default": "Alt+Shift+D"
      },
      "description": "Switch to Do Not Disturb (DND) mood"
    },
    "switch_to_WORK": {
      "suggested_key": {
        "default": "Alt+Shift+W"
      },
      "description": "Switch to Work mood"
    },
    "switch_to_PERSONAL": {
      "suggested_key": {
        "default": "Alt+Shift+P"
      },
      "description": "Switch to Personal Mood"
    },
    "switch_to_custom1": {
      "description": "Switch to Custom1 Mood"
    },
    "switch_to_custom2": {
      "description": "Switch to Custom2 Mood"
    },
    "switch_to_custom3": {
      "description": "Switch to Custom3 Mood"
    },
    "switch_to_NOTHING": {
      "description": "Clear Selected Mood"
    },
    "open_side_panel": {
      "suggested_key": {
        "default": "Alt+Shift+S"
      },
      "description": "View Site Mood Settings"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "tabGroups",
    "storage",
    "contentSettings",
    "scripting",
    "contextMenus",
    "sidePanel"
  ]
}
