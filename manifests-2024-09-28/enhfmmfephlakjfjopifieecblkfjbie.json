  "manifest_version": 3,
  "name": "__MSG_name__",
  "version": "1.34",
  "description": "__MSG_description__",
  "default_locale": "en",
  "permissions": [
    "declarativeNetRequest",
    "unlimitedStorage",
    "webRequest",
    "storage",
