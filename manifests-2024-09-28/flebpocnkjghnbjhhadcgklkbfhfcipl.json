{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "TimeTamer",
  "version": "0.5.1",
  "description": "TimeTamer - the Chrome extension that blocks distracting websites so you can focus on what matters.",
  "default_locale" : "en",
  "icons": {
    "16": "icons/icon_16.png",
    "32": "icons/icon_32.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "options_page": "options.html",
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "TimeTamer",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage", "scripting", "activeTab", "alarms"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "all_frames": false,
      "run_at": "document_idle",
      "js": [
        "contentScript.js"
      ],
      "css": [
        "overlay.css",
        "flip.min.css"
      ]
    }
  ],
  "web_accessible_resources": [
      { 
        "resources": ["overlay.css", "overlay.html","flip.min.css"],
        "matches": ["<all_urls>"]
      }
  ],
  "host_permissions": [
    "https://*/*",
    "http://*/*"
  ]
}