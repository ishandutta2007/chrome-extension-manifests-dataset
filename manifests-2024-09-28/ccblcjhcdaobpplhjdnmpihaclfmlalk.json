      "matches": [
        "*://*.linkedin.com/*"
      ],
      "css": [
        "content.css"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "images/generate.png"
      ],
      "matches": [
        "*://*.linkedin.com/*"
      ]
    }
  ]
