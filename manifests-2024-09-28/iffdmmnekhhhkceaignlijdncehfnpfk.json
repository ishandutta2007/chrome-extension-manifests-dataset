{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "Glassdoor Mallet - paywall remover",
    "description": "This extension will automatically remove paywall screen and blocks from Glassdoor so you can navigate it freely.",
    "version": "1.0",
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*.glassdoor.com/*",
                "*://*.glassdoor.es/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "icons": {
        "48": "safemallet.png"
    }
}