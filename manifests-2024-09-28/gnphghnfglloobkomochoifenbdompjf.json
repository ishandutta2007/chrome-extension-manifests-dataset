{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Evaporator Extension",
  "version": "0.0.1",
  "manifest_version": 3,
  "icons": {
    "128": "logo-128.png"
  },
  "permissions": [
    "*://*/*",
    "http://*/*",
    "https://*/*",
    "http://127.0.0.1:8002/*",
    "http://127.0.0.1:6001/*",
    "cookies"
  ],
  "host_permissions": [
    "*://*/*",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources": [{
    "resources": ["interceptor.js"],
    "matches": ["<all_urls>"]
  }],
  "browser_action": {
    "default_title": "Arex",
    "default_icon": "logo-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content-scripts.js"
      ],
      "run_at": "document_start"
    }
  ]
}
