{
"update_url": "https://clients2.google.com/service/update2/crx",

    "name": "SearchDeck",
    "short_name": "SearchDeck",
    "description": "Manage your search in a single tab!",
    "version": "1.0",
    "author": "developer@guiyum.com",
 "homepage_url": "http://guiyum.com",
    "manifest_version": 2,
    "permissions": [
  "storage",
  "tabs",
  "history",
  "bookmarks",
  "*://*/*"
 ],
    "background": {
        "scripts": [
   "js/background.min.js"
  ],
        "persistent": false
    },
    "omnibox": { "keyword" : "s" },
    "icons": {
        "16": "images/logo/16_guiyum_logo_purple.png",
        "32": "images/logo/32_guiyum_logo_purple.png",
        "48": "images/logo/48_guiyum_logo_purple.png",
        "128": "images/logo/128_guiyum_logo_purple.png"
    },
   "browser_action": {
  "default_title": "SearchDeck"
 },
    "web_accessible_resources": [
        "images/icons/40_submit.png",
        "images/logo/256_guiyum_logo_purple.png",
        "locales/*"
    ],
 "content_scripts": [{
  "js": [
            "js/content.min.js"
        ],
  "css": [
            "css/search.css",
            "css/content.css"
        ],
  "matches": [
     "http://*/*",
     "https://*/*"
  ],
        "exclude_globs": [
     "http://*guiyum.com/*",
     "https://*guiyum.com/*"
        ],
  "match_about_blank": true,
       "run_at": "document_idle"
 }]
}
