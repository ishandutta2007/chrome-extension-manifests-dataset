      "48": "icon.png",
      "128": "icon.png"
  },
  "background": {
      "service_worker": "./js/background.js"
     },
  "content_scripts": [{
      "matches": ["https://www.amazon.co.jp/*", "https://www.amazon.com/*"],
      "js": ["js/loader.js"],
      "css": ["css/styles.css"],
      "run_at": "document_end",
      "all_frames": true
  }],
  "web_accessible_resources": [
    {"resources":["js/*"], "matches": ["https://www.amazon.co.jp/*"]},
    {"resources":["img/*.png"], "matches": ["https://*/*"]}
  ]
