      "matches": [
        "https://*.jobinja.ir/*"
      ],
      "css": [
        "css/content-styles.css"
      ],
      "js": [
        "js/jobinja.js"
      ]
    },
    {
      "matches": [
        "https://*.e-estekhdam.com/hr/pipelines*"
      ],
      "css": [
        "css/content-styles.css"
      ],
      "js": [
        "js/e-estekhdam.js"
      ]
    },
    {
      "matches": [
        "https://*.jobvision.ir/jobs/*/applications/*/jobvision-profile"
      ],
      "css": [
        "css/content-styles.css"
      ],
      "js": [
        "js/jobvision.js"
      ]
    },
    {
      "matches": [
        "https://*.linkedin.com/in/*"
      ],
      "css": [
        "css/content-styles.css"
      ],
      "js": [
        "js/linkedin.js"
      ]
    }
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
