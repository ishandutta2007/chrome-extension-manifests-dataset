    "16": "./assets/logo_16.png"
  },
  "host_permissions": ["<all_urls>"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content/content.js"]
    }
  ],
  "permissions": ["storage", "activeTab", "notifications"],
  "web_accessible_resources": [
    {
      "resources": ["assets/*"],
