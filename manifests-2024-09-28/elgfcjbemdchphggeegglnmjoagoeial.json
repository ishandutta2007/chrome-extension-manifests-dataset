    "activeTab"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "options_page": "dashboard.html",
  "action": {
    "default_title": "LinkedIn Automation",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "logo-16.png",
    "32": "logo-32.png",
    "48": "logo-48.png",
    "128": "logo-128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.linkedin.com/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    },
    {
      "matches": ["https://www.linkedin.com/feed/*"],
      "js": ["inject-comment-reply.js"],
      "css": ["inject-comment-reply.css"]
    },
    {
      "matches": ["https://www.linkedin.com/search/results/people/*"],
      "js": ["inject-form.js"]
    }
  ],

  "web_accessible_resources": [
    {
      "resources": [ "content.js", "dashboard/index.html"],
      "matches": [ "https://www.linkedin.com/*"],
      "extension_ids": []
