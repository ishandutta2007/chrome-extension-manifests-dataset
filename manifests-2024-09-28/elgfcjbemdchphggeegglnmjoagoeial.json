"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "LinkedIn Automation Tool 2024 - powered by ChatGPT",
  "description": "Bulk send LinkedIn invitations, messages, ai auto commenting and AI personalized messages to accelerate your B2B lead generation",
  "version": "3.0.3",
  "manifest_version": 3,
  "permissions": [
    "tabs",
    "storage",
