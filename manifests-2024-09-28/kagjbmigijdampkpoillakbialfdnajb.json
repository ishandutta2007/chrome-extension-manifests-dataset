{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Easy Tabs",
  "version": "3.2",
  "description": "Close or Go to your tabs easily.",
  "permissions": ["tabs"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "img/icon-16.png",
      "48": "img/icon-48.png",
      "128": "img/icon-128.png"
    }
  },
  "icons": {
    "16": "img/icon-16.png",
    "48": "img/icon-48.png",
    "128": "img/icon-128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'",
    "sandbox": "sandbox 'allow-scripts'"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Shift+Ctrl+Space",
        "mac": "Shift+Command+Space",
        "windows": "Shift+Ctrl+Space"
      },
      "description" : "Start the extension"
    }
  }
