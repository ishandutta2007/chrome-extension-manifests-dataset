{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "AI Description Generator",
    "version": "1.1",
    "description": "Use AI Description Generator for automated ai content creation. A product description generator — the ultimate content writer tool.",
    "permissions": [
        "activeTab",
        "scripting",
        "storage",
        "contextMenus"
    ],
    "default_locale": "en",
    "action": {},
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": [
                "content.js",
                "scripts.js",
                "functions.js"
            ],
            "run_at": "document_end"
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "index.html",
                "latoregular.woff2",
                "latoregular.woff",
                "latoitalic.woff2",
                "latoitalic.woff",
                "latobold.woff2",
                "latobold.woff",
                "latobolditalic.woff2",
                "latobolditalic.woff"
            ],
            "matches": ["<all_urls>"]
        }
    ],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ],
    "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'self'; style-src 'self'"
    },
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "64": "icon64.png",
        "96": "icon96.png",
        "128": "icon128.png"
