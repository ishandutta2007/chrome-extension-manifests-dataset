{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "Cloudbreakr Extension",
    "description": "Basic extension to load influencer data by detecting social sites.",
    "version": "1.0.5",
    "icons":{
        "57": "images/apple-icon-57x57.png" 
    },
    "action": {
        "default_title": "Cloudbreakr",
        "default_popup": "popup.html"
    },
    "host_permissions": [
        "*://www.instagram.com/",
        "*://www.facebook.com/",
        "*://www.youtube.com/"
    ],
    "permissions":[
        "tabs",
        "storage",
        "scripting",
        "webNavigation"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "web_accessible_resources": [
        {
          "resources": ["assets/tailwind.css", "images/*"],
          "matches": ["<all_urls>"]
        }
    ]
}