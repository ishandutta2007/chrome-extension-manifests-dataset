{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,

  "name": "Shortkits",
  "description": "This extension enables shortcut keys on your favorite web-apps.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Shortkits"
  },
  "content_scripts": [
    {
      "matches": ["*://*.producthunt.com/*"],
      "js": ["./scripts/apps/ph.js"]
    },
    {
      "matches": ["*://news.ycombinator.com/*"],
      "js": ["./scripts/apps/hn.js"]
    },
    {
      "matches": ["*://dribbble.com/*"],
      "js": ["./scripts/apps/db.js"]
    }
  ],
  "permissions": [
    "https://news.ycombinator.com/*",
    "https://producthunt.com/*",
    "https://dribbble.com/*"
  ]
}
