{
"update_url":"http://clients2.google.com/service/update2/crx",
  "name": "Feed Intent Viewer",
  "version": "1.2",
  "manifest_version": 2,
  "app": {
    "launch": {
      "local_path": "main.html"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "intents": {
    "http://webintents.org/view": [
      {
        "type": ["application/atom+xml", "application/rss+xml"],
        "href": "main.html",
        "title": "Feed Intent Viewer",
        "disposition": "window"
      }
    ]
  },
  "permissions": [
    "<all_urls>"
  ]
}
