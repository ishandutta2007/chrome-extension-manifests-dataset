{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Passfieldlogger",
  "description": "Logs password fields unless the URL is defined on a whitelist.",
  "version": "1.0",
  "host_permissions": [
    "https://*/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "128.png"
    }
  },
  "icons": {
    "128": "128.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["Data/config.json", "Data/whitelist.txt"],
      "matches": ["<all_urls>"]
    }
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'; connect-src 'self' https://passfieldguard.ballast-nedam.nl"
  }
}