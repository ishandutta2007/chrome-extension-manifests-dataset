{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "ReversingLabs Browser Extension",
  "version": "0.3.9",
  "minimum_chrome_version": "116",
  "description": "Find, explore and submit indicators to Spectra Intelligence or Spectra Analyze",
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "action": {
    "default_title": "Click to open settings",
    "default_icon": "images/icon32.png"
  },
  "background": {
    "service_worker": "js/background.js"
  },
  "content_scripts": [{
    "css": ["css/content.css"],
    "js": ["js/rl_content_script.js", "js/inject_toast.js" ],
    "matches": ["<all_urls>"]
  }],
  "host_permissions": ["<all_urls>"],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "side_panel": {
    "default_path": "side_panel.html"
  },
  "permissions": ["contextMenus", "storage", "tabs", "alarms", "sidePanel", "declarativeNetRequest"],
  "web_accessible_resources": [{
    "matches": ["<all_urls>"],
    "resources": ["images/rl_logo.svg"]
  }]
}
