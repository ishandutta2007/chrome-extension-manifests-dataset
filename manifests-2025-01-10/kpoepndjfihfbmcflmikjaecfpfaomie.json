{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "Masker@",
  "version": "1.0",

  "description": "Browser extension for Masker@ (https://www.masker.at)",

  "icons": {
    "48": "icons/logo-square.svg",
    "96": "icons/logo-square.svg"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["browser-polyfill.js", "content.js"]
    }
  ],

  "browser_action": {
    "default_icon": "icons/logo-square.svg",
    "default_title": "Masker@",
    "default_popup": "popup.html"
  },

  "permissions": ["cookies", "menus", "contextMenus", "activeTab", "notifications", "*://masker.at/*", "*://app.masker.at/*", "*://api.masker.at/*"],

  "background": {
    "scripts": ["browser-polyfill.js", "background.js"]
  }
}
