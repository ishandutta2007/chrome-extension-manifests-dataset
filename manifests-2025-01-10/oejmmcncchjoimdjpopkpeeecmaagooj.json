{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "Promptly for ChatGPT",
    "description": "Create and reuse ChatGPT prompts",
    "version": "1.0.2",
    "permissions": [
        "storage",
        "contextMenus",
        "tabs",
