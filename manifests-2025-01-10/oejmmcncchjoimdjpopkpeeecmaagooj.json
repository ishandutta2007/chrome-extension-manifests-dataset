    ],
    "icons": {
        "16": "promptly-32.png",
        "48": "promptly-96.png",
        "128": "promptly-256.png"
    },
    "options_page": "options.html",
    "action": {
        "default_icon": {
            "16": "promptly-32.png",
            "48": "promptly-96.png",
            "128": "promptly-256.png"
        },
        "default_title": "Promptly for ChatGPT",
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": [
                "https://chat.openai.com/*"
            ],
            "js": [
                "content.js",
                "usePrompt.js"
            ]
        }
    ],
    "host_permissions": [
        "<all_urls>"
    ]
}