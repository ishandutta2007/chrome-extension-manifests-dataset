    "https://*.qalampir.uz/*",
    "https://*.gazeta.uz/*",
    "https://*.demokrat.uz/*"
  ],
  "background": {
    "service_worker": "scripts/background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://*.kun.uz/*", "https://*.daryo.uz/*"],
      "js": ["scripts/main.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["icons/*"],
      "matches": ["<all_urls>"]
    }
  ]
