  "manifest_version": 3,
  "name": "Cusbo Side Widget",
  "description": "Allows you to use Cusbo on any browser tab.",
  "version": "1.1.3",
  "icons": {
    "512": "assets/images/logo/cusbo-512x512.png"
  },
  "action": {
    "default_icon": "assets/images/logo/cusbo-128x128.png",
