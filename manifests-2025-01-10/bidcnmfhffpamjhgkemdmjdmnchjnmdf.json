{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "RussianGram",
  "description": "Adds stress marks to Russian websites",
  "version": "2.0.0",
  "icons": {
    "16": "icon/icon.png",
    "48": "icon/icon.png",
    "128": "icon/icon.png"
  },
  "action": {
    "default_icon": {
      "19": "icon/icon.png",
      "38": "icon/icon.png"
    },
    "default_popup": "html/popup.html"
  },
  "background": {
    "service_worker": "js/background.js"
  },
  "incognito": "spanning",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["css/popover.css"],
      "js": [
        "js/jquery.min.js",
        "js/popper.min.js",
        "js/bootstrap.min.js",
        "js/handlebars.min.js",
        "js/tables.js",
        "js/config.js",
        "js/content.js",
        "js/wholepage.js"
      ],
      "run_at": "document_end",
      "all_frames": true
    }
  ],
  "permissions": ["contextMenus", "activeTab"],
  "host_permissions": ["https://api.russiangram.com/*", "https://russiangram.com/*"],
  "web_accessible_resources": [
    {
      "resources": ["html/*", "css/*", "js/*", "fonts/*"],
      "matches": ["<all_urls>"]
    }
  ]
}
