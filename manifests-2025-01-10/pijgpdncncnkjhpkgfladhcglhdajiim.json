{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Direct Link playlist",
  "version": "1.0.0",
  "description": "A leightweight playlist handler for direct link media",
  "permissions": ["activeTab", "storage", "tabs", "unlimitedStorage"],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "PNG/icon16.png",
      "32": "PNG/icon32.png",
      "48": "PNG/icon48.png",
      "128": "PNG/icon128.png"
    }
  },
  "icons": {
    "16": "PNG/icon16.png",
    "32": "PNG/icon32.png",
    "48": "PNG/icon48.png",
    "128": "PNG/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*.mp3", "*://*/*.mp4", "*://*/*.webm"],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ],
  "manifest_version": 2,
  "web_accessible_resources": ["SVG/player.svg"]
}
