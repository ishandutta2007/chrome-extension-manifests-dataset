"manifest_version": 2,
    "name": "Text to Image",
    "description": "This extension replaces text urls that link to images with the actual images inline.",
    "version": "1.1.1",
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
