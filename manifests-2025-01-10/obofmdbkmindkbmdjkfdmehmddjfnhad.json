      ],
      "css": [
        "assets/css/open.css"
      ]
    },
    {
      "matches": [
        "https://web.whatsapp.com/*"
      ],
      "js": [
        "assets/js/wppconnect-wa.js"
      ],
      "world": "MAIN",
      "run_at": "document_idle",
      "all_frames": true
    },
    {
      "matches": [
        "https://web.whatsapp.com/*"
      ],
      "js": [
        "assets/js/wa.js"
      ],
      "world": "MAIN",
      "run_at": "document_start",
      "all_frames": true
    }
  ]
