  "options_ui": {
    "page": "options/open-options-in-new-tab.html"
  },
  "page_action": {
    "default_popup": "popup/index.html",
    "default_icon": {
        "16": "icon/16.png",
        "19": "icon/19.png",
        "38": "icon/38.png",
        "48": "icon/48.png",
        "128": "icon/128.png"
