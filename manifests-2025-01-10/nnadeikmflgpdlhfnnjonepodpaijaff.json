{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "ADPC prototype",
  "version": "0.0.2",
  "description": "Demonstrates the Advanced Data Protection Control mechanism",
  "background": {
    "scripts": [
      "browser-polyfill.js",
      "background/index.js"
    ]
  },
  "browser_specific_settings": {
    "gecko": {
