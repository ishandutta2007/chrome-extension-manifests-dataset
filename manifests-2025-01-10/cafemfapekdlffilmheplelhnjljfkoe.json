      "matches": [
        "*://*.facebook.com/*",
        "*://*.twitter.com/*",
        "*://*.soundcloud.com/*",
        "*://*.medium.com/*",
        "*://*.quora.com/*",
        "*://*.pinterest.com/*",
        "*://*.linkedin.com/*"
      ],
      "js": ["constants/domainList.js", "content.js"]
    }
  ],

  "page_action": {
    "default_title": "Automate Follows",
    "default_icon": "images/icon-128.png",
    "default_popup": "popup.html"
