{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "X-Alpha",
  "version": "4.6.2",
  "description": "X-Alpha chrome extension",
  "action": {
    "default_icon": "./assets/x-alpha-extension-icon.png",
    "default_popup": "./dist/popup/index.html"
  },
  "background": {
    "service_worker": "./dist/background/background.js"
  },
  "icons": {
    "16": "./assets/x-alpha-extension-icon.png",
    "48": "./assets/x-alpha-extension-icon.png",
    "128": "./assets/x-alpha-extension-icon.png"
  },
  "permissions": [
    "storage",
    "activeTab"
  ],
  "host_permissions": [
    "http://*/",
    "https://*/",
    "file:///*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*/*"
      ],
      "js": [
        "./dist/contentScripts/index.global.js"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "dist/contentScripts/style.css",
        "dist/contentScripts/sdk.global.js",
        "assets/*",
        "fonts/*",
        "fonts/poppins-regular.woff2",
        "fonts/poppins-bold.woff2"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'; style-src 'self' 'unsafe-inline' https://fonts.googleapis.com https://accounts.google.com/gsi/style https://*.twimg.com; style-src-elem 'self' 'unsafe-inline' https://fonts.googleapis.com https://accounts.google.com/gsi/style https://*.twimg.com"
  }
