{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,

    "name": "ColorfulPWA",
    "description": "This extension allows you to choose web application titlebar color.",
    "version": "1.0.1",

    "author": "antoinel12",
    "homepage_url": "https://github.com/antoinel12/ColorfulPWA",
  
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },

    "permissions": [
        "activeTab",
        "storage",
        "offscreen"
    ],

    "background": {
        "service_worker": "js/eventPage.js",
        "type": "module"
    },

    "action": {
        "default_icon": {
            "16": "icon16.png"
        },
        "default_popup": "popup.html"
    },

    "content_scripts": [{
        "matches": [
            "<all_urls>"
        ],
        "js": [
            "js/contentScript.js"
        ],
        "run_at": "document_end"
    }]
}