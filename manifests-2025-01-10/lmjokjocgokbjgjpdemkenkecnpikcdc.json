  "manifest_version": 3,
  "name": "LazyPaste",
  "author": "jM",
  "version": "1.0",
  "description": "A Chrome extension to paste in template text with the last clipboard item slipped in using the {$$} token.",
  "permissions": [
    "activeTab",
    "scripting",
    "storage",
    "clipboardRead",
    "storage",
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "/icons/Lazypaster_icon_with_text_16x16.png",
    "48": "/icons/Lazypaster_icon_with_text_48x48.png",
