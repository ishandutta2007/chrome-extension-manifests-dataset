{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "SPM File Edit Native Messaging Bridge",
  "version": "3.0.0.1",
  "manifest_version": 3,
  "description": "Manages Microsoft Word Document editing from the Google Chrome and Microsoft Edge browsers.",
  "background": {
    "service_worker": "extToNative.js"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*", 
        "https://*/*"
      ],
      "js": [
        "pageToExtn.js"
      ],
     "run_at":"document_end",
     "all_frames": true
    }
  ],
  "permissions": [
    "nativeMessaging",
    "background"
  ],
  "host_permissions":[
    "http://*/*",
    "https://*/*"
  ],
 "icons": {
    "16": "img/16.png",
    "48": "img/48.png",
    "128": "img/128.png" 
  }  
