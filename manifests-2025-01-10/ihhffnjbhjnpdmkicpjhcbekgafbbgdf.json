{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "bookmark panel",
  "version": "1.1.1",
  "author": "tiny-worker",
  "description": "对书签进行自定义管理，同时支持页面缩略图的展示",
  "background": {
    "service_worker": "js/background.js"
  },
  "action": {
    "default_icon": {
      "32": "imgs/icon.png"
    },
    "default_title": "Preview",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "imgs/logo.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "match_about_blank": true,
      "css": [
        "css/sidebar.css"
      ],
      "js": [
        "js/sidebar.js"
      ]
    }
  ],
  "commands": {
    "toggle": {
      "suggested_key": {
        "default": "Ctrl+B",
        "mac": "Command+B"
      },
      "description": "快捷打开侧边书签栏"
    }
  },
  "permissions": [
    "storage",
    "bookmarks",
    "unlimitedStorage"
  ],
  "host_permissions": [
    "<all_urls>"
  ]
}