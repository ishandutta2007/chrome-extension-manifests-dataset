{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Google Apps Script Custom IDE",
  "version": "0.0.3",
  "manifest_version": 2,
  "author": "Rubén Sánchez",
  "description": "Es una extensión que agrega funcionalidades al entorno de desarrollo de Google Apps Script.",
  "icons": {
    "16": "/icon16.png",
    "48": "/icon48.png",
    "128": "/icon128.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "content_scripts": [{
    "matches": [
      "https://script.google.com/*/*/edit*"
    ],
    "js": [
      "js/jquery.js",
      "js/gasThemes.js",
      "js/mainFunctions.js"
    ],
    "all_frames": false,
    "run_at": "document_end"
  }],
  "web_accessible_resources": [
    "js/gasTools.js",
    "js/popover.js",
    "js/foldCode.js",
    "html/options.html",
    "html/menu.html",
    "css/gasThemes.css",
    "css/gasStylesheet.css"
  ],
  "browser_action": {
    "default_title": "Google Apps Script Custom IDE",
    "default_icon": "icon128.png"
  },
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "permissions": [
    "declarativeContent",
    "https://script.google.com/*/*/edit*",
    "storage"
  ]
}