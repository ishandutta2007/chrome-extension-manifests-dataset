      {
        "matches": ["*://*.esa.io/*"],
        "js": ["contentScript.js"],
        "css": ["styles.css"],
        "run_at": "document_idle"
      }
    ],

    "background": {
      "service_worker": "background.js"
    },

    "action": {
      "default_popup": "popup.html"
    }
  }
  