  "manifest_version": 3,
  "name": "Scrabulizer Importer",
  "version": "2.0.0",
  "description": "Imports Lexulous, Words With Friends and Wordscraper games into Scrabulizer.",
  "icons": {
     "16": "images/s_tile_16px.png",
