{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "SVG Importer for Canva",
    "version": "1.0",
    "description": "Search and import free SVG files directly to Canva",
    "permissions": [
      "activeTab",
      "storage",
      "sidePanel",
      "scripting",
      "tabs"
    ],
    "host_permissions": [
      "https://*.svgviewer.dev/*",
      "https://*.canva.com/*"
    ],
    "side_panel": {
      "default_path": "sidepanel.html"
    },
    "action": {
      "default_title": "SVG Drag to Canva",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "commands": {
      "_execute_action": {
        "suggested_key": {
          "default": "Ctrl+B",
          "mac": "Command+B"
        }
      }
    },
    "content_scripts": [
      {
        "matches": ["*://*.canva.com/*"],
        "js": ["content.js"]
      }
    ],
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }