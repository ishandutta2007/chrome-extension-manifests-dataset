{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "/r/TickTockManitowoc De-Initializer",
  "version": "1.1",

  "description": "This extension adds a tooltip '?' beside known initials for easier reading",
  "icons": { "16": "icon16.png",
             "48": "icon48.png",
             "128": "icon128.png" },
             

  "permissions": [
    "http://reddit.com/r/TickTockManitowoc/comments/*",
    "http://*.reddit.com/r/TickTockManitowoc/comments/*",
    "https://reddit.com/r/TickTockManitowoc/comments/*",
    "https://*.reddit.com/r/TickTockManitowoc/comments/**"
  ],

  "content_scripts": [
    {
      "matches": ["http://reddit.com/r/TickTockManitowoc/comments/*",
                  "http://*.reddit.com/r/TickTockManitowoc/comments/*",
                  "https://reddit.com/r/TickTockManitowoc/comments/*",
                  "https://*.reddit.com/r/TickTockManitowoc/comments/*"],
      "css": ["ticktock.css"],
      "js": ["jquery-3.1.0.slim.min.js", "findreplace.js", "abbrev.js", "ticktock.js"],
      "run_at": "document_end"
    }
  ],

  "browser_action": {}
