{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "ProElevate Chrome Extension",
    "description": "ProElevate Chrome Extension",
    "version": "1.0.0",
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": [
                "js/floatingIcon.js"
            ],
            "run_at": "document_end"
        },
        {
            "matches": [
                "https://*.proelevate-portal.pages.dev/*",
                "http://localhost/*",
                "http://127.0.0.1/*"
            ],
            "js": [
                "js/injectPortal.js"
            ],
            "run_at": "document_end"
        }
    ],
    "background": {
        "service_worker": "js/background.js",
        "type": "module"
    },
    "permissions": [
        "activeTab",
        "storage",
        "scripting",
        "contextMenus"
    ],
    "host_permissions": [
        "*://*/*"
    ],
    "icons": {
        "16": "images/proelevate_floating_icon_16.png",
        "48": "images/proelevate_floating_icon_48.png",
        "128": "images/proelevate_floating_icon_128.png",
        "256": "images/proelevate_floating_icon_256.png"
    },
    "externally_connectable": {
        "matches": [
            "https://*.proelevate-portal.pages.dev/*",
            "http://localhost/*",
            "http://127.0.0.1/*"
        ]
    },
    "web_accessible_resources": [{
        "matches": ["<all_urls>"],
        "resources": ["*"]
    }]
}