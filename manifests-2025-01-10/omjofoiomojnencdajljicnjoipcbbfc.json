  },
  "side_panel": {
    "default_path": "panel.html",
    "openPanelOnActionClick": true
  },
  "action": {
    "default_title": "Click to open panel"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "scripts/WebUI/selection.js"
      ],
      "run_at": "document_end",
      "type": "module"
    },
    {
      "matches": [
        "https://www.google.com/*"
      ],
      "js": [
        "lib/marked.min.js",
        "lib/prism.js",
        "scripts/WebUI/googleResultsComponent.js"
      ],
      "run_at": "document_end",
      "css": [
        "output.css",
        "css/prism.css",
        "css/light.css"
      ],
      "type": "module"
    },
