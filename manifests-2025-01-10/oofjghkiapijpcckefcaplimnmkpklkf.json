{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "DeepNotes",
  "version": "1.0.0",
  "description": "专为深度阅读者打造的沉浸式网页笔记工具。一键收藏内容、撰写笔记和读后感，支持导出为PDF、TXT、Markdown格式，可同步至Flomo",
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "permissions": [
    "storage",
    "activeTab",
    "contextMenus",
    "alarms"
  ],
  "host_permissions": [
    "https://tinyurl.com/*"
  ],
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "action": {
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    },
    "default_title": "DeepNotes",
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["config.html", "comment.html", "comment.css", "images/*", "pdfobject.min.js"],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    },
    {
      "resources": ["html2canvas.min.js"],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    },
    {
      "resources": ["html2pdf.bundle.local.min.js"],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ]
}
