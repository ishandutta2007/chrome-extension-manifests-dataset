  "name": "Collibra Browser Extension",
  "description": "The Everywhere Browser Extension provides seamless access to Collibra metadata from within Tableau and other web apps.",
  "manifest_version": 3,
  "version": "1.5.1",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "action": {
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_idle",
      "css": [
        "overlay.css"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "cookies",
    "storage",
    "contextMenus"
  ],
  "host_permissions": [
    "https://*.collibra.com/*",
    "https://*/"
