"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "version": "1.1",
  "name": "Youtube Autostop",
  "description": "Loads youtube videos in the unstarted state",
  "host_permissions": [ "*://*.youtube.com/*" ],
  "content_scripts": [{
    "matches": [ "*://*.youtube.com/*" ],
    "run_at":"document_start",
    "js": [ "inject.js" ]
