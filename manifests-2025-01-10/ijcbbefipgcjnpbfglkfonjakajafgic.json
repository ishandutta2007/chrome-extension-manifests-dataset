  "manifest_version": 3,
  "name": "Star Player | Adblock for Youtube",
  "version": "1.1",
  "description": "Removes video ads from Youtube, Immersive experience with youtube videos",
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "*://*.youtube.com/*",
    "https://www.googleapis.com/*"
  ],
  "web_accessible_resources": [{
    "resources": ["player.html", "player.js"],
    "matches": ["*://*.youtube.com/*", "chrome-extension://*/*"]
  }],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
   "16": "icon16.png",
   "32": "icon32.png",
   "48": "icon48.png",
   "128": "icon128.png"
 },
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["content.js"]
    }
  ]
} 