---

{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "default_locale": "en",
    "name": "__MSG_AppName__",
    "version": "1.0.0",
    "icons": {
        "128": "images/iPro_V410_128x128.png"
    },
    "description": "__MSG_AppDescription__",
    "offline_enabled": true,
    "background": {
        "service_worker": "service-worker.js"
    },
    "action": {
        "default_icon": {
            "16": "images/iPro_V410_16x16.png",
            "24": "images/iPro_V410_24x24.png",
            "36": "images/iPro_V410_36x36.png"
        },
        "default_title": "__MSG_AppName__"
    },
    "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'self'"
    },
    "permissions": [
        "contextMenus",
        "scripting",
        "notifications"
    ],
    "host_permissions": [
        "http://100.115.92.22/iprojection/*"
    ],
    "content_scripts": [
        {
            "matches": [
                "http://100.115.92.22/iprojection/tutorial.html*"
            ],
            "js": [
                "js/redirect.js"
            ]
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "mirroring.html"
            ],
            "matches": [
                "http://100.115.92.22/*"
            ]
        }
    ]
}