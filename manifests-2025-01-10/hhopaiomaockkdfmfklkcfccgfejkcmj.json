  "manifest_version": 3,
  "name": "X to Twitter Redirect",
  "version": "1.2",
  "description": "Redirects x.com, *.x.com, and *.twitter.com to twitter.com, adding the mx=1 parameter",
  "permissions": [
    "webNavigation"
