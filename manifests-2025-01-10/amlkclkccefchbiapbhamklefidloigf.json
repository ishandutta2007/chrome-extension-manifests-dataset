{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Auto Image Scroller for Airbnb™",
  "version": "1.0",
  "description": "Automatically scrolls through Airbnb listing images on hover",
  "permissions": ["activeTab", "storage", "scripting"],
  "host_permissions": [
    "*://*.airbnb.com/*",
    "*://*.airbnb.de/*",
    "*://*.airbnb.co.uk/*",
    "*://*.airbnb.fr/*",
    "*://*.airbnb.es/*",
    "*://*.airbnb.it/*",
    "*://*.airbnb.ca/*",
    "*://*.airbnb.com.au/*",
    "*://*.airbnb.com.br/*",
    "*://*.airbnb.co.jp/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.airbnb.com/*",
        "*://*.airbnb.de/*",
        "*://*.airbnb.co.uk/*",
        "*://*.airbnb.fr/*",
        "*://*.airbnb.es/*",
        "*://*.airbnb.it/*",
        "*://*.airbnb.ca/*",
        "*://*.airbnb.com.au/*",
        "*://*.airbnb.com.br/*",
        "*://*.airbnb.co.jp/*"
      ],
      "js": ["content.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/icons/icon16.png",
      "48": "/icons/icon48.png",
      "128": "/icons/icon128.png"
    }
  },
  "icons": {
    "16": "/icons/icon16.png",
    "48": "/icons/icon48.png",
    "128": "/icons/icon128.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["close_icon.svg"],
      "matches": ["<all_urls>"]
    }
  ]
}