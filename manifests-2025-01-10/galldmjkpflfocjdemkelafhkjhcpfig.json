{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Fehris",
  "short_name": "fehris-ng-extension",
  "version": "1.0.12",
  "description": "Chrome Extension for Fehris",
  "permissions": [
    "tabs",
    "activeTab",
    "contextMenus",
    "notifications",
    "storage"
  ],
  "host_permissions": [
    "http://localhost:4200/*",
    "https://fehris-staging.qcri.org/*",
    "https://fehris.qcri.org/*"
  ],
  "action": {
    "default_popup": "index.html?#/popup",
    "default_title": "Fehris EXT"
  },
  "icons": {
    "16": "assets/images/favicon-16.png",
    "48": "assets/images/favicon-48.png",
    "128": "assets/images/favicon-128.png"
  },
  "content_scripts": [
    {
      "js": ["contentPage.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "assets/images/*.png",
        "assets/images/*.jpg",
        "style/double-rainbow.css",
        "script/double-rainbow.js",
        "script/main.js",
        "templates/*"
      ],
      "matches": ["<all_urls>"]
    }
  ],
  "background": {
    "service_worker": "serviceWorker.js"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}
