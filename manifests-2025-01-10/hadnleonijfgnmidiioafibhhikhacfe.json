{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Paste to vSphere VM",
  "version": "1.2.0",
  "description": "This extension helps to paste the clipboard (password, etc) to vSphere VM HTML console via keystrokes",
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "action": {
    "default_icon": {
      "16": "icon.png",
      "32": "icon.png"
    }
  },
  "permissions": [
    "activeTab",
    "clipboardRead"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "commands": {
    "paste": {
      "suggested_key": {
        "default": "Ctrl+Shift+V",
        "mac": "Command+Shift+V"
      },
      "description": "Paste keypress listener"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "https://*/ui/webconsole.html*",
        "http://*/ui/webconsole.html*"
      ],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["pasteToVM.js"],
      "matches": ["<all_urls>"]
    }
  ]
}