{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "CopyTables",
  "version": "1.0.9",
  "description": "Copy web-based HTML and DIV Tables to clipboard with ease, supporting dynamic tables and sanitized output",
  "permissions": [
    "activeTab",
    "scripting",
    "clipboardWrite",
    "contextMenus"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "16": "images/table16.png",
      "48": "images/CopyTables48.png",
      "128": "images/CopyTables128.png"
    }
  },
  "icons": {
    "16": "images/table16.png",
    "48": "images/CopyTables48.png",
    "128": "images/CopyTables128.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["lodash.min.js", "background/purify.min.js", "content.js"]
    }
  ],
  "background": {
    "service_worker": "background/background.js"
  }
}