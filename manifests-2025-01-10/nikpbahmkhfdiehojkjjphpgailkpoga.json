"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "DocuStack.ai",
  "version": "0.1.8",
  "description": "Chrome extension that allows you to use the docustack platform",
  "permissions": [
    "storage",
    "sidePanel",
    "contextMenus",
    "tabs",
    "activeTab",
    "scripting"
  ],
  "background": {
    "service_worker": "src/pages/background/index.js",
    "type": "module"
  },
  "side_panel": {
    "default_path": "src/pages/panel/index.html"
  },
  "icons": {
    "128": "icon-128.png"
  },
  "host_permissions": [
    "http://*/",
    "https://*/"
  ],
  "content_scripts": [],
  "web_accessible_resources": [
    {
      "resources": [
        "assets/js/*.js",
        "assets/css/*.css",
        "icon-128.png",
        "icon-48.png",
        "icon-32.png",
        "icon-16.png",
        "assets/fonts/*.ttf"
      ],
      "matches": [
        "*://*/*"
      ]
    }
  ]
}