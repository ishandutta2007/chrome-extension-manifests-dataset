  "name": "HIVO Extension",
  "description": "Extension for the HIVO Library",
  "version": "0.0.7",
  "permissions": [
    "cookies",
    "storage",
    "downloads",
    "downloads.open"
  ],
  "host_permissions": [
    "https://app.hivo.co/api/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://app.hivo.co/api/extension/chrome/redirect?type=google",
