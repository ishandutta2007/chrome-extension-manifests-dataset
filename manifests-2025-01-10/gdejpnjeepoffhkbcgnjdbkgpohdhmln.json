{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "ContentLens C2PA Validator",
  "version": "1.0.1",
  "description": "Detect images and videos embedded with C2PA content credentials and verify their validity.",
  "icons": {
    "16": "images/icon-48.png",
    "48": "images/icon-72.png",
    "72": "images/icon-128.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>",
        "file:///"
      ],
      "js": [
        "contentScript.js"
      ],
      "all_frames": true
    }
  ],
  "background": {
    "service_worker": "serviceWorker.js"
  },
  "action": {
      "default_popup": "popup.html"
  },
  "permissions": [
    "contextMenus",
    "offscreen",
    "storage"
  ],
  "web_accessible_resources": [
    {
      "resources": ["toolkit_bg.wasm"],
      "matches": ["<all_urls>"]
    }
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self';"
  }
}
