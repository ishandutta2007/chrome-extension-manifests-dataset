{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "CSS Injector",
  "manifest_version": 3,
  "version": "0.2",
  "permissions": ["activeTab", "storage"],
  "options_page": "index.html",
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": "disabled.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ]
}
