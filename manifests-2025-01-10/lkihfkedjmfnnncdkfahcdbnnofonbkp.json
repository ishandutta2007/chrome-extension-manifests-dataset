{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "MediaBiz IMDb extension",
    "version": "1.0.6.2",
    "description": "This extension modifies the original IMDb page, so it's more suitable for MediaBiz researchers.",
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "permissions": [
      "storage", "clipboardWrite", "clipboardRead"
    ],
    "content_scripts": [
      {
        "js": ["script.js"],
        "matches": ["https://www.imdb.com/*"]
      }
    ]
}

