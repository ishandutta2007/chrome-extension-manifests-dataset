{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Youtube AD Blocker - Ad blocker for Youtube ",
  "description": "Removes ads from Youtube",
  "version": "1.0.0",
  "manifest_version": 3,
  "icons": {
    "16": "YoutubeAdBlocker16x16.png",
    "48": "YoutubeAdBlocker48x48.png",
