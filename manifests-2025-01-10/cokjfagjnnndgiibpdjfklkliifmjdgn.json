{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Capy | CRM Updater",
  "version": "0.0.17",
  "description": "A Chrome extension to update CRM from within the browser",
  "permissions": [
    "identity",
    "tabs"
  ],
  "host_permissions": [
    "https://*.salesforce.com/*"
  ],
  "action": {
    "default_icon": "logo256.png",
    "default_popup": "index.html"
  },
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.google.com/*",
        "https://otter.ai/u/*"
      ],
      "js": [
        "content/index.mjs"
      ]
    }
  ],
  "background": {
    "service_worker": "background.js"
  }
}
