{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name":"Agency Dashboard SEO Extension",
  "description": "Boost your SEO with on-page audits, keyword ideas, domain analysis, tech insights, and mobile view checks in a Chrome extension!",
  "version": "2.2",
  "manifest_version": 3,
  "permissions": [
    "declarativeNetRequest",
    "scripting",
    "activeTab",
    "tabs",
    "storage"
  ],
  "background": {
    "service_worker": "js/background.js"
  },
  "host_permissions": [
    "https://*/",
    "http://*/*"
  ],
  "action": {
    "default_popup": "agencyDashboard.html",
    "default_icon": "favicon.png"
  },
  "icons": {
    "128": "favicon.png"
  },
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "header_rules",
        "enabled": true,
        "path": "rule.json"
      }
    ]
  },
  "web_accessible_resources": [
    {
      "resources": ["js/mobileView.js","images/new-moon.svg"],
      "matches": ["<all_urls>"]
    }
  ],
  
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/bootstrap/updateResponsiveView.js"],
      "run_at": "document_start"
    }
  ]
  
}