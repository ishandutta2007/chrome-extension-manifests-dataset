{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Responsive Viewer",
  "description": "View any webpage in different device dimensions with custom sizing",
  "version": "1.1",
  "permissions": [
    "activeTab"
    ],
  "host_permissions": [
    "<all_urls>"
  ],
  "icons": {
    "16": "ima/icon.png",
    "48": "ima/icon.png",
    "128": "ima/icon.png"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'; style-src 'self' 'unsafe-inline' https://fonts.googleapis.com; font-src 'self' https://fonts.gstatic.com; frame-src *"
  },
  "action": {
    "default_icon": "ima/icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources": [{
    "resources": ["viewer.html", "viewer.js", "viewer.css"],
    "matches": ["<all_urls>"]
  }]
} 