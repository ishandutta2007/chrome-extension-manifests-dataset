{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Algumon Filter",
  "description": " It is a browser extension that helps make using the Algomon site more convenient.",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "https://www.algumon.com/"
  ],
  "permissions": [
    "storage",
    "scripting",
    "unlimitedStorage"
  ],
  "optional_host_permissions": [
    "https://*/*",
    "http://*/*"
  ],
  "content_scripts": [
    {
      "css": [],
      "js": [
        "jquery.min.js",
        "content.js"
      ],
      "matches": [
        "https://www.algumon.com/*"
      ]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/icon.png",
      "32": "/images/icon.png",
      "48": "/images/icon.png",
      "128": "/images/icon.png"
    }
  },
  "icons": {
    "16": "/images/icon.png",
    "32": "/images/icon.png",
    "48": "/images/icon.png",
    "128": "/images/icon.png"
  },
  "options_page": "options.html"
}