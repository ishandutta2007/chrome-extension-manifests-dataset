{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "ChatGPT Comment Navigator",
  "version": "1.0",
  "manifest_version": 3,
  "description": "Navigate through your comments on ChatGPT using shortcut keys.",
  "permissions": ["activeTab"],
  "host_permissions": ["https://chat.openai.com/*", "https://chatgpt.com/*"],
  "commands": {
    "scroll-previous": {
      "suggested_key": { "default": "Alt+K" },
      "description": "Scroll to previous user message"
    },
    "scroll-next": {
      "suggested_key": { "default": "Alt+J" },
      "description": "Scroll to next user message"
    }
  },
  "background": { "service_worker": "background.js" },
  "content_scripts": [
    {
      "matches": ["https://chat.openai.com/*", "https://chatgpt.com/*"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
