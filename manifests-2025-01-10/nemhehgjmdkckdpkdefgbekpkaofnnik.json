{
"update_url": "https://clients2.google.com/service/update2/crx",

    "name": "SWMU教务系统静态资源加载器",
    "description" : "将教务系统的静态文件重定向到本地文件，解决高峰时期加载静态资源发生卡顿问题，助力更快抢课！",
    "version": "1.1",
    "manifest_version": 3,
 "icons": {
   "16": "logo.png",
   "32": "logo.png",
   "48": "logo.png",
   "128": "logo.png"
   },
   
   "permissions": [
    "declarativeNetRequest"
   ],
   
   "host_permissions": ["http://ea.swmu.edu.cn/*"],

 "action": {
  "default_icon": {
    "16": "popup.png",
    "48": "popup.png",
    "128": "popup.png"
   },
   "default_title": "点我打开教务系统",
   "default_popup": "popup.html"
   },
 
 "declarative_net_request": {
    "rule_resources": [
      {
        "id": "ruleset_1",
        "enabled": true,
        "path": "rules.json"
      }
    ]
  },
  
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self';"
  },
  
    "web_accessible_resources": [
    {
      "resources": ["asset/*"],
      "matches": ["http://ea.swmu.edu.cn/*"]
    }
  ]
}