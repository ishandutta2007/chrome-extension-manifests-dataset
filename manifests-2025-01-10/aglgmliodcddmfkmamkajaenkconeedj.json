{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "Tripadvisor Review Replies",
    "version": "1.0",
    "description": "Chrome extension for effortless posting of TripAdvisor replies on Extranet platform.",
    "permissions": [
        
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup/popup.html",
        "default_icon": {
            
        }
    },
    "icons": {
        "16": "owl.png",
        "48": "owl.png",
        "128": "owl.png"
    },
   "content_scripts": [
    {
        "matches": ["https://www.tripadvisor.in/*"],
        "js": ["content.js"],
        "content_security_policy": {
            "extension_pages": "script-src 'self'; object-src 'self'",
            "sandbox": "script-src 'self' 'unsafe-eval'; object-src 'self'",
            "script-src": ["'self'", "'unsafe-eval'", "https://code.jquery.com"]
        }
    }
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "jquery-3.6.0.min.js"
            ],
            "matches": ["<all_urls>"] 
        }
    ]
}
