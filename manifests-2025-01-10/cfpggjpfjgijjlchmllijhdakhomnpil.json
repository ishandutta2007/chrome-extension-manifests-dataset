  ],
  "js":["ext_page.js"],
  "run_at" : "document_idle",
  "all_frames": false
 }],
 "browser_action":{
  "default_icon": {
   "16": "images/16.png",
   "32": "images/32.png"
  },
  "default_popup":"ext_popup.html"
 },
 "background":{
  "scripts":["ext_background.js"],
  "persistent": true
 },
 "icons":{
  "16": "images/16.png",
  "48": "images/48.png",
  "96": "images/96.png",
  "128": "images/128.png",
  "512": "images/512.png"

 }
