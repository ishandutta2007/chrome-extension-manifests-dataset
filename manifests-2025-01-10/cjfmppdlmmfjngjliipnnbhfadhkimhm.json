{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "GPT Sidebar Token Counter",
  "description": "This extension shows a count of tokens in the selected text.",
  "version": "1.0",
  "permissions": ["contextMenus"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "tkk4.png",
      "48": "tkk4.png",
      "128": "tkk4.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "16": "tkk4.png",
    "48": "tkk4.png",
    "128": "tkk4.png"
  }
}
