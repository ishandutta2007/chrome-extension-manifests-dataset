{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Autopilot Recorder",
  "version": "5.0",
  "background": {
    "service_worker": "background.bundle.js"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "*.html",
        "*.css",
        "*.js",
        "*.min.js",
        "*.png",
        "*.jpg",
        "*.jpeg",
        "assets/icons/*",
        "assets/antd/*",
        "assets/*"
      ],
      "matches": ["<all_urls>"],
      "use_dynamic_url": false
    }
  ],
  "permissions": [
    "tabs",
    "storage",
    "webNavigation",
    "matchAboutBlank",
    "https://cdnjs.cloudflare.com/",
    "https://autopilotqa.com/*",
    "https://test.autopilotqa.com/*",
    "https://dev.autopilotqa.com/*"
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'; img-src 'self' *;"
  },
  "action": {
    "default_popup": "index.html",
    "default_title": "Autopilot Recorder"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "match_about_blank": true,
      "js": ["contentScript.bundle.js"],
      "css": ["styles.css", "modal.css"],
      "run_at": "document_idle",
      "all_frames": true,
      "match_origin_as_fallback": true
    }
  ],

  "incognito": "spanning",

  "host_permissions": ["<all_urls>"],
  "icons": {
    "16": "assets/icons/icon.png"
  }
}
