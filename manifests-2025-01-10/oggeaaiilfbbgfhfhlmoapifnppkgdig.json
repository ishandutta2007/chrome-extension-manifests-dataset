    "insert_tag_3": {
      "suggested_key": {
        "default": "Ctrl+Shift+3"
      },
      "description": "Insert Tag #3"
    },
    "insert_button": {
      "suggested_key": {
        "default": "Ctrl+B"
      },
      "description": "Insert Button Tag"
    }
