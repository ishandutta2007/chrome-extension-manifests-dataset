{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "GitHub Filters",
  "description": "Save your GitHub filters",
  "version": "1.0",
  "manifest_version": 3,
  "externally_connectable": {
    "matches": ["*://*.github.com/*"]
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["*://*.github.com/*"],
      "js": [
        "jquery.min.js",
        "lodash.min.js",
        "github.js"
      ]
    }
  ]
}
