    "management",
    "webRequest",
    "declarativeNetRequestFeedback"
  ],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "index.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "/assets/icons/icon_64.png"
  },
  "icons": {
    "64": "assets/icons/icon_64.png",
    "128": "assets/icons/icon_128.png"
  },
  "options_ui": {
    "open_in_tab": true,
    "page": "/options.html"
  },
  "options_page": "/options.html",
  "externally_connectable": {
    "matches": ["*://*/*"]
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.aliexpress.com/p/trade/confirm.html*",
        "https://www.aliexpress.us/p/trade/confirm.html*"
      ],
      "js": ["AliExpressCouponContent.js"]
    },
    {
      "matches": ["https://*.aliexpress.com/**", "https://*.aliexpress.us/**"],
      "js": ["AliexpressQuickActionContent.js"]
    },
    {
      "matches": ["<all_urls>"],
      "js": ["GoogleGoggles.js", "TaoBaoSecuritySDK.js"],
      "css": ["GoogleGoggles.css"],
      "run_at": "document_end"
    },
    {
      "matches": ["https://shoppingcart.dhgate.com/order/pageload.do*"],
      "js": ["DHgateCouponContent.js"]
    },
    {
      "matches": ["https://*.dhgate.com/**"],
      "js": ["DHgateQuickActionContent.js"]
    },
    {
      "matches": ["https://*.temu.com/bgt_order_checkout.html*"],
      "js": ["TemuCouponContent.js"]
    },
    {
      "matches": ["https://*.temu.com/**"],
      "js": ["TemuQuickActionContent.js"],
      "exclude_matches": ["https://*.temu.com/bgt_order_checkout.html*"]
    },
    {
      "matches": ["https://elk-top-coupon.vercel.app/**", "*://localhost/*"],
      "js": ["ElkTopCoupon.js"]
    },
    {
      "matches": [
        "https://*.aliexpress.com/p/order/index.html*",
        "https://*.aliexpress.us/p/order/index.html*"
      ],
      "js": ["AliExpressAsk.js"]
    },
    {
      "matches": ["https://*.aliexpress.com/*", "https://*.aliexpress.us/*"],
      "js": ["AliexpressHotProductContent.js"]
    }
  ],
