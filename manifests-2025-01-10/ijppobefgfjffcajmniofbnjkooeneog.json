{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "__MSG_name__",
  "version": "1.0.2",
  "homepage_url": "https://twitter.com/",
  "description": "A feature-rich extension to mass follow and unfollow on Twitter.",
  "default_locale": "en",
  "permissions": [
    "activeTab"
  ],
  "host_permissions": [
    "*://twitter.com/*"
  ],
  "icons": {
    "128": "icons/logo.png"
  },
  "background": {
    "service_worker": "js/background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "*://twitter.com/*"
      ],
      "js": [
        "js/inject/content-script.js"
      ],
      "run_at": "document_end"
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "icons/logo.png"
    }
  },
  "web_accessible_resources": [
    {
      "resources": [
        "fonts/*",
        "img/*"
      ],
      "matches": [
        "*://twitter.com/*"
      ]
    }
  ]
}