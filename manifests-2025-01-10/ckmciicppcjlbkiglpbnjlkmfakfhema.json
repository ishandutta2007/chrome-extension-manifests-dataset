{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "TIME: Block and Control Ads",
  "description": "TIME is a revolutionary Browser extension that empowers you to own your web browsing experience",
  "version": "1.2.5",
  "icons": {
    "48": "images/48.png",
    "96": "images/96.png",
    "128": "images/128.png",
    "256": "images/256.png",
    "512": "images/512.png"
  },
  "action": {
    "default_title": "Time",
    "default_popup": "./popup/index.html",
    "default_icon": {
      "16": "images/16.png",
      "19": "images/19.png",
      "32": "images/32.png",
      "38": "images/38.png",
      "48": "images/48.png",
      "72": "images/72.png"
    }
  },
  "background": {
    "service_worker": "./background/index.js"
  },
  "permissions": [
    "tabs",
    "storage",
    "management",
    "cookies",
    "history",
    "system.display"
  ],
  "host_permissions": [
    "*://*/*"
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self' 'wasm-unsafe-eval'"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "./content/ads.js"
      ],
      "run_at": "document_end",
      "all_frames": true
    },
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "./content/extension-info.js"
      ],
      "run_at": "document_end",
      "all_frames": true
    }
  ]
}