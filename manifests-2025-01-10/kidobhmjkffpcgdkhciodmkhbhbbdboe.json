  "web_accessible_resources": [
    {
      "resources": ["fonts/*", "images/*"],
      "matches": ["<all_urls>"]
    }
  ],

  "permissions": ["storage", "identity", "activeTab"],
  "options_page": "options.html",
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
