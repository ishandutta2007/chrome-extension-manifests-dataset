{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "FB+1",
  "description": "FB+1 控制台插件",
  "version": "1.2.1",
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "scripting",
    "nativeMessaging", 
    "background",
    "downloads"
  ],
  "host_permissions": [
    "*://*.facebook.com/*",
    "https://fbplusone.kh-kuan-han.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": "img/icon.png",
    "default_popup": "popup.html",
    "default_title": "FB+1"
  },
  "manifest_version": 3,
  "icons": {
    "16": "img/16.png",
    "48": "img/48.png",
    "128": "img/128.png",
    "256": "img/icon.png",
    "512": "img/512.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.facebook.com/groups/*"
      ],
      "exclude_matches": [
        "https://www.facebook.com/groups/*/posts/*"
      ],
      "js": [
        "contentScript.js"
      ]
    },
    {
      "matches": [
        "https://www.facebook.com/groups/*/posts/*"
      ],
      "js": [
        "postScript.js"
      ]
    }
  ]
}