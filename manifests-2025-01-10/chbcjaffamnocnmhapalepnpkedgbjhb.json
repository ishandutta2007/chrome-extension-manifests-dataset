{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "LinkedIn Profile Notes",
  "version": "1.0.2",
  "description": "Add personal notes to LinkedIn profiles",
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "https://*.linkedin.com/*"
  ],
  "action": {
    "default_icon": {
      "16": "/assets/icon16.png",
      "48": "/assets/icon48.png",
      "128": "/assets/icon128.png"
    }
  },
  "icons": {
    "16": "/assets/icon16.png",
    "48": "/assets/icon48.png",
    "128": "/assets/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://*.linkedin.com/*"],
      "css": ["styles/notes-panel.css"],
      "js": [
        "js/utils/dom.js",
        "js/utils/storage.js",
        "js/utils/profile.js",
        "js/templates/PanelTemplate.js",
        "js/components/DraggablePanel.js",
        "js/components/NotesPanel.js",
        "js/content.js"
      ],
      "run_at": "document_end"
    }
  ]
}