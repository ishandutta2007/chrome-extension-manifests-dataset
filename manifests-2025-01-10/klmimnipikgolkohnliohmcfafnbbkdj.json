{
"update_url": "https://clients2.google.com/service/update2/crx",

    "name": "Simple Test E2E Recorder",
    "version": "1.1.13",
    "version_name": "1.1.13",
    "description": "Record E2E interactions with your web applications to the cloud.",
    "permissions": [
      "cookies",
      "storage", 
      "scripting", 
      "webNavigation",
      "webRequest"
    ],
    "host_permissions": ["<all_urls>"],
    "manifest_version": 3,
    "background": {
      "service_worker": "background.js"
    },
  
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "all_frames": true,
        "js" : ["frame.js"]
      }
    ],
  
    "action": {
      "default_icon": {
        "16": "assets/images/spt16.png",
        "32": "assets/images/spt32.png",
        "48": "assets/images/spt48.png",
        "128": "assets/images/spt128.png"
      }
    },
    "web_accessible_resources": [
      {
        "resources": [ "/assets/images/*" , "dialog.js"],
        "matches": ["<all_urls>" ]
      }
    ],
    "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self'; default-src 'self'; frame-ancestors 'none'; connect-src 'self' https://web.simpletest.ai;"
    }
  }