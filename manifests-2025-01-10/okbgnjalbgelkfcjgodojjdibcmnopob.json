    "https://*.hh.ru/*",
    "https://*.habr.ru/*",
    "https://*.habr.com/*",
    "https://*.headly.ru/*",
    "https://*.amazinghiring.com/*",
    "https://*.behance.net/*",
    "https://*.dribbble.com/*",
    "https://*.matchy.ru/*",
    "https://*.huntshare.tech/*",
    "https://*.artstation.com/*",
    "http://localhost:5173/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.headly.ru/*",
        "http://localhost:5173/*"
      ],
      "js": [
        "assets/skailer.js"
      ]
    }
