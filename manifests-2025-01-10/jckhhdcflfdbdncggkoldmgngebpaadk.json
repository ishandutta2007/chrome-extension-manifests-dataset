    "description": "This is an extension to extract data from tiktok",
    "manifest_version": 3,
    "author": "Miguel Salazar K",
    "content_scripts" : [
        {
            "matches" : ["https://www.tiktok.com/*"],
            "js" : ["tiktok-script.js"]
        }  
    ],
    "background": {
        "service_worker": "background.js"
    },
    "host_permissions": [
        "https://www.tiktok.com/*"
    ],
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "action": {
        "default_popup" :"index.html",
        "default_icon": {
            "16": "icon16.png",
            "48": "icon48.png",    
            "128": "icon128.png"
        }
    },
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
