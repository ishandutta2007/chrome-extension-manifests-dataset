{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Remove COOP Header from Google Lens",
  "description": "The Cross-Origin-Opener-Policy response header occasionally breaks opening links to lens.google.com - this extension removes it.",
  "version": "1.0",
  "permissions": [
    "declarativeNetRequest"
  ],
  "host_permissions": [
    "https://lens.google.com/*"
  ],
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "1",
        "enabled": true,
        "path": "rules.json"
      }
    ]
  }
}