{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "News Archivist",
  "version": "0.0.0.2",
  "description": "Redirect paywalled news articles to an archive site automatically.",
  "permissions": ["tabs"],
  "host_permissions": [
    "*://*.nytimes.com/*",
    "*://*.washingtonpost.com/*",
    "*://*.wsj.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["*://*.nytimes.com/*", "*://*.washingtonpost.com/*", "*://*.wsj.com/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "options.html",
    "default_icon": "icons/icon48.png"
  }
}
