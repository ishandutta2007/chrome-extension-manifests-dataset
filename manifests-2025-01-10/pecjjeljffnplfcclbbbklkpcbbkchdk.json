    "manifest_version": 3,
    "name": "Popup Blocker",
    "description": "This extension automatically closes tabs and windows opened by ad-requiring sites",
    "version": "3.1.0",
    "action": {
        "default_icon": {
            "19": "images/icon19.png",
            "38": "images/icon38.png"
