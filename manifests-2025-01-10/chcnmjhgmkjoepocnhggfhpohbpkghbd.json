  "description": "Add Oliver to youtube thumbnails (mostly made by LiamHarrison25)",
  "host_permissions": ["https://www.youtube.com/*"],
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["main.js"],
      "run_at": "document_idle"
