"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 2,
    "name": "DokuLink",
    "version": "1.3.0",
    "description": "Saves current tab link to dokuwiki.",
    "permissions": [
        "activeTab",
        "storage",
        "sessions",
        "https://*/*"
    ],
    "icons": { "64": "img/64-dokuwiki.png" },
