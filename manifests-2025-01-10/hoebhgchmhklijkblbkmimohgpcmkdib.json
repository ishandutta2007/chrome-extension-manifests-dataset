        "persistent": false,
        "scripts": [
            "lib/jquery-3.1.0.min.js",
            "util.js",
            "background.js",
            "sessions.js"
        ]
    },
    "commands": {
      "save-sessions": {
        "suggested_key": {
          "default": "Ctrl+Shift+S",
          "mac": "Ctrl+Shift+S"
        },
        "description": "Open sessions page"
      }
    },
    "browser_action": {
        "default_title": "DokuLink",
        "default_icon": "img/64-dokuwiki.png"
    },
    "options_ui": {
        "page": "options/options.html",
        "chrome_style": true
    },
