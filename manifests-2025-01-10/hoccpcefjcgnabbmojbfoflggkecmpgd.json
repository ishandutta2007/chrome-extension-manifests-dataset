{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "github-vscode-icons",
  "description": "This extension shows a VS Code icons in Github Repositories",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "version": "1.24",
  "author": "Daniel Derevjanik <daniel.derevjanik@gmail.com>",
  "offline_enabled": true,
  "browser_action": {
    "default_icon": {
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "web_accessible_resources": [
    "icons/*.svg",
    "images/*.gif"
  ],
  "content_scripts": [
    {
      "css": [
        "content.css"
      ],
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "permissions": [
    "storage",
    "*://bitbucket.org/*",
    "*://github.com/*",
    "*://gist.github.com/*",
    "*://gitlab.com/*",
    "*://pastebin.com/*",
    "*://sourceforge.net/*"
  ]
}