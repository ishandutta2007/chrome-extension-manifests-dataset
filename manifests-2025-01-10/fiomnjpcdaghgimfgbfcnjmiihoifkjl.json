  "action": {
    "default_title": "Click to Detangle"
  },
  "permissions": ["contextMenus"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content/index.js"],
      "css": ["content/css/styles.css"]
    }
  ],
  "host_permissions": [
    "https://*.detangle.ai/*",
    "http://*.detangle.ai/*",
    "https://detangle.ngrok.io/*"
  ]
