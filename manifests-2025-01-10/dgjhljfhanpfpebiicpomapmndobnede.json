{
"update_url": "https://clients2.google.com/service/update2/crx",

    "name": "Postr",
    "description": "Convert your social network post to image with nice design",
    "version": "2.1",
    "manifest_version": 3,
    "permissions": [
        "downloads",
        "background",
        "storage",
        "tabs"
