{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Quix VPN - Free & Secure VPN Proxy for Chrome",
  "version": "1.0.4",
  "description": "Quix VPN is a Chrome VPN extension for secure, private browsing. Enjoy unlimited internet access with multiple servers.",
  "action": {
    "default_popup": "./src/scripts/popup/popup.html"
  },
  "options_ui": {
    "page": "./src/scripts/options/options.html",
    "open_in_tab": true
  },
  "background": {
    "service_worker": "js/service-worker.js",
    "type": "module"
  },
  "icons": {
    "16": "./assets/icon-16.png",
    "48": "./assets/icon-48.png",
    "128": "./assets/icon-128.png"
  },
  "permissions": [
    "proxy",
    "webRequest",
    "webRequestAuthProvider",
    "storage",
    "unlimitedStorage",
    "activeTab",
    "background"
  ],
  "externally_connectable": {
    "matches": [
      "https://my.quix-vpn.com/*"
    ]
  },
  "web_accessible_resources": [
    {
      "matches": [
        "https://*/*",
        "http://*/*",
        "http://localhost/*"
      ],
      "resources": [
        "dist/js/index.js",
        "dist/js/options.js",
        "dist/js/popup.js",
        "dist/js/service-worker.js"
      ]
    }
  ],
  "host_permissions": [
    "http://*/*",
    "<all_urls>",
    "https://*/*"
  ],
  "commands": {
    "refresh_extension": {
      "suggested_key": {
        "default": "Ctrl+Space"
      },
      "description": "Refresh Extension"
    }
  }
