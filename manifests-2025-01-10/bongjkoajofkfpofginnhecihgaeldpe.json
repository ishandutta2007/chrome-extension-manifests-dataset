{
"update_url": "https://clients2.google.com/service/update2/crx",

    "name": "Video Resumer",
    "version": "2.0.0",
    "manifest_version": 3,
    "default_locale": "en",
    "description": "__MSG_description__",
    "icons": {
        "48": "img/icon48.png",
        "128": "img/icon128.png"
    },
    "options_page": "content/options.html",
    "background": {
        "service_worker": "main.js",
        "type": "module"
    },
    "content_scripts": [
        {
            "matches": ["https://www.youtube.com/watch?v=*"],
            "js": ["content/yt.js"]
        },
        {
            "matches": ["https://www.youtube.com/embed/*"],
            "js": ["content/yt.js"],
            "all_frames": true
        }
    ],
    "web_accessible_resources": [
        {
          "resources": [
            "content/console.js",
            "content/settings.js"
          ],
          "matches": [ "https://www.youtube.com/*" ]
        }
    ],
    "permissions": [
        "storage",
        "alarms"
    ]
}
