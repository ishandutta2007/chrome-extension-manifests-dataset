{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "English Training",
  "version": "1.0.4",
  "description": "English Training Extension",
  "action": {
    "default_icon": "./assets/icon-512.png",
