{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "task-tackler",
  "description": "A homework extension designed to ease homework sites",
  "version": "4",

  "background": {
    "service_worker": "js/background.js"
  },

  "content_scripts": [
    {
      "matches": ["https://www.sparxmaths.uk/*"],
      "js": ["js/sparx-maths/main.js"],
      "type": "module",
      "run_at": "document_end"
    },
    {
      "matches": ["https://app.bedrocklearning.org/*"],
      "js": ["js/bedrock-learning/main.js"],
      "type": "module",
      "run_at": "document_end"
    },
    {
      "matches": ["https://app.senecalearning.com/*"],
      "js": ["js/seneca-learning/main.js"],
      "type": "module",
      "run_at": "document_end"
    }
  ],

  "action": {
    "default_icon": "icon.png",
    "default_title": "Open link"
  },

  "permissions": ["tabs", "activeTab", "declarativeNetRequest"],

  "host_permissions": [
    "https://www.sparxmaths.uk/*",
    "https://my.educake.co.uk/*",
    "https://app.senecalearning.com/*",
    "https://www.knowitallninja.com/*",
    "https://app.bedrocklearning.org/*",
    "http://localhost:3000/*",
    "https://task-tackler.com/*",
    "https://www.task-tackler.com/*"
  ],

  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self';"
  },

  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "block_rules",
        "enabled": true,
        "path": "rules.json"
      }
    ]
  }
}
