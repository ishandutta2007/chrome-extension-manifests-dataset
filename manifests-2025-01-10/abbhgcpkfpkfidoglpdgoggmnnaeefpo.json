{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "ZooKeep Chrome Extension",
  "description": "Make your talent acquisition flow with ZooKeep",
  "version": "2.1",
  "manifest_version": 3,
  "homepage_url": "https://zookeep.com",
  "background": {
    "service_worker": "background.bundle.js"
  },
  "host_permissions": ["*://zookeep.com/*", "*://www.linkedin.com/*"],
  "permissions": ["cookies", "activeTab", "tabs", "identity", "storage", "scripting"],
  "content_scripts": [
    {
      "matches": ["*://*.linkedin.com/*", "*://*.cr-support.jp/*"],
      "js": ["main.bundle.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "256": "/img/logo_256.png"
    }
  },
  "icons": {
    "256": "/img/logo_256.png"
  },
  "content_security_policy": {
    "script-src": "self unsafe-eval",
    "object-src": "self"
  }
}
