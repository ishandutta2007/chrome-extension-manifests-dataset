{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "r/popular Filter",
    "version": "1.2",
    "description": "Blocks certain Reddit subreddits & posts from showing up in r/popular",
    "permissions": [
        "storage"
    ],
    "content_scripts": [
        {
            "matches": ["https://www.reddit.com/*"],
            "js": ["scripts/content.js", "scripts/storage.js"]
        }
    ],
    "background": {
        "service_worker": "scripts/background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": "icon.png"
    }
}