{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Straico Assistant",
  "version": "1.2.0",
  "description": "A Chrome extension side panel to interact with the Straico API.",
  "author": "Hossam Gouda",
  "homepage_url": "https://github.com/HossamGouda/Straico-Assistant",
  "permissions": ["storage", "sidePanel", "https://api.straico.com/*"],
  "action": {
    "default_title": "Click to open panel",
    "default_icon": {
      "16": "public/icons/icon16.png",
      "48": "public/icons/icon48.png",
      "128": "public/icons/icon128.png"
    }
  },
  "side_panel": {
    "default_path": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "16": "public/icons/icon16.png",
    "48": "public/icons/icon48.png",
    "128": "public/icons/icon128.png"
  }
}
