{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "简渠",
  "version": "1.1.0",
  "description": "科锐简渠浏览器插件",
  "manifest_version": 2,
  "author": "Jesse Zhang",

  "minimum_chrome_version": "58",

  "icons": {
    "16": "skin/logo-blue.png",
    "48": "skin/logo-blue.png"
  },

  "browser_action": {
    "default_icon": {
      "16": "skin/logo-blue.png",
      "48": "skin/logo-blue.png"
    },
    "default_title": "简渠",
    "default_popup": "./browser.html"
  },

  "content_scripts": [
    {
      "run_at": "document_idle",
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "./xhrSpyLoader.js",
        "./xhrSpyBridge.js",
        "./contentLoader.js"
      ]
    }
  ],

  "background": {
    "scripts": [
      "./third-pack/hot-reload.js",
      "./background.js"
    ]
  },

  "web_accessible_resources": [
    "*"
  ],

  "permissions": [
    "http://*/*",
    "https://*/*",
    "cookies",
    "notifications",
    "storage",
    "tabs",
    "webNavigation",
    "webRequest",
    "webRequestBlocking"
  ]
