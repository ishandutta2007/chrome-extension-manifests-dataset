  "content_scripts": [
    {
      "js": ["contentscript.js"],
      "matches": ["https://app.tapsi.cab/*"]
    }
  ],
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "web_accessible_resources": ["assets/*"],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
