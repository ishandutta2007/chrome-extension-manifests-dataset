"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "My Tapsi",
  "short_name": "My Tapsi",
  "version": "3.1.0",
  "description": "Analyze your Tapsi rides",
  "permissions": ["tabs", "storage"],
  "browser_action": {
    "default_popup": "popup/index.html",
    "default_title": "My Tpasi"
