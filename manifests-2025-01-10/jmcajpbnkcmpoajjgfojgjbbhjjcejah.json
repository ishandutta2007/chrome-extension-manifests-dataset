{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "description": "This plugin adds custom buttons next to the reply buttons inside Gmail.",
  "name": "Gmail Buttons",
  "version": "4.3",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": ["*://mail.google.com/*"],
      "run_at": "document_end",
      "js": ["jquery.js", "content.js"]
    }
  ]
}
