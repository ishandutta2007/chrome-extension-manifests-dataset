{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "GitHub Text to Poe",
  "version": "1.1",
  "description": "GitHubで選択したテキストをPoeと連携させる機能",
  "permissions": ["scripting", "activeTab"],
  "host_permissions": [
    "https://github.com/*",
    "https://poe.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*",
        "https://poe.com/*"
      ],
      "js": ["content.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources": [
    {
      "resources": ["injected.js", "icon.jpeg"],
      "matches": ["<all_urls>"]
    }
  ],
  "content_security_policy": {
    "extension_pages": "default-src 'self'; script-src 'self'; style-src 'self'; object-src 'self'; img-src 'self' data: blob: https://github.githubassets.com https://poe.com https://poeusercontent.com; font-src 'self' https://github.githubassets.com; connect-src 'self' https://github.com https://avatars.githubusercontent.com https://poe.com; media-src 'self' data: blob:"
  },
  "icons": {
    "128": "icon.png"
  }
}