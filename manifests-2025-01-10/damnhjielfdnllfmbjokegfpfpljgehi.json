{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Audio Element",
  "version": "1.0",
  "description": "An extension for managing audio playback on web pages.",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "action": {
    "default_title": "Open Modal"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["modal.css"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["modal.html", "audio/*"],
      "matches": ["<all_urls>"]
    }
  ],
  "icons": {
    "48": "icon.png"
  }
}
