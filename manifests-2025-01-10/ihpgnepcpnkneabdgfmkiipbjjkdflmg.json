{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,

  "name": "B2B Wiki Redirect",
  "description": "This extension redirects your Wikipedia accesses to our B2B version",
  "version": "1.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "B2B Wiki"
  },
  "icons" : {
    "32" : "icon.png",
    "32" : "icon_off.png"
  },

  "background": {
    "scripts": ["background.js"]
  },

  "permissions": [
    "activeTab",
    "webRequest",
    "webRequestBlocking",
    "webNavigation",
    "storage",
    "tabs",
    "*://*.wikipedia.org/",
 "*://b2bwiki.cc.gatech.edu/"
  ]
}
