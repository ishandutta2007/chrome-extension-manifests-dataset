{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Doormat Extension",
  "description": "Unlimited keys, no seed phrases",
  "version": "0.1.45",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "host_permissions": [
    "*://*/*"
  ],
  "background": {
    "service_worker": "background.bundle.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "assets/img/favicon-16x16.png",
      "32": "assets/img/favicon-32x32.png"
    }
  },
  "icons": {
    "32": "assets/img/favicon-32x32.png",
    "128": "assets/img/favicon-128x128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "browser-polyfill.js",
        "contentScript.bundle.js",
        "injectable.bundle.js"
      ],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "setupProvider.bundle.js",
        "assets/*"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}
