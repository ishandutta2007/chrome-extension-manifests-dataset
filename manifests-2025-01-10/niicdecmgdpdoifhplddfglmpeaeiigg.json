{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "ReadLingo",
  "description": "生词高亮 | 无数据收集 | 安心阅读 | Local First 生词管理 | 单词拼写检查",
  "version": "1.1.5.0",
  "version_name": "1.1.5",
  "icons": {
    "128": "src/assets/icon.png"
  },
  "action": {
    "default_icon": "src/assets/icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "notifications",
    "contextMenus"
  ],
  "content_scripts": [
    {
      "js": [
        "assets/content.tsx-loader.js"
      ],
      "matches": [
        "*://*/*"
      ],
      "all_frames": true,
      "run_at": "document_end",
      "match_about_blank": true
    }
  ],
  "options_page": "options.html",
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "web_accessible_resources": [
    {
      "matches": [
        "*://*/*"
      ],
      "resources": [
        "assets/select.js",
        "assets/animated-tab.js",
        "assets/constants.js",
        "assets/oulu-api.js",
        "assets/highlight-style.js",
        "assets/___vite-browser-external_commonjs-proxy.js",
        "assets/content.tsx.js"
      ],
      "use_dynamic_url": false
    }
  ]
