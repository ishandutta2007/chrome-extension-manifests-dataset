  "manifest_version": 3,
  "name": "Car Bid Helper",
  "version": "2.0.0",
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "https://www.carbidhelper.com/*",
    "https://www.copart.com/*",
    "https://www.iaai.com/*"
  ],
  "externally_connectable": {
    "matches": ["https://www.carbidhelper.com/*"]
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://www.copart.com/*"],
      "js": ["copart.js"]
    },
    {
      "matches": ["https://www.iaai.com/*"],
      "js": ["iaai.js"]
    }
  ],
  "action": {
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  }
