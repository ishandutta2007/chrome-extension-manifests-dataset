  "tabs", 
  "storage"
 ],

 "content_security_policy": {
  "script-src": "self",
  "object-src": "self"
 },

 "content_scripts": [{
  "matches": [ 
   "https://twitter.com/*"
  ],
  "js": [
   "js/content.js"
  ],
