    "storage",
    "alarms",
    "cookies"
  ],
  "host_permissions": [
    "*://*.twitter.com/*"
  ],
  "content_scripts": [
    {
      "js": [
        "content/import-index.js"
      ],
      "matches": [
        "*://*.twitter.com/*"
      ]
    }
  ],
  "options_page": "pages/popup/index.html",
  "action": {
    "default_popup": "pages/popup/index.html"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "content/closeInfoPopup.js",
        "content/autoBlock.js"
      ],
      "matches": [
        "*://*.twitter.com/*"
      ]
    },
    {
      "resources": [
        "chunks/*-*.js",
        "content/index.js"
      ],
