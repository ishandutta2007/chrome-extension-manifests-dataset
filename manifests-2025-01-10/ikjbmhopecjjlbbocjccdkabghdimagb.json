{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Webflow Productivity Extension",
  "version": "1.1.0",
  "description": "A little extension to be more productive when developing on Webflow",
  "permissions": ["activeTab"],
  "host_permissions": ["https://*.webflow.com/*"],
  "background": {
    "service_worker": "scripts/background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://*.webflow.com/*"],
      "js": ["scripts/content.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["background.js", "scripts/injected.js"],
      "matches": ["https://*.webflow.com/*"]
    }
  ],
  "icons": {
    "16": "settings-webflow.png",
    "48": "settings-webflow.png",
    "128": "settings-webflow.png"
  }
}
