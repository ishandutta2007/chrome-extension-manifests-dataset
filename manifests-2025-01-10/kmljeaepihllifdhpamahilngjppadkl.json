{
"update_url": "https://clients2.google.com/service/update2/crx",
  "name": "SparkReach.ai - Automation Tool for LinkedIn",
  "manifest_version": 3,
  "version": "1.8.8",
  "author": "Florent Duthoit",
  "description": "Connect your LinkedIn account easily on SparkReach.ai.",
  "permissions": ["cookies", "background", "storage", "alarms", "webNavigation"],
  "host_permissions": [
    "https://www.linkedin.com/*"
  ],
  "icons": { "16": "Logo.png", "48": "Logo.png", "128": "Logo.png" },
  "action": {
    "default_icon": "Logo.png",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "externally_connectable": {
    "matches": ["*://localhost/*", "*://localhost/*", "*://apps.sparkreach.ai/*", "*://app.sparkreach.ai/*"]
  },
  "content_scripts": [
    {
      "matches": ["https://www.linkedin.com/*"],
      "css": ["./linkedin/app.css"],
      "js": ["./linkedin/app.js", "./env.js", "./utils/request_utility.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "iframe.html",
        "Logo.png",
        "logo_horizontal_white.svg"
      ],
      "use_dynamic_url": false
    },
    {
      "matches": [
        "https://www.linkedin.com/*"
      ],
      "resources": [],
      "use_dynamic_url": true
    }
  ]
}
