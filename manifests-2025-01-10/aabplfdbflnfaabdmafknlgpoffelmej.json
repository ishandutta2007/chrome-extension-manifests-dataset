        {
            "matches": ["https://post.craigslist.org/*"],
            "js": ["/js/jquery.js", "js/utils.js", "js/craigslist.js"]
        },
        {
            "matches": ["*://*.craigslist.org/"],
            "js": ["/js/jquery.js", "js/utils.js", "js/craigslist_redirect.js"]
