  "version": "0.1",
  "manifest_version": 3,
  "description": "Adds a Print PDF button to the chat.openai page.",
  "content_scripts": [
    {
      "matches": ["https://chat.openai.com/*"],
      "js": ["html2canvas.js", "jspdf.js", "html2pdf.js", "content.js"],
      "css": ["myscript.css"]
    }
