  "name": "imageupload.app Uploader",
  "version": "1.0",
  "description": "Help you select images from web pages, upload them to imageupload.app and keep them in your history.",
  "permissions": ["activeTab", "storage"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html",
