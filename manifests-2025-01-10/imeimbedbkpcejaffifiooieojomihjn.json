    "default_icon": {
      "16": "icon_16.png",
      "32": "icon_32.png"
    },
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon_16.png",
    "32": "icon_32.png",
    "48": "icon_48.png",
    "72": "icon_72.png",
    "96": "icon_96.png",
    "144": "icon_144.png",
    "192": "icon_192.png",
    "512": "icon_512.png"
  },
  "permissions": ["declarativeNetRequestWithHostAccess"],
  "host_permissions": ["*://*.desmos.com/calculator/*"],
  "web_accessible_resources": [
    {
      "resources": ["calculator_desktop.js"],
      "matches": ["*://*.desmos.com/*"]
    }
  ]
