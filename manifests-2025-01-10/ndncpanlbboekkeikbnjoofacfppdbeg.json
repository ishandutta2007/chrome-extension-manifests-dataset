{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "__MSG_extName__",
  "description": "__MSG_extDescription__",
  "default_locale": "en",
  "version": "1.1.2",
  "homepage_url": "https://growthbot.extensionsfox.com/",
  "action": {
    "default_icon": "./assets/icon-512.png",
