{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "Fluentez-extension",
    "version": "1.0.1",
    "description": "Chrome extension that allows users to quickly build their vocabulary on the Fluentez website by selecting text anywhere",
    "action": {
        "default_popup": "index.html"
    },
    "permissions": ["declarativeNetRequest", "declarativeNetRequestWithHostAccess", "storage"],
    "host_permissions": ["https://*/*", "http://*/*"],
    "icons": {
        "16": "vitefavicon.png",
        "48": "vitefavicon.png",
        "128": "vitefavicon.png"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["contentScript/index.global.js"],
            "css": ["contentScript/style.css"],
            "type": "module"
        }
    ],
    "web_accessible_resources": [
        {
            "resources": ["vitefavicon.png", "/contentScript/*.css"],
            "matches": ["<all_urls>"]
        }
    ],
    "background": {
        "service_worker": "background/index.global.js",
        "type": "module"
    },
    "declarative_net_request": {
        "rule_resources": [
            {
                "id": "ruleset_1",
                "enabled": true,
                "path": "rules.json"
            }
        ]
    }
}
