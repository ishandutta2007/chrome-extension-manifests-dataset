{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Heimdall",
  "description": "__MSG_app_desc__",
  "version": "1.8.0",
  "minimum_chrome_version": "109",
  "content_security_policy": {
    "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self';"
  },
  "permissions": [
    "identity",
    "identity.email",
    "storage",
    "proxy",
    "alarms",
    "webRequest",
