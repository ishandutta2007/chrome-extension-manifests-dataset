    "cookies",
    "management",
    "downloads",
    "webNavigation",
    "offscreen",
    "scripting"
  ],
  "icons": {
    "128": "logo.png"
  },
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "loader-1.8.0.js",
    "type": "module"
  },
  "action": {
    "default_popup": "index.html",
    "default_icon": "logo.png"
  },
  "default_locale": "en",
  "oauth2": {
    "client_id": "heimdall.shopee.sg",
    "scopes": [
      "openid",
      "email",
      "profile"
