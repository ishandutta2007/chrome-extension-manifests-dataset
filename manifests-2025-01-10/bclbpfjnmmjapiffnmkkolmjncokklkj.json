{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "FilterOut",
  "version": "1.1",
  "description": "Filter YouTube feed based on slots",
  "permissions": [
    "storage",
    "identity",
    "scripting"
    ],
  "host_permissions": [
    "https://www.youtube.com/*",
    "https://www.googleapis.com/*",
    "https://dev.filterout.in/*",
    "https://filterout.in/*"
  ],
  "action": {
    "default_popup": "index.html",
    "default_icon": "filter128.png"
  },
    "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],      
      "js": ["contentScript.js"],
      "css": ["youtube-filter.css"],
      "run_at": "document_idle"
    }
  ],

  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
    "web_accessible_resources": [
    {
      "resources": ["youtube-filter.css", "config.js","filter-white-outline128.png"],
      "matches": ["https://www.youtube.com/*"]
    }
  ],
  "icons": {
    "48": "filter48.png",
    "128": "filter128.png"
  },
"oauth2": {
  "client_id": "892830849168-gc5pbkl5d2q0t0ld80pt9tnl0r1rngul.apps.googleusercontent.com",
  "scopes": [
    "https://www.googleapis.com/auth/userinfo.email",
    "https://www.googleapis.com/auth/userinfo.profile"
    ]
}
