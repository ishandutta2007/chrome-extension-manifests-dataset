  },
  "content_scripts": [
    {
      "js": ["content.js"],
      "all_frames": true,
      "matches": ["<all_urls>"],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "assets/*",
        "styles/*",
        "js/content/*"
      ],
      "matches": ["<all_urls>"]
    }
  ]
