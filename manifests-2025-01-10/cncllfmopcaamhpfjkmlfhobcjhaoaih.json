{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Sumy",
  "version": "1.1.9",
  "author": "Omeyer Najjar",
  "description": "Web browsing shouldn't be a task! Make browsing FAST by summarizing websites with a simple shortcut or a right-click.",
  "permissions": [
    "activeTab",
    "storage",
    "contextMenus"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://www.google.com/*", "https://www.bing.com/*", "https://*/*", "http://*/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Configure API Keys"
  },
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["summary.html", "welcome.html"],
      "matches": ["<all_urls>"]
    }
  ]
}
