            "matches": [
                "https://*.disneyplus.com/*",
                "https://*.netflix.com/*",
                "https://*.peacocktv.com/*",
                "https://*.hotstar.com/*",
                "https://*.youtube.com/*"
            ],
            "js": [
                "/vendor/youtube/page_script.js",
                "/vendor/youtube/iframe_api.js",
                "/vendor/youtube/www-widgetapi.js",
