"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Peoples Choice Extender",
  "version": "1.1.1",
  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/watch*"],
      "js": ["pages/youtube.js"],
      "css": ["styles/buttonStyles.css"]
    }
  ],
  "permissions": [
    "storage"
