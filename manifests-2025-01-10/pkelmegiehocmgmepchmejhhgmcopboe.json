  "background": {
    "scripts": ["main.js", "bg.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "css": [
        "css/selectize.css",
        "css/selectize.default.css",
        "css/main.css"
      ],
      "js": [
        "jquery-1.11.0.min.js",
        "selectize.js",
        "main.js",
        "content.js"
      ]
    }
  ]
