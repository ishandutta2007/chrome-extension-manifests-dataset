    "32": "icons/pt-icon-32_neutral.png",
    "48": "icons/pt-icon-48_neutral.png",
    "128": "icons/pt-icon-128_neutral.png"
  },
  "devtools_page": "src/pages/devtools/index.html",
  "host_permissions": [
    "*://*.etsy.com/*",
    "https://app.profittree.io/*"
  ],
  "externally_connectable": {
    "matches": [
      "https://app.profittree.io/*"
