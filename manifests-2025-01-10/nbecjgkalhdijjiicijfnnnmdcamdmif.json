      "js": ["js/_libs.js", "js/_myapp.js"],
      "css": ["css/styles.css", "css/emojis.css"],
      "run_at": "document_end"
    }
  ],

  "icons" : {
    "16" : "images/icon16.png",
    "24" : "images/icon19.png",
    "32" : "images/icon38.png",
    "48" : "images/icon48.png",
    "128" : "images/icon128.png"
  },

  "web_accessible_resources": [
    {
      "resources": [
        "images/icons/*",
        "images/emoji/*"
      ],
      "matches": ["<all_urls>"]
    }
    
