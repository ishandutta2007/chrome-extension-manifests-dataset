  "manifest_version": 3,
  "name": "YouTube Channel Whitelist",
  "version": "1.0",
  "description": "Hide YouTube videos from channels not on the whitelist.",
  "permissions": ["storage"],
  "host_permissions": ["*://www.youtube.com/*"],
  "action": {
