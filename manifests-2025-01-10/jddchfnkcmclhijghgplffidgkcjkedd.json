{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "NoType.ai",
  "version": "1.0",
  "description": "Voice-to-text input for any website",
  "permissions": ["activeTab", "storage"],
  "host_permissions": ["https://notypeai-backend.onrender.com/*"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "assets/icon16.png",
      "48": "assets/icon48.png",
      "128": "assets/icon128.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": [
        "services/voice-storage.js",
        "services/web-speech.js",
        "content/gmail-config.js",
        "content/position-handler.js",
        "content/content.js"
      ],
      "css": ["content/styles.css"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources": [
    {
      "resources": ["assets/*"],
      "matches": ["<all_urls>"]
    }
  ]
}
