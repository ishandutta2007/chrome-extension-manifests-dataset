    {
      "matches": ["<all_urls>"],
      "js": ["content-script.js"],
      "css": ["tooltip.css"]
    }
  ],
  "commands": {
    "toggle-side-panel": {
      "suggested_key": {
        "default": "Ctrl+Shift+S",
        "mac": "Command+Shift+S"
      },
      "description": "Toggle the side panel"
