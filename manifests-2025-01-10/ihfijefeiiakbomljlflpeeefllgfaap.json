{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "version": "0.0.3",
  "name": "Gecko learn",
  "icons": {
    "16": "img/gecko-learn-paw-16.png",
    "48": "img/gecko-learn-paw-48.png",
    "128": "img/gecko-learn-paw-128.png"
  },
  "description": "Add words to your personal dictionary on Gecko learn",
  "background": {
    "service_worker": "background.js"
  },
  "externally_connectable": {
    "matches": [
      "http://localhost:5173/*",
      "https://gecko-learn.solop.cc/*"
    ]
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["content.js"]
    }
  ],
  "commands": {
    "Ctrl+K": {
      "suggested_key": {
        "default": "Ctrl+K",
        "mac": "Command+K"
      },
      "description": "Mark word as known."
    },
    "Ctrl+I": {
      "suggested_key": {
        "default": "Ctrl+I",
        "mac": "Command+I"
      },
      "description": "Send word to ignore."
    },
    "Ctrl+L": {
      "suggested_key": {
        "default": "Ctrl+L",
        "mac": "Command+L"
      },
      "description": "Send word for learning."
    },
    "Ctrl+U": {
      "suggested_key": {
        "default": "Ctrl+U",
        "mac": "Command+U"
      },
      "description": "Highlight unknown words on page."
    }
  },
  "permissions": ["contextMenus", "activeTab", "scripting", "storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "img/icon.png",
      "24": "img/icon.png",
      "32": "img/icon.png"
    }
  }
