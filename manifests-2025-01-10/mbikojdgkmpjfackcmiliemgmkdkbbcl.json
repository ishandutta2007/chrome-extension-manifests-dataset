    "128": "img/icon128.png"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "matches": [
        "https://*/*",
        "http://*/*"
      ],
      "js": [
        "dist/contentScript/main.js"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "util/common.js"
      ],
      "matches": ["<all_urls>"]
    }
  ]
