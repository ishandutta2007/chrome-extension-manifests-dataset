      "js": ["detector.js"],
      "matches": [
        "https://*.nextspreed.com/*",
        "https://next.spreed.com/*",
        "https://*.spreed.eu/*",
        "https://*.spreed.com.au/*",
        "https://*.spreed.net/*"
      ],
      "run_at": "document_end",
      "all_frames": true
