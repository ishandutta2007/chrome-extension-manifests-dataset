{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 2,
    "name": "Tab Renamer",
    "version": "0.23.0",
    "description": "Take control of your tabs",
    "homepage_url": "https://github.com/imageslr/chrome-tab-modifier",

