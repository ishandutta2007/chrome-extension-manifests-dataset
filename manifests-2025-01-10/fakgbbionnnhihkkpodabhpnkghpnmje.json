{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Truvit browser extension",
  "version": "1.0.10",
  "description": "this is Truvit browser extension, it can help you to manage and learn your vocabulary.",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "content_scripts": [
    {
      "js": [
        "assets/content.ts-loader-cIffuILp.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "all_frames": true,
      "run_at": "document_end",
      "match_about_blank": true,
      "exclude_matches": [
        "https://truvit.book2gpt.cn/#/vocabulary/list",
        "http://localhost:1002/*",
        "https://www.book2gpt.com/*"
      ]
    }
  ],
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "permissions": [
    "storage",
    "contextMenus",
    "activeTab",
    "notifications",
    "contextMenus.onShown",
    "cookies"
  ],
  "host_permissions": [
    "<all_urls>",
    "http://localhost:1002/",
    "https://*/api/*",
    "https://dict.youdao.com/*"
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "assets/config-BypDz6pl.js",
        "assets/content.ts-0ujV49ER.js"
      ],
      "use_dynamic_url": false
    }
  ]
}
