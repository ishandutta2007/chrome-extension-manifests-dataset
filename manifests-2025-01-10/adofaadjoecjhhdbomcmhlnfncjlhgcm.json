{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "IVAN to Gamma",
  "description": "This extension copies IVAN output text to Gamma prompt.",
  "version": "1.0",
  "action": {
    "default_popup": "description.html"
  },
  "content_scripts": [
    {
      "js": ["lib/purify.min.js", "scripts/paste.js"],
      "matches": [
        "https://gamma.app/create/paste*"
      ]
    },
    {
      "js": ["scripts/installed.js"],
      "matches": [
        "http://localhost:4200/*",
        "https://shikakukaigi-staging.web.app/*",
        "https://shikaku-kaigi.minmeeting.com/*"
      ]
    }
  ],
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}