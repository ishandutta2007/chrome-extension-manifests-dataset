{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "Utility Toolkit for Webflow",
    "version": "1.01.0",
    "description": "A handy lil extension for Webflow developers. Provides quality-of-life upgrades for building in Webflow.",
    "permissions": [
      "storage"
    ],
    "action": {
      "default_popup": "popup/popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "content_scripts": [
      {
        "matches": ["https://webflow.com/dashboard", "https://webflow.com/dashboard?*"],
        "js": ["features/workspace-manager/content.js"],
        "css": ["features/workspace-manager/styles.css"]
      },
      {
        "matches": ["https://*.design.webflow.com/*"],
        "js": [
          "utils/designer-load-observer.js",
          "features/publish-favicon/content.js",
          "features/close-cms/content.js",
          "features/collapse-cms/content.js",
          "features/dropdown-titles/content.js"
        ],
        "css": ["features/collapse-cms/styles.css"]
      }
    ],
    "web_accessible_resources": [
      {
        "resources": [
          "features/publish-favicon/images/*.svg",
          "features/collapse-cms/images/*.svg"
        ],
        "matches": ["https://*.design.webflow.com/*"]
      }
    ],
    "icons": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }