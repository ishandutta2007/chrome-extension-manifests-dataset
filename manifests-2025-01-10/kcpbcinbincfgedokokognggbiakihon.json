  "manifest_version": 3,
  "name": "Promptsion",
  "version": "1.1",
  "description": "Optimize your prompts, Search with an AI assistant, and organize your ChatGPT conversations with folders.",
  "permissions": ["activeTab", "storage", "contextMenus", "scripting", "tabs"],
  "host_permissions": [
    "*://*.chatgpt.com/*",
    "*://*.chat.openai.com/*",
    "*://*.claude.ai/*",
    "*://*.anthropic.com/*",
    "*://*.gemini.google.com/*",
    "*://*.promptsion.com/*"
  ],
  "background": {
    "service_worker": "background/background.js"
  },
  "externally_connectable": {
    "matches": ["https://www.promptsion.com/*"]
  },
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "16": "assets/icon-16.png",
      "32": "assets/icon-32.png",
      "48": "assets/icon-48.png",
      "128": "assets/icon-128.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content/content.js"],
      "run_at": "document_end"
    }
  ],
  "options_page": "options/options.html",
  "icons": {
    "16": "assets/icon-16.png",
    "32": "assets/icon-32.png",
    "48": "assets/icon-48.png",
    "128": "assets/icon-128.png"
  }
