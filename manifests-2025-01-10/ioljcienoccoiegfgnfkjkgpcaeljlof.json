{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Wiggli LinkedIn extension",
  "version": "0.0.6",
  "description": "Unlock the power of seamless recruitment with Wiggli’s Chrome Extension for LinkedIn.",
  "action": {
    "default_icon": "assets/38.png",
    "default_popup": "./dist/popup/index.html"
  },
  "options_ui": {
    "page": "./dist/options/index.html",
    "open_in_tab": true
  },
  "background": {
    "service_worker": "./dist/background.js",
    "type": "module"
  },
  "icons": {
    "48": "assets/48.png",
    "96": "assets/96.png"
  },
  "permissions": [
    "activeTab",
    "cookies"
  ],
  "host_permissions": [
    "*://*.wiggli.io/*",
    "*://*.linkedin.com/*",
    "*://localhost/*",
    "http://localhost:3000/*",
    "*://*.hme.ovh/*",
    "*://front.develop.hme.ovh/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.linkedin.com/*"
      ],
      "js": [
        "./dist/contentScripts/index.global.js"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "dist/contentScripts/style.css"
      ],
      "matches": [
        "*://*.linkedin.com/*"
      ]
    }
  ]
}
