  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "web_accessible_resources": [{
    "resources": ["logos/*", "stolen-logos.js"],
    "matches": ["<all_urls>"]
  }]
