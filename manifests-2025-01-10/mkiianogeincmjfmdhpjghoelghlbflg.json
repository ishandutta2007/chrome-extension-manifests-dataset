            "css": ["mal-top/mal-top.css"],
            "js": ["mal-top/mal-top.js"],
            "matches": ["https://myanimelist.net/topanime.php*"]
        },
        {
            "css": ["mal-anime/mal-anime.css"],
            "js": ["mal-anime/mal-anime.js"],
            "matches": ["https://myanimelist.net/anime/*"]
        }
