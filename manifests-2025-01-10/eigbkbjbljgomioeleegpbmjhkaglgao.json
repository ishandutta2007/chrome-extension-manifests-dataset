{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "Workable Rating Remover.",
  "version": "1.0.1",
  "description": "Hides rating information in Workable.",
  "background": {
    "scripts": ["event_page.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Hide Ratings",
    "default_icon": {
      "16": "icons/disabled16.png"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.workable.com/backend/jobs/*"
      ],
      "css": [
        "hide_content.css"
      ]
    }
  ]
}
