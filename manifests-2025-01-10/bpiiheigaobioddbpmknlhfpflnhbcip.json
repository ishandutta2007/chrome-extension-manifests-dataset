{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Phone on TikTok Shop",
  "version": "1.0.0",
  "description": "Extension lấy thông tin đơn hàng trên TikTok Shop by: giapducthang.com",
  "permissions": ["tabs", "storage", "activeTab", "scripting", "webRequest"],

  "web_accessible_resources": [
    {
      "resources": ["addOnTools.png"],
      "matches": ["https://seller-vn.tiktok.com/*"]
    }
  ],
  "host_permissions": [
    "https://seller-vn.tiktok.com/api/v3/trade/orders/get*",
    "http://localhost:3000/*",
    "http://localhost:8080/*",
    "https://add-on.tools/*",
    "https://seller-vn.tiktok.com/api/fulfillment/order/list*"
  ],
  "content_scripts": [
    {
      "matches": ["https://seller-vn.tiktok.com/order*"],
      "js": ["temcontentscr.js"],
      "css": ["style.css"]
    },
    {
      "matches": ["https://add-on.tools/"],
      "js": ["temcontentscr.js"]
    }
  ],
  "background": {
    "service_worker": "tempbg.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "logo.png",
    "32": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  }
