{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "GPTPrompt",
  "version": "1.0.1",
  "description": "You can define your own prompts for GPT and use them in the browser.",
  "permissions": ["activeTab", "storage"],
  "background.service_worker": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/prompt_gpt.js"],
      "css": ["css/style.css"]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "options_ui": {
      "page": "options.html",
      "open_in_tab": true
    },
    "default_icon": {
      "16": "img/icon16.png",
      "48": "img/icon48.png",
      "128": "img/icon128.png"
    }
  }
}