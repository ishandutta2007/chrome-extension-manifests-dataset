{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "__MSG_ext_name__",
  "short_name": "Notion Exporter",
  "description": "__MSG_description__",
  "version": "2.3.0",
  "permissions": [
    "scripting",
    "storage",
    "clipboardWrite",
    "sidePanel",
    "tabs"
