  "default_locale": "en",
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "action": {
    "default_title": "Notion Exporter",
    "default_popup": "src/popup/index.html"
  },
  "host_permissions": [
    "https://www.notion.so/*",
    "https://img.notionusercontent.com/*",
    "https://api.notionexporter.com/*"
  ],
  "content_scripts": [
    {
      "js": [
        "assets/entry.ts-loader-3GVL802G.js"
      ],
      "matches": [
        "https://www.notion.so/*"
      ]
    }
  ],
  "options_page": "src/options/index.html",
  "side_panel": {
    "default_path": "src/sidepanel/index.html"
  },
  "web_accessible_resources": [
    {
      "matches": [
        "https://www.notion.so/*"
      ],
      "resources": [
        "public/*",
        "src/app/*",
        "assets/const-BxXvt9De.js",
        "assets/utils-D9ykLQBV.js",
        "assets/entry.ts-C6AYK-Sa.js"
      ],
      "use_dynamic_url": false
    }
  ]
