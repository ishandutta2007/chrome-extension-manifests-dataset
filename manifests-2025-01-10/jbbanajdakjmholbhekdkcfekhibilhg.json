    {
      "matches": ["https://www.linkedin.com/*"],
      "css": ["./linkedin/app.css"],
      "js": ["./linkedin/app.js", "./env.js", "./utils/salesmind_request_utility.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
