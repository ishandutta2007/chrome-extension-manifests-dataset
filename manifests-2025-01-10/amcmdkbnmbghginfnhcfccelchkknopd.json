      ],
      "js": [
        "./src/static/lib/jquery.min.js",
        "./src/static/lib/bootstrap.min.js",
        "./src/static/lib/accounting.min.js",
        "./src/static/lib/dataBase.js",
        "./src/static/lib/JsBarcode.all.min.js",
        "./src/utils.js",
