{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "OlibeeJobsTracker",
  "version": "1.0.0",
  "description": "a simple research tracking extension",
  "icons": {
    "32": "icons/favicon.ico"
  },
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "jTrack.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["entry.js"]
    }
  ],
  "action": {
    "default_icon": "icons/favicon.ico",
    "default_title": "See OlibeeJobsTracker dialogue!",
    "default_popup": "popup/selection_popup.html"
  },
  "permissions": [
    "activeTab",
    "scripting",
    "tabs"
  ],
  "author": {
    "email": "khairul.nasri895@gmail.com"
  }
}
