{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "version": "2.0.0",
  "author": "Bluepixel",
  "name": "Thumbnail Boss - YouTube thumbnail previewer",
  "short_name": "Thumbnail Boss",
  "description": "Preview and test your thumbnails directly on YouTube",
  "key": "",
  "icons": {
    "16": "assets/icons/16.png",
    "32": "assets/icons/32.png",
    "48": "assets/icons/48.png",
    "128": "assets/icons/128.png"
  },
  "action": {
    "default_title": "Open YouTube with Thumbnail Boss"
  },
  "background": {
    "service_worker": "service-worker.js"
  },
  "content_scripts": [
    {
      "all_frames": false,
      "js": [
        "content-scripts/inject-thumbnail.js"
      ],
      "css": [
        "content-css/index.css"
      ],
      "match_about_blank": false,
      "matches": [
        "https://*.youtube.com/*"
      ],
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "tabs",
    "storage",
    "unlimitedStorage"
  ],
  "host_permissions": [
    "https://*.youtube.com/*"
  ],

  "externally_connectable": {
    "matches": [
      "*://*.youtube.com/*",
      "*://*.thumbnailboss.com/*"
    ]
  },

  "web_accessible_resources": [
    {
      "resources": [
        "assets/icons/48.png",
        "assets/css/styles.css",
        "widget/app.js"
      ],
      "matches": ["https://*.youtube.com/*"]
    }
  ]
}
