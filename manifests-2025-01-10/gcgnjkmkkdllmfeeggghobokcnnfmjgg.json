{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "github-bugspots-extension",
  "version": "0.2.0",
  "description": "Display bugspots in GitHub.",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage",
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_title": "github-bugspots",
    "default_icon": {
      "16": "images/logo16.png",
      "32": "images/logo32.png",
      "48": "images/logo48.png",
      "128": "images/logo128.png"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*/*"
      ],
      "css": [
        "content.css"
      ],
      "js": [
        "content.bundle.js"
      ]
    }
  ],
  "icons": {
    "16": "images/logo16.png",
    "32": "images/logo32.png",
    "48": "images/logo48.png",
    "128": "images/logo128.png"
  },
  "key": ""
}
