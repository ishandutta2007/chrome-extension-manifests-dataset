    "js": ["js/jquery.min.js", "js/sweetalert.min.js", "js/content.js"],
    "css": ["styles/sweetalert.css", "styles/custom.css"]
  }],
  "web_accessible_resources": [{
    "resources": ["logo.png", "background.png"],
    "matches": ["<all_urls>"]
  }]
