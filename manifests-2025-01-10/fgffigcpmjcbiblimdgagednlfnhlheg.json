{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "Subsreader",
    "author": "Diego López",
    "description": "Lector de subtitulos de plataformas de video.",
    "version": "2.2.3",
    "content_scripts": [
        {
            "matches": [
                "*://www.youtube.com/*",
                "*://*.netflix.com/*",
                "*://play.hbomax.com/*",
                "*://hbomax.com/*",
                "*://www.primevideo.com/*",
                "*://*.disneyplus.com/*",
                "*://*.paramountplus.com/*"
            ],
            "js": [
                "content-scripts/main.js"
            ],
            "allFrames": true
        }
    ],
    "action": {
        "default_title": "Guía de uso",
        "default_popup": "embed/popup.html"
    },
    "permissions": [
        "storage"
    ],
    "background": {
        "service_worker": "background/worker.js"
    },
    "icons": {
        "16": "./assets/logo.png",
        "48": "./assets/logo.png",
        "128": "./assets/logo.png"
    },
    "web_accessible_resources": [
        {
            "resources": [
                "embed/*",
                "assets/*",
                "assets/logo.png"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ]
}
