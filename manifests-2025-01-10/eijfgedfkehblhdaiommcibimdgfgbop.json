{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Winning Hunter",
  "version": "1.0.0",
  "description": "Works with Shopify updates. Real-time revenue tracking, product research, and Facebook Ads in one powerful dropshipping tool.",
  "icons": {
    "16": "icon96.png",
    "48": "icon96.png",
    "128": "icon96.png"
  },
  "background": {
    "service_worker": "./background/background.js"
  },
  "content_scripts": [
    {
      "matches": [ "<all_urls>" ],
      "exclude_matches": ["https://www.facebook.com/*"],
      "js": ["/static/js/main.js"]
    },
    {
      "js": ["./AdApp/facebook/index.js"],
      "matches": ["*://*.facebook.com/ads/library*"],
      "run_at": "document_start"
    },
    {
      "js": ["./AdApp/tiktok/index.js"],
      "matches": ["<all_urls>"],
      "run_at": "document_start"
    },
    {
      "js": ["./AdApp/tiktok/creativeCenterDashboard.js"],
      "matches": ["*://ads.tiktok.com/business/creativecenter/inspiration/topads*"],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [ "/static/*" ],
      "matches": [ "<all_urls>" ]
    }
  ],
  "action": {
    "default_popup": "./AdApp/popup.html",
    "default_title": "Winning Hunter"
  },
  "permissions": [
    "identity",
    "identity.email",
    "storage",
    "tabs",
    "scripting",
    "webRequest",
    "cookies"
  ],
  "host_permissions": ["*://*/*"]
}
