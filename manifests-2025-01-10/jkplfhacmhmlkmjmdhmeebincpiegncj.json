{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Rubber Ducking for Colab",
  "version": "1.3.1",
  "description": "An AI assistant for Google Colab that helps you understand and improve your code",
  "permissions": [
    "storage",
    "activeTab",
    "tabs"
  ],
  "host_permissions": [
    "https://colab.research.google.com/*",
    "https://api.openai.com/*",
    "https://api.deepseek.com/*"
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'",
    "sandbox": "sandbox allow-scripts allow-forms allow-popups allow-modals; script-src 'self' 'unsafe-inline' 'unsafe-eval'; child-src 'self'"
  },
  "web_accessible_resources": [{
    "resources": ["lib/*"],
    "matches": ["https://colab.research.google.com/*"]
  }],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon.png",
      "48": "images/icon.png",
      "128": "images/icon.png"
    }
  },
  "icons": {
    "16": "images/icon.png",
    "48": "images/icon.png",
    "128": "images/icon.png"
  },
  "content_scripts": [
    {
      "matches": ["https://colab.research.google.com/*"],
      "js": ["lib/marked.min.js", "lib/highlight.min.js", "content.js"],
      "css": ["lib/github.min.css", "styles.css"]
    }
  ],
  "options_page": "options.html",
  "background": {
    "service_worker": "background.js"
  }
} 