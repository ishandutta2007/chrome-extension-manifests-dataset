{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Spartix",
  "description": "This extensions make it easy to connect some web plataforms (like instagram) to Spartix",
  "version": "1.0",
  "action": {
    "default_popup": "src/popup/index.html",
    "default_icon": "src/assets/favicon.png"
  },
  "background": {
    "service_worker": "src/background.js"
  },
  "permissions": ["webRequest", "storage", "activeTab"],
  "host_permissions": [
    "*://instagram.com/*",
    "*://*.instagram.com/*"
  ],
  "externally_connectable": {
    "matches": [
      "https://*.spartix.xyz/*",
      "http://localhost:7878/*"
    ]
  }
}
