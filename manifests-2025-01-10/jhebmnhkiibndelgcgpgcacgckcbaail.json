{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Avature Import Extension",
  "short_name": "AvaImport",
  "version": "2.5",
  "manifest_version": 3,
  "description": "This extension lets you import profiles from the web into Avature by importing downloaded files as person records in Avature.",
  "permissions": [
    "activeTab",
    "cookies",
    "downloads",
    "storage"
  ],
  "host_permissions": [
    "https://*/"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
      "16": "icon_red16.png",
      "32": "icon_red32.png",
      "48": "icon_red48.png",
      "128": "icon_red128.png"
    }
  },
  "icons": {
    "16": "icon_red16.png",
    "32": "icon_red32.png",
    "48": "icon_red48.png",
    "128": "icon_red128.png"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "index.html"
      ],
      "matches": [
        "https://*/*"
      ]
    }
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*/*"
      ],
      "js": [
        "/scripts.js"
      ],
      "css": [
        "/scripts.css"
      ]
    }
  ]
}