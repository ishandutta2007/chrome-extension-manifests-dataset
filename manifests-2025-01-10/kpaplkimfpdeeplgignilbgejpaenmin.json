{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "__MSG_extensionName__",
  "version": "1.0.0",
  "description": "Automatically hides elements with z-index greater than specified value",
  "default_locale": "en",
  "permissions": [
