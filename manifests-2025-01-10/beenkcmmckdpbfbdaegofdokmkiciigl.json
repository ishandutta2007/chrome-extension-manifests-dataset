  }],

  "action": {
    "default_title": "AppsheetTranslator"
  },

  "icons": {
    "128": "icons/icon.png"
  },

 "web_accessible_resources": [{
  "resources": ["popup.js", "widget/widget.html", "widget/widget.css"],
    "matches": ["<all_urls>"]
  }],
  "commands": {
    "inject-script": {
      "suggested_key": "Ctrl+Shift+Y",
