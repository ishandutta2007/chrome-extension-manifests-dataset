"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "bilibili Ads Remover",
    "version": "1.2.1",
    "description": "bilibili插件，屏蔽首页换一换广告、动态广告(关键词匹配)",
    "icons": {
      "128": "assets/image/logo.png"
    },
    "action": {
      "default_popup": "index.html",
