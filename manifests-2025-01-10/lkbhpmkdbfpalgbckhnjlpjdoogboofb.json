          "js/index.js"
        ],
        "css": [
          "assets/style/index.css"
        ],
        "matches": [
          "https://bilibili.com/*",       
          "https://www.bilibili.com/*"       
        ],
        "run_at":"document_start"
      },
      {
        "js": [
          "js/index_init.js"
        ],
        "matches": [
          "https://bilibili.com/*",       
          "https://www.bilibili.com/*"       
        ],
        "run_at":"document_end"
      }
    ],
    "author": "rsndm.14k@gmail.com",
    "permissions": [
      "storage"
    ],
    "web_accessible_resources": [
      {
        "resources": [ "js/dynamic_injected.js", "js/index_injected.js"],
