{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "inbox-manager",
    "description": "Inbox manager for Google Chrome",
    "version": "0.0.1",
    "background": {
        "service_worker": "/background.js",
        "perstistent": true
    },
    "permissions": [
        "storage",
        "activeTab",
        "tabs",
        "contextMenus"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_title": "Pressez Ctrl(Win)/Command(Mac)+Shift+Y pour ajouter un élément à votre inbox",
        "default_icon": {
            "16": "/icons/saving16.png",
            "32": "/icons/saving32.png",
            "48": "/icons/saving48.png",
            "128": "/icons/saving128.png"
        }
    },
    "commands": {
        "_execute_action": {
            "suggested_key": {
                "default": "Ctrl+Shift+Y"
            }
        }
    },
    "icons": {
        "16": "/icons/saving16.png",
        "32": "/icons/saving32.png",
        "48": "/icons/saving48.png",
        "128": "/icons/saving128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "*://mail.google.com/*"
            ],
            "js": [
                "/gmail.js"
            ],
            "css": [
                "/css/gmail.css"
            ],
            "run_at": "document_end"
        },
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "/selection.js"
            ],
            "css": [
                "/css/selection.css"
            ],
            "run_at": "document_end"
        }
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "options_page": "options.html"
}