{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "PaliApps Secure Email",
  "version": "1.3.2",
  "description": "This extension allows PaliApps secure email to be opened with Gmail and Office365 email clients",
  "author": "theraaz",
  "permissions": [
    "tabs",
    "declarativeContent"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://mail.google.com/*",
        "*://outlook.office.com/mail/*",
        "*://outlook.live.com/mail/*",
        "*://outlook.office365.com/mail/*"
      ],
      "run_at": "document_idle",
      "css": [
        "styles/style.css"
      ],
      "js": [
        "scripts/gmail.js",
        "scripts/office365.js",
        "scripts/decryptor.js",
        "util.js",
        "script.js",
        "openpgp.min.js",
        "FileSaver.min.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "images/gearicon.png",
    "images/pa-clr-full.png",
    "images/pa-logo.png",
    "images/pa-logo-min.png",
    "images/padlockWhite-icon.png",
    "images/atch1.png",
    "images/mail-img.png",
    "images/android.png",
    "images/ios.png",
    "images/outlook.png",
    "scripts/o365-react-handler.js"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_title": "PaliApps Decryption Helper",
    "default_popup": "popup.html",
    "default_icon": {
      "128": "images/pa-clr-128.png"
    }
  },
  "icons": {
    "128": "images/pa-clr-128.png"
  },
  "manifest_version": 2
}
