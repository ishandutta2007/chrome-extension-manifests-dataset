{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "URLtown Helper",
  "version": "0.0.5",
  "description": "This extension provides the tab opening/closing functionality required for the URLtown.io app",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": [
        "https://urltown.io/*"
      ],
      "js": [
        "runtime.js",
        "polyfills.js",
        "main.js"
      ],
      "css": [
        "styles.css"
      ]
    }
  ],
  "externally_connectable": {
    "matches": [
      "https://urltown.io/*"
    ]
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "background": {
    "service_worker": "./apps/hometown-extension/src/background.js",
    "type": "module"
  },
  "icons": {
    "16": "icon16.png",
    "19": "icon19.png",
    "38": "icon38.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
