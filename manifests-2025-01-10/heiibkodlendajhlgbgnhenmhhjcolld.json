{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "GCal Default Calendar Setter Tester", 
  "version": "1.2.1", 
  "manifest_version": 3,
  "description":"This extension allows you to set your own default Google Calendar", 
  "author": "Kikokidayo",
  "icons":  {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Google Calendar Default Calendar Setter"
  },
  "permissions": ["identity", "storage","tabs","activeTab","scripting", "alarms"],
  "content_scripts": [
    {
      "matches": ["*://calendar.google.com/*"],
      "js": ["contentScript.js"]
    }
  ],
  "host_permissions": ["*://calendar.google.com/*","*://www.google.com/*"],
  "oauth2": {
    "client_id": "393804977139-piajt9j5ss289l58ainn5aphkg04qdp2.apps.googleusercontent.com" ,
    "scopes": ["https://www.googleapis.com/auth/calendar"]
  },
  "background": {
    "service_worker": "background.js"
  },
  "commands": {
    "create-event": {
      "suggested_key": {
        "default": "Ctrl+X",
        "mac": "Command+X"
      },
      "description": "Create a new event"
    }
    }
}