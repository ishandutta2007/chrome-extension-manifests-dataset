{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Vid Inspector",
  "version": "1.13",
  "description": "Get analytics on YouTube to find winning content",
  "permissions": ["activeTab", "storage", "scripting", "webNavigation"],
  "host_permissions": ["https://www.youtube.com/*", "*://m.youtube.com/*"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon-16.png",
      "48": "icons/icon-48.png",
      "128": "icons/icon-128.png"
    }
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.youtube.com/*",
        "http://localhost:5173/*",
        "https://vidinspector.com/*",
        "*://m.youtube.com/*"
      ],
      "js": ["content.js"],
      "type": "module"
    }
  ],
  "icons": {
    "16": "icons/icon-16.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "web_accessible_resources": [{
    "resources": ["icons/Arrow.svg"],
    "matches": ["<all_urls>"]
  }]
}
