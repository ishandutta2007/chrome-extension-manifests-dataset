{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Popup blocker",
  "description": "block popup through window.open",
  "version": "0.4.3",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "match_about_blank": false,
      "all_frames": true,
      "js": [
        "content-script.js"
      ],
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "tabs", 
    "storage"
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "Popup lists"
  },
  "icons": { 
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png" 
  },
  "content_security_policy": "default-src 'self'; script-src 'self';",
  "manifest_version": 2
}
