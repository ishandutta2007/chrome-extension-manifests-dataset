{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Attachment Printer",
  "version": "1.0",
  "icons": {
    "16": "images/icon48.png"
  },
  "permissions": [
    "activeTab"
  ],
  "web_accessible_resources": [{
    "resources": ["images/icon48.png"],
    "matches": ["https://mail.google.com/*"]
  }],
  "host_permissions": [
    "https://mail.google.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://mail.google.com/*"],
      "js": ["content.js"]
    },
    {
      "matches": ["<all_urls>"],
      "js": ["print-content.js"]
    }
  ]
}