{
"update_url": "https://clients2.google.com/service/update2/crx",

    "name": "Tab Disguiser",
    "version": "1.1.0",
    "description": "Disguises your tabs in the browser.",
    "manifest_version": 3,
    "author": "Mathk",
    "permissions": [ "storage", "scripting" ],
    "host_permissions": [ "https://*/*", "http://*/*" ],
 "icons": {
        "128": "icon.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "action":{
        "default_popup": "popup.html",
        "default_title": "Tab Disguiser"
    },
    "commands": {
        "abort": {
            "suggested_key": {
                "default": "Alt+X"
            },
            "description": "Aborts open tab and replaces it."
        },
        "abort-last": {
            "suggested_key": {
                "default": "Alt+Shift+X"
            },
            "description": "Aborts the previous tab and replaces it."
        },
        "disguise": {
            "suggested_key": {
                "default": "Alt+S"
            },
            "description": "Disguises current tab."
        }
    }
