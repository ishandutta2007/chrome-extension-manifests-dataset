{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "Dark.Me - Dark Mode Any Website",
  "version": "0.01",
  "description": "This extension will automatically convert any website to Dark Mode!",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "16": "images/darkme16.png",
      "32": "images/darkme32.png",
      "48": "images/darkme48.png",
      "128": "images/darkme128.png"
    }
  },
  "icons": {
    "16": "images/darkme16.png",
    "32": "images/darkme32.png",
    "48": "images/darkme48.png",
    "128": "images/darkme128.png"
  }
}