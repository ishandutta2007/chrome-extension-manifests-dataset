{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "stul soundcloud styling",
    "version": "1.0",
    "description": "A simple extension to change the look of SoundCloud.",
    "default_locale": "en",
    "icons": {
        "16": "./img/icons/16x16.png",
        "32": "./img/icons/32x32.png",
        "57": "./img/icons/57x57.png",
        "150": "./img/icons/150x150.png"
    },
    "action": {
        "default_icon": "./img/icons/32x32.png",
        "default_popup": "index.html",
        "default_title": "stulSC"
    },
    "content_scripts": [{
        "matches": ["https://soundcloud.com/*"],
        "css": ["./css/sc.css"],
        "js": [
            "./js/jquery-3.7.1.min.js",
            "./js/main.js"
        ]
    }],
    "web_accessible_resources": [{
        "resources": [
            "img/back.svg",
            "img/deflogo.png",
            "img/stulmenu.png"
        ],
        "matches": ["https://soundcloud.com/*"]
    }],
    "host_permissions": [
        "https://soundcloud.com/*"
    ],
    "permissions": [
        "storage"
    ],
    "short_name": "stulSC",
    "options_ui": {
        "page": "index.html",
        "open_in_tab": true
    }
}