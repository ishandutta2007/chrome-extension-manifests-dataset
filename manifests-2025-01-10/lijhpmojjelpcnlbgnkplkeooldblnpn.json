{
"update_url": "https://clients2.google.com/service/update2/crx",

  "version": "1.0.3",
  "manifest_version": 3,
  "name": "AdStat Digital Connect",
  "description": "Connect the telegram ads advertising account to the Adstat Digital platform",
  "permissions": [
    "cookies",
    "activeTab",
    "storage",
    "tabs"
  ],
  "icons": {
    "16": "images/icon.png",
    "48": "images/icon.png",
    "128": "images/icon.png"
  },
  "host_permissions": ["https://ads.telegram.org/*","https://client.adstat.digital/*"],
  "background": {
    "service_worker": "background.js"
  },

  "action": {
    "default_popup": "js/index.html",
    "default_title": "AdStat Digital"
  },
  "content_scripts": [
  {
    "matches": ["https://ads.telegram.org/*","https://client.adstat.digital/*"],
    "js": ["content-script.js"]
  }
]
