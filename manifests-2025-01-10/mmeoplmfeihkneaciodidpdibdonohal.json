 "name": "KLE Extension",
    "description": "Make the KLE a little easier to use.",
 "manifest_version": 3,
 "version": "0.4.0",
 "permissions": [],
    "content_scripts": [
        {
          "run_at":"document_end",
          "js": ["content/lib/jquery-3.6.3.min.js", "content/lib/html2canvas.min.js", "content/index.js"],
          "matches": ["https://www.keyboard-layout-editor.com/*"]
        }
    ],
    "icons": {
        "16": "icons/icon_16x16.png",
        "48": "icons/icon_48x48.png",
        "128": "icons/icon_128x128.png"
    },
    "action": {
       "default_icon": "icons/icon_128x128.png"
    },
    "web_accessible_resources": [{
        "resources": [
            "content/*"
        ],
        "matches": ["https://www.keyboard-layout-editor.com/*"]

    }]
}