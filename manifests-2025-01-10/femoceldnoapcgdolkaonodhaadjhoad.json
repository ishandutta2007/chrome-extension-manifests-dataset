    "permissions" : ["*://docs.google.com/*", "*://drive.google.com/*", "notifications"],
    "background" : {
        "scripts": ["back.js"],
        "persistent": false
    },
    "content_scripts" : [
        {
            "matches" : ["*://drive.google.com/file/*"],
            "js" : ["content-preview-file.js"],
            "run_at" : "document_end"
        },
        {
            "matches" : ["*://drive.google.com/drive/*"],
            "js" : ["content-preview.js"],
            "run_at" : "document_end"
        },
        {
            "matches" : ["*://docs.google.com/*"],
            "js" : ["content.js"],
            "run_at" : "document_end"
        }
    ]
