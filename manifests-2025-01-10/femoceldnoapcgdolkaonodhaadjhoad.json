    "description" : "Adds the option of direct copy to the clipboard of Google drive text content, or download links found within spreadsheets.",
    "minimum_chrome_version" : "18",
    "icons" : {
        "16" : "icon-16.png",
        "32" : "icon-32.png",
        "48" : "icon-48.png",
