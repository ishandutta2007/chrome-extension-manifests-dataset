{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Youtube Enhancer",
  "description": "Activate this extension with color options, instant ðŸ“¸ screenshots, extended skip time, and video adjustments in one click",
  "version": "1.0.1",
  "icons": {
    "16": "Icon 16.png",
    "32": "Icon 32.png",
    "48": "Icon 48.png"
  },
  "action": {
    "default_icon": {
      "64": "Icon 64.png",
      "128": "Icon 128.png",
      "2000": "Icon 2000.png"
    },
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "activeTab", "notifications"],
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "css": ["content.css"],
      "js": ["content.js"]
    }
  ],
"host_permissions": [
    "<all_urls>"
  ],
  "commands": {
    "ctrl-active": {
      "suggested_key": {
        "default": "Ctrl+Right",
        "windows": "Ctrl+Right"
      },
      "description": "Forward"
    },
    "ctrl-close": {
      "suggested_key": {
        "default": "Ctrl+Left",
        "windows": "Ctrl+Left"
      },
      "description": "Backward"
    }
  }

