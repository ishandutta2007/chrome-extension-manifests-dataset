{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Chrome password setting",
  "version": "1.0.0",
  "description": "This extension is designed to enable you to set secure passwords for Chrome users.",
  "options_page": "options.html",
  "action": {
    "default_title": "Chrome password setting ",
    "default_Icon": {
      "16": "Icon 16.png",
      "32": "Icon 32.png",
      "48": "Icon 48.png"
    }
  },
  "icons": {
    "64": "Icon 64.png",
    "128": "Icon 128.png",
    "2000": "Icon 2000.png"
  },
  "host_permissions": ["*://*/*"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["content.js"],
      "css": ["content.css"],
      "run_at": "document_end"
    }
  ],
  "permissions": ["storage", "notifications"]
}
