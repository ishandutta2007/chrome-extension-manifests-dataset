{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Card Style Tab Search",
  "version": "2.5.0",
  "description": "List and manage all open tabs in card style, efficiently",
  "permissions": [
    "activeTab",
    "scripting",
    "bookmarks",
    "tabs",
    "contextMenus",
    "sessions",
    "storage"
  ],
  "action": {
    "default_popup": "tabs.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "commands": {
    "tab-to-next-shortcut": {
      "suggested_key": {
        "default": "Ctrl+Shift+U"
      },
      "description": "Open a new tab next to the active tab."
    }
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
  "host_permissions": ["https://*/*", "http://*/*", "file:///*"]
