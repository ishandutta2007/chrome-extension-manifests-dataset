{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "__MSG_manifest_name__",
  "short_name": "__MSG_manifest_short_name__",
  "description": "__MSG_manifest_description__",
  "default_locale": "en",
