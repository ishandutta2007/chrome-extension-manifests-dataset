      "matches": [
        "https://*/*",
        "http://*/*"
      ],
      "exclude_globs": [
        "*://meeting.*anymeeting*.com/**",
        "*://webinar.*anymeeting*.com/**",
        "*://meeting.gomeet*.com/**",
        "*://webinar.meeting.gomeet*.com/**",
