{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Limit Distracting Website",
  "description": "An extension which helps you to set a limit on any website usage.",
  "version": "0.0.0.1",
  "manifest_version": 3,
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Limit Distracting Website",
    "default_icon": {
      "16": "icons/16icon.png",
      "32": "icons/32icon.png",
      "48": "icons/48icon.png",
      "72": "icons/72icon.png",
      "128": "icons/128icon.png",
      "512": "icons/512icon.png"
    },
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/16icon.png",
    "32": "icons/32icon.png",
    "48": "icons/48icon.png",
    "72": "icons/72icon.png",
    "128": "icons/128icon.png",
    "512": "icons/512icon.png"
  }
}
