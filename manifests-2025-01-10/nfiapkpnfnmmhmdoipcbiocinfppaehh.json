  "name": "Octofilter",
  "version": "1.0.3",
  "description": "File filter for GitHub code reviews as a Chrome extension.",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["*://*.github.com/*"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "32": "images/icon32.png",
    "64": "images/icon64.png",
    "128": "images/icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "32": "images/icon32.png",
      "64": "images/icon64.png",
      "128": "images/icon128.png"
    }
  },
  "permissions": ["tabs", "*://*.github.com/*"],
  "manifest_version": 2
}
