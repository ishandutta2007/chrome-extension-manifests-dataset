        "assets/*",
        "fonts/*",
        "icons/*",
        "build/unsafeClick.js",
        "signup-menu.html",
        "lastpass-sso-login.html",
        "lastpass-sso-callback.html",
        "error.html",
        "okta-tiles-tenants-flow.html",
        "login-checks.html",
