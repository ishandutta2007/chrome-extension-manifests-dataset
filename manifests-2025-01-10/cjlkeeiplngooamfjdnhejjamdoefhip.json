{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "GLB.GE extension",
  "version": "2.1.9",
  "description": "Copart/IAAI/Manheim sites action recorder and account manager",

  "permissions": ["activeTab","storage","browsingData","proxy","management","geolocation"],
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "matches": [
        "https://*.copart.com/*",
        "https://*.iaai.com/*",
        "https://*.manheim.com/*",

        "https://portal.auctionnow.iaai.com/*",
        "https://g2auction.copart.com/g2/#/"


      ],
      "js": [
        "content/utils.js",
        "content/init.js",
        "content/api.js",

        "content/copart/copart.js",
        "content/copart/loginCopart.js",
        "content/copart/watchlist.js",
        "content/copart/payments-restrictions.js",
        "content/copart/total-restrictions.js",

        "content/iaai/iaai.js",
        "content/iaai/loginIAAI.js",
        "content/iaai/watchlist.js",
        "content/iaai/restrictions.js",

        "content/manheim/manheim.js",
        "content/manheim/loginManheim.js",
        "content/manheim/workbook.js",
        "content/manheim/restrictions.js",
        "content/manheim/proxyBids.js"


      ]
    }
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },

  "author": "Imran Rahimov Mammad",
  
  "action": {
    "default_popup": "popup.html",
    "default_title": "GLB.GE"
  },

  "web_accessible_resources": [
    {
      "resources": ["assets/*", "content/*"],
      "matches": ["<all_urls>"]
    }
  ],
  "icons": {
    "16": "assets/logo16.png",
    "32": "assets/logo32.png",
    "48": "assets/logo48.png",
    "128": "assets/logo128.png"
  }
}
