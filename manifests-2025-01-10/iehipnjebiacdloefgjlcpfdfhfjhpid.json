{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "YouTools: Youtube Shorts Views",
  "version": "1.0",
  "description": "This extension will help see youtube shorts views on desktop",
  "manifest_version": 3,
  "background": {
    "service_worker": "scripts/background/main.js"
  },
  "action": {
    "default_icon": {
      "16": "icons/icon.jpg",
      "32": "icons/icon.jpg",
      "48": "icons/icon.jpg",
      "128": "icons/icon.jpg"
    }
  },
  "icons": {
    "16": "icons/icon.jpg",
    "32": "icons/icon.jpg",
    "48": "icons/icon.jpg",
    "128": "icons/icon.jpg"
  },
  "web_accessible_resources": [
    {
      "resources": ["scripts/content/shorts/*"],
      "matches": ["https://*/*", "http://*/*"]
    }
  ],
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/shorts/*"],
      "css": ["scripts/content/shorts/css/index.css"],
      "js": [
        "scripts/imports/js/jQuery.min.js",
        "scripts/content/shorts/js/apiReq.js",
        "scripts/content/shorts/js/content.js",
        "scripts/content/shorts/js/index.js"
      ]
    }
  ],
  "permissions": ["storage"]
}
