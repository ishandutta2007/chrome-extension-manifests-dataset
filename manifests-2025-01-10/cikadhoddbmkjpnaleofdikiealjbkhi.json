{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Trustia",
  "description": "This Google Chrome extension will allow you to add crypto assets to your Trustia portfolio from Coinmarketcap, Coingecko and Binance",
  "version": "0.0.1",
  "action": {
    "default_icon": "icon.png"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.coingecko.com/*/coins/*",
        "https://*.coinmarketcap.com/currencies/*",
        "https://*.binance.com/*/trade/*",
        "http://127.0.0.1:5000/*",
        "http://localhost:3000/*",
        "https://app.trustia.finance/*"
      ],
      "run_at": "document_end",
      "js": [
        "dom.js"
      ]
    }
  ],
  "permissions": [
    "storage"
  ],
  "externally_connectable": {
    "matches": [
      "http://localhost:3000/*",
      "https://app.trustia.finance/*"
    ]
  }
}
