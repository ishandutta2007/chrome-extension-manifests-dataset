  "scripts": ["background.js"],
  "persistent": false
 },
 "browser_action": {
  "default_icon": "icons/icon-32.png",
  "default_title": "Censor all emoji"
 },
 "content_scripts": [{
  "matches": ["<all_urls>"],
  "match_about_blank": true,
  "all_frames": true,
  "js": ["emoji-censor.js", "emoji-censor-comms.js"]
 }],
 "permissions": [
  "<all_urls>"
 ]
