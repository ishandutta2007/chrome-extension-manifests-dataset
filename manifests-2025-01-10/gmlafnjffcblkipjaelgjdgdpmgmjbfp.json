  "content_scripts": [
    {
      "matches": [
        "https://www.google.com/*"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ],

  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ]
