{
"update_url": "https://clients2.google.com/service/update2/crx",

   "name": "QuikProxy",
   "version": "2.0.1",
   "manifest_version": 3,
   "default_locale": "en",
   "background": {
      "service_worker": "main.js"
   },
   "action": {
      "default_icon": "icon.png",
      "default_popup": "proxy/dist/index.html?path=popup",
      "default_title": "代理设置"
   },
   "options_ui": {
      "open_in_tab": true,
      "page": "proxy/dist/index.html"
   },
   "commands": {
      "Duplicate Tab": {
         "description": "Duplicate Tab",
         "suggested_key": {
            "default": "Alt+Shift+K"
         }
      },
      "close-left-tabs": {
         "description": "Close Left Tabs",
         "suggested_key": {
            "default": "Alt+Shift+1"
         }
      },
      "close-other-tabs": {
         "description": "Close other Tabs",
         "suggested_key": {
            "default": "Alt+Shift+W"
         }
      },
      "close-right-tabs": {
         "description": "Close Right Tabs",
         "suggested_key": {
            "default": "Alt+Shift+2"
         }
      }
   },
   "icons": {
      "128": "icon.png",
      "16": "icon.png",
      "48": "icon.png"
   },
   "permissions": [
      "storage",
      "proxy",
      "history",
      "activeTab"
   ],
   "host_permissions": [
      "*://*.aliyuncs.com/*"
   ],
   "web_accessible_resources": [
      {
         "resources": [
            "*"
         ],
         "matches": [
            "<all_urls>"
         ]
      }
   ]
}