{
"update_url": "https://clients2.google.com/service/update2/crx",

 "name": "Viz Tab",
 "description": "Save a visual log of open tabs.",
 "version": "1.0.2",
 "manifest_version": 2,
 
 "permissions": [
  "<all_urls>",
  "tabs", 
  "storage", 
  "activeTab",
  "unlimitedStorage"
 ],
   
   "browser_action": {
     "default_icon": "tabButton.png",
     "default_popup": "popup.html"
   },

 "background": {
  "scripts": ["background.js"],
  "persistent": false
 },

 "content_scripts": [{
  "matches": ["http://*/*", "https://*/*"],
  "js": ["scrollScript.js"]
 }]
 

}
