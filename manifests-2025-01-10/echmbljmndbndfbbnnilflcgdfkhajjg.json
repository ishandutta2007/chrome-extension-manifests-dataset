{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Brightlight Appointments",
  "version": "0.2.10",
  "action": {
    "default_title": "Create Brightlight Appointments Meetings",
    "default_popup": "popup.html"
  },
  "description": "A simple extension that allows you to schedule Brightlight Appointments Meetings.",
  "icons": {
    "16": "blh-logo-16x16.png",
    "48": "blh-logo-48x48.png",
    "128": "blh-logo-128x128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://calendar.google.com/calendar/*", "https://outlook.live.com/owa/*"],
      "js": ["jquery.js", "RandomUtil.js", "RoomnameGenerator.js", "meet-calendar.js"],
      "css": ["/css/all.css"],
      "all_frames" : false,
      "run_at" : "document_end"
    }
  ],
  "host_permissions": [
      "https://calendar.google.com/*",
      "https://outlook.live.com/*"
  ],
  "externally_connectable": {
      "matches": [
        "*://appointment.brightlight.ai/*"
      ]
  },
  "minimum_chrome_version": "89",
  "web_accessible_resources": [{
    "resources": ["blh-logo-48x48.png","blh-logo.svg"],
    "matches": ["https://calendar.google.com/*", "https://outlook.live.com/*"]
  }]
}
