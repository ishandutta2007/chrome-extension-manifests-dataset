  "background": {
    "scripts": ["refresh-new.js"]
  },
  "content_scripts": [
    {
      "js": ["display-new.js"],
      "matches": ["*://news.ycombinator.com/", "*://news.ycombinator.com/news*"],
      "run_at": "document_end"
    }
  ]
