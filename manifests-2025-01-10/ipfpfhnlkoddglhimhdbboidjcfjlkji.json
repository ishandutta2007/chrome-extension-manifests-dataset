{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Click-free Browsing",
  "description": "Enables browsing using only mouse movements",
  "version": "2.6",
  "manifest_version": 3,
  "background": {
   "service_worker": "background.js"
  },
  "options_page": "options.html",
