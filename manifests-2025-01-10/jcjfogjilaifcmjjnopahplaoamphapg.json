{
"update_url": "https://clients2.google.com/service/update2/crx",

    "name": "Bird Extension",
    "description": "Making rater lives easier",
    "manifest_version": 3,
    "version": "0.3.1",
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "scripting",
        "storage"

    ],
    "background": {
        "service_worker": "Background.js"
        },
    "host_permissions": [
        "https://*.raterhub.com/*",  
        "file:///*"
    ],
    
    "content_scripts": [
        {
            "matches": [
                "https://*.raterhub.com/*"
            ],
            "js": [
                "AutoSubmitBar.js"
            ]
        },
        {
            "matches": [
                "https://*.google.com/*"
            ],
            "js": [
                "LinkAddress.js"
            ]
        },
        {
            "matches":[
                "file:///*"
            ],
            "js":[
                "AutoSubmitBar.js"
            ]
        },
        {
            "matches": [
                "https://*.raterhub.com/*",
                "file:///*"
            ],
            "js": [
                "Error.js"
            ]
        },
        {
            "matches": [
                "https://*.raterhub.com/*",
                "file:///*"
            ],
            "js": [
                "Header.js"
            ]
        },
        {
            "matches": [
                "https://*.raterhub.com/*"

            ],
            "js": ["AutoAcquireSound.js"]
          },
          {
            "matches": [
                "https://*.raterhub.com/evaluation/rater/*"
            ],
            "js": [
                "AutoAcquireButton.js"
            ]
        }
        
    ], 
"web_accessible_resources": [
    {
        "resources": ["Sounds/success.mp3"],
        "matches": ["<all_urls>"]
    },
    {
        "resources": ["Christmas/*"],
        "matches": ["https://*.raterhub.com/*"]
    }
]
