{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "YouTube Dual Panel AD Blocker",
    "version": "1.0",
    "description": "Open YouTube in the left and videos in the right, block ads in the right panel, following youtube policies.",
    "action": {
      "default_popup": "popup.html"
    },
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "icons": {
    
    "128": "icon128.png"
  },
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["https://www.youtube.com/*"],
        "js": ["content.js"]
      }
    ],
    "host_permissions": [
      "https://www.youtube.com/*"
    ],
    "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self'; frame-src https://www.youtube.com https://*.youtube.com"
    }
  }
  
  