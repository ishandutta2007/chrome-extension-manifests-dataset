{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "YouTube URL Playlist remover",
  "version": "1.1",
  "description": "Removes the 'list' URL parameter from YouTube links.",
  "permissions": ["declarativeNetRequest"],
  "host_permissions": ["*://www.youtube.com/*"],
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "remove_list_parameter",
        "enabled": true,
        "path": "rules.json"
      }
    ]
  },
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/*"],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ]
}
