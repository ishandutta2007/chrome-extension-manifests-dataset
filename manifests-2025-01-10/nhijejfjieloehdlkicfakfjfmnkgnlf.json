  "manifest_version": 3,
  "name": "HTL Debug",
  "description": "View and analyze your ad configuration",
  "version": "2.6.0",
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "icons": {
    "16": "extension/img/icon-16.png",
    "48": "extension/img/icon-48.png",
