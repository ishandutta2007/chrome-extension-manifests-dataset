{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 2,
    
    "name": "Scholar Count",
    "description": "Add a count to your google scholar citations page.",
    "version": "0.0.1",
    
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    
    "permissions": [
        "<all_urls>"
    ],
    
    "content_scripts": [{
        "matches": [
            "*://scholar.google.com/citations*"
        ],
        "js": [
            "jquery-1.11.1.js",
            "scholar-count.js"
        ],
        "css": [
            "scholar-count.css"
        ],
        "run_at": "document_start"
    }]
}
