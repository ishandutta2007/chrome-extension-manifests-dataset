"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "Element Selectors",
    "description": "Chrome extension to find element selectors",
    "version": "0.0.1",
    "author": "Gurvinder Dhillon",
    "devtools_page": "devtools/devtools.html",
    "icons": {
