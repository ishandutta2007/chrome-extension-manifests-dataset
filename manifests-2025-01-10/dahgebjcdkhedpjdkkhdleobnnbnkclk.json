{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "youtube_ad_remover",
  "version": "3.2.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "bg.js"
  },
  "permissions": [
    "declarativeNetRequest",
    "alarms"
  ],
  "host_permissions": [
    "https://*/*",
    "http://*/*"
  ],
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "content.js"
      ],
      "matches": [
        "https://*/*"
      ],
      "run_at": "document_start"
    }
  ]
}