{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "PDF Text Viewer",
  "version": "1.7",
  "description": "Visualiza el texto de PDFs como HTML en el navegador",
  "permissions": [
    "activeTab",
    "contextMenus",
    "downloads"
  ],
  "host_permissions": [
    "*://*/*",
    "file:///*",
    "<all_urls>"
  ],
  "icons": {
    "16": "32.png",
    "32": "64.png",
    "48": "128.png",
    "128": "512.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["pdf.min.js", "content.js"],
      "css": ["styles.css"]
    }
  ],
  "web_accessible_resources": [{
    "resources": ["pdf.worker.min.js"],
    "matches": ["<all_urls>"]
  }],
  "action": {
    "default_title": "Ver PDF como HTML"
  }
}