    "name":  "SAP Enable Now, browser application recorder",
    "description":  "Extension to create help content for web-based applications.",
    "version":  "1.0.11",
    "manifest_version":  3,
    "icons":  {
                  "128":  "./src/assets/chrome_icon_128.png",
                  "16":  "./src/assets/chrome_icon_16.png",
                  "32":  "./src/assets/chrome_icon_32.png",
                  "48":  "./src/assets/chrome_icon_48.png"
              },
    "background":  {
                       "service_worker":  "background.js",
                       "type":  "module"
                   },
    "content_scripts":  [
                            {
                                "js":  [
                                           "./src/js/effect.js",
                                           "./src/js/constants.js",
                                           "./src/js/utils.js",
                                           "./src/js/dom.js",
                                           "./src/js/http.js",
                                           "./src/js/localization.js",
                                           "./src/js/wr_content_script.js"
                                       ],
                                "matches":  [
                                                "<all_urls>"
                                            ],
                                "css":  [
                                            "./src/css/styles.css"
                                        ],
                                "all_frames":  true,
                                "match_about_blank":  true
                            }
                        ],
    "content_security_policy":  {
                                    "extension_pages":  "script-src 'self'; object-src 'self'; script-src-elem 'self'; default-src 'self'; connect-src 'self' * data:; img-src 'self' * data:; style-src 'self' 'unsafe-inline'; media-src 'self' * data:"
                                },
    "permissions":  [
                        "activeTab",
                        "tabs",
                        "storage",
                        "tabCapture",
                        "scripting"
                    ],
    "host_permissions":  [
                             "<all_urls>"
                         ],
    "incognito":  "split",
    "web_accessible_resources":  [
                                     {
                                         "resources":  [
                                                           "src/js/ProdInfo.js",
                                                           "src/assets/*.png",
                                                           "src/assets/*.svg",
                                                           "src/css/styles.css",
                                                           "src/html/shadow_doms.html",
                                                           "src/html/camPreview.html",
                                                           "src/assets/*.woff",
                                                           "src/localization/*.txt"
                                                       ],
                                         "matches":  [
                                                         "<all_urls>"
                                                     ]
                                     }
                                 ],
    "action":  {
                   "default_popup":  ""
               }
}
