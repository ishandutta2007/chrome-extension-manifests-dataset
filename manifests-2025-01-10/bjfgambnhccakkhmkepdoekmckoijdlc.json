{
"update_url": "https://clients2.google.com/service/update2/crx",

    "description": "Automate your browser with AI.",
    "version": "1.0.6",
    "name": "AutoBrowser - Automate your browser with AI",
    "manifest_version": 3,
    "default_locale": "en",
    "background": {
        "service_worker": "background.bundle.js"
    },
    "permissions": [
        "debugger",
        "storage",
        "scripting",
        "sidePanel",
        "tabs",
        "webNavigation"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "icons": {
        "16": "assets/icon-16.png",
        "48": "assets/icon-48.png",
        "128": "assets/icon-128.png"
    },
    "externally_connectable": {
        "matches": [
            "https://*.autobrowser.ai/*"
        ]
    },
    "action": {
        "default_icon": "assets/icon-48.png",
        "default_title": "AutoBrowser"
    },
    "commands": {
        "_execute_action": {
            "suggested_key": {
                "default": "Alt+J"
            }
        }
    },
    "key": ""
}