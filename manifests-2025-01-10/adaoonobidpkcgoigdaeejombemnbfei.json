      ],
      "js": [
        "3rdPartyLibs/zepto.min.js",
        "KhanAcademyLibs/jipt-hack.js",
        "KhanAcademyLibs/math-translator.js",
        "app.js"
      ],
      "run_at": "document_idle",
      "all_frames": true
    }
  ],
  "icons": {
    "16": "khan-logo-new-16.png",
