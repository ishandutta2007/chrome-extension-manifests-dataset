{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "QuickReach",
  "version": "1.0.2",
  "description": "Quickly reach your favorite websites",
  "permissions": [
    "sidePanel",
    "declarativeNetRequestWithHostAccess",
    "contextMenus"
