  "host_permissions": ["<all_urls>"],
  "action": {
    "default_icon": {
      "16": "images/16x16.png",
      "32": "images/32x32.png",
      "48": "images/48x48.png",
      "128": "images/128x128.png"
    }
  },
  "icons": {
      "128": "images/128x128.png",
      "16": "images/16x16.png",
      "32": "images/32x32.png",
      "48": "images/48x48.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "side_panel": {
    "default_path": "panel.html"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+B",
        "mac": "Command+B"
      },
      "description": "Open QuickReach"
    }
  },
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "removeHeader",
        "enabled": true,
        "path": "removeHeader.json"
      }
