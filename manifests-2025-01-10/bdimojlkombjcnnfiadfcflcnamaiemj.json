{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,

  "name": "ToolTipTeacher",
  "author": "See background.js for attributions and comments",

  "description": "This extension teaches the meaning of the word ‘tooltip’",
  "version": "1.0",

  "browser_action": {
    "default_icon": "ToolTip.png",
    "default_popup": "popup.html",
    "default_title": "This text which you are now reading is a tooltip. To read more about tooltips, go to the omnibox (aka 'search bar'), and type 'tt' and then hit the space bar."
  },

  "omnibox": {
    "keyword": "tt"
  },

  "background": {
    "scripts": ["background.js"]
  },

  "permissions": [
    "activeTab"
  ]
}
