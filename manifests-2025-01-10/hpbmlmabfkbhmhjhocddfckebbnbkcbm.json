  "version": "2.1",
  "manifest_version": 3,
  "description": "Chrome Extension for Ollacart. Add your items to OllaCart.",
  "permissions": ["storage", "tabs", "activeTab"],
  "background": {
    "service_worker": "app/background.js"
  },
  "action": {},
  "content_scripts": [
