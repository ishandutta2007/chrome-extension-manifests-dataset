      "matches": ["<all_urls>"],
      "css": ["/css/root.css"],
      "js": ["/static/js/content.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["/static/css/content.css", "/static/media/*", "/images/*"],
      "matches": ["<all_urls>"]
    }
  ],
  "icons": {
    "32": "/icons/icon_32.png",
    "48": "/icons/icon_48.png",
    "64": "/icons/icon_64.png",
    "128": "/icons/icon_128.png"
  }
