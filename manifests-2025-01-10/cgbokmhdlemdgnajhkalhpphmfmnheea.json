"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Baobab image compressor",
  "description": "Download a compressed version of images in a webpage",
  "version": "1.0",
  "permissions": ["activeTab", "contextMenus"],
  "content_scripts": [{
    "matches": ["<all_urls>"],
