{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Smart Website Blocker",
  "version": "1.1.1",
  "action": {
    "default_icon": {
      "128": "assets/128_128.png"
    },
    "default_title": "Smart Website Blocker",
