    "128": "assets/128_128.png"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "run_at": "document_end",
      "js": [
        "content-scripts/import-main.js"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "alarms"
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "chunks/*-*.js",
        "content-scripts/main.js"
      ],
      "matches": [
        "*://*/*"
      ]
    }
  ]
}