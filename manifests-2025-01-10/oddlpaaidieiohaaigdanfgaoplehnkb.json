    "js": ["src/js/inject.js","src/js/main.js"],
    "all_frames": false
  }],
  "permissions": [
    "*://*.amazon.com/*",
    "*://*.ebay.com/*"
  ],
  "web_accessible_resources": ["src/html/*","src/img/*","src/js/*","src/css/*"]
