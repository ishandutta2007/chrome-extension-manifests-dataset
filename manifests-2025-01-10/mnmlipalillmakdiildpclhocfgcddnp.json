{
"update_url": "https://clients2.google.com/service/update2/crx",

    "author": "Rapid7",
    "name": "Rapid7 AppSec Plugin",
    "short_name": "R7 Plugin",
    "description": "Rapid7 AppSec Plugin",
    "version": "4.6.8",
    "manifest_version": 3,
    "incognito": "split",
    "background": {
        "service_worker": "app.js",
        "type": "module"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": [
                "./packages/browser-polyfill/browser-polyfill.min.js"
            ],
            "all_frames": true,
            "run_at": "document_end"
        }
    ],
    "permissions": [
        "activeTab",
        "cookies",
        "notifications",
        "tabs",
        "storage",
        "background",
        "debugger",
        "scripting",
        "webRequest"
    ],
    "host_permissions": [
        "*://*/*",
        "file://*/*",
        "http://*/*",
        "https://*/*"
    ],
    "icons": {
        "128": "browser/images/icon.png"
    },
    "action": {
        "default_icon": {
            "128": "browser/images/icon.png"
        },
        "default_title": "Rapid7 AppSec Extension",
        "default_popup": "browser/index.html"
    },
    "web_accessible_resources": [{
        "resources": [
            "packages/*.js",
            "packages/*.map",
            "packages/*.png",
            "dictionary/*.json",
            "hooks/*.json",
            "patterns/*.json",
            "contentScript.js",
            "injected.min.js",
            "alf-bundle.min.js",
            "packages/alf/app.js",
            "packages/alf/images/*",
            "packages/alf/notification.js",
            "packages/alf/notification.css",
            "appScript.js",
            "dragDropScript.js"
        ],
        "matches": ["<all_urls>"]
    }]
}
