{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "HTML Reader",
    "version": "1.0",
    "description": "A simple extension to read the HTML of the current page.",
    "action": {
        "default_popup": "popup.html"
    },

    "permissions": [
        "activeTab",
        "scripting",
        "tabs"
    ],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "jquery-3.6.4.js",
                "popup.js",
                "content.js"
            ]
        }
    ]
}
