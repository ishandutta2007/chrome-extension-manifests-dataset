    "manifest_version": 3,
    "name": "Extension On and Off",
    "version": "1.0",
    "description": "Toggle other Chrome extensions on and off.",
    "permissions": [
      "management",
      "activeTab"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_title": "Extension Toggle"
