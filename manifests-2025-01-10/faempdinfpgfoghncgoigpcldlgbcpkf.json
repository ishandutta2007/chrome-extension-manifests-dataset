  "description": "This will extension will fix double click to full screen by confining Twitch overlay extensions to have a max widt of 33%",
  "permissions": ["activeTab"],

  "content_scripts": [
   {
     "matches": ["https://*.twitch.tv/*"],
      "css": ["styles.css"]
   }
 ],
