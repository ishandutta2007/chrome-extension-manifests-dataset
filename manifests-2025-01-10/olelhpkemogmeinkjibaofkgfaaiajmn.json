{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "CofilmAI Chrome Extension",
  "version": "1.0.2",
  "description": "Cofilm - youtube summary AI tool.",
  "permissions": [
    "storage",
    "cookies"
  ],
  "host_permissions": [
    "http://localhost:4040/*",
    "https://app.cofilm.ai/*",
    "https://app.dev.cofilm.ai/*"
  ],
  "externally_connectable": {
    "matches": [
      "http://localhost:4040/*",
      "https://app.cofilm.ai/*",
      "https://app.dev.cofilm.ai/*"
    ]
  },
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "action": {
    "default_popup": "src/popup/index.html"
  },
  "options_page": "src/options/index.html",
  "content_scripts": [
    {
      "js": [
        "inject-marker.ts-loader.js"
      ],
      "matches": [
        "*://*.cofilm.ai/*",
        "http://localhost:4040/*"
      ],
      "run_at": "document_start"
    },
    {
      "js": [
        "main.tsx-loader.js"
      ],
      "matches": [
        "*://*.youtube.com/*"
      ]
    }
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "web_accessible_resources": [
    {
      "matches": [
        "*://*.youtube.com/*"
      ],
      "resources": [
        "src/assets/logo.svg",
        "src/assets/logo-white.svg",
        "logo.svg",
        "logo-white.svg",
        "enums.js",
        "main.tsx.js"
      ],
      "use_dynamic_url": false
    },
    {
      "matches": [
        "*://*.cofilm.ai/*",
        "http://localhost:4040/*"
      ],
      "resources": [
        "src/content/inject-marker-code.js",
        "inject-marker.ts.js"
      ],
      "use_dynamic_url": false
    }
  ],
  "icons": {
    "16": "public/logo.png",
    "48": "public/logo.png",
    "128": "public/logo.png"
  }
}
