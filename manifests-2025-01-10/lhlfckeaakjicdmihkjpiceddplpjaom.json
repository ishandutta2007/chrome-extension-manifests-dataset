{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Toleio",
  "description": "Chrome extension for Norwegian sign language dictionary.",
  "version": "1.1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "build/scripts/background.js"
  },
  "host_permissions": ["<all_urls>"],
  "web_accessible_resources": [
    {
      "resources": [
        "data/dict.json",
        "html/overlay.html",
        "assets/player/poster.jpg",
        "assets/icons/*.png",
        "assets/icons/*.svg",
        "assets/btns/*.png",
        "assets/bg/*.png",
        "assets/lang/*.json",
        "build/styles/*.css"
      ],
      "matches": ["<all_urls>"]
    }
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "all_frames": true,
      "run_at": "document_end",
      "css": ["build/styles/content.css", "build/styles/fonts.css"],
      "js": ["build/scripts/content.js", "lib/jquery.js"]
    }
  ],
  "permissions": ["storage"],
  "action": {
    "default_popup": "html/popup.html",
    "default_icon": {
      "16": "assets/icons/logo.png",
      "32": "assets/icons/logo.png",
      "48": "assets/icons/logo.png",
      "128": "assets/icons/logo.png"
    }
  },
  "icons": {
    "16": "assets/icons/logo.png",
    "32": "assets/icons/logo.png",
    "48": "assets/icons/logo.png",
    "128": "assets/icons/logo.png"
  },
  "commands": {
    "toggle-show": {
      "suggested_key": {
        "default": "Ctrl+Shift+Y",
        "mac": "Command+Shift+Y"
      },
      "description": "Toggle word highlighting display"
    },
    "toggle-active": {
      "suggested_key": {
        "default": "Ctrl+Shift+Y",
        "mac": "Command+Shift+Y"
      },
      "description": "Toggle word highlighting engine"
    },
    "trigger-search": {
      "suggested_key": {
        "default": "Alt+S",
        "mac": "Alt+S"
      },
      "description": "Open"
    }
  }
}
