{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "DispatchCenter Chrome Extension",
  "version": "1.0.14",
  "description": "Import Loads to Dispatch Center",
  "action": {
    "default_popup": "src/pages/Popup/index.html"
  },
  "permissions": [
    "activeTab",
    "scripting",
    "tabs",
    "storage"
  ],
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "js": [
        "assets/BatsScript.tsx-loader-CbtfaQpn.js",
        "assets/CentralDispatchScript.tsx-loader-B_3QDoUW.js"
      ],
      "matches": [
        "https://www.batscrm.com/pages/my-orders*",
        "https://site.centraldispatch.com/protected/dispatch/view?dsid*",
        "https://app.centraldispatch.com/search*",
        "https://app.centraldispatch.com/dispatch/carrier",
        "https://app.centraldispatch.com/dispatch/*/carrier"
      ],
      "css": [
        "assets/index-D3zHsDkA.css",
        "assets/index-D3zHsDkA.css"
      ]
    }
  ],
  "options_ui": {
    "page": "src/pages/Options/index.html",
    "open_in_tab": true
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "web_accessible_resources": [
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "src/pages/Content/CentralDispatch/network-script.js",
        "images/*"
      ],
      "use_dynamic_url": false
    },
    {
