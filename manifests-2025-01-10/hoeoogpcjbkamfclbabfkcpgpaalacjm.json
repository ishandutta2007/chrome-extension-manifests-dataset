{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "AdLibrary.io",
  "version": "1.0.11",
  "description": "Save Facebook ads to your library",
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.facebook.com/ads/library/*"
      ],
      "js": [
        "import-content.js"
      ],
      "css": [
        "content.css"
      ],
      "run_at": "document_end"
    }
  ],
  "host_permissions": [
    "https://www.facebook.com/ads/library/*"
  ],
  "permissions": [
    "storage"
  ],
  "externally_connectable": {
    "matches": [
      "https://app.adlibrary.io/*"
    ]
  },
  "icons": {
    "16": "icons/icon.png",
    "48": "icons/icon.png",
    "128": "icons/icon.png"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "chunks/*-*.js",
        "content.js"
      ],
      "matches": [
        "https://www.facebook.com/*"
      ]
    }
  ]
