    "service_worker": "background.js",
    "type": "module"
  },
  "options_page": "options.html",
  "icons": {
    "16": "16.png",
    "38": "48.png",
    "48": "48.png",
    "128": "128.png"
  },
  "author": "Robert Brewer",
  "commands": {
    "_execute_action": {
    },
    "save-to-blue": {
      "suggested_key": {
        "default": "Ctrl+Shift+S",
        "mac": "Command+Shift+S"
      },
      "description": "Save current page to Blue"
    }
  },
  "web_accessible_resources": [
    {
      "resources": ["api.js"],
      "matches": ["<all_urls>"]
