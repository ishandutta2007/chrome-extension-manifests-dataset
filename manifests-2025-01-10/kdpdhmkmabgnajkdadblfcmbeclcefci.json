      "run_at": "document_end",
      "js": ["cleaner.js"],
      "matches":["https://twitter.com/*"]
    }
  ],
  "action": {
    "default_icon": {
      "16": "images/TSRLogo-16.png",
      "32": "images/TSRLogo-32.png",
      "48": "images/TSRLogo-48.png",
      "128": "images/TSRLogo-128.png"
    },
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab","storage"],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+U",
        "mac": "Command+U"
      }
    }
  }
