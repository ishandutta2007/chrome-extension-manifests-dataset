{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Conga Platform Extension",
  "description": "Conga Platform helper",
  "version": "1.2",
  "manifest_version": 3,
  "background" : {
    "service_worker" : "service-worker.js"
  },
  "content_scripts" : [
    {
      "run_at": "document_end",
      "js": ["external/jquery-3.7.1.min.js","external/jquery-ui.min.js","scripts/content.js"],
      "css": ["external/jquery-ui.min.css"],
      "matches": [
        "https://*.congacloud.com/*",
        "https://*.congacloud.io/*"
      ]
    }
  ],
  "host_permissions": ["https://*.congacloud.com/*", "https://*.congacloud.io/*"],
  "web_accessible_resources": [
    {
      "resources": ["pages/*","external/pure-min.css"],
      "matches": ["https://*.congacloud.com/*", "https://*.congacloud.io/*"]
    }
  ],
  "incognito": "split",
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "scripting"
  ],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  }
