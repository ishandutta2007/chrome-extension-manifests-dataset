{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "default_locale": "en",
  "name": "blangket-extension",
  "version": "1.0.1",
  "description": "blangket-extension",
  "key": "",
  "host_permissions": [
    "https://www.netflix.com/*",
    "https://api.blangket.ai/*",
    "http://localhost:5173/*",
    "http://localhost:3000/*",
    "https://*.supabase.co/*",
    "https://language-pilot-livid.vercel.app/*"
  ],
  "permissions": [
    "storage",
    "scripting",
    "tabs",
    "notifications",
    "activeTab",
    "sidePanel"
  ],
  "options_page": "options/index.html",
  "background": {
    "service_worker": "background.iife.js",
    "type": "module"
  },
  "action": {
    "default_popup": "popup/index.html",
    "default_icon": "icon-34.png"
  },
  "icons": {
    "128": "icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.netflix.com/*",
        "http://localhost:5173/*"
      ],
      "js": [
        "content/index.iife.js"
      ],
      "run_at": "document_start"
    },
    {
      "matches": [
        "https://www.netflix.com/*",
        "http://localhost:5173/*"
      ],
      "js": [
        "content-ui/index.iife.js"
      ]
    },
    {
      "matches": [
        "https://www.netflix.com/*",
        "http://localhost:5173/*"
      ],
      "css": [
        "content.css"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "*.js",
        "*.css",
        "*.svg",
        "icon-128.png",
        "icon-34.png"
      ],
      "matches": [
        "*://*/*"
      ]
    },
    {
      "resources": [
        "background/inject.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "externally_connectable": {
    "matches": [
      "http://localhost:5173/*",
      "http://localhost:3000/*",
      "https://www.blangket.ai/*",
      "https://language-pilot-livid.vercel.app/*"
    ]
  },
  "side_panel": {
    "default_path": "side-panel/index.html"
  }
}