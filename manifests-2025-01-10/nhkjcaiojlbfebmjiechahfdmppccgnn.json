        "default_popup": "html/popup.html"
    },

    "permissions": ["activeTab", "storage","<all_urls>", "*://*.oceanengine.com/*"],

    "web_accessible_resources": [
        "inc/jquery-2.1.0.min.js",
        "inc/jquery.scrollstop.js",
