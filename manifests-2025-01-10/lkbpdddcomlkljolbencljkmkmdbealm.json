{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Renomate",
  "version": "1.0",
  "description": "A Chrome extension to pair with the Renomate web app to bring the Renomate experience with you as you shop!",
  "permissions": [
      "webNavigation",
      "storage"
  ],
  "background": {
      "service_worker": "background.bundle.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.bundle.js"]
    }
  ],
  "icons": {
      "16": "icons/logo16.png",
      "48": "icons/logo48.png",
      "64": "icons/logo64.png",
      "128": "icons/logo128.png"
  },
  "action": {
      "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    {
      "resources": [
          "icons/logo16.png",
          "icons/logo48.png",
          "icons/logo64.png",
          "icons/logo128.png",
          "icons/dropdown_caret.svg",
          "icons/material.svg",
          "icons/project.svg",
          "popup.bundle.js", 
          "content.bundle.js", 
          "styles.css",
          "popup.html",
          "icons/google-icon.svg",
          "icons/eye.svg",
          "icons/eye-off.svg",
          "platform.js" 
      ],
      "matches": ["<all_urls>"]
    }
  ]
}
