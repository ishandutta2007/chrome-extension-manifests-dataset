{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Basic Chrome Extension Test Example 2",
  "version": "1.4",
  "description": "A simple Chrome extension with popup and service worker",
  "action": {
    "default_popup": "popup.html",
    "default_title": "Basic Chrome Extension",
    "default_icon": "icon.png"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "permissions": ["storage", "nativeMessaging"],
  "host_permissions": ["https://*.observe.ai/*", "http://*.observe.ai/*"],
  "content_scripts": [
    {
      "matches": ["https://*.observe.ai/*", "http://*.observe.ai/*"],
      "js": ["content.js"]
    }
  ]
}
