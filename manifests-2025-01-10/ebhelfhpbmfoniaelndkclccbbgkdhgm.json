{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "Video Speed Controller",
    "version": "1.0.1",
    "description": "Control video playback speed on specific URL.",
    "permissions": [
        "storage",
        "webRequest",
        "scripting"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icon.png",
            "48": "icon.png",
            "128": "icon.png"
        }
    },
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "host_permissions": [
        "*://ehrd.104.com.tw/cltcms/play-index-home.do*",
        "*://ehrms.104.com.tw/*"
    ],
    "content_scripts": [
        {
            "matches": [
                "*://ehrd.104.com.tw/cltcms/play-index-home.do*"
            ],
            "js": ["content.js"]
        }
    ]
}