      "run_at": "document_idle",
      "js": [
        "js/constants.js",
        "js/ArticleData.js",
        "js/UserSettings.js",
        "js/utils.js",
        "js/ArticleSelection.js",
        "js/ArticleAnalysis.js",
        "js/content.js"
      ]
    }
  ],
  "background": {
