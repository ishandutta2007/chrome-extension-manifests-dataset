{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "WordPress Translation Filler",
  "version": "1.6.14",
  "description": "Automatically fill translations from APIs and indicate the accuracy of translation according to locale glossary.",
  "web_accessible_resources": [
    {
      "resources": [ "/img/*", "wptf-cute-alert.css", "error-alert.flac", "clipboard-icon.png", "/_metadata", "wptf-options.html" ],
      "matches": [ "https://translate.wordpress.org/*" ]

    }
  ],
  "background.service_worker": {
    "scripts": [
      "jsstore.js",
      "jsstore.worker.js"
    ]
  },
  "permissions": [
    "storage"
  ],
  "options_page": "wptf-options.html",
  "content_scripts": [
    {
      "matches": [
        "https://translate.wordpress.org/*"
      ],
      "js": [
        "wptf-purify.min.js",
        "wptf-cute-alert.js",
        "wptf-functions.js",
        "wptf-consistency.js",
        "jsstore.js",
        "jsstore.worker.js",
        "wptf-database.js",
        "wptf-commonTranslate.js",
        "wptf-deeplTrans.js",
        "wptf-googleTrans.js",
        "wptf-microsoftTrans.js",
        "contentScript.js",
        "jsdiff.js"
      ],
      "css": [
        "wptf-cute-alert.css",
        "wptf-style.css"
      ]
    }
  ]
}