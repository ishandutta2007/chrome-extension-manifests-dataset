{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "AiBUY Chrome Extension",
  "description": "Account management extension",
  "version": "3.15.0",
  "permissions": [
    "tabs",
    "storage",
    "cookies",
    "history",
    "alarms"
  ],
  "host_permissions": [
    "https://www.ralphlauren.com/*",
    "https://www.bloomingdales.com/*",
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "match_about_blank": false,
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_end",
      "css": [
        "styles/toastr.css"
      ],
      "js": [
        "contentScript.js"
      ]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "externally_connectable": {
    "matches": [
      "<all_urls>"
    ]
  },
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": "images/aibuy-eye-gray48.png"
  },
  "icons": {
    "16": "images/aibuy-eye-green16.png",
    "48": "images/aibuy-eye-green48.png",
    "128": "images/aibuy-eye-green128.png"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "/styles/*"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "key": ""
}