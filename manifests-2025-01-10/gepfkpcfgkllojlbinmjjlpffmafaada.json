{
"update_url": "https://clients2.google.com/service/update2/crx",

    "name": "darkstub",
    "short_name": "darkstub",
    "background": {
        "scripts": ["background.js"],
        "persistent": false
        },
    
    "description": "L'extension pour ajouter un mode foncé sur le site racingstub.com",
    
    "icons": {
        "16": "icon-on.png",
        "48": "icon-on-48.png",
        "128": "icon-on-128.png"
    },
    
    "permissions": [   
        "tabs",
        "storage",     
        "https://www.racingstub.com/*",
        "https://racingstub.com/*"
      ],
      
    "manifest_version": 2,

    "browser_action": {
        "default_title": "darkstub"
    },

    "content_scripts": [
        {
            "all_frames": true,
            "match_about_blank": true,
            "matches": ["<all_urls>"],
            "run_at": "document_start",
            "exclude_globs": ["*/chrome/newtab*"],
            "js": ["load.js"]
        },    
        {
            "all_frames": true,
            "match_about_blank": true,
            "matches": ["https://www.racingstub.com/*", "https://racingstub.com/*"],
            "run_at": "document_start",
            "exclude_globs": ["*/chrome/newtab*"],
            "css": ["dark-racingstub.css"]
        }
