{
"update_url": "https://clients2.google.com/service/update2/crx",

 "manifest_version": 2,

 "name": "Docs",
 "description": "Chrome extension for getting the documentation about a class or function in a certain language from the official docs",
 "version": "1.0",

 "omnibox": {
  "keyword": "docs"
 },
 "background": {
  "scripts": ["background.js"],
  "persistent": false
 },
 "permissions":[
  "https://docs.oracle.com/*",
  "https://docs.python.org/*",
  "https://developer.mozilla.org/*",
  "tabs"
 ],
 "icons": {
  "1024": "icon.png",
  "128": "icon128.png",
  "48": "icon48.png",
  "16": "icon16.png"
 }
}