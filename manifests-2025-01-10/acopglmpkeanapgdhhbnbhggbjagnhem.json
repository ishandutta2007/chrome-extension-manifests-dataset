{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Compra Asistida con SRM",
  "version": "1.0.19",
  "description": "Recolecta información de productos tus carritos de compra para luego importarlos al asistente de compras de Remesita.com",
  "permissions": [
    "tabs",
    "activeTab",
    "scripting",
    "storage", 
    "cookies",
    "notifications",
    "alarms"
  ],
  "background": {
    "service_worker": "background.min.js"
  },
  "action": {
    "default_popup": "popup.min.html",
    "default_icon": {
      "16": "icons/icon.png",
      "48": "icons/icon.png",
      "128": "icons/icon.png"
      }
  }, 
  "icons": {
    "16": "icons/icon.png",
    "48": "icons/icon.png",
    "128": "icons/icon.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.amazon.com/*"],
      "js": ["amazonus.min.js","core.min.js","qrcode.min.js"]
    },
    {
      "matches": ["https://www.tuambia.com/*","https://tuambia.com/*"],
      "js": ["tuambia.min.js","core.min.js","qrcode.min.js"]
    },
    {
      "matches": ["https://www.aliexpress.com/*","https://m.aliexpress.com/*","https://www.aliexpress.us/*","https://m.aliexpress.us/*"],
      "js": ["aliexpress.min.js","core.min.js","qrcode.min.js"]
    },
    {
      "matches": ["https://www.shein.com/*","https://m.shein.com/*","https://m.shein.com.mx/*","https://us.shein.com/*"],
      "js": ["sheinus.min.js","core.min.js","qrcode.min.js"]
    },
    {
      "matches": ["https://www.temu.com/*","https://m.temu.com/*","https://us.temu.com/*"],
      "js": ["temu.min.js","core.min.js","qrcode.min.js"]
    },
    {
      "matches": ["https://www.walmart.com/*"],
      "js": ["walmartus.min.js","core.min.js","qrcode.min.js"]
    },
    {
      "matches": ["https://www.supermarket23.com/*"],
      "js": ["supermarket23.min.js","core.min.js","qrcode.min.js"]
    },
    {
      "matches": ["https://www.bestbuy.com/*"],
      "js": ["bestbuyus.min.js","core.min.js","qrcode.min.js"]
    },
    {
      "matches": ["https://*.ebay.com/*"],
      "js": ["ebayus.min.js","core.min.js","qrcode.min.js"]
    },
    {
      "matches": ["https://*.dhgate.com/*"],
      "js": ["dhgate.min.js","core.min.js","qrcode.min.js"]
    },
    {
      "matches": ["https://*.caiman53.com/*"],
      "js": ["caiman53.min.js","core.min.js","qrcode.min.js"]
    }, 
    {
      "matches": ["https://*.gd-mart.com/*"],
      "js": ["gd-mart.min.js","core.min.js","qrcode.min.js"]
    },
    {
      "matches": ["https://*.newegg.com/*"],
      "js": ["newegg.min.js","core.min.js","qrcode.min.js"]
    },
    {
      "matches": ["https://dev.remesita.com/*","https://remesita.com/*"],
      "js": ["self.min.js"]
    } 
  ],
  "host_permissions": [
    "*://*.remesita.com/*",
    "*://*.amazon.com/*",
    "*://*.temu.com/*",
    "*://*.shein.com/*",
    "*://*.shein.com.mx/*",
    "*://*.shein.us/*",
    "*://*.aliexpress.com/*",
    "*://*.aliexpress.us/*",
    "*://*.walmart.com/*",
    "*://*.tuambia.com/*",
    "*://*.bestbuy.com/*",
    "*://*.supermarket23.com/*",
    "*://*.ebay.com/*",
    "*://*.caiman53.com/*",
    "*://*.gd-mart.com/*",
    "*://*.newegg.com/*" 
  ],
  "web_accessible_resources": [{
    "resources": ["injectScript.min.js"],
    "matches": ["https://dev.remesita.com/*","https://remesita.com/*"]
  },
  {
    "resources": ["injectTemu.min.js"],
    "matches": ["*://*.temu.com/*"]
  }]
}
