{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Costco Receipt Scanner",
  "version": "2.0",
  "description": "Extracts receipt information from Costco",
  "permissions": [
    "scripting"
  ],
  "host_permissions": [
    "https://www.costco.com/*",
    "https://www.costco.ca/*",
    "https://www.costco.co.uk/*",
    "https://www.costco.com.au/*",
    "https://*.costco.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.costco.com/myaccount/*",
        "https://www.costco.ca/myaccount/*",
        "https://www.costco.co.uk/myaccount/*",
        "https://www.costco.com.au/myaccount/*",
        "https://*.costco.ca/myaccount/*"
      ],
      "js": ["lib/chart.js", "content.js"],
      "run_at": "document_end"
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "show_matches": [
      "https://www.costco.com/*",
      "https://www.costco.ca/*",
      "https://www.costco.co.uk/*",
      "https://www.costco.com.au/*",
      "https://*.costco.com/*"
    ],
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}