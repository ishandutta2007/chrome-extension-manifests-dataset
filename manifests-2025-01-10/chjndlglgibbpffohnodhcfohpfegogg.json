    "manifest_version": 3,
    "name": "malConnect",
    "description": "MAL Linking Tool",
    "version": "3.0",
    "icons": {
        "16": "assets/meta/logo16.png",
        "32": "assets/meta/logo32.png",
        "64": "assets/meta/logo64.png",        
