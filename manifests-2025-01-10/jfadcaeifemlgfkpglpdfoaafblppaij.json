  "commands": {
    "command1ToAmap": {
      "description": "切换至 高德地图",
      "suggested_key": {
        "default": "Alt+1",
        "mac": "Command+1"
      }
    },
    "command2ToBaiduMap": {
      "description": "切换至 百度地图",
      "suggested_key": {
        "default": "Alt+2",
        "mac": "Command+3"
      }
    },
    "command3ToGoogleSatellite": {
      "description": "切换至 Google卫星",
      "suggested_key": {
        "default": "Alt+3",
        "mac": "Command+3"
      }
    },
    "command4ToGoogleEarth": {
      "description": "切换至 Google地球",
      "suggested_key": {
        "default": "Alt+4",
        "mac": "Command+4"
      }
    },
    "command5ToGoogleMap": {
      "description": "切换至 Google地图"
    },
    "command6ToOverpassTurbo": {
      "description": "切换至 overpass turbo"
    }
