{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "FACEIT CheckMate",
  "version": "1.0",
  "description": "This extension enhances notifications for banned users by adding a button to redirect you to common match.",
  "permissions": ["cookies"],
  "host_permissions": [
    "https://*.faceit.com/*",
    "https://www.steamcommunity.com/profiles/*",
    "https://www.steamcommunity.com/id/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["https://*.faceit.com/*"],
      "js": ["content.js"]
    }
  ]
}
