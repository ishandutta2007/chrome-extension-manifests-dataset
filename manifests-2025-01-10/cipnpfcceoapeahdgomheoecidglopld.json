        "<all_urls>"
      ]
    }
  ],
  "icons": {
    "16": "logo/16.png",
    "48": "logo/48.png",
    "192": "logo/192.png"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "jq.wasm",
        "logo/16.png",
        "static/css/content.css",
        "loading.gif"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "action": {
    "default_icon": {
      "192": "logo/192.png"
    },
    "default_title": "Virtual Json Viewer"
