    "options_ui": {
        "page": "src/options.html",
        "chrome_style": true
    },
    "background": {
        "scripts": [
            "src/DisableSettings.js",
            "src/background.js"
        ],
        "persistent": false
    },
    "content_scripts": [
        {
            "matches": ["*://*/*"],
            "all_frames": true,
            "js" : [
                "src/content_scripts.js"
            ]
        }
    ],
    "permissions": [
        "<all_urls>",
        "storage",
        "tabs"
    ],
    "web_accessible_resources" : [
        "/web_accessible_resources/index.js"
    ]
