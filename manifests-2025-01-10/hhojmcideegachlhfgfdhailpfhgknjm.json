{
"update_url": "https://clients2.google.com/service/update2/crx",

    "name": "Enable right click",
    "version": "0.0.10",
    "manifest_version": 2,
    "default_locale": "en",
    "icons": {
        "48": "img/icon48.png"
    },
    "browser_action": {
        "default_title": "Disable right click",
        "default_icon": "img/icon128.png"
