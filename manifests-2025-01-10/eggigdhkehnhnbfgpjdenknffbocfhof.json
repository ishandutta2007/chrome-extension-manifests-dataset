{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,

  "name": "1ClickSaver",
  "description": "This extension helps save music from vk.com.",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["http://vk.com/*"],
      "css": ["mystyles.css"],
      "js": ["jquery.js", "scripts.js"]
    }
  ],
  "permissions": [
    "http://vk.com/*",
    "http://*.vk.com/*",
    "http://*.vk.me/*",
    "contentSettings",
    "downloads",
    "tabs"
  ],
  "web_accessible_resources": [
    "images/save.png"
  ],
  "background": {
    "scripts": ["background.js"]
  }
}