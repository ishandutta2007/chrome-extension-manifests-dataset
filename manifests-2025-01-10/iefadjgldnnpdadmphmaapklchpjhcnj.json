{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "__MSG_extName__",
  "description": "__MSG_extDescription__",
  "default_locale": "en",
  "version": "1.0.5",
  "action": {
    "default_popup": "src/layouts/popup.html",
    "default_icon": "icons/icon-128_x_128.png"
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_end",
      "all_frames": true,
      "js": [
        "javascript/content.js"
      ]
    }
  ],
  "background": {
    "service_worker": "javascript/bg.js",
    "type": "module"
  },
  "icons": {
    "16": "icons/icon-16_x_16.png",
    "32": "icons/icon-32_x_32.png",
    "64": "icons/icon-64_x_64.png",
    "128": "icons/icon-128_x_128.png",
    "256": "icons/icon-512_x_512.png"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "host_permissions": [
    "<all_urls>"
  ]
}