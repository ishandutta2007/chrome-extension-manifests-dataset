{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Logify",
  "version": "3.6",
  "background": {
    "service_worker": "background.js",
    "background.service_worker": true
  },
  "action": {
    "default_popup": "index.html",
    "default_title": "Logify"
  },
  "icons": {
    "16": "/assets/images/black-logo.png",
    "32": "/assets/images/black-logo.png",
    "48": "/assets/images/black-logo.png",
    "128": "/assets/images/black-logo.png"
  },
  "content_scripts": [
    {
      "matches": ["https://admin.shopify.com/*"],
      "js": [
        "content.js",
        "scripts/rrweb-recorder.js",
        "staff-management.js",
        "scripts/rrweb.js",
        "scripts/ua-parser.js",
        "common/config.js",
        "common/utils/utilities.js",
        "create-task.js",
        "create-project.js",
        "create-tag.js",
        "login-screen.js",
        "timesheet.js",
        "task-timer.js",
        "punchin-punchout.js"
      ],
      "css": ["main.css"],
      "all_frames": false
    }
  ],

  "web_accessible_resources": [
    {
      "resources": [
        "*",
        "/assets/images/logo.png",
        "/assets/images/logo-with-text.svg",
        "/assets/images/black-logo.png",
        "/assets/images/logo-recording.png",
        "/assets/images/logo-150x150.png",
        "/assets/images/marker-icon.png",
        "punchin-punchout.html"
      ],
      "matches": ["https://admin.shopify.com/*"]
    }
  ],
  "permissions": ["tabs", "webRequest", "storage", "unlimitedStorage"],
  "host_permissions": ["https://admin.shopify.com/*", "<all_urls>"]
}
