{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Manual Works Assistant",
  "version": "1.2",
  "description": "Manual Works Chrome Extension.",
  "permissions": [
    "tabs",
    "scripting",
    "activeTab",
    "storage",
    "sidePanel",
    "unlimitedStorage"
  ],
  "side_panel": {
    "default_path": "src/sidepanel/sidepanel.html",
    "js": [
      "src/sidepanel/sidepanel.tsx"
    ],
    "css": [
      "/styles.css"
    ]
  },
  "host_permissions": [
    "http://*/*",
    "https://*/*",
    "<all_urls>",
    "http://localhost:3000/*",
    "https://manual-works.jp/*"
  ],
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "js": [
        "assets/content.ts-loader.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_idle",
      "all_frames": false
    }
  ],
  "icons": {
    "128": "src/assets/icon128.png"
  },
  "web_accessible_resources": [
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "assets/config.js",
        "assets/content.ts.js"
      ],
      "use_dynamic_url": false
    }
  ]
}
