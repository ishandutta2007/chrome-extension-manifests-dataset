{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "MaxGPT Web Automation",
    "version": "1.1.0",
    "permissions": [
        "declarativeNetRequest",
        "userScripts"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "scripts/lib/jsep.iife.js",
                "scripts/evaluateExpression.js",
                "scripts/content.js"
            ]
        }
    ],
    "externally_connectable": {
        "matches": [
            "https://maxflow.ai/*",
            "https://sidekick.cat.com/*",
            "https://gpt.cat.com/*"
        ]
    },
    "web_accessible_resources": [
        {
            "resources": [
                "scripts/embedded/clickElement.js"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ],
    "icons": {
        "16": "images/icon-mf.png",
        "128": "images/icon-mf.png"
    },
    "action": {
        "default_popup": "./popup/popup.html",
        "default_icon": {
            "19": "images/icon-mf.png",
            "38": "images/icon-mf.png"
        }
    },
    "declarative_net_request": {
        "rule_resources": [
            {
                "id": "removeHeader",
                "enabled": false,
                "path": "removeHeader.json"
            }
        ]
    }
}