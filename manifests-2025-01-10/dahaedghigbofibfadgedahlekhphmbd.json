{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "GeneSetCart",
  "version": "1.0.1",
  "description": "This is an extension to add gene sets that are found in Rummagene and RummaGEO to GeneSetCart",
  "manifest_version": 3,
  "author": "Ma'ayan Lab",
  "action": {
    "default_popup": "index.html",
    "default_title": "GeneSetCart"
  },
  "icons": {
    "32": "G2SG-logo.png"
  },
  "permissions": [
    "scripting",
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.ncbi.nlm.nih.gov/pmc/articles/*",
        "https://pubmed.ncbi.nlm.nih.gov/*",
        "https://www.ncbi.nlm.nih.gov/pmc/?term=*",
        "https://www.ncbi.nlm.nih.gov/pmc"
      ],
      "css": [
        "fix.css"
      ],
      "js": [
        "content-script.js"
      ]
    },
    {
      "matches": [
        "https://www.ncbi.nlm.nih.gov/geo/query/*"
      ],
      "css": [
        "fix.css"
      ],
      "js": [
        "content-script-geo.js"
      ]
    },
    {
      "matches": [
        "https://www.ncbi.nlm.nih.gov/gds/*"
      ],
      "css": [
        "fix.css"
      ],
      "js": [
        "content-script-geo-search.js"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "white-g2sg-logo.png",
        "g2sg-logo-nbg.png",
        "fix.css"
      ],
      "matches": [
        "https://*/*"
      ]
    }
  ],
  "background": {
    "service_worker": "background.js"
  }
}