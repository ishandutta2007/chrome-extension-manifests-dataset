{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Nocodable - Components Library for Bubble.io",
  "version": "0.0.0.20",
  "description": "Boost your bubble.io app design! Instantly access & save custom components with Nocodable.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "https://bubble.io/*",
    "https://*.bubble.is/*",
    "https://app.nocodable-components.com/*",
    "https://framer.com",
    "clipboardRead",
    "clipboardWrite"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://bubble.io/page*",
        "https://*.bubble.is/*",
        "https://framer.com/projects/*"
      ],
      "js": [
        "content.js"
      ],
      "css": [
        "styles.css"
      ]
    }
  ],
  "action": {
    "default_popup": "popup_message.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "default_width": 640,
    "default_height": 640
  },
  "host_permissions": [
    "https://*/"
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "logo.png",
        "close.png",
        "popup_message.html",
        "popup.html",
        "icon16.png",
        "white-logo.png",
        "manifest.json"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "background": {
    "service_worker": "background.js"
  }
}