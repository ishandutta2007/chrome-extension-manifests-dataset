{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "多窗口分屏丨一键打开多个浏览器窗口多任务并行",
  "version": "1.0",
  "description": "一个用于Chrome浏览器窗口分屏的插件",
  "permissions": [
    "tabs",
    "windows",
    "system.display",
    "commands",
    "storage"
  ],
  "commands": {
    "split-5-5": {
      "suggested_key": {
        "default": "Alt+1"
      },
      "description": "左右分屏 5:5"
    },
    "split-4-6": {
      "suggested_key": {
        "default": "Alt+2"
      },
      "description": "左右分屏 4:6"
    },
    "split-vertical": {
      "suggested_key": {
        "default": "Alt+3"
      },
      "description": "上下分屏"
    },
    "split-quad": {
      "suggested_key": {
        "default": "Alt+4"
      },
      "description": "四分屏"
    }
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
