{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "JimApply",
    "version": "1.0",
    "description": "JimApply - The world's best linkedin autoapplier",
    "permissions": [
      "activeTab",
      "storage",
      "tabs",
      "scripting",
      "power"
    ],
    "host_permissions": [
      "https://*.linkedin.com/*",
      "http://apply.comicfresh.com/*"
    ],
    "action": {
      "default_popup": "index.html",
      "default_icon": {
        "128": "cflogo.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
      "128": "cflogo.png"
    },
    "content_scripts": [
      {
        
        "matches": ["https://*.linkedin.com/*",
        "http://apply.comicfresh.com/*"],
        "js": ["content.js"],
        "run_at": "document_idle",
        "all_frames": false,
        "css": ["styles.css"]
      }
    ],
    "web_accessible_resources": [
      {
        "resources": ["cflogo.png", "*.html", "*.js", "*.css" ],
        "matches": ["<all_urls>"]
      }
    ],
    "externally_connectable": {
      "matches": ["<all_urls>"]
    }
  }
  