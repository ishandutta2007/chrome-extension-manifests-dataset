{
"update_url": "https://clients2.google.com/service/update2/crx",

   "background": {
      "matches": [ "https://www.youtube.com/watch*", "http://www.youtube.com/watch*" ],
      "persistent": false,
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_popup": "widget.html"
   },
   "icons":{
      "128": "res/icon128.png"
   },
   "description": "This extension allows you to set a timer on Youtube's Autoplay",
   "manifest_version": 2,
   "name": "Stop Youtube Autoplay Timer",
   "permissions": [ "activeTab", "storage", "tabs", "https://www.youtube.com/*", "http://www.youtube.com/*" ],
   "short_name": "Timed Autoplay",
   "version": "1.1"
}
