{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Kaven Browser Extension",
  "description": "Proxy, Custom Scripts, Copy Links, Translate, QRCode",
  "version": "1.3.8",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png",
    "512": "icons/icon512.png"
  },
  "options_page": "options/options.html",
  "action": {
    "default_popup": "popup/popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content_script.js"
      ],
      "css": [
        "styles/content_script.css"
      ],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "background": {
    "service_worker": "service_worker.js"
  },
  "permissions": [
    "contextMenus",
    "declarativeNetRequest",
    "proxy",
    "scripting",
    "storage",
    "webNavigation",
    "webRequest",
    "webRequestAuthProvider"
  ],
  "host_permissions": [
    "<all_urls>",
    "*://*/*",
    "http://*/",
    "https://*/"
  ],
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "rules_remove_csp",
        "enabled": false,
        "path": "/rules/rules_remove_csp.json"
      },
      {
        "id": "rules_default",
        "enabled": false,
        "path": "/rules/rules_default.json"
      }
    ]
  }
}