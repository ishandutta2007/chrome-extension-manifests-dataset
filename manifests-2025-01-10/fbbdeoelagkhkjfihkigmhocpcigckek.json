    },
    {
      "js": ["select-script.js"],
      "matches": ["https://*/*", "http://*/*"]
    }
  ],
  "permissions": ["sidePanel", "contextMenus", "storage", "tabs", "activeTab"],
  "icons": {
    "16": "JD_logo.png",
    "48": "JD_logo.png",
    "128": "JD_logo.png"
  },
  "action": {
    "default_title": "Open JDoodle",
    "default_icon": "JD_logo.png"
  }
