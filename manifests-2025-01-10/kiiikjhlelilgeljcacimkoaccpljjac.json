    "https://portal.phishcloud.io/DesktopSignIn",
    "https://portal.phishcloud.io/PortalSignIn",
    "http://*/*",
    "https://*/*",
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "*.png",
        "*.html",
        "*.css",
        "public/img/Logo.png"
