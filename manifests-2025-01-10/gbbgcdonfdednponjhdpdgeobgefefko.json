    "manifest_version": 3,
    "name": "Reckoner Plugin",
    "version": "1.0",
    "permissions": [ 
        "storage"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://edeklarimi.atk-ks.org/DeclareTax/OpenFormForDeclare*"
