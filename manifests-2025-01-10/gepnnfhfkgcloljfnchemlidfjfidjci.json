{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Accepty - Auto Accept for Renown",
  "version": "2.5",
  "description": "Let the User accept Matches on Renown.gg automatically",

  "permissions": [
      "storage"
  ],

  "host_permissions": [
    "*://*.renown.gg/*"
    
  ],

  "icons": {
    "128": "icon.png"
  },

"content_scripts": [
    {
      "matches": [
        "*://*.renown.gg/*"
        
      ],
      "js": ["accepty.js"],
      "run_at": "document_end"
    }
  ],

  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },

  "web_accessible_resources": [
    {
      "resources": ["sounds/notification.mp3"],
      "matches": ["*://*.renown.gg/*"]
    }
  ]

}

