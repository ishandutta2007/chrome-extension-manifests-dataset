   },
   "action": {
       "default_popup": "popup.html",
       "default_icon": {
         "16": "icon16.png",
         "48": "icon48.png",
         "128": "icon128.png"
       }
   },
   "permissions": [
       "storage",
       "clipboardWrite"
   ],
   "web_accessible_resources": [
    {
      "resources": [ "Bamini.woff" ],
      "matches": [ "https://fonts.cdnfonts.com/*" ]
    }
 ]
