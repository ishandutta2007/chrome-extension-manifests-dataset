{
"update_url": "https://clients2.google.com/service/update2/crx",

   "manifest_version": 3,
   "name": "Surfed",
   "description": "Organize and filter your browsing history with Surfed. Surfed respects your privacy and ensures that your data stays on your device.",
   "version": "3.1.1",
   "default_locale": "en",  
   "icons": {
      "48": "images/icon-48.png",
      "96": "images/icon-96.png",
      "128": "images/icon-128.png",
      "256": "images/icon-256.png",
      "512": "images/icon-512.png"
   }, 
   "background": {
      "service_worker": "background.min.js"
   },
   "content_scripts": [{
      "js": [ "Readability.js", "content.min.js" ],
      "matches": [ "<all_urls>" ]
   }],
   "action": {
      "default_popup": "popup.html"
   },
   "permissions": [
      "nativeMessaging",
      "activeTab",
      "tabs",
      "clipboardWrite",
      "contextMenus",
      "scripting"
   ],
   "host_permissions": ["<all_urls>"],
   "omnibox": { "keyword" : "su" },
   "commands": {
      "keycommand-favorite": {
         "suggested_key": {
            "default": "Ctrl+Shift+F",
            "mac": "Command+Shift+F"
         },
         "description": "Favorite"
      },
      "keycommand-tag": {
         "suggested_key": {
            "default": "Ctrl+Shift+I",
            "mac": "Command+Shift+I"
         },
         "description": "Add to lastest Tag"
      },
      "keycommand-dashboard": {
         "suggested_key": {
            "default": "Ctrl+Shift+0",
            "mac": "Command+Shift+0"
         },
         "description": "Show Dashboard"
      }
   },
   "chrome_url_overrides": {
      "newtab": "dashboard.html"
   }
}
