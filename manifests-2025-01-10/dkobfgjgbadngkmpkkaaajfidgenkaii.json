{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,

  "name": "Reddit Megathreader",
  "description": "This extension improves reading experience on Reddit megathreads",
  "version": "1.0.1",
  "permissions": [

    "*://*.reddit.com/*/comments/*"
  ],
  "content_scripts": [
    {
      "matches": ["*://*.reddit.com/*/comments/*"],
      "css": [ "mega.css" ],
      "js": [ "mega.js" ]
    }
  ]
}
