{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Quick Cast",
  "version": "0.0.3",
  "description": "Chrome extension that allows you to quickly control your tabs, bookmarks and histories.",
  "action": {
    "default_popup": "popup.html",
    "default_title": "Quick Cast",
    "default_icon": {
      "16": "icons/action-16.png",
      "48": "icons/action-48.png",
      "128": "icons/action-128.png"
    }
  },
  "background": {
    "type": "module",
    "service_worker": "background.js"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+P",
        "mac": "Command+Shift+P"
      }
    }
  },
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "options_page": "options.html",
  "permissions": [
    "notifications",
    "storage",
    "tabs",
    "tabGroups",
    "bookmarks",
    "history",
    "favicon"
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "assets/*",
        "chunks/*",
        "icons/*",
        "fonts/*"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}