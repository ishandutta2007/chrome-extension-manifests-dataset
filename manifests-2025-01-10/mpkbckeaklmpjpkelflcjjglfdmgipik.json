{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Accessibility Image Alt Text Generator",
  "version": "1.1",
  "description": "An extension to help visually impaired users.",
  "permissions": ["activeTab", "scripting", "storage"],
  "host_permissions": [
    "<all_urls>",
