{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "LeapSearch",
  "version": "0.2.0",
  "description": "Quickly switch between search engines without retyping keywords",
  "permissions": ["tabs", "storage"],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
