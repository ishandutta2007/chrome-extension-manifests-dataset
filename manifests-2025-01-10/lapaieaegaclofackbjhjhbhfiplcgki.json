  "content_scripts": [
    {
      "matches": [
        "*://*.google.com/*",
        "*://*.bing.com/*",
        "*://*.baidu.com/*",
        "*://duckduckgo.com/*",
        "*://*.yandex.com/*",
        "*://*.sogou.com/*",
        "*://*.sogou.cn/*",
        "*://*.qwant.com/*",
        "*://*.ecosia.org/*",
        "*://*.search.naver.com/*",
        "*://*.search.brave.com/*",
        "*://*.youtube.com/*"
      ],
      "css": ["styles.css"],
      "js": ["config.js", "content.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "options_page": "options.html",
