{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Mews Auto-Lock",
  "version": "2.0",
  "background": {
      "service_worker": "background.js"
  },
  "permissions": [
    "tabs",
    "scripting"
  ],
  "content_scripts": [
      {
          "matches": [
              "*://app.mews.com/*",
              "*://app.mews-demo.com/*"
          ],
          "js": ["content_script.js"]
      }
  ],
  "host_permissions": [
      "*://app.mews.com/*",
      "*://app.mews-demo.com/*"
  ]
}
