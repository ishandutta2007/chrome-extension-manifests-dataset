{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "SMILE DS SCREEN",
  "description": "web page URL view as a full-screen kiosk mode.",
  "manifest_version": 2,
  "version": "1.0",
  "author": {
    "name": "KK",
    "email": "kkcarex@gmail.com"
  },
  "kiosk_enabled": true,
  "offline_enabled": true,
  "app": {
 "background": {
      "scripts": ["www/js/jquery.min.js","js/lib/wsc-chrome.js","js/main.js"],
      "persistent": false
 }
  },
  "icons": {
    "16": "img/icon_16.png",
    "128": "img/icon_128.png"
  },
 
  "sockets":{
    "tcpServer": {
      "listen": "*"
    }
  },
  "permissions": [
    "<all_urls>",
    "system.display",
    "system.network",
    "power",
    "webview",
    "alwaysOnTopWindows",
    "storage",
    "app.window.fullscreen",
    "app.window.fullscreen.overrideEsc",
    {"fileSystem": ["directory","retainEntries"]}
  ],
  "optional_permissions": [ "audioCapture", "videoCapture" ]
}
