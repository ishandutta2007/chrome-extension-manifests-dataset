"update_url": "https://clients2.google.com/service/update2/crx",

    "name": "TDS Credential Manager",
    "version": "1.2",
    "description": "Browser Extension for managing TDS website credentials with one-click copy-paste functionality",
    "content_scripts": [
        {
            "js": [
                "content.js"
            ],
            "matches": [
                "https://www.tdscpc.gov.in/app/login.xhtml?usr=Ded",
                "https://www.tdscpc.gov.in/app/login.xhtml?usr=Tpy",
                "https://www.tdscpc.gov.in/app/login.xhtml?usr=Pao"
            ]
        }
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": "icon-128.png"
    },
    "permissions": [
        "storage",
        "tabs",
        "activeTab",
        "clipboardWrite"
    ],
    "host_permissions": [
        "https://www.tdscpc.gov.in/*"
    ],
    "manifest_version": 3,
    "minimum_chrome_version": "102"
