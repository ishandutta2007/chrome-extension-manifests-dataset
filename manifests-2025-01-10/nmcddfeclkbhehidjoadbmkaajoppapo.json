    }
  ],
  "background": {
    "service_worker": "background.bundle.js",
    "type": "module"
  },
  "permissions": [
    "storage",
    "unlimitedStorage",
    "webRequest",
    "tabs"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "options_page": "options/index.html",
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'; script-src-elem 'self' 'unsafe-inline';"
  }
