{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "version": "1.0.0.1",
  "name": "GithubX",
  "description": "__MSG_description__",
  "default_locale": "en",
  "action": {
    "default_title": "GithubX"
  },
  "icons": {
    "48": "assets/logo48.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["*.js", "*.css"],
