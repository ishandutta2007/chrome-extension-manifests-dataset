{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "SAMextension",
  "description": "Adds tooltips to SAM.gov",
  "manifest_version": 3,
  "version": "2.1",
  "background": {
    "service_worker": "service-worker.js"
  },
  "permissions": ["storage", "activeTab"],
  "icons": {
    "16": "images/extension_icon.png",
    "48": "images/extension_icon.png",
    "128": "images/extension_icon.png"
  },
  "action": {
    "default_icon": "images/extension_icon.png",
    "default_title": "SAMextension",
 "default_popup": "menu/login.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>", "file:///*", "*://*/*"],
      "js": ["include/js/jquery-3.5.1.min.js"],
      "run_at": "document_end",
      "all_frames": false
    },
    {
      "matches": ["https://sam.gov/SAM/pages/secured/entity/*"],
      "css": ["content/content.css"],
      "js": ["content/content.js"],
      "run_at": "document_end",
      "all_frames": false
    }
  ],
  "web_accessible_resources": [
    {
      "matches": ["<all_urls>", "*://*/*"],
      "resources": ["data/*.json"]
    }
  ],
  "host_permissions": ["*://*/*"]
}
