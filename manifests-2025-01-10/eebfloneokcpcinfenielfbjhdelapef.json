{
"update_url": "https://clients2.google.com/service/update2/crx",

    "name": "SaePlus extension",
    "description": "Extensión empresarial para uso privado de ciertos asesores de la empresa con fines administrativos.",
    "author": "Jhonattan Ramirez",
    "version": "2.3.1",
    "manifest_version": 3,
    "action": {
        "default_popup": "./index.html",
        "default_icon": "./assets/images/saeplus.png"
    },
    "icons": {
        "16": "./assets/images/saeplus-16x16.png",
        "32": "./assets/images/saeplus-32x32.png",
        "48": "./assets/images/saeplus-48x48.png",
        "128": "./assets/images/saeplus-128x128.png"
    },
    "permissions": [
        "tabs",
        "storage",
        "scripting",
        "http://192.168.16.15/",
        "webRequest",
        "webRequestBlocking"
    ],
    "background":{
        "service_worker":"./js/background.js"
    },
    "content_scripts": [
        {
            "matches": [
                "https://app.saeplus.com/*",
                "https://app.saeplus.net/*"
            ],
            "js": [
                "./js/queryVerificationAction.js",
                "./js/lib/crypto-js/crypto-js.js",
                "./js/manageBreadcrumb.js",
                "./js/lib/axios/axios.min.js",
                "./js/switch.js",
                "./assets/bundles/sweet-alert/sweetalert2.all.js",
                "./assets/bundles/sweet-alert/sweetalert2.all.min.js"
            ]
        }
    ],
    "host_permissions": [
        "https://app.saeplus.com/",
        "https://app.saeplus.net/"
    ]
}