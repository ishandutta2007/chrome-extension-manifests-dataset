{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "OneAuth 浏览器插件",
  "version": "1.0.34",
  "icons": {
    "16": "assets/icons/favicon-16.png",
    "32": "assets/icons/favicon-32.png",
    "48": "assets/icons/favicon-48.png",
    "128": "assets/icons/favicon-128.png"
  },
  "description": "OneAuth 浏览器插件",
  "homepage_url": "https://oneauth.cn/",
  "short_name": "OneAuth",
  "permissions": [
    "tabs",
    "unlimitedStorage",
    "storage",
    "webRequest"
  ],
  "content_security_policy": {
    "script-src": "self",
    "object-src": "self"
  },
  "author": "OneAuth",
  "minimum_chrome_version": "88",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "assets/icons/favicon-16.png",
      "32": "assets/icons/favicon-32.png",
      "48": "assets/icons/favicon-48.png",
      "128": "assets/icons/favicon-128.png"
    },
    "default_title": "OneAuth插件"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+E",
        "linux": "Ctrl+Shift+K",
        "windows": "Alt+Shift+P",
        "mac": "Alt+Shift+P"
      }
    }
  },
  "options_page": "options.html",
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "background": {
    "service_worker": "js/background.bundle.js"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "js/contentScript.bundle.js"
      ],
      "all_frames": true,
      "run_at": "document_idle"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "notifaction.html"
      ],
      "matches": [
        "<all_urls>"
      ]
    },
    {
      "resources": [
        "js/injectScript.bundle.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}