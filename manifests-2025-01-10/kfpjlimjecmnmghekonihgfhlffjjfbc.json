      "resources": [
        "content.css",
        "assets/icon.png",
        "assets/store.png",
        "assets/extension-tick.svg",
        "assets/copyIcon.svg",
        "assets/redirectIcon.svg",
        "assets/tickIcon.svg",
        "assets/icon.svg"
      ],
      "matches": ["<all_urls>"]
