  "content_scripts": [
    {
      "matches": ["https://worldtabletennis.com/eventslist"],
      "js": [
        "locationList.js",
        "library/moment.js",
        "library/moment-timezone-with-data.js",
        "events.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": ["*://worldtabletennis.com/eventInfo*", "*://worldcupresults.ittf.com/eventInfo*"],
      "js": [
        "locationList.js",
        "library/moment.js",
        "library/moment-timezone-with-data.js",
        "contentscript.js"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
