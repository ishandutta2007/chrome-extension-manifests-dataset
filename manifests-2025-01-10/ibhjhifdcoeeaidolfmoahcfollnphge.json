  "action": {
    "default_popup": "./popup.html"
  },
  "icons": {
    "16": "assets/logo.png",
    "32": "assets/logo.png",
    "64": "assets/logo.png",
    "128": "assets/logo.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "tabs",
    "activeTab"
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "fonts/element-icons.ttf",
