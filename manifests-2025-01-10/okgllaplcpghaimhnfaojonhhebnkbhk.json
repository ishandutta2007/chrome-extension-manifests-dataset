    "manifest_version": 3,
    "name": "Skip Fast",
    "version": "0.0.0.1",
    "description": "Automatically skip YT video ads!",
    "permissions": [
        "storage"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://www.youtube.com/*",
                "https://youtube.com/*"
            ],
            "js": [
                "scripts/content.js"
            ],
            "run_at": "document_end"
        }
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "./images/images-16.png",
            "32": "./images/images-32.png",
            "128": "./images/images-128.png"
        }
    },
    "icons": {
        "16": "./images/images-16.png",
        "32": "./images/images-32.png",
        "128": "./images/images-128.png"
    }
