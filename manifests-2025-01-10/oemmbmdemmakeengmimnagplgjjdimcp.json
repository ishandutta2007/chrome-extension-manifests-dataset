{
"update_url": "https://clients2.google.com/service/update2/crx",

    "name": "SafeImages",
    "version": "1.01",
    "description": "Detects profane/indecent images in the webpage and blurs them out. Doesn't access the user's data.",
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "browser_action": {
        "default_icon": "library/Images/shield_green128.png",
        "default_popup": "library/popup/popup.html"
    },
    "background": {
        
        "scripts": [  
            "library/tf.js",
            "library/code/background/debug_print.js",                              
            "library/backend/backenddata.js",
            "library/code/background/variables.js",            
            "library/code/background/callbacks.js",
            "library/code/background/current_tab_updater.js",
            "library/code/background/database.js",            
            "library/code/background/delete_maps_data.js",
            "library/code/background/delete_restore_imagemodel.js",
            "library/code/background/image_classification.js",
            "library/code/background/image_model.js",
            "library/code/background/listeners.js",
            "library/code/background/notification.js",
            "library/code/background/show_all_images.js",
            "library/code/background/stop_timer.js",
            "library/code/background/text_classification.js",
            "library/code/background/text_model.js",            
            "background.js"
        ]
      },  
    "manifest_version": 2,    
    "content_scripts": [
        {
            "matches": ["http://*/*", "https://*/*"],
            "run_at": "document_start",            
            "js": [ "library/jquery-3.5.1.min.js",   
                    "library/code/content/debug_print.js",                  
                    "library/code/content/delete_maps_data.js", 
                    "library/code/content/hideable_content.js", 
                    "library/code/content/listeners.js", 
                    "library/code/content/process_images_data.js",
                    "library/code/content/scroll.js", 
                    "library/code/content/show_all_images.js",
                    "library/code/content/stop_timer.js", 
                    "library/code/content/variables.js",
                    "content.js"
                ],
            "css":  ["/library/ImageBlockerContent/esload.css"],            
            "match_about_blank": true,
            "all_frames" : true          
        }
      ],
      "permissions": [ 
        "http://*/*",
        "https://*/*",      
        "storage",        
        "webRequest",
        "webNavigation",                                 
        "notifications",
        "contextMenus"        
