{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Autoium",
  "version": "2.2",
  "description": "An extension for Autoium.",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "constants.js",
      "helpers.js",
      "background.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "default.png",
    "48": "default.png",
    "128": "default.png"
  },
  "permissions": [
    "storage",
    "contextMenus"
  ],
  "web_accessible_resources": [
    "main.html"
  ],
  "browser_action": {
    "default_icon": "default.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "jquery.min.js",
        "content.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      },
      "description": "Opens main.html"
    }
  }
}