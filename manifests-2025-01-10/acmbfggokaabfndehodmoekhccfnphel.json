{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Private Notes for LinkedIn",
  "version": "0.0.5",
  "description": "This free extension allows you to save your private notes for any profile on Linkedin. Only you can see these notes.",
  "permissions": ["storage", "declarativeContent", "activeTab"],
  "host_permissions": ["https://www.linkedin.com/*"],
  "manifest_version": 3,
  "web_accessible_resources": [
    {
      "resources": ["dist/updates.html", "dist/welcome.html"],
      "matches": ["<all_urls>"]
    }
  ],
  "background": {
    "service_worker": "dist/bundle-background.js"
  },
  "action": {
    "default_popup": "dist/popup/popup.html",
    "default_title": "Show Private Notes",
    "default_icon": {
      "16": "icons/icon_16.png",
      "24": "icons/icon_48.png",
      "32": "icons/icon_128.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["https://www.linkedin.com/*"],
      "js": ["dist/bundle-contents.js"]
    }
  ],
  "incognito": "spanning",
  "icons": {
    "16": "icons/icon_16.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  }
}
