{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,

  "name": "CSS替换器",
  "description": "替换网站样式",
  "version": "0.1.0",

  "background": {
    "scripts": ["js/background.js"]
  },

  "content_scripts": [{
    "matches": ["*://*/*"],
    "run_at": "document_end",
    "js": ["js/content.js"]
  }],

  "permissions": ["activeTab", "storage"],
  "browser_action": {
    "default_icon": {
      "19": "img/icon_19.png",
      "38": "img/icon_38.png"
    },
    "default_popup": "html/popup.html"
  }
}