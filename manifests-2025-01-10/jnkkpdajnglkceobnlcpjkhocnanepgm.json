{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "Nueva Unofficial Source Citer",
    "description": "An Extension that will cite the website that you are currently on",
    "version": "1.0",
    "permissions": [
      "activeTab"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["citelogic.js"]
      }
    ],
    "action": {
      "default_icon": {              
        "16": "yay.png"  
      },
      "default_popup": "hello.html",
      "default_title": "Cite Your Source!"
    }
}