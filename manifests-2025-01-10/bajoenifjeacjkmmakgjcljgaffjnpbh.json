  "version": "0.1.1",
  "manifest_version": 3,
  "icons": {
    "128": "./images/extension_icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "./images/extension_icon.png"
    }
  },
  "options_page": "option.html",
  "content_scripts": [
    {
      "matches": ["https://cssbattle.dev/*"],
      "js": ["./content.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["content.js"],
      "matches": ["https://cssbattle.dev/*"]
    }
  ]
