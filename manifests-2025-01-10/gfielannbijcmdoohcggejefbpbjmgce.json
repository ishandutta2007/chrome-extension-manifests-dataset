{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "HOOBS Nest Sidecar",
  "version": "0.0.3",
  "description": "This extension displays your Nest token and cookies information on your screen. No information is sent to HOOBS.",
  "icons": {
    "128": "hoobs-logo.png"
  },
  "manifest_version": 3,
  "minimum_chrome_version": "101",
  "permissions": [
    "webNavigation",
    "webRequest",
    "clipboardWrite"
  ],
  "host_permissions": [
    "*://accounts.google.com/",
    "https://home.nest.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://home.nest.com/home/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "toolbar.html"
      ],
      "matches": [
        "https://home.nest.com/*"
      ]
    },
    {
      "resources": [
        "title-logo.png"
      ],
      "matches": [
        "https://home.nest.com/*"
      ]
    }
  ]
}