      "128": "/media/logos/plusidentity/default/plusidentity128.png"
    }
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+P",
        "windows": "Ctrl+Shift+P",
        "mac": "Command+Shift+P",
        "linux": "Ctrl+Shift+P"
      }
    }
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
