{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "LinQ Extractor",
    "version": "1.1.0",
    "description": "A basic Chrome extension for External Link extraction by infinitysofthint",
    "permissions": [
      "storage",
      "activeTab"
    ],
    "options_page": "options.html",
    "content_scripts": [
      {
        "matches": ["<all_urls>"], 
        "js": ["utility_functions.js","content.js"],
        "css": ["extractor.extension.css"],
        "run_at": "document_end"
      }
    ], 
    
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "icons": {
      "48": "icon48.png",
      "128": "icon128.png"
    } 
  }
  