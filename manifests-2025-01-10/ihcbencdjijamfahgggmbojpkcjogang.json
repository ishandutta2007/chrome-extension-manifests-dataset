{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "maplelegends-webext",
  "version": "0.4.2",

  "description": "Adds plots to leveling history, tracks current server status, and maintains voting history.",

  "icons": {
    "48": "icons/omok.png"
  },
  "permissions": [
    "webNavigation",
    "storage",
    "https://maplelegends.com/*",
    "https://maplestory.io/api/character/*",
    "https://storage.googleapis.com/geospiza/query/maplelegends-online-count-7-day.json",
    "https://gtop100.com/topsites/MapleStory/*",
    "https://topg.org/maplestory-private-servers/*"
  ],

  "content_scripts": [
    {
      "matches": ["*://maplelegends.com/levels?name*"],
      "js": ["content/leveling.js"]
    },
    {
      "matches": ["*://maplelegends.com/vote"],
      "js": ["content/voting.js"],
      "css": ["content/voting.css"]
    },
    {
      "matches": ["https://gtop100.com/*", "https://topg.org/*"],
      "js": ["content/vote-history.js"]
    }
  ],
  "background": {
    "scripts": ["background-bundle.js"]
  },
  "browser_action": {
    "default_icon": "icons/omok.png",
    "default_title": "MapleLegends extension",
    "default_popup": "popup/index.html"
  }
}
