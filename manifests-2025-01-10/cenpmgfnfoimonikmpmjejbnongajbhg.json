    "scripts": [
      "lib/lib.js",
      "lib/wasm_exec.js",
      "search_engine/google.js",
      "search_engine/arxiv.js",
      "search_engine/search.js",
      "core/conf.js",
      "service_worker.js"
    ]
  },
  "browser_action": {
    "default_popup": "ui/popup.html",
    "default_icon": {
      "16": "images/minedive16.png",
