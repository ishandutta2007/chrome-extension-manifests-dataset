  "manifest_version": 2,
  "name": "minedive",
  "version": "1.0.0",
  "description": "Minedive is a privacy conscious P2P search engine",
  "permissions": [
    "storage",
    "idle",
    "*://*.google.com/",
    "*://*.google.it/",
    "*://arxiv.org/",
    "*://*.arxiv.org/"
