{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Chrome Tab Organizer",
  "description": "Efficiently manage and organize your browser tabs with a single click.",
  "version": "1.4",
  "permissions": [
    "tabs",
    "scripting",
    "windows",
    "system.display"
  ],
  "host_permissions": [
    "*://*.youtube.com/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources": [
    {
      "resources": ["stopYouTubeAutoplay.js"],
      "matches": ["*://*.youtube.com/*"]
    }
  ],
  "author": "Vinny Stocker",
  "homepage_url": "https://pukunui.com"
}
