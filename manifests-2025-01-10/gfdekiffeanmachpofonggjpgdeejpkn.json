{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Bunked Image Authenticator",
  "version": "0.0.0.15",
  "permissions": ["contextMenus", "sidePanel", "scripting", "storage", "tabs"],
  "description": "Use this extension to view contextual information about images in your browser.",
  "background": {
    "service_worker": "js/background.js",
    "type": "module"
  },
  "host_permissions": [
    "https://lens.google.com/search*"
  ],
  "content_scripts": [
    {
      "matches": ["https://lens.google.com/search*"],
      "js": ["js/content.js"]
    }
  ],
  "side_panel": {
    "default_path": "popup.html"
  },
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_title" : "Bunked Image Authenticator"
    
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "sidepanel.html",
        "js/bunked_apis.js",
        "js/bunked_community.js",
        "js/contributor_score.js",
        "js/google_lens.js",
        "js/panes.js",
        "js/sidepanel.js",
        "js/trending.js",
        "js/utils.js",
        "css/sidepanel.css",
        "icons/icon16.png",
        "icons/icon48.png",
        "icons/icon128.png"
      ],
      "matches": ["<all_urls>"]
    }
  ]
}
