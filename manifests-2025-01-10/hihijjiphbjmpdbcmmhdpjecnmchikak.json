{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Coveo Ranking Info",
  "short_name": "coveo-ranking-info",
  "version": "1.02",
  "description": "A Chrome extension to show the ranking information",
  "icons": {
    "16": "images/square.png",
    "48": "images/120 B rounded square.png",
    "128": "images/180 B rounded square.png"
  },
  "background": {
    "service_worker": "js/background.js"
  },
  "action": {
    "default_icon": "/images/square.png"
  },
  "content_scripts": [
    {
      "run_at": "document_start",
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "/dependencies/jquery-3.6.0.min.js",
        "/js/chart.min.js",
        "/js/chartlab.min.js",
        "/js/content.js"
      ],
      "css": [
        "/css/inject.css"
      ],
      "all_frames": false
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "js/checkCoveo.js",
        "js/enableCRI.js",
        "js/ajaxTracker.js",
        "js/consoleTracker.js",
        "images/copy.svg",
        "js/ajaxhook.min.js",
        "js/chart.min.js",
        "js/chartlab.min.js",
        "dependencies/jquery-3.6.0.min.js",
        "images/square.png"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "host_permissions": [
    "*://*/"
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}