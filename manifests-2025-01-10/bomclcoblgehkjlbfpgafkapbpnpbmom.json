{
"update_url": "https://clients2.google.com/service/update2/crx",


    "manifest_version": 3,
  
    "name": "YouTube Limits Extension",

    "icons": {
        "16": "./img/16.png",
        "32": "./img/32.png",
        "48": "./img/48.png",
        "128": "./img/128.png"
    },
  
    "version": "1.0",

    "description": "An extension that provides methods to prevent scrolling on YouTube",
  
    "permissions": [
  
      "alarms",
      "tabs",
      "storage"
  
    ],
  
    "background": {
  
      "service_worker": "background.js"
  
    },

    "action": {

      "default_popup": "popup.html",

      "default_title": "extensionPopup"

    },
  
   "content_scripts": [
  
     {
  
       "matches": ["*://*.youtube.com/*"],
  
       "js": ["contentScript.js"],

       "run_at": "document_idle"
  
     }
  
   ]
  
  }