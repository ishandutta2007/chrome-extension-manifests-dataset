{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "Push Notifications Blocker",
    "version": "1.0",
    "description": "Block all push notifications.",
    "permissions": [
        "notifications",
        "storage",
        "tabs",
        "contentSettings"
      ],
    "host_permissions": [
        "<all_urls>",
        "http://*/*",
        "https://*/*"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/prohibition.png"

  
      }
  },
  "content_scripts": [
    {
        "matches": ["<all_urls>"],
        "js": ["/scripts/content.js"],
        "run_at": "document_start"
    }
    ],
    "web_accessible_resources": [
        {
        "resources": ["notification.html", "notification.css"],
        "matches": ["<all_urls>"]
    }
  ]
}
  