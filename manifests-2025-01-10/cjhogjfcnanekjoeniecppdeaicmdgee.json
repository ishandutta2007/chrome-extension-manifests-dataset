{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "SplatNet Authenticator",
  "description": "This extension update a persistent cookie used to authenticate on SplatNet 2 and SplatNet 3 websites",
  "version": "1.2",
  "manifest_version": 3,
  "icons": { "16": "icon16.png",
             "32": "icon32.png",
             "48": "icon48.png",
    "128": "icon128.png" },
  "action": {
    "default_icon": { "16": "icon16.png",
                      "32": "icon32.png",
                      "48": "icon48.png",
                      "128": "icon128.png" },
 "default_title": "Click Me",
    "default_popup": "options.html"
  },
  "host_permissions": [
    "*://*.splatoon2.nintendo.net/",
 "*://*.lp1.av5ja.srv.nintendo.net/"
  ],
  "permissions": [
 "storage"
  ],
  "content_scripts": [
    {
      "matches": ["https://app.splatoon2.nintendo.net/home*"],
      "js": ["addcookie.js"]
    },
 {
      "matches": ["https://api.lp1.av5ja.srv.nintendo.net/*"],
      "js": ["addcookie3.js"]
    }
  ]
}
