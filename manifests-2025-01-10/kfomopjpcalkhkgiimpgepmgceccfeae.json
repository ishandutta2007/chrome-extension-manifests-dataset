{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "CSS Reloader",
    "version": "1.0",
    "permissions": ["activeTab"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": "icon64.png"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"],
            "run_at": "document_start"
        }
    ],
    "icons": {
        "64": "icon64.png"
    }
}
