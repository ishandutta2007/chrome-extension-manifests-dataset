{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "DeskStrong",
    "version": "1.1",
    "description": "A Chrome extension to manage work sessions and exercise breaks.",
    "permissions": [
        "alarms",
        "notifications",
        "storage"
    ],
    "background": {
        "service_worker": "background/background.js",
        "type": "module"
    },
    "action": {
        "default_popup": "views/timer.html",
        "default_icon": {
            "16": "assets/icon16.png",
            "48": "assets/icon48.png",
            "128": "assets/icon128.png"
        }
    },
    "icons": {
        "16": "assets/icon16.png",
        "48": "assets/icon48.png",
        "128": "assets/icon128.png"
    },
    "options_ui": {
        "page": "views/settings.html",
        "open_in_tab": false
    },
    "web_accessible_resources": [{
        "resources": [
            "assets/*",
            "views/history.html",
            "views/welcome.html",
            "styles/history.css",
            "styles/welcome.css",
            "components/history.js",
            "components/welcome.js",
            "data/exercises.js",
            "data/quotes.js"
        ],
        "matches": ["https://*/*", "http://*/*"]
    }]
}