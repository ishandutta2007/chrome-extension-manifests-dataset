    "default_path": "popup.html"
  },
  "action": {
    "default_title": "Click to open panel",
    "default_popup": "popup.html",

    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "host_permissions": [
    "https://www.linkedin.com/hiring/jobs/*/applicants/*/detail"
  ]
