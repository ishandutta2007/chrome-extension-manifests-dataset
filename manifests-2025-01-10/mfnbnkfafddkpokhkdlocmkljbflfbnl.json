{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Upclip",
  "version": "1.1.5",
  "manifest_version": 2,
  "description": "Capture parts of a page or entire sites and save into your directory!",
  "browser_action": {
    "default_icon": "logo.png",
    "default_title": "Upclip"
