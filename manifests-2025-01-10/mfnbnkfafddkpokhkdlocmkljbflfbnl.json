    "scripts": [
      "lib/jquery.min.js",
      "js/storage.js",
      "js/background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*", "https://*/*", "ftp://*/*"
      ],
      "js": [
        "js/html2canvas.min.js",
        "js/storage.js",
        "js/content.js" 
      ],
      "run_at": "document_end"
    }
  ],
  "icons": { "16": "img/icon16.png",
           "48": "img/icon48.png",
          "128": "img/icon128.png" },
  "web_accessible_resources": [
    "popup/popup.html",
    "popup/project-popup.html",
    "popup/saving.html",
