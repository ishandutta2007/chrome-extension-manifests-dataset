{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "PTechFilter Chrome Extension",
  "version": "1.1",
  "description": "filter people in images using PTechFilter",
  "permissions": [
    "contextMenus"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "src/service_worker.js"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["src/content.js"],
      "css": ["src/style.css"],
      "all_frames": true,
      "run_at": "document_end"}
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "action": {
    "default_popup": "src/popup.html",
    "default_title": "PTech Filter"
  },

  "manifest_version": 3,
  "icons": {
    "32": "images/icon32.png",
   "128": "images/icon128.png"
  }

