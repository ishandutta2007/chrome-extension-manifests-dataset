        "48": "icons/imis-plus-48.png",
        "64": "icons/imis-plus-64.png",
        "128": "icons/imis-plus-128.png",
        "256": "icons/imis-plus-256.png"
    },
    "web_accessible_resources": [
        {
            "resources": [
                "assets/*"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ],
    "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'self';",
