{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "X-Flix Extension",
  "version": "1.0",
  "description": "This will allow user to directly enable X-Flix user from their Xtream Code pannel",
  "permissions": [
    "activeTab",
    "storage",
    "tabs"
  ],
  "host_permissions": [
    "https://x-flix.stream/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "./img/icon16.png",
      "48": "./img/icon48.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources": [{
    "resources": [
      "img/add.png",
      "img/delete.png",
      "img/checkok.png",
      "img/checkerror.png"
    ],
    "matches": ["<all_urls>"]
  }],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}