{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "SureMark Browser Extension",
  "version": "1.10.0",
  "action": {
    "default_icon": {
      "32": "images/32x32.png"
    },
    "default_title": "SureMark Browser Extension",
    "default_popup": "popup.htm"
  },
  "background": {
    "service_worker": "background-bundle.js"
  },
  "content_scripts": [
    {
      "matches": ["*://suremark.digital/*", "*://www.suremark.digital/*"],
      "js": ["content-script-pdf-bundle.js", "openpgp-bundle.js"]
    },
    {
      "matches": ["https://x.com/*"],
      "js": ["content-script-twitter-bundle.js", "openpgp-bundle.js"]
    },
    {
      "matches": ["https://instagram.com/*", "https://www.instagram.com/*"],
      "js": ["content-script-instagram-bundle.js", "openpgp-bundle.js"]
    },
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["content-script-youtube-bundle.js", "openpgp-bundle.js"]
    },
    {
      "matches": ["https://web.archive.org/*"],
      "js": ["content-script-web-archive-bundle.js", "openpgp-bundle.js"]
    },
    {
      "matches": ["https://directory.suremark.digital/*"],
      "js": ["content-script-video-tag-bundle.js", "openpgp-bundle.js"]
    }
  ],
  "externally_connectable": {
    "matches": ["https://directory.suremark.digital/*"]
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self'"
  },
  "permissions": ["activeTab", "storage"]
}
