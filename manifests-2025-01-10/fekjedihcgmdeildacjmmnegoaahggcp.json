"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "AppleTV Wrapped 2024 by UWatcher",
  "version": "1.0.3",
  "permissions": ["storage", "cookies"],
  "description": "Extension analyzes your Apple TV watching history and creates personalized statistics dashboard.",
  "manifest_version": 3,
  "host_permissions": [
    "*://*.amazon.co.jp/*",
    "*://*.amazon.co.uk/*",
    "*://*.amazon.com/*",
    "*://*.amazon.de/*",
    "*://*.apple.com/*",
    "*://*.bamgrid.com/*",
    "*://*.disneyplus.com/*",
    "*://*.hbo.com/*",
    "*://*.hbomax.com/*",
    "*://*.max.com/*",
    "*://*.hulu.com/*",
    "*://*.netflix.com/*",
    "*://*.paramountplus.com/*",
    "*://*.peacocktv.com/*",
    "*://*.primevideo.com/*",
    "*://*.tubitv.com/*",
    "*://*.pluto.tv/*",
    "*://*.crunchyroll.com/*",
    "*://ip-api.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://tv.apple.com/*"
      ],
      "js": [
        "vendor/lodash.min.js",
        "vendor/json2csv.min.js",
        "vendor/jquery.min.js",
        "vendor/purify.min.js",
        "vendor/chart.min.js",
        "utils/DaysMonthsConst.js",
        "utils/waitForElm.js",
        "utils/template.js",
        "utils/loader.js",
        "utils/db.js",
        "utils/stats.js",
        "utils/format.js",
        "utils/debug.js",
        "utils/globalStats.js",
        "utils/charts.js",
        "utils/copybtns.js",
        "utils/reloader.js",
        "common/urls.js",
        "dashboard/dashboard.js",
        "utils/activity.js"
      ],
      "run_at": "document_start"
    },
    {
      "matches": [
        "https://tv.apple.com/?stats=true"
      ],
      "css": ["styles/styles.css"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources":[
        "dashboard/dashboard.html",
        "empty.html",
        "error.html",
        "fonts/*",
        "images/*"
      ],
      "matches": ["https://tv.apple.com/*"]
    }
  ],
  "background": {
    "service_worker": "background/background.js"
  },
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "16": "images/logo16.png",
      "32": "images/logo32.png",
