{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Ngmisr Helper",
  "version": "1.0",
  "description": "Enhance your experience with ngmisr.com: automatic bookmarking, new tab customization, and site search.",
  "permissions": [
    "tabs",
    "bookmarks",
    "storage"
  ],
  "host_permissions": [
    "https://ngmisr.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "action": {
    "default_popup": "search.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
