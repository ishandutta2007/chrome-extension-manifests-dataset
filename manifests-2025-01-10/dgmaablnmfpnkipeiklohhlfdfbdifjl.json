{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Synch",
  "description": "Text To Speech for the visually impaired.",
  "version": "1.3",
  "manifest_version": 3,
  "permissions": [
    "tts",
    "storage",
    "contextMenus",
    "declarativeNetRequest",
    "declarativeNetRequestFeedback"
  ],

  "declarative_net_request":{
    "rule_resources":[
      {
        "id":"ruleset_1",
        "enabled":true,
        "path":"easy_list_4.json"
      },
      {
        "id":"ruleset_2",
        "enabled":true,
        "path":"rules.json"
      }
    ]
  },

  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },

  "background":{
    "service_worker": "background.js"
  },

  "icons":{
    "16": "imgs/logo.png",
    "24": "imgs/logo.png",
    "32": "imgs/logo.png"
  },

  "action":{
     "default_title":"Synch Settings",
     "default_popup": "popup.html"
  },

  "content_scripts": [
   {
     "matches": ["<all_urls>"],
     "js": ["app.js" ]
   }
 ]
}
