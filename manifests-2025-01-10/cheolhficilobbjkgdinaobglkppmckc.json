  "default_icon": {
   "16": "icon16-disabled.png",
   "48": "icon48-disabled.png",
   "128": "icon128-disabled.png"
  }
 },
 "icons": {
  "16": "icon16.png",
  "48": "icon48.png",
  "128": "icon128.png"
 },
