  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "host_permissions": ["https://www.youtube.com/*"],
  "web_accessible_resources": [
    {
      "resources": ["contentScript.js"],
      "matches": ["https://www.youtube.com/*"]
    }
  ]
