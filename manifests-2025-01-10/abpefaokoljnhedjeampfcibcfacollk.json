{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "VideoSnap Pro - Video Frame Capture & Screenshot Tool",
  "version": "1.0.0",
  "description": "Easily capture and save video frames from YouTube, Bilibili and other streaming platforms. Support clipboard copy and file save.",
  "permissions": [
    "activeTab",
    "scripting",
    "storage",
    "downloads",
    "clipboardWrite"
  ],
  "host_permissions": [
    "*://*.youtube.com/*",
    "*://*.bilibili.com/*"
  ],
  "web_accessible_resources": [{
    "resources": ["toast.css"],
    "matches": ["*://*.youtube.com/*", "*://*.bilibili.com/*"]
  }],
  "icons": {
    "16": "icons/icon_128.png",
    "48": "icons/icon_128.png",
    "128": "icons/icon_128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon_128.png",
      "48": "icons/icon_128.png",
      "128": "icons/icon_128.png"
    }
  },
  "commands": {
    "capture-to-clipboard": {
      "suggested_key": {
        "default": "Alt+S",
        "mac": "Alt+S"
      },
      "description": "Capture current frame to clipboard"
    },
    "capture-to-file": {
      "suggested_key": {
        "default": "Alt+Shift+S",
        "mac": "Alt+Shift+S"
      },
      "description": "Save current frame to file"
    }
  },
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*", "*://*.bilibili.com/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "background": {
    "service_worker": "background.js"
  }
}
