  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "host_permissions": [
    "https://api.qrserver.com/*"
  ],
  "web_accessible_resources": [
    {
      "resources": ["history.css"],
      "matches": ["<all_urls>"]
    }
