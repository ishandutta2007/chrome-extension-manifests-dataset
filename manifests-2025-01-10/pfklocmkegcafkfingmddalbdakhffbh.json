{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "LinkedIn Job Turbo",
  "version": "4.0",
  "description": "Streamline and accelerate your job search on LinkedIn",
  "permissions": ["downloads", "storage"],
  "host_permissions": ["*://*.linkedin.com/*"],
  "icons": {
    "16": "icons/img.png",
    "48": "icons/img.png",
    "128": "icons/img.png"
  },
  "action": {
    "default_popup": "landing/landing.html",
    "default_title": "LinkedIn Job Turbo",
    "default_icon": {
      "16": "icons/img.png",
      "48": "icons/img.png",
      "128": "icons/img.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "scripts/pdf.worker.js",
        "popup/keywords.css",
        "content/toast.css"
      ],
      "matches": ["<all_urls>"]
    }
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self';"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": [
        "scripts/pdf.worker.js",
        "scripts/config.js",
        "scripts/chatGPT.js",
        "content/fuzzyFindElement.js",
        "content/toast.js",
        "content/analyzeKeyword.js",
        "content/findLca.js",
        "content/inspection_mode.js",
        "content/autoFormFill.js",
        "content/contentScript.js"
      ],
      "css": [
        "popup/keywords.css",
        "content/inspection_mode.css",
        "content/toast.css"
      ],
      "all_frames": true,
      "run_at": "document_end"
    }
  ],
  "commands": {
    "fill-form": {
      "suggested_key": {
        "default": "Ctrl+F",
        "mac": "MacCtrl+F"
      },
      "description": "Fill Form"
    },
    "fill-form-select": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      },
      "description": "Fill Form with Selection"
    }
  }
}
