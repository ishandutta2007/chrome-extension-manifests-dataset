{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "Facebook Reels Control",
    "description": "Hide Reels and Short Videos from Facebook Feed and redirect Reels URLs.",
    "version": "1.2",
    "author": "Pigeon Soft",
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "permissions": ["storage", "activeTab", "webNavigation"],
    "host_permissions": ["https://*.facebook.com/*"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["https://*.facebook.com/*"],
        "js": ["content.js"],
        "run_at": "document_idle"
      }
    ]
  }
  