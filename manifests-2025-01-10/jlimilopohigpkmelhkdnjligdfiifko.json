{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Cookie Jar",

  "description": "View your cookies live in every shape or form! View cross domain cookies! Copy cookies into JSON format for developer use.",

  "version": "1.0",

  "manifest_version": 3,

  "permissions": [
    "cookies",
    "tabs",
    "activeTab"
  ],

  "host_permissions": [
  "http://*/*",
  "https://*/*"
],
  "icons": {
    "16": "icon-16.png",
    "32": "icon-32.png",
    "64": "icon-64.png",
    "128": "icon-128.png"
  },
  "action": {
    "default_title": "Live Cookies",
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "web_accessible_resources": [{
    "resources": ["cookie_window.html", "background.js"],
    "matches": ["<all_urls>"]
  }]

}
