{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Shopify Email Template Exporter by UpOrder",
  "version": "0.0.0.29",
  "description": "Automate setting email templates live from UpOrder to Shopify",
  "content_security_policy": {
