{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "SupaReply",
  "version": "1.0",
  "description": "AI-powered assistant for Twitter and LinkedIn",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "storage",
    "activeTab"
  ],
  "host_permissions": [
    "https://*.twitter.com/*",
    "https://*.x.com/*",
    "https://*.linkedin.com/*",
    "https://gapdrpbojmzmkivdgdsr.supabase.co/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.twitter.com/*",
        "https://*.x.com/*"
      ],
      "js": [
        "config.js",
        "content.js"
      ]
    },
    {
      "matches": [
        "https://*.linkedin.com/*"
      ],
      "js": [
        "config.js",
        "linkedin/content.js"
      ]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "lib/supabase-client.js",
        "lib/supabase-js.js",
        "lib/supabase.js",
        "config.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self'"
  }
}