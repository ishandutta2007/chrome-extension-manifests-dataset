"update_url": "https://clients2.google.com/service/update2/crx",

 "manifest_version": 2,

 "name": "JS-Exec",
 "description": "Inject your own JavaScript in any webpage.",
 "version": "3.0",
 "author": "SeevenDev",

 "options_page": "options.html",

 "permissions": [
  "storage",
  "unlimitedStorage",
  "activeTab",
