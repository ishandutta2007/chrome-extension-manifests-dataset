{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "VuAgain Chrome extension",
  "version": "1.3.2",
  "description": "Manage information about web pages that I've already seen",
  "permissions": [
    "activeTab",
    "identity",
    "scripting",
    "storage",
    "tabs"
  ],
  "action": {
    "default_popup": "index.html",
    "default_title": "Generate a Vupoint for this page"
  },
  "background": {
    "service_worker": "tooling/background.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": ["*://*.google.com/search*"],
      "js": ["tooling/google_content.js"],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "icon16-trans.png",
    "48": "icon48-trans.png",
    "128": "icon128-trans.png"
  },
  "oauth2": {
    "client_id": "612107680215-hdcskisn6h69k1do4uupber53kduihmo.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/spreadsheets",
      "https://www.googleapis.com/auth/drive",
      "https://www.googleapis.com/auth/userinfo.profile",
      "https://www.googleapis.com/auth/userinfo.email",
      "https://www.googleapis.com/auth/documents.readonly"
    ]
  },
  "externally_connectable": {
    "matches": ["*://localhost/*", "*://deg.github.io/va_dashboard/*"]
  }
}
