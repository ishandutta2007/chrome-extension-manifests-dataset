"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "Youtube Minimalista",
    "description": "Con esta extension podras usar youtube sin recomendaciones para usarlo unicamente para ver videos especificos",
    "version": "2.0",
    "action": {
      "default_popup": "info.html"
    },
    "icons": {
        "16": "images/icon16.png",
        "32": "images/icon32.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png",
        "512": "images/icon512.png"
      },
    "content_scripts": [
      {
        "matches": ["https://www.youtube.com/"],
        "css": ["scripts/mainYoutube.css"]
      },
      {
        "matches": ["https://www.youtube.com/watch*"],
        "css": ["scripts/viewYoutube.css"]
      }
    ]
  }