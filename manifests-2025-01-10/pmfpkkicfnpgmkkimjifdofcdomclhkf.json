{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "GitHub Module Highlighter",
  "description": "Highlights JavaScript modules (in CommonJS or ES6 imports) on GitHub",
  "version": "1.0.1",
  "permissions": [
    "storage",
    "webRequest",
    "webRequestBlocking",
    "*://github.com/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "css": [
        "content.css"
      ],
      "matches": ["*://github.com/*"]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "manifest_version": 2
