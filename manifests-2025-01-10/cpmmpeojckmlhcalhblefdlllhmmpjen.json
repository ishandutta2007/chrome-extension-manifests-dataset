{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Yess.ai Extension",
  "version": "0.3.9.1",
  "description": "Maximize your team potential, anywhere you go!",
  "host_permissions": [
    "https://*.yess.ai/*",
    "https://*.linkedin.com/*",
    "https://*.sentry.io/*"
  ],
  "permissions": [
    "storage",
    "alarms",
    "webRequest",
    "tabs",
    "scripting",
    "cookies",
    "activeTab"
  ],
  "icons": {
    "48": "yess-logo-black.png"
  },
  "action": {
    "default_icon": "yess-logo-blue.png"
  },
  "content_scripts": [
    {
      "js": [
        "assets/main.tsx-loader.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "assets/main.css"
      ]
    }
  ],
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "web_accessible_resources": [
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "assets/getExtensionVersion.js",
        "assets/main.tsx.js"
      ],
      "use_dynamic_url": false
    }
  ]
}
