{
"update_url": "https://clients2.google.com/service/update2/crx",

  "$schema": "https://json.schemastore.org/chrome-manifest",
  "manifest_version": 3,
  "name": "Gener8",
  "author": "Gener8",
  "description": "Be rewarded for your data while browsing free from intrusive ads and annoying cookie banners.",
  "version": "3.0.4",
  "version_name": "3.0.4",
  "homepage_url": "https://gener8ads.com",
  "minimum_chrome_version": "120",
  "icons": {
    "16": "dist/assets/img/icons/icon16.png",
    "48": "dist/assets/img/icons/icon48.png",
    "128": "dist/assets/img/icons/icon128.png"
  },
  "permissions": [
    "tabs",
    "cookies",
    "offscreen",
    "alarms",
    "declarativeNetRequestWithHostAccess"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "action": {
    "default_popup": "dist/popup.html",
    "default_icon": {
      "16": "dist/assets/img/icons/icon16.png",
      "48": "dist/assets/img/icons/icon48.png",
      "128": "dist/assets/img/icons/icon128.png"
    }
  },
  "background": {
    "service_worker": "dist/background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "dist/content.js"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "/dist/assets/*",
        "/dist/*.ttf",
        "/dist/*.woff"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "default",
        "enabled": true,
        "path": "rulesets/default.json"
      },
      {
        "id": "cookies",
        "enabled": true,
        "path": "rulesets/annoyances-cookies.json"
      },
      {
        "id": "overlays",
        "enabled": true,
        "path": "rulesets/annoyances-overlays.json"
      }
    ]
  }
}
