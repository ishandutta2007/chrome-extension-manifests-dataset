{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Mannat.io Browser Extension",
  "version": "1.0",
  "description": "Generate OpenAI responses in any text field",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
      "16": "16.png",
      "32": "32.png",
      "48": "48.png",
      "128": "128.png"
    },
    "default_popup": "options.html"
  },
  "commands": {
    "generate_openai_response": {
      "suggested_key": {
        "default": "Ctrl+Shift+K"
      },
      "description": "Generate OpenAI response"
    }
  }
}
