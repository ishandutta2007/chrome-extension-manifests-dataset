{
"update_url": "https://clients2.google.com/service/update2/crx",

  "background": {
    "scripts": [
      "background.js"
    ]
  }, 
  "content_scripts": [
    {
      "js": [
        "contentInstagram.js"
      ], 
      "matches": [
        "https://www.instagram.com/p/*", 
        "https://www.instagram.com/*"
      ]
    },
    {
      "js": [
        "contentTistory.js"
      ],
      "matches": [
        "http://*.uf.tistory.com/image/*"
   ],
   "run_at": "document_start"
    },
    {
      "js": [
        "contentDaumcdn.js"
      ],
      "matches": [
        "https://t1.daumcdn.net/cfile/tistory/*"
   ],
   "run_at": "document_start"
    },
    {
      "js": [
        "contentNaver.js"
      ], 
      "matches": [
        "http://m.star.naver.com/*"
      ]
    },
    {
      "js": [
        "contentTwitter.js"
      ],
      "matches": [
        "https://pbs.twimg.com/media/*"
   ],
   "run_at": "document_start"
    },
    {
      "js": [
        "contentYoutube.js"
      ],
      "matches":[
        "https://www.youtube.com/*"
      ],
      "run_at":"document_end"
    }
  ], 
  "description": "Extension to help getting Instagram, m.star.naver images and automatically changing twitter, Tistory and Daumcdn images to original",
  "icons": {
    "128": "icon128.png", 
    "48": "icon48.png"
  }, 
  "manifest_version": 2, 
  "name": "WebSite-Helper", 
  "permissions": [
    "https://www.instagram.com/p/*", 
    "http://www.m.star.naver.com/*", 
    "http://m.star.naver.com/*",
    "https://pbs.twimg.com/media/*",
    "https://t1.daumcdn.net/cfile/tistory/*",
    "http://*.uf.tistory.com/image/*",
    "https://www.youtube.com/*"
  ],
  "version": "1.6.0"
}