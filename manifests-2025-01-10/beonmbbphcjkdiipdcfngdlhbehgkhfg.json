  "manifest_version": 3,
  "name": "Smart Page Analyzer",
  "version": "2.0",
  "description": "Analyze web pages using AI to extract insights, summaries, and structured data",
  "permissions": [
    "activeTab",
    "storage",
    "sidePanel",
    "scripting"
  ],
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "side_panel": {
    "default_path": "src/panel/panel.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "options_page": "src/options/options.html",
  "background": {
    "service_worker": "src/scripts/background.js",
    "type": "module"
  },
  "host_permissions": [
    "https://api.openai.com/*",
    "https://mail.google.com/*",
    "<all_urls>"
  ],
  "web_accessible_resources": [{
    "resources": [
      "src/panel/*.css",
      "src/panel/*.js",
      "src/services/*.js",
      "src/utils/*.js",
      "src/options/*.css",
      "src/options/*.js"
    ],
    "matches": ["<all_urls>"]
