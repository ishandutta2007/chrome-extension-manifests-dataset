{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "Google Photos Delete Tool",
    "version": "1.0",
    "description": "Bulk delete photos from Google Photos",
    "permissions": ["activeTab"],
    "host_permissions": [
      "https://photos.google.com/*"
