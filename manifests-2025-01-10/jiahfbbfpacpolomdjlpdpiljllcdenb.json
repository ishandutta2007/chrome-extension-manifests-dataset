    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_icon": "icon.png"
    },
    "content_scripts": [
      {
        "matches": ["https://photos.google.com/*"],
        "js": ["content.js"]
      }
    ],
    "icons": {
      "128": "icon.png"
    }
