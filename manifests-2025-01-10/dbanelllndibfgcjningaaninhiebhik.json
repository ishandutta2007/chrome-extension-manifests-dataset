  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "tesorio_cross_origin",
        "enabled": true,
        "path": "tesorio_cross_origin_rules.json"
      }
    ]
  },
  "web_accessible_resources": [
    {
      "resources": ["*.map", "*.js"],
      "matches": ["https://mail.google.com/*"]
    }
