{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Pretty Links",
  "version": "1.0",
  "description":"A pink bookmarks extension with cute folder icons and right-click features to save your favorite webpages to Pretty Links collection"
  ,
  "icons": {
    "16": "icons/hearts.png",
    "48": "icons/ribbon.png",
    "128": "icons/kawaii_icon.png"
  },
  "permissions": ["contextMenus", "bookmarks", "storage", "activeTab"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "sidepanel.html",
    "default_title": "Add Current Page to Pretty Links",
    "default_icon": {
      "16": "icons/ribbon.png",
      "48": "icons/ribbon.png",
      "128": "icons/ribbon.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content-script.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "icons/hearts.png",
        "icons/ribbon.png",
        "icons/gummy-bear.png",
        "icons/beauty.png",
        "icons/bag.png",
        "icons/magic-wand.png",
        "icons/peach.png",
        "icons/popsicle.png",
        "icons/kawaii_icon.png"
      ],
      "matches": ["<all_urls>"]
    }
  ]
}

