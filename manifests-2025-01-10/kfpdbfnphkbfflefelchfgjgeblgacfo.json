  ],
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "ruleset_1",
        "enabled": true,
        "path": "block_rules.json"
      }
    ]
  },
  "manifest_version": 3,
  "permissions": [
    "storage",
    "declarativeNetRequest"
