    "*://*.facebook.com/*",
    "*://*.twitter.com/*",
    "*://*.pinterest.com/*",
    "*://*.reddit.com/*",
    "*://*.aliyuncs.com/*",
    "*://*.twitch.tv/*",
    "*://googleads.g.doubleclick.net/*"
  ],
  "background": {
    "service_worker": "js/background.js"
  },
  "action": {
    "default_popup": "popup.html",
