{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "网站拉黑插件",
    "version": "1.0",
    "permissions": [
        "storage",
        "activeTab"
    ],
    "background": {
        "service_worker": "background.js" 
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "logo/16.png",
            "48": "logo/48.png",
            "128": "logo/128.png"
        }
    },
    "icons": {
        "16": "logo/16.png",
        "48": "logo/48.png",
        "128": "logo/128.png"
    },
    "web_accessible_resources": [
        {
            "resources": ["logo/*"],
            "matches": ["<all_urls>"]
        }
    ]
}
