{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3, 
  "name": "工具箱",
  "version": "1.1.7",
  "description": "为高效办公赋能、提高办公效率、提供常用功能集合、为互联网用户提供便捷实用功能。", 
   
  "content_security_policy": {
    "extension_pages": "default-src 'self'; script-src 'self'; style-src 'self' 'unsafe-inline'; img-src * data:; connect-src * data:; font-src * data:;"
  },

  "background": {
    "service_worker": "background.js",
    "type": "module"
  },

  "content_scripts": [{
      "js":["content.js"],
      "matches":["<all_urls>"],
      "run_at": "document_end"
  }],

  "icons": {
    "64": "/assets/images/64x64.png",
    "128": "/assets/images/128x128.png",
    "256": "/assets/images/256x256.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "64": "/assets/images/64x64.png",
      "128": "/assets/images/128x128.png",
      "256": "/assets/images/256x256.png"
    }
  },

  "commands":{
    "_execute_action":{
        "suggested_key":{
            "default":"Ctrl+Shift+H",
            "mac":"MacCtrl+Shift+H"
        },
        "description":"打开地址栏"
    }
  },
  "web_accessible_resources": [
    {
      "resources": [ "/rules_interceptor.json" ],
      "matches": ["<all_urls>" ]
    }
  ],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
 },
  "permissions": [
    "activeTab",
    "contextMenus",
    "tabs",
    "storage",
    "browsingData",
    "cookies",
    "scripting",
    "declarativeNetRequest",
    "declarativeNetRequestFeedback",
    "proxy"
  ],

  "host_permissions": ["<all_urls>"]

}