{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Excerpt Saver",
  "version": "1.0",
  "permissions": [
      "storage",
      "tabs",
      "activeTab"
  ],
  "background": {
      "service_worker": "background.js"
  },
  "content_scripts": [
      {
          "matches": ["<all_urls>"],
          "js": ["content.js"]
      }
  ],
  "web_accessible_resources": [
    {
      "resources": ["images/save_icon.png"],
      "matches": ["<all_urls>"]
    }
  ],
  "action": {
      "default_icon": {
          "16": "icon.png"
      },
      "default_title": "Open Excerpts"
  }
}

