    "rule_resources": [
      {
        "id": "net_rules",
        "enabled": true,
        "path": "net_rules.json"
      }
    ]
  },
  "permissions": [
    "tabs",
    "storage",
    "activeTab",
    "contextMenus",
    "notifications",
    "declarativeNetRequest",
    "declarativeNetRequestWithHostAccess"
  ],
  "host_permissions": [
    "*://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "dist/contentScripts/index.global.js"
      ]
    }
