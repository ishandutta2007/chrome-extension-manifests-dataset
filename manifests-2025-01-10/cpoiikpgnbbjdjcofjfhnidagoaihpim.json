{
"update_url": "https://clients2.google.com/service/update2/crx",

    "name": "Browsly - Search Visually",
    "version": "0.0.0.11",
    "manifest_version" : 2,

    "description": "Visual highlighting context from your Searches to find what you need fast on web pages",

    "minimum_chrome_version": "35",
    "chrome_settings_overrides": {
        "search_provider": {
            "name": "Accessibility",
            "keyword": "Accessibility",
            "search_url": "https://www.browsly.co/?a=b&q={searchTerms}",
            "favicon_url": "https://www.browsly.co/browslylogo.png",
            "encoding": "UTF-8",
            "is_default": true
          }
    },

    "icons": {
        "16": "img/icon16.png",
        "32": "img/icon32.png",
        "48": "img/icon48.png",
        "64": "img/icon64.png",
        "128": "img/icon128.png"
    },
    "browser_action": {
        "default_title": "Browsly - Search Visually",
        "default_icon": {
            "16": "img/icon16.png",
            "32": "img/icon32.png",
            "48": "img/icon48.png",
            "64": "img/icon64.png",
            "128": "img/icon128.png"
        }
      },

   

    "background": {
        "page" : "background/background.html",
        "persistent" : true
    },

 

    "content_scripts": [
        {
            "matches": ["http://*/*", "https://*/*", "file://*/*"],
            "js": ["/accessibility-core.js"],
                "run_at" : "document_end",
                "all_frames" : true
        }
    ],

    "permissions": [
        "contextMenus",
        "<all_urls>",
        "activeTab",
        "storage",
        "webRequest",
        "webRequestBlocking"
    ]


}
