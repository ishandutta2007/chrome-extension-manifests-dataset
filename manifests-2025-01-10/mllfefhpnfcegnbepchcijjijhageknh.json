    "16": "icon_16.png",
    "32": "icon_32.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "icon_off_32.png",
    "default_title": "WordPress Detector"
  }
