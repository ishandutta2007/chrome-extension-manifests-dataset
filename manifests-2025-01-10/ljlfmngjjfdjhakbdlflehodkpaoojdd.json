        "content.js"
      ]
    }
  ],
  "commands": {
    "run-foo": {
      "suggested_key": {
        "default": "Alt+Shift+F",
        "mac": "Alt+Shift+F"
      },
      "description": "Run \"foo\" on the current page."
    },
    "_execute_action": {
      "suggested_key": {
        "windows": "Alt+Shift+F",
        "mac": "Alt+Shift+F",
        "chromeos": "Alt+Shift+F",
        "linux": "Alt+Shift+F"
      }
    }
