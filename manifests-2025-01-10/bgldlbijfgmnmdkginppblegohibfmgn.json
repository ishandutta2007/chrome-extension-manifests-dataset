{
"update_url": "https://clients2.google.com/service/update2/crx",

    "name": "Pagetok Extension",
    "version": "0.0.1.1",
    "description": "Pagetok Extension",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "activeTab",
        "tabs"
    ],
    "content_scripts": [{
        "js": ["lib/jquery.js", "lib/Readability.js", "content-script.js" ],
        "css": ["css/inject.css"],
        "run_at": "document_idle",
        "matches": ["<all_urls>"]
     }],    
    "action": {
        "default_icon": {
          "16": "img/logo.png",
          "32": "img/logo.png",
          "48": "img/logo.png",
          "128": "img/logo.png"
        }
    },
    "icons": {
        "16": "img/logo.png",
        "32": "img/logo.png",
        "48": "img/logo.png",
        "128": "img/logo.png"
    },
     "web_accessible_resources": [{
         "resources" : ["img/*", "fonts/*", "app.html"],
         "matches": ["<all_urls>"]
     }]
}