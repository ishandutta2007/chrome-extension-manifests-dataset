  },
  "content_scripts": [
    {
      "matches": [
        "https://meet.google.com/*"
      ],
      "js": [
        "googleMeet-control.js"
      ]
    },
    {
      "matches": [
        "https://teams.live.com/*"
      ],
      "js": [
        "teams-control.js"
      ]
    }
