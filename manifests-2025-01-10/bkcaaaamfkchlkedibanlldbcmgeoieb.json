{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "ReadSmart",
  "version": "1.0",
  "description": "A Chrome extension to summarize web content.",
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "https://www.readsmart.cc/"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "img/*",
        "css/*"
      ],
      "matches": [
        "*://*/*"
      ]
    }
  ],
  "action": {
    "default_popup": "",
    "default_icon": {
      "16": "icon-small.png",
      "32": "icon.png"
    }
  },
  "permissions": [
    "tabs",
    "contextMenus"
  ],
  "icons": {
    "16": "icon-small.png",
    "32": "icon.png"
  }
}
