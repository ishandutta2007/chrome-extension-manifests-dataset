{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Lightfunnels Domain Connector",
  "version": "1.3.4",
  "description": "Connect your domain name with your funnels",
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "scripting"
  ],
  "host_permissions": [
    "https://*.godaddy.com/*",
    "https://*.namecheap.com/*",
    "https://*.dynadot.com/*",
    "https://*.hostinger.com/*",
    "https://dns.google/*",
    "https://your-whois-api.com/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "48": "icons/favicon.png",
      "128": "icons/favicon-large.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.godaddy.com/*",
        "https://*.namecheap.com/*",
        "https://*.dynadot.com/*",
        "https://hpanel.hostinger.com/*"
      ],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["popup.html"],
      "matches": ["<all_urls>"]
    }
  ],
  "icons": {
    "48": "icons/favicon.png",
    "128": "icons/favicon-large.png"
  }
}
