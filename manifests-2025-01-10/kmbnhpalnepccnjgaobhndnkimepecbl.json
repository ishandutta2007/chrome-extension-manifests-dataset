      "matches": [
        "http://*.ac.kr/mod/vod/viewer.php*",
        "https://*.ac.kr/mod/vod/viewer.php*"
      ],
      "js": ["videoHelper.js"]
    },
    {
      "matches": ["http://*.ac.kr/*", "https://*.ac.kr/*"],
      "js": [
        "tableHelper.js",
        "shortcutHelper.js",
        "reportHelper.js",
        "signupHelper.js"
      ]
    }
  ],
  "web_accessible_resources": ["js/*.js", "css/*.css", "images/TR_GR.png"]
