      "https://jwilliams-chatserver.herokuapp.com/"
    ],
    "permissions": ["storage"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
          "16": "/images/chat16.png",
          "32": "/images/chat32.png"
          }
    },
    "icons": {
      "16": "/images/chat16.png",
      "32": "/images/chat32.png",
