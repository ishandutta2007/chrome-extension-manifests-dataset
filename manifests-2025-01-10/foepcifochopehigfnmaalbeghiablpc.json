{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "BDQ Tab Manager for Atlassian",
  "version": "1.0",
  "permissions": [ "storage","tabs", "activeTab", "scripting"],
  "host_permissions": ["https://*.atlassian.net/*"],
  "action": {
    "default_icon": {
      "16": "icons/default-16-icon.png",
      "32": "icons/default-32-icon.png",
      "48": "icons/default-48-icon.png"
    },
    "default_popup": "popup.html",
    "default_title": "Atlassian Tab Manager"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["*://*.atlassian.net/*"],
      "js": ["contentScript.js"]
    }
  ],
  "options_page": "settings.html",
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+H",
        "mac": "Command+Shift+H"
      },
      "description": "Show the Atlassian tabs list"
    }
  }
}
