    "manifest_version": 3,
    "name": "DesignDupe",
    "version": "1.2",
    "description": "Identify colors and fonts on any website instantly to elevate your creativity!",
    "permissions": ["activeTab", "scripting"],
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
