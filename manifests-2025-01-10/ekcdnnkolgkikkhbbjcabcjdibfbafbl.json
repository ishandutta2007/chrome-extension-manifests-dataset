  "manifest_version": 3,
  "name": "Hide Youtube Watched Videos",
  "description": "This extension will hide watched youtube videos from the subscriptions page.",
  "version": "1.7.1",
  "action": {
    "default_icon": "images/icon128.png"
  },
  "icons": {
