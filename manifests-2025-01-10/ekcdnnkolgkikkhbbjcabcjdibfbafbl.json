  "options_page": "src/options.html",
  "background": {
    "service_worker": "script.js"
  },
  "permissions": ["scripting", "storage", "tabs", "webNavigation"],
  "host_permissions": ["*://*.youtube.com/"],
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "css": ["src/hideYoutube.css"]
    }
  ]
