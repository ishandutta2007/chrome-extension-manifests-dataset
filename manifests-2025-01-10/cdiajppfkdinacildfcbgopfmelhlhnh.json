{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Prio365 Extension",
  "version": "0.0.5",
  "description": "Simplify your file upload process from Prio365 with our independent and user-friendly Google Chrome extension.",
  "options_page": "src/modules/options/index.html",
  "background": {
    "service_worker": "src/modules/background/index.js",
    "type": "module"
  },
  "action": {
    "default_popup": "src/modules/popup/index.html",
    "default_icon": "prio_logo.png"
  },
  "icons": {
    "128": "prio_logo.png"
  },
  "content_scripts": [
    {
      "exclude_globs": [],
      "matches": [
        "http://*/*",
        "https://*/*",
        "<all_urls>"
      ],
      "js": [
        "src/modules/content/index.js"
      ],
      "css": [
        "assets/css/contentStyle.chunk.css"
      ]
    }
  ],
  "devtools_page": "src/modules/devtools/index.html",
  "permissions": [
    "activeTab",
    "storage",
    "identity",
    "scripting"
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "assets/js/*.js",
        "assets/css/*.css",
        "prio_logo.png",
        "icon-34.png"
      ],
      "matches": [
        "*://*/*"
      ]
    }
  ]
}