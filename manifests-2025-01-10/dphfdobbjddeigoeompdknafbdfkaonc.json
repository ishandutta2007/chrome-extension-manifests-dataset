"update_url": "https://clients2.google.com/service/update2/crx",

 "manifest_version": 2,
 "name": "Remove WS Ad",
 "version": "0.4",
 "browser_action": {
  "default_icon":  "icon.png"
 },
 "content_scripts": [
  {
   "matches":  ["*://*.mail.google.com/*"],
   "js":   ["jquery.min.js", "jquery-2.1.3.min.js", "gmail.js", "content.js"]
  }
 ],
 "background": {
  "scripts": ["background.js"]
 }
