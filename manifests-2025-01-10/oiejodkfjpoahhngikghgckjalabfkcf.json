{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "id": "y-dictionary",
  "name": "Y Dictionary",
  "description" : "瀏覽網頁時可輕鬆查閱單字及片語。",
  "version": "1.1.0",
  "minimum_chrome_version": "88",
  "action": {
    "default_icon" : "logo128.png",
    "default_title": "Y Dictionary"
  },
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "activeTab",
    "contextMenus",
    "sidePanel",
    "https://*/*"
  ],
  "side_panel": {
    "default_path": "index.html"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "logo16.png",
        "logo48.png",
        "logo128.png"
      ],
      "matches": ["https://*/*"]
    }
  ]
}
