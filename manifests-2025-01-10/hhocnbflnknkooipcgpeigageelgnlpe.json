{
"update_url": "https://clients2.google.com/service/update2/crx",

    "name": "Swish QR-code generator",
    "version": "0.0.2",
    "description": "Generates Swish QR-codes from selections",
    "manifest_version": 2,
    "permissions": [
        "https://api-proxy.swish.nu/*",
        "contextMenus",
        "commands"
    ],
    "content_scripts": [
        {
        "matches": ["<all_urls>"],
            "run_at": "document_end",
            "js": [
                "main.js"
            ]
        }
    ],
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": true
    },
    "commands": {
        "create-code-from-selection": {
            "suggested_key": {
                "default": "Ctrl+Shift+S"
            },
            "description": "Create QR-code from selected text"
        }
    },
    "icons": {
        "16": "logo_16.png",
  "128": "logo_128.png",
        "512": "logo_512.png",
  "1000": "logo_1000.png"
    }
}
