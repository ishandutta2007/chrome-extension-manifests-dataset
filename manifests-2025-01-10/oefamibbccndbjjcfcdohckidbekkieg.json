{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Facebook Auto Unmute",
  "description": "Automatically unmutes Facebook videos.",
  "version": "1.1",
  "permissions": [
    "activeTab",
    "storage",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": "icon.png",
    "default_title": "Facebook Auto Unmute"
  },
  "content_scripts": [
    {
      "matches": ["*://*.facebook.com/*"],
      "js": ["content.js"]
    }
  ],
  "commands": {
    "toggle-unmute": {
      "suggested_key": {
        "default": "Ctrl+Shift+U"
      },
      "description": "Toggle Auto Unmute"
    }
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  }
}
