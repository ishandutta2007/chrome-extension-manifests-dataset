    "service_worker": "js/backgroundPage.js"
  },
  "icons": {
    "16": "muzzo-16.png",
    "48": "muzzo-48.png",
    "128": "muzzo-128.png"
  },
  "host_permissions": [
    "https://*.app.muzzo.io/*",
    "https://*.linkedin.com/*"
  ],
  "externally_connectable": {
    "matches": [
      "https://*.app.muzzo.io/*",
      "http://localhost:3000/*",
      "https://*.vercel.app/*"
    ]
  },
  "content_scripts": [
    {
      "run_at": "document_start",
      "matches": [
        "https://linkedin.com/*",
        "https://www.linkedin.com/*"
      ],
      "exclude_matches": [
        "https://linkedin.com/talent/*",
