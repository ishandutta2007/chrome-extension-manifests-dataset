{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,

  "name": "isear",
  "description": "複数ワードに対応したページ内検索ツール",
  "version": "2.7",

  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F"
      },
      "description": "検索を開始する"
    },
    "switch_mode": {
      "suggested_key": {
        "default": "Alt+M"
      },
      "description": "コマンドモード切り替え"
    },
    "toggle_bars": {
      "suggested_key": {
        "default": "Alt+B"
      },
      "description": "ハイライトバーをON/OFF"
    },
    "retry": {
      "suggested_key": {
        "default": "Alt+R"
      },
      "description": "ハイライトを更新"
    }
  },

  "content_security_policy": {
    "script-src" : "self",
    "object-src" : "self"
  },
  "background": {
    "service_worker": "background.js"
  },
  "options_page": "option.html",
  "action": {
    "default_icon": "data/icons/icon32.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "data/icons/icon32.png",
    "16": "data/icons/icon32.png",
    "48": "data/icons/icon32.png"
  },
  "permissions": [
    "tabs", "storage", "contextMenus", "scripting"
  ],
  "host_permissions" : [
    "http://*/*", "https://*/*", "file:///*/*"
  ],
  "homepage_url": "https://isear.intelfike.net/"
