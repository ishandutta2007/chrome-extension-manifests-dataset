  "commands": {
    "_execute_action": {},
    "search": {
      "description": "Find SWF and Open in Emulator"
    },
    "inject-ruffle": {
      "description": "Load all embedded SWF objects"
    },
    "open-empty": {
      "description": "Open Empty Emulator (Local SWF)"
    }
  }
