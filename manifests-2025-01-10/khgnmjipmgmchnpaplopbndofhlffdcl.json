      "128": "icon128.png"
  },

  "commands":
  {
    "play":
    {
      "suggested_key":
      {
        "default": "Ctrl+Comma",
        "mac": "Command+Comma"
      },

      "description": "Go to playing tab"
    }
  },

  "options_page": "options.html"
