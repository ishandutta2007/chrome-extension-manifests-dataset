{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "APISpy Enabler",
  "description": "This extension enables user to send API requests from apispy.io to endpoints where CORS is not enabled",
  "version": "0.9.6",
  "background": {
    "persistent": true,
    "scripts": [
      "src/background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": ["http://*.apispy.io/*", "https://*.apispy.io/*"],
      "js": ["src/inject-extension-installed.js"]
    }
  ],
  "icons": {
    "16": "images/16.png",
    "48": "images/48.png",
    "128": "images/128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "images/16.png",
      "48": "images/48.png",
      "128": "images/128.png"
    },
    "default_title": "APISpy Enabler"
  },
  "permissions": [
    "http://*/",
    "https://*/",
    "contextMenus"
  ],
  "externally_connectable": {
    "matches": [
      "*://*.apispy.io/*",
      "*://*.apispy.io:*/*"
    ]
  },
  "web_accessible_resources": [
    "images/16.png", "images/24.png","images/32.png","images/48.png","images/128.png"
  ]
}
