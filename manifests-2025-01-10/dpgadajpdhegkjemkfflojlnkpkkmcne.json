{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Mr. Brief",
  "version": "1.1.0",
  "description": "Select and summarize any content using Google Gemini AI",
  "icons": {
  "16": "/images/Icon-16.png",
  "48": "/images/Icon-48.png",
  "128": "/images/Icon-128.png"
 },
  "permissions": [
    "activeTab",
    "scripting",
    "storage",
    "contextMenus"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["content.css"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+S",
        "mac": "Command+Shift+S"
      },
      "description": "Open summarizer"
    },
    "summarize_selection": {
      "suggested_key": {
        "default": "Alt+S",
        "mac": "Alt+S"
      },
      "description": "Summarize selected text"
    }
  },
  "offline_enabled": true,
  "web_accessible_resources": [{
    "resources": ["welcome.html", "welcome.css", "welcome.js", "images/*"],
    "matches": ["<all_urls>"]
  }]
}