{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "CodeBetter",
    "version": "1.1",
    "description": "AI-powered LeetCode assistant.",
    "permissions": [
      "storage"
    ],
    "content_scripts": [
      {
        "matches": ["https://leetcode.com/problems/*"],
        "js": [
          "libs/marked.min.js",
          "libs/dompurify.min.js",
          "libs/highlight.min.js",
          "content.js"
        ],
        "css": [
          "libs/styles/default.min.css"
        ]
      },
      {
        "matches": ["https://extensionpay.com/*"],
        "js": ["libs/ExtPay.js"],
        "run_at": "document_start"
      }
    ],
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
      "48": "icon.png"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "web_accessible_resources": [
      {
        "resources": ["companies.json", "libs/*", "libs/ExtPay.js"],
        "matches": ["https://leetcode.com/*"]
      }
    ],
    "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'self'; connect-src 'self' https://codebetterserver.onrender.com https://extensionpay.com"
    }
  }