{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "LinkStack",
  "version": "1.0.0",
  "description": "Save and sync your links with GitHub. Organize your bookmarks with tags and access them anywhere through your GitHub repository.",
  "author": "hp77",
  "homepage_url": "https://github.com/hp77-creator/linkstack-extension",
  "permissions": [
    "activeTab",
    "storage",
    "identity"
  ],
  "host_permissions": [
    "https://github.com/*",
    "https://api.github.com/*"
  ],
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "background": {
    "service_worker": "background/background.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content/pageInfo.js"]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "web_accessible_resources": [{
    "resources": ["background/*.js"],
    "matches": ["<all_urls>"]
  }],
  "options_ui": {
    "page": "options/settings.html",
    "open_in_tab": true
  }
}
