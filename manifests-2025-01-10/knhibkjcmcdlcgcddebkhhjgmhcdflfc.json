   "matches": ["https://www.desmos.com/calculator/*"],
   "js": ["desmos.js", "chatgpt.js"]
  }
 ],
 "action": {
  "default_popup": "popup.html",
  "default_title": "Auto Desmos"
 },
 "icons": {
  "16": "icon16.jpg",
  "32": "icon32.jpg",
  "128": "icon128.jpg"
 }
