 "description": "An extension which brings ChatGPT's powers into Desmos",
 "permissions": ["scripting", "storage"],
 "host_permissions": ["https://www.desmos.com/calculator/*"],
 "background": {
  "service_worker": "background.js"
 },
 "content_scripts": [
