{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Thynkr",
  "version": "1.0.0",
  "description": "Quickly save content to your Thynkr dashboard",
  "permissions": [
    "activeTab",
    "storage",
    "tabs",
    "contextMenus",
    "scripting",
    "notifications"
  ],
  "host_permissions": [
    "https://thynkr.me/*",
    "https://www.thynkr.me/*",
    "https://www.youtube.com/*",
    "https://twitter.com/*",
    "https://x.com/*",
    "https://podcasts.apple.com/*"
  ],
  "externally_connectable": {
    "matches": [
      "http://localhost:3000/*",
      "https://thynkr.me/*",
      "https://www.thynkr.me/*"
    ]
  },
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["content-scripts/youtube.js"],
      "css": ["content-scripts/youtube.css"]
    },
    {
      "matches": [
        "https://twitter.com/*",
        "https://x.com/*"
      ],
      "js": ["content-scripts/twitter.js"],
      "css": ["content-scripts/twitter.css"]
    },
    {
      "matches": ["https://podcasts.apple.com/*"],
      "js": ["content-scripts/podcast.js"],
      "css": ["content-scripts/podcast.css"]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
} 