{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 2,
    "name": "fit4gt",
    "description": "Fit4gt is a Chrome Extension that automatically inserts and removes line-breaks for Google Translate.",
    "version": "1.1",

    "page_action": {
        "default_icon": {
            "16": "icons/16.png",
            "48": "icons/48.png",
            "128": "icons/128.png"
        },
        "default_title": "gt-ex"
    },

    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },

    "permissions": [
        "tabs",
        "https://translate.google.com/*",
        "https://translate.google.co.jp/"
    ]
}
