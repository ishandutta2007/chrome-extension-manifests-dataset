        "extension_pages": "script-src 'self'; object-src 'self';",
        "content_scripts": "script-src 'self';"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*.thriveglobal.com/*",
                "*://*.stag.thriveglobal.com/*",
                "*://*.admin.thriveglobal.engineering/*",
                "*://*.admin.stag.thriveglobal.engineering/*",
                "*://localhost/*"
            ],
            "run_at": "document_start",
            "js": [ "inject_scripts.js" ]
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [ "install.js" ],
            "matches": [
                "*://*.thriveglobal.com/*",
                "*://*.stag.thriveglobal.com/*",
                "*://*.admin.thriveglobal.engineering/*",
                "*://*.admin.stag.thriveglobal.engineering/*",
                "*://localhost/*"
            ]
        }
    ],
    "icons": {
        "16": "dev-icon.png",
        "48": "dev-icon.png",
