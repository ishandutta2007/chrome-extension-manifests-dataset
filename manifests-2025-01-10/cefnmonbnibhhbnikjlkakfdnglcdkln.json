    "manifest_version": 3,
    "name": "Thrive Tools",
    "description": "A devtools panel for Thrive Tools for MicroFrontends.",
    "version": "1.2",
    "host_permissions": [
        "*://*.thriveglobal.com/*",
        "*://*.stag.thriveglobal.com/*",
        "*://*.admin.thriveglobal.engineering/*",
        "*://*.admin.stag.thriveglobal.engineering/*",
