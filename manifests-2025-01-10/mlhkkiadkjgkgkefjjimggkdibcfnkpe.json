{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Password Peeper",
  "description": "Show your password on the web page filled in by browser. Double click to copy your password to clipboard in case you forgot it.",
  "version": "1.1",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "activeTab", "notifications"],
  "action": {
    "default_popup": "ui/index.html"
  },
  "options_page": "ui/index.html",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],

  "commands": {
    "_execute_action": {
      "suggested_key": {
        "windows": "Ctrl+Shift+L",
        "mac": "Command+Shift+L",
        "linux": "Ctrl+Shift+L",
        "chromeos": "Ctrl+Shift+L"
      }
    },
    "toggle-password": {
      "suggested_key": {
        "default": "Ctrl+Shift+S",
        "mac": "Command+Shift+S",
        "linux": "Ctrl+Shift+S",
        "chromeos": "Ctrl+Shift+S"
      },
      "description": "Toggle Password On Page"
    }
  },
  "icons": { "16": "icons/16.png", "48": "icons/48.png", "128": "icons/128.png" }
}
