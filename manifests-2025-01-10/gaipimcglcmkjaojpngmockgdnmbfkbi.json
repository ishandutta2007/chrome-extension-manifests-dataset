{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Chrome API 百宝箱",
  "version": "1.0.2",
  "description": "Chrome API 测试工具，提供丰富的 Chrome 扩展 API 示例和测试功能",
  "permissions": [
    "tabs",
    "storage",
    "notifications",
    "scripting",
    "activeTab",
    "sidePanel",
    "contextMenus",
    "downloads",
    "downloads.open",
    "alarms",
    "cookies",
    "downloads.ui"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "src/pages/popup/index.html",
    "default_icon": {
      "16": "assets/icons/icon16.png",
      "48": "assets/icons/icon48.png",
      "128": "assets/icons/icon128.png"
    }
  },
  "background": {
    "service_worker": "src/background/service-worker.js",
    "type": "module"
  },
  "icons": {
    "16": "assets/icons/icon16.png",
    "48": "assets/icons/icon48.png",
    "128": "assets/icons/icon128.png"
  },
  "side_panel": {
    "default_path": "src/pages/sidepanel/index.html"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "Command+Shift+F"
      },
      "description": "打开 ChromeAPIBox"
    },
    "toggle_side_panel": {
      "suggested_key": {
        "default": "Ctrl+Shift+S",
        "mac": "Command+Shift+S"
      },
      "description": "切换侧边栏"
    }
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "minimum_chrome_version": "114",
  "web_accessible_resources": [
    {
      "resources": [
        "src/pages/api-list/*",
        "src/pages/custom-api/*",
        "src/pages/about/*",
        "assets/icons/*"
      ],
      "matches": ["<all_urls>"]
    }
  ]
} 