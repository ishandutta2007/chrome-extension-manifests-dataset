{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "SFDX Metadata Backup",
    "version": "1.2",
    "description": "Download Salesforce metadata, components, and configurations directly to your local machine with one click.",
    "permissions": [
      "activeTab",
      "storage",
      "cookies"
    ],
    "host_permissions": [
      "https://*.salesforce.com/*",
      "https://*.force.com/*",
      "https://*.lightning.force.com/*"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "content_scripts": [
      {
        "matches": [
          "https://*.salesforce.com/*",
          "https://*.force.com/*",
          "https://*.lightning.force.com/*"
        ],
        "js": ["content.js"],
        "run_at": "document_end"
      }
    ],
    "background": {
      "service_worker": "background.js"
    },
    "web_accessible_resources": [{
      "resources": ["home.html"],
      "matches": [
        "https://*.salesforce.com/*",
        "https://*.force.com/*",
        "https://*.lightning.force.com/*"
      ]
    }],
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
}
  