      "run_at": "document_start"
    }
  ],

  "permissions": [
      "storage",
      "webNavigation",
      "webRequest",
      "webRequestBlocking",
      "<all_urls>"
  ],

  "applications": {
    "gecko": {
      "strict_min_version": "57.0a1"
