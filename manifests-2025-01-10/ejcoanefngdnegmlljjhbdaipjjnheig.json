{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "Mali Order Extension",
    "description": "Extension đặt hàng Taobao cho Mali Order",
    "version": "1.0.2",
    "permissions": [
        "storage"
    ],
    "icons": {
        "16": "logo.png",
        "48": "logo.png",
        "128": "logo.png"
    },
    "action": {
        "default_popup": "popup.html",
        "default_title": "Đặt hàng",
        "default_icon": "logo.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "host_permissions": [
        "https://*.maliorder.com/*"
    ],
    "content_scripts": [
        {
            "matches": [
                "http://*.taobao.com/*",
                "https://*.taobao.com/*",
                "http://*.tmall.com/*",
                "https://*.tmall.com/*",
                "http://*.tmall.hk/*",
                "https://*.tmall.hk/*",
                "http://*.1688.com/*",
                "https://*.1688.com/*"
            ],
            "js": [
                "contentScript.js"
            ],
            "run_at": "document_end"
        }
    ]
}