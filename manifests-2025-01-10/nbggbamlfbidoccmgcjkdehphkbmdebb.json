{
"update_url": "https://clients2.google.com/service/update2/crx",

  "version": "1.0.13",
  "manifest_version": 3,
  "name": "SEQUESTO",
  "description": "A SEQUESTO Chrome extension for accessing your company's library",
  "permissions": ["storage", "identity", "contextMenus", "activeTab"],
  "background": {
    "service_worker": "js/background.js"
  },
  "action": {
    "default_popup": "js/index.html",
    "default_title": "SEQUESTO"
  },
  "icons": {
    "16": "logo_16.png",
    "32": "logo_32.png",
    "48": "logo_48.png",
    "128": "logo_128.png"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self';"
  },
  "web_accessible_resources": [
    {
      "resources": ["static/media/*.woff2", "static/media/*.woff", "static/media/*.ttf"],
      "matches": ["<all_urls>"]
    }
  ]
}
