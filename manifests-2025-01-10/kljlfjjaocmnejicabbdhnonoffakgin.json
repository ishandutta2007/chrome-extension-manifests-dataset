    "default_popup": "html/popup.html"
  },
  "background": {
    "service_worker": "js/background.min.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.etsy.com/search*",
        "https://*.etsy.com/shop/*",
        "https://*.etsy.com/c/*",
        "https://*.etsy.com/*/search*",
        "https://*.etsy.com/*/shop/*",
        "https://*.etsy.com/*/c/*"
      ],
      "js": [
        "js/jquery.min.js",
        "js/message.min.js",
        "js/category.min.js"
      ],
      "css": [
        "css/icons.css",
        "css/message.css",
        "css/category.css"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "https://*.etsy.com/listing/*",
        "https://*.etsy.com/*/listing/*"
      ],
      "js": [
        "js/jquery.min.js",
        "js/message.min.js",
        "js/product.min.js"
      ],
      "css": [
        "css/icons.css",
        "css/message.css",
        "css/product.css"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "https://*.etsy.com/*"
      ],
      "js": [
        "js/jquery.min.js",
        "js/refresh.min.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "https://*/wp-admin/?handmade_setup_wizard=1&*",
        "https://*/*/wp-admin/?handmade_setup_wizard=1&*",
        "https://*/*/*/wp-admin/?handmade_setup_wizard=1&*",
        "https://*/wp-admin/admin.php?page=handmade*",
        "https://*/*/wp-admin/admin.php?page=handmade*",
        "https://*/*/*/wp-admin/admin.php?page=handmade*"
      ],
      "js": [
        "js/jquery.min.js",
        "js/message.min.js",
        "js/wordpress.min.js"
      ],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "js/*",
        "css/*",
        "fonts/*",
