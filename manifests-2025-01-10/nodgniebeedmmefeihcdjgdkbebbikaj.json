{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "SHIP Extension",
  "version": "1.1",
  "icons": {
    "16": "icons/16.png",
    "24": "icons/24.png",
    "32": "icons/32.png",    
    "48": "icons/48.png",
    "64": "icons/64.png",
    "128": "icons/128.png"
  }, 
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icons/16.png",
      "24": "icons/24.png",
      "32": "icons/32.png",
      "64": "icons/64.png",
      "128": "icons/128.png"
    }
  },
  "content_security_policy": {
    "extension_pages": "frame-ancestors 'self' https://login.devship.scea.com; default-src 'self'"
  },
  "permissions": [
    "declarativeNetRequest",
    "declarativeNetRequestFeedback",
    "storage"
  ],  
  "host_permissions": [
    "https://*.devship.scea.com/*",
    "https://*.ppship.scea.com/*",
    "https://*.ship.scea.com/*",
    "https://sie.okta.com/*",
    "https://coda.io/*",
    "https://miro.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.devship.scea.com/*",
        "https://*.ppship.scea.com/*",
        "https://*.ship.scea.com/*",
        "https://sie.okta.com/*",
        "https://miro.com/*",
        "https://coda.io/*"
      ],
      "js": ["src/js/ship.js"]
    }    
  ],
  "declarative_net_request": {
    "rule_resources": [{
      "id": "ruleset_1",
      "enabled": true,
      "path": "rules.json"
    }]
  } 
}
