{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Full Text Tabs Forever",
  "description": "Search your full browsing history",
  "version": "1.2.2",
  "manifest_version": 3,
  "permissions": [
    "unlimitedStorage",
    "downloads",
    "tabs",
    "activeTab"
  ],
  "icons": {
    "16": "assets/icon_16.png",
