{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Genius.AI",
  "description": "The Genius.AI extension allows you to bring the power of the AI in the DMs of your favorite social media platform.",
  "version": "1.10",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "activeTab",
    "storage",
    "tabs"
  ],
  "host_permissions": [
    "https://www.facebook.com/*",
    "https://www.messenger.com/*",
    "https://promptmaker.genius.ai/*",
    "https://app.genius.ai/*",
    "https://login.genius.ai/*",
    "https://web.whatsapp.com/*",
    "https://www.linkedin.com/*",
    "https://www.google.com/*",
    "https://www.instagram.com/*"
    
  ],
  "web_accessible_resources": [
    {
      "resources": ["genius-ai-logo.png"],
      "matches": ["https://*.facebook.com/*", "https://*.messenger.com/*","https://web.whatsapp.com/*","https://www.linkedin.com/*","https://www.instagram.com/*","https://*.google.com/*"]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/get_started16.png",
      "32": "/images/get_started32.png",
      "48": "/images/get_started48.png",
      "128": "/images/get_started128.png"
    }
  },
  "icons": {
    "16": "/images/get_started16.png",
    "32": "/images/get_started32.png",
    "48": "/images/get_started48.png",
    "128": "/images/get_started128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.facebook.com/*"],
      "js": ["content.js"],
      "css": ["styles.css"]
    },
    {
      "matches": ["*://www.instagram.com/*"],
      "js": ["InstagramContent.js"],
      "css": ["InstagramStyles.css"]
    },
    {
      "matches": ["https://www.linkedin.com/*","https://linkedin.com/*"],
      "exclude_matches": ["https://www.linkedin.com/messaging/*","https://linkedin.com/messaging/*"],

      "js": ["LinkedInContent.js"],
      "css": ["LinkedinStyles.css"]
    },
    {
      "matches": ["https://www.linkedin.com/messaging/*","https://linkedin.com/messaging/*"],
      "js": ["LinkedInMessaging.js"],
      "css": ["LinkedinStyles.css"]
    },
    {
      "matches": ["https://app.genius.ai/*"],
      "js": ["grabLogin.js"],
      "run_at": "document_idle"
    },
    {
      "matches": ["https://web.whatsapp.com/*"],
      "js": ["whatsAppContent.js"],
      "css": ["whatsAppStyles.css"]
    }
  ],
  "options_page": "options.html"
}

