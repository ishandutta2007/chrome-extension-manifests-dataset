{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "OCU AI",
  "version": "1.8.0",
  "description": "Transform daily content consumption into tangible skills",
  "permissions": [
    "sidePanel",
    "scripting",
    "storage",
    "tabs",
    "webNavigation"
  ],
  "host_permissions": [
    "https://app.diplomade.com/*",
    "https://app-preview.diplomade.com/*"
  ],
  "action": {
    "default_title": "Ocu",
    "default_popup": "index.html"
  },
  "side_panel": {
    "default_path": "index.html"
  },
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "js": [
        "assets/medium-main.tsx-loader.js"
      ],
      "matches": [
        "https://medium.com/*"
      ],
      "run_at": "document_end"
    },
    {
      "js": [
        "assets/youtube-main.tsx-loader.js"
      ],
      "matches": [
        "https://www.youtube.com/*"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "/src/assets/images/*",
        "/src/assets/*",
        "/src/*",
        "src/assets/*",
        "src/assets/images/*",
        "src/assets/videos/*"
      ],
      "use_dynamic_url": false
    },
    {
      "matches": [
        "https://medium.com/*"
      ],
      "resources": [
        "client.js",
        "use-extension-state.js",
        "extension-state.js",
        "medium-main.js"
      ],
      "use_dynamic_url": false
    },
    {
      "matches": [
        "https://www.youtube.com/*"
      ],
      "resources": [
        "client.js",
        "index.js",
        "api.js",
        "youtube.js",
        "use-extension-state.js",
        "extension-state.js",
        "youtube-main.js"
      ],
      "use_dynamic_url": false
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
