{
"update_url": "https://clients2.google.com/service/update2/crx",

  "description": "Easily control web browsing within authenticated sites using centralized certificates managed in Redtrust.",
  "manifest_version": 3,
  "name": "Redtrust authentication controller BETA",
  "version": "100.2.8.0",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "nativeMessaging",
    "tabs",
    "activeTab",
    "webNavigation"
  ],
  "options_ui": {
    "page": "./options.html",
    "open_in_tab": true
  },

  "host_permissions": [
    "*://*/*"
  ],

  "background": {
    "service_worker": "RTCheck.js",
    "type": "module",
    "js":[
      "regex.js",
      "saml.js", 
      "functions.js", 
      "console.js", 
      "handlinghosts.js", 
      "openId.js",
      "constants.js"
    ]
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "64": "icon64.png",
    "128": "icon128.png",
    "256": "icon256.png",
    "512": "icon512.png"
  },
  "web_accessible_resources": [
    {
      "resources": [ "./redirect.html" ],
      "matches": [ "<all_urls>" ]
    }
  ]
}
