{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "DocMate AI Extension",
    "version": "0.0.13",
    "description": "Our Google Chrome extension for DocMate AI. Helps you easily Contribute and chat with your MedMate",
    "commands": {
        "paste_text": {
            "suggested_key": {
                "default": "Alt+Z",
                "mac": "Alt+Z"
            },
            "description": "Paste predefined text"
        }
    },
    "icons": {
        "48": "images/logo.png"
    },
    "permissions": [
        "activeTab",
        "sidePanel",
        "storage",
        "tabs",
        "scripting"
    ],
    "background": {
        "service_worker": "scripts/background.js"
    },
    "action": {
        "default_popup": "pages/index.html#/popup",
        "default_icon": {
            "48": "images/logo.png"
        }
    },
    "side_panel": {
        "default_path": "pages/index.html"
    },
    "web_accessible_resources": [
        {
            "resources": [
                "dist/*"
            ],
            "matches": [
                "*://*/*"
            ]
        },
        {
            "resources": [
                "images/*"
            ],
            "matches": [
                "*://*/*"
            ]
        },
        {
            "resources": [
                "pages/*"
            ],
            "matches": [
                "*://*/*"
            ]
        }
    ],
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "exclude_matches": [
                "*://localhost/*",
                "https://docmate.ai/*",
                "https://*.docmate.dev/*",
                "https://staging.docmate.ai/*"
            ],
            "js": [
                "dist/extension-bot.js"
            ]
        }
    ],
    "host_permissions": [
        "*://*/*"
    ]
}
