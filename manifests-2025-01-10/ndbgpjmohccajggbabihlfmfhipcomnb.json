  "icons": {
    "16": "icons/on/icon16.png",
    "32": "icons/on/icon32.png",
    "48": "icons/on/icon48.png",
    "128": "icons/on/icon128.png"
  },
  "host_permissions": [
    "*://www.youtube.com/*",
    "*://www.netflix.com/*",
    "*://www.instagram.com/*",
    "*://vimeo.com/*",
    "*://www.facebook.com/*",
    "*://*.bettersleep.com/*"
  ],
  "permissions": ["tabCapture", "storage"],
  "content_scripts": [
    {
      "all_frames": true,
      "matches": [
        "*://www.youtube.com/*",
        "*://www.netflix.com/*",
