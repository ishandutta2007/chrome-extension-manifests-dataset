{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "FTA Buddy",
    "description": "Enable the field monitor and notes sync for FTA Buddy app",
    "version": "1.11",
    "permissions": [
        "storage",
        "scripting"
    ],
    "host_permissions": [
        "http://10.0.100.5/*"
    ],
    "content_scripts": [
        {
            "run_at": "document_end",
            "matches": [
                "http://10.0.100.5/FieldMonitor",
                "http://10.0.100.5/FieldMonitor/"
            ],
            "js": [
                "injector.js"
            ]
        },
        {
            "run_at": "document_end",
            "matches": [
                "https://ftabuddy.com/*",
                "http://localhost:5173/*"
            ],
            "js": [
                "app.js"
            ]
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "injected.js"
            ],
            "matches": [
                "http://10.0.100.5/*",
                "https://ftabuddy.com/*"
            ]
        }
    ],
    "icons": {
        "48": "img/logo48.png",
        "128": "img/logo128.png"
    },
    "action": {
        "default_popup": "menu.html",
        "default_icon": "img/logo.png"
    },
    "background": {
        "service_worker": "background.js"
    }
