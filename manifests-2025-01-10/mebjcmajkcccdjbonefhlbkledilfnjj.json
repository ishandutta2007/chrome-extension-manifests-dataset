  "icons": {
    "16": "/images/icon16.png",
    "32": "/images/icon32.png",
    "48": "/images/icon48.png",
    "128": "/images/icon128.png"
  },
  "options_page": "options.html",
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "windows": "Ctrl+J",
        "mac": "Command+J",
        "chromeos": "Ctrl+J",
        "linux": "Ctrl+J"
      },
      "description": "Snooze current tab"
