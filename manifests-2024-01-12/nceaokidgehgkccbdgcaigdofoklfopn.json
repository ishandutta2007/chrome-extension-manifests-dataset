    },
    "content_scripts": [
        {
          "matches": [
            "<all_urls>"
          ],
          "js": ["jquery-3.0.0.min.js", "options.js"]
        }
     ],
    
    "permissions": [
    "activeTab",
    "tabs",
    "storage"
