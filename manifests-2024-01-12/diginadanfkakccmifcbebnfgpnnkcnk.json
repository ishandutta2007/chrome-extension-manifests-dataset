  "name": "Testernotes",
  "description": "Record sessions to generate end-to-end tests",
  "version": "1.6",
  "manifest_version": 3,
  "permissions": [
    "storage",
    "activeTab",
    "tabs",
    "cookies",
    "webRequest"
  ],
