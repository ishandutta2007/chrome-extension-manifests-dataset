    "default_popup": "popup.html",
    "default_title": "Synchrolog",
    "default_icon": {
      "16": "/images/icon16.png",
      "48": "/images/icon48.png",
      "128": "/images/icon128.png"
    }
  },
  "icons": {
    "16": "/images/icon16.png",
    "48": "/images/icon48.png",
    "128": "/images/icon128.png"
  },
  "host_permissions": [
    "<all_urls>"
  ],
  "web_accessible_resources": [
    {
      "resources": [ "messenger.html", "server_logs.html", "dev_browser_player.html" ],
      "matches": ["<all_urls>"]
    }
  ]
