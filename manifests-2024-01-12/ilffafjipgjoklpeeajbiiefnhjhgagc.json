{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Synchrolog - Debugging Screen Recorder",
  "version": "0.1",
  "description": "Record clips, add comments and annotations",
  "background": {
    "service_worker": "./background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["inject.js"],
      "run_at": "document_start"
    }
