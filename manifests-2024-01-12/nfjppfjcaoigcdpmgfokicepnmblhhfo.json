{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,

  "name": "Noted",
  "description": "This extension saves notes to a database for any url",
  "version": "0.0.1",

  "content_scripts": [{
    "matches": [
      "<all_urls>"
    ],
    "js": ["annotations.js"]
  }],
  "browser_action": {
    "icons": ["yummypanguin.png"],
    "default_icon": "yummypanguin.png",
    "default_popup": "/src/browser_action/browser_action.html"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "./jquery-3.2.1.min.js",
      "./env.js",
      "node_modules/auth0-chrome/dist/auth0chrome.min.js",
      "src/main.js",
      "src/browser_action/browser_action.js"
    ]
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "<all_urls>",
    "identity",
    "notifications",
    "background",
    "contextMenus"
  ],
  "icons": {
    "16": "yummypanguin.png"
  },
  "web_accessible_resources": [
    "src/*",
    "*"
  ]
}
