{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Ybe Browser Extension",
  "description": "Get a vibe check based on your real interests with the Ybe Browser Extension",
  "version": "1.3.1",
  "permissions": [
    "activeTab",
    "storage",
    "alarms",
    "tabs",
    "history",
    "cookies",
    "http://*/*",
    "https://*/*"
  ],
  "host_permissions": [
    "*://*.ybe.life/*",
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Ybe Browser Extension",
    "default_popup": "",
    "default_icon": {
      "16": "icons/icon16.png",
      "64": "icons/icon64.png",
      "128": "icons/icon128.png"
    }
  },
  "externally_connectable": {
    "matches": [
      "https://app.ybe.life/*"
    ]
  },
  "web_accessible_resources": [
    {
      "resources": [
        "icons/*.png"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "content_scripts": [
    {
      "run_at": "document_end",
      "js": [
        "content.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "styles/vibeButton.css",
        "styles/notifButton.css"
      ]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "64": "icons/icon64.png",
    "128": "icons/icon128.png"
  }
}