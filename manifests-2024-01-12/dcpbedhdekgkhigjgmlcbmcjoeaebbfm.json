  "commands": {
    "start": {
      "suggested_key": {
        "default": "Ctrl+Shift+Y",
        "mac": "Command+Shift+Y"
      },
      "description": "Start/Stop the timer."
    },
    "pause": {
      "suggested_key": {
        "default": "Ctrl+Shift+U",
        "mac": "Command+Shift+U"
      },
      "description": "Pause/Resume the timer."
    }
  },
  "icons": {
    "16": "img/icon.png",
    "32": "img/icon.png",
    "48": "img/icon.png",
    "128": "img/icon.png"
  },
  "options_page": "options.html",
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "background": {
    "page": "bg.html"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["scripts/checker.js"],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    "img/*",
    "scripts/*",
    "styles/*",
    "audio/*"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "background",
    "tabs",
    "storage",
    "notifications",
    "unlimitedStorage"
  ]
