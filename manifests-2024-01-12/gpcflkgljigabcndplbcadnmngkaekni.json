{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Sidebar Remover",
  "version": "0.1.0",
  "manifest_version": 2,
  "description": "Remove sidebars. ",
  "content_scripts": [{
    "matches": ["http://*/*", "https://*/*"],
    "css": [
        "main.css"
    ],
    "js": [
      "main.js"
    ]
  }],
  "permissions": [
      "storage"
  ], 
  "browser_action": {
    "default_icon": {
      "38": "assets/disabled.png"
    }
  },
  "background": {
    "scripts": [
      "background.js"
    ], 
    "persistent": false
  },
  "icons": {
    "38": "assets/disabled.png",
    "128": "assets/enabled_128.png"
  }
}
