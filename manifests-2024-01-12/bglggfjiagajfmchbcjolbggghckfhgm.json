        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png"
    },
    "manifest_version": 3,
    "name": "__MSG_appName__",
    "permissions": [
        "tabs",
        "webRequest",
        "downloads",
        "webNavigation",
        "storage",
        "cookies",
        "unlimitedStorage",
        "scripting",
        "declarativeNetRequestWithHostAccess"
    ],
    "sandbox": {
        "pages": [
            "js/main-index.html"
        ]
    },
    "version": "1.0.2",
    "web_accessible_resources": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
