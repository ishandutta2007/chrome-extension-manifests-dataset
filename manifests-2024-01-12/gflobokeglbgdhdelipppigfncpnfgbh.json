            "<all_urls>"
        ]
    },
    "background":{
        "service_worker": "scripts/background.js"
    },
    "action":{
        "default_popup": "popup.html",
        "default_icon" : "img/dbicon16.png"
    },
    "icons":{
        "16": "img/dbicon16.png",
