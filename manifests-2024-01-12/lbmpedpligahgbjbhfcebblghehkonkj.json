  "content_scripts": [
    {
      "matches": ["https://meet.google.com/*"],
      "js": ["js/content.js", "js/controller.js"],
      "run_at": "document_idle"
    }
  ],
  "web_accessible_resources": ["js/content.js", "js/controller.js"]
