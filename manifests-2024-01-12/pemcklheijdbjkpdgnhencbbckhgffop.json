    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/app_16.png",
    "32": "icons/app_32.png",
    "48": "icons/app_48.png",
    "128": "icons/app_128.png"
  },
  "web_accessible_resources": [{
    "resources": ["icons/play_24.png", "icons/pause_24.png", "icons/replay_24.png", "icons/collapse_24.png", "icons/expand_24.png"],
    "matches": ["<all_urls>"]
