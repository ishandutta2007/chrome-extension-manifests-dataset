      "matches": ["https://store.gaijin.net/*"],
      "js": ["creators.js", "content_script.js"],
      "css": ["style.css"]
    }
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'; img-src 'self' https://i.imgur.com;"
  }
