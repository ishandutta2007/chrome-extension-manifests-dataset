          "css": ["template.css"],
          "run_at": "document_end"
        }
    ],
    "permissions": [
        "tabs", "storage"
    ],
    "icons": {
        "16": "images/meta_16.png",
        "48": "images/meta_48.png",
        "128": "images/meta_128.png"
    }
