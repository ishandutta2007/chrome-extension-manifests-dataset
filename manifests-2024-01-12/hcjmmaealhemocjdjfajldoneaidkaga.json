{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "YouTube Mix Blocker",
  "version": "1.2",
    "permissions": [
    "declarativeContent"
  ],
  "host_permissions": [
    "https://www.youtube.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/*"],
      "js": ["content.js"]
    }
  ],
  "description": "Blocks youtube mixes from \"youtube.com/results\" page.",
  "action": {
    "default_popup": "",
    "default_icon": {
      "16": "icon-16.png",
      "64": "icon-64.png",
      "128": "icon-128.png"
    }
  },
  "icons": {
    "16": "icon-16.png",
    "48": "icon-64.png",
    "128": "icon-128.png"
  }
}
