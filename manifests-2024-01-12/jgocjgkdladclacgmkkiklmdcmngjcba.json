"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "GPT Workspace",
  "description": "Run GPT in Google Workspace",
  "version": "1.9",
  "permissions": ["activeTab", "sidePanel", "scripting", "storage"],
  "host_permissions": ["<all_urls>"],
  "content_scripts": [
