    "manifest_version" : 2,
    "name" : "ui-events-recorder",
    "version" : "1.0.0",
    "permissions": [
        "tabs",
        "activeTab",
        "webRequest",
        "contextMenus",
        "downloads",
        "webNavigation",
        "notifications",
        "storage",
        "<all_urls>"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
