  "manifest_version": 3,
  "name": "Calofree",
  "version": "1.0.2",

  "description": "Hides calorie information on websites",

  "permissions": [
      "storage"
  ],

  "host_permissions": [
      "<all_urls>"
  ],

  "icons": {
