{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Leethint",
  "description": "Leethint | Get hints to help you solve leetcode.com coding problems.",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "permissions": [
    "storage"
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "icon.png",
        "temps/main.html",
        "temps/main.css",
        "temps/main-closed.html",
        "temps/main-closed.css",
        "temps/main-unrecognized.html",
        "temps/typing-indicator.html",
        "temps/typing-indicator.css",
        "temps/hint.html",
        "img/emoj/*"
      ],
      "matches": ["<all_urls>"],
      "exclude_matches": ["*://*.leetcode.com/problems/all/"]
    }
  ],
  "content_scripts": [
    {
      "matches": ["*://*.leetcode.com/problems/*/"],
      "exclude_matches": ["*://*.leetcode.com/problems/all/"],
      "js": ["content/main.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  }
}
