        "js/constants/index.js",
        "js/listeners/index.js",
        "js/helpers/helper.js",
        "js/domHandlers/index.js"
      ],
      "css": ["css/index.css"]
    },
    {
      "matches": ["https://calendar.google.com/calendar/u/0/r/eventedit*"],
      "js": [
        "js/constants/index.js",
        "js/listeners/eventEdit.js",
        "js/helpers/helper.js",
        "js/domHandlers/index.js"
      ],
      "css": ["css/index.css"]
    }
  ]
