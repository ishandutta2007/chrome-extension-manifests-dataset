        "js": ["main.js"]
      },
      {
        "matches" : ["https://www.amazon.com/gp/your-account/order-history?ref_=ppx_yo2ov_dt_b_tb_cancel&orderFilter=cancelled"],
        "js": ["canceled.js"]
      }
  ],
  "action": {
    "default_popup": "popup.html"
