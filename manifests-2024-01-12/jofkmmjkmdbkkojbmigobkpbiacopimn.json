{
"update_url": "https://clients2.google.com/service/update2/crx",

 "name": "tnb+",
 "version": "1.4",
 "manifest_version": 2,
 "description": "This extension adds basic enhancements to thenewboston.com.",

 "permissions": [
  "https://www.thenewboston.com/*",
  "storage",
  "tabs"
 ],

 "background": {
  "scripts": ["Scripts/background.js"],
  "persistent" : true
 },

 "browser_action": {
  "default_icon": "icon.png",
  "default_popup": "popup.html"
 },

 "content_scripts": [
  {
   "run_at": "document_start",
   "matches": ["*://*.thenewboston.com/*"],
   "js": ["Scripts/lib/jquery.min.js", "Lightbox/lightbox.min.js", "Scripts/image_viewer.js"],
   "css": ["Lightbox/lightbox.css", "Content/Core.css"]
  }
 ],

 "web_accessible_resources": [
  "Lightbox/screen.css",
  "Lightbox/lightbox.css",
  "Lightbox/lightbox.min.js",
  "Lightbox/lightbox.min.map",
  "Content/Themes/Basic.css",
  "Content/Images/dark_hardwood.jpg",
  "Content/Core.css",
  "Content/ProfilePage.css",
  "Content/Material/Dark-Navagation.css",
  "Content/Material/Dark-Stream.css",
  "Content/Material/Light/BaseStream.css"
 ]

}