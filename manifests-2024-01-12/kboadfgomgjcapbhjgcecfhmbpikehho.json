      "16": "/images/logo_off16.png",
      "32": "/images/logo_off32.png",
      "48": "/images/logo_off48.png",
      "128": "/images/logo_off128.png"
    }
  },
  "icons": {
    "16": "/images/logo16.png",
    "32": "/images/logo32.png",
    "48": "/images/logo48.png",
    "128": "/images/logo128.png"
