{
"update_url": "https://clients2.google.com/service/update2/crx",

    "name": "Yet Another Hints Extension",
    "short_name": "YAHE",
    "manifest_version": 2,
    "description": "A simple hit-a-hint extension",
    "options_ui": {
        "page": "options/index.html",
        "chrome_style": true
    },
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "permissions": [
        "storage",
        "http://*/*",
        "https://*/*"
    ],
    "content_scripts": [
        {
            "match_about_blank": true,
            "matches": [
                "http://*/*",
                "https://*/*",
                "file://*/*"
            ],
            "css": [
                "yahe.css"
            ],
            "js": [
                "yahe.js"
            ],
            "run_at": "document_end",
            "all_frames": true
        }
    ],
    "background": {
        "scripts": [
            "yahe-bg.js"
        ]
    },
    "version": "1.4.0"
