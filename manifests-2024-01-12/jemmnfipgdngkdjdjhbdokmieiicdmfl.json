{
   "name": "Steam Link Filter Blocker",
   "version": "1.0.0",
   "description": "You are leaving steam <- Stop that annoying warning, we're grown ups.",
   "manifest_version": 2,
   "permissions": [
      "tabs",
      "activeTab",
      "<all_urls>",
      "http://*/*",
      "https://*/*"
   ],
   "icons": {
      "16": "icon16.png",
      "19": "icon19.png",
      "48": "icon48.png",
     "128": "icon128.png"
   },
   "browser_action": {
      "name": "Manipulate DOM",
      "icons": {
          "16": "icon16.png",
          "19": "icon19.png",
          "48": "icon48.png",
         "128": "icon128.png"
      },
      "default_icon": {
         "16": "icon16.png",
         "19": "icon19.png",
         "48": "icon48.png",
        "128": "icon128.png"
      }
   },
   "content_scripts": [
      {
        "matches": ["https://steamcommunity.com/linkfilter/?url=*"],
        "css":     ["style.css"]
      }
   ],
   "background": {
      "persistent": true,
      "scripts": [
         "background.js"
      ]
   },
   "update_url": "https://clients2.google.com/service/update2/crx"
}
