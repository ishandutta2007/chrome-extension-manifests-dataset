  "homepage_url": "https://glocal.baseed.net",

  "permissions": ["contextMenus", "storage", "<all_urls>"],
  "options_page": "options.html",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/on16.png",
      "32": "images/on32.png",
      "48": "images/on48.png",
      "128": "images/on128.png"
    },
    "default_title": "Glocal"
  },
  "icons": {
    "16": "images/on16.png",
    "32": "images/on32.png",
    "48": "images/on48.png",
    "128": "images/on128.png"
  },
  "web_accessible_resources": [
    "a/l/*.json",
    "a/p/*.json",
    "a/d/*.json",
    "js/*.js",
    "images/*.png",
    "css/*.css"
  ],
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "include_globs": ["https://www.google.*/*"],
      "exclude_globs": ["*chromewebdata*"],
      "match_about_blank": true,
      "all_frames": false,
      "run_at": "document_idle",
      "js": ["js/icon.js"],
     "css": ["css/icon.css"]
    }
  ],
  "omnibox": { "keyword" : "wq" },
  "externally_connectable": {
    "matches": ["https://www.google.com/*"]
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true,
    "open_in_tab": true
  },
  "incognito": "spanning",
  "manifest_version": 2
