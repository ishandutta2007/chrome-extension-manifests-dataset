{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 2,

    "name": "Hodorify",
    "description": "This extension will let you replace all text in a website with Hodor. Why? Why not! Created by Shai Lachmanovich.",
    "version": "1.0",
    "permissions": [
        "activeTab"
    ],
    "background": {
        "scripts": [
            "hodor.js"
        ],
        "persistent": false
    },
    "browser_action": {
        "default_icon": "icon16.png",
        "default_title": "Hodorify your page!"
    },
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    }
