{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Wallabagger",
  "manifest_version": 2,
  "default_locale": "en",
  "version": "1.16.0",
  "description": "__MSG_Extension_description__",
  "icons": {
    "48": "img/wallabag-icon-48.png",
    "128": "img/wallabag-icon-128.png"
  },
  "browser_action": {
    "default_title": "Wallabagger",
    "default_icon": "img/wallabag-icon-48.png",
    "theme_icons": [
      {
        "dark": "img/wallabagger.svg",
        "light": "img/wallabagger-light.svg",
        "size": 32
      }
    ],
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "js/common.js",
      "js/fetch-api.js",
      "js/wallabag-api.js",
      "js/background.js"
    ]
  },
  "permissions": [
    "tabs",
    "storage",
    "contextMenus",
    "activeTab"
  ],
  "optional_permissions": [
    "*://*/api/*"
  ],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Alt+W",
        "windows": "Alt+W",
        "mac": "Alt+W"
      }
    },
    "wallabag-it": {
      "suggested_key": {
        "default": "Alt+Shift+W",
        "windows": "Alt+Shift+W",
        "mac": "Alt+Shift+W"
      },
      "description": "__MSG_Wallabag_it_description__"
    }
  }
}
