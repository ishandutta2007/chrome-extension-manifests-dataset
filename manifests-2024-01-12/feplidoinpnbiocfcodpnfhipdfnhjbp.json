  "manifest_version": 3,
  "name": "read-this.ai",
  "version": "1.0.8",
  "description": "Transform any articles into podcast-quality audio with just a click.",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
