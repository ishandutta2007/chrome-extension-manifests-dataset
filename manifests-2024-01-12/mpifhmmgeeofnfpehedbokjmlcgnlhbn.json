        {
            "resources": [
                "*"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ],
    "icons": {
        "48": "public/logo/eye-able-logo_rounded.png",
        "128": "public/logo/eye-able-logo.png"
    },
    "action": {
        "default_title": "Eye Able Plugin Einstellungen",
        "default_popup": "popup.html"
    },
    "options_ui": {
        "page": "options.html",
        "open_in_tab": false
    },
    "externally_connectable": {
        "matches": [
            "*://*.eye-able.com/*",
            "http://localhost/*"
        ]
    }
