    },
    "manifest_version": 3,
    "name": "__MSG_appName__",
    "permissions": [
        "storage",
        "cookies",
        "unlimitedStorage",
        "tabs",
        "scripting",
        "webRequest",
        "declarativeNetRequestWithHostAccess"
