  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "http://localhost:3000/*",
        "https://github.com/*"
      ],
      "js": [
        "content.js"
      ],
      "media": []
    }
  ]
