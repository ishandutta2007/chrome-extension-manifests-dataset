"update_url": "https://clients2.google.com/service/update2/crx",

    "name": "PostsDash",
    "version": "1.0.9",
    "description": "Show tweets in a tiled layout and watch them automatically update!",
    "manifest_version": 3,
    "incognito": "split",
    "icons": {
        "128": "images/logo.png"
    },
    "content_scripts": [
        {
            "matches": [
                "http://*.twitter.com/*",
                "https://*.twitter.com/*"
            ],
            "css": ["styles/styles.css"],
            "js": [
                "all.min.js"
            ]
        }
    ],
    "permissions": [
        "storage"
    ],
    "host_permissions": [
        "http://*.twitter.com/*",
        "https://*.twitter.com/*"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "web_accessible_resources": [
        {
            "matches": [
                "<all_urls>"
            ],
            "resources": [
                "images/x.svg",
                "styles/style.css",
                "images/repeat.svg",
                "images/logo.svg",
                "images/arrow-left.svg",
                "images/arrow-right.svg",
                "images/x-logo.svg",
                "images/copy.svg",
                "images/check.svg",
                "images/arrow-up.svg",
                "images/settings.svg",
                "images/external-link.svg",
                "images/logo.ico",
                "fonts/logofont.ttf"
            ]
        }
    ]
