{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Under Cloud for Google Chrome",
  "description": "Save web pages and updates on social media to the Under Cloud.",
  "version": "1.3.2",
  "manifest_version": 2,
  "web_accessible_resources": [
    "/icons/logo.png",
    "/icons/icon_250.png",
    "/icons/icon_128.png",
    "/icons/icon_64.png"
  ],
  "icons": {
    "16": "/icons/icon_16.png",
    "48": "/icons/icon_48.png",
    "128": "/icons/icon_128.png"
  },
  "browser_action": {
    "default_title": "Extension for the Under Cloud",
    "default_popup": "popup/popup.html",
    "default_icon": "/icons/icon_250.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "exclude_matches": [
        "http://udrcld.com/*",
        "https://udrcld.com/*",
        "http://staging.theundercloud.com/*",
        "https://staging.theundercloud.com/*",
        "http://www.theundercloud.com/*",
        "https://www.theundercloud.com/*"
      ],
      "js": [
        "content_script.js"
      ],
      "all_frames": false
    }
  ],
  "options_ui": {
    "page": "options/options.html",
    "chrome_style": true
  },
  "permissions": [
    "<all_urls>",
    "activeTab",
    "tabs",
    "storage",
    "clipboardRead",
    "http://udrcld.com/*",
    "https://udrcld.com/*",
    "http://staging.theundercloud.com/*",
    "https://staging.theundercloud.com/*",
    "http://www.theundercloud.com/*",
    "https://www.theundercloud.com/*",
    "http://localhost:8080/",
    "http://localhost:3000/"
  ]
}