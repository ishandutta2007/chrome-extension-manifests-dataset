      "48": "data/icons/48.png",
      "64": "data/icons/64.png"
    }
  },
  "declarative_net_request": {
    "rule_resources": [
      {
        "enabled": true,
        "id": "url-filter",
        "path": "data/filters/url-filter.json"
      },
      {
        "enabled": true,
        "id": "regex-filter",
        "path": "data/filters/regex-filter.json"
      }
    ]
  },
  "icons": {
    "16": "data/icons/16.png",
    "32": "data/icons/32.png",
