    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["*.css"],
      "matches": ["<all_urls>"]
    }
  ],
  "permissions": [
    "scripting",
    "activeTab"
  ],
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
