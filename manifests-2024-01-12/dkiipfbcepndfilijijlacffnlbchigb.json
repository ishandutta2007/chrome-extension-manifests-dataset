  "default_popup": "popup.html",
  "default_title": "__MSG_name__"

 },
 "background": {
  "service_worker": "js/background_script.js"
 },

 "permissions": [
  "storage",
  "webRequest",
  "tabs",
  "downloads"
 ],

 "host_permissions": [
  "*://*.vimeo.com/*"
 ],
 "content_scripts": [
  {
   "matches": [
    "*://*.vimeo.com/*",
    "*://embed.vhx.tv/*"
   ],
   "js": [
    "js/content_script.js"
   ],
   "css":[
    "css/content.css"
   ],
   "run_at": "document_idle",
   "all_frames": true
  }
 ]
