{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "SSS Autofill",
  "description": "Open source autofill extension for Shopify, Supreme and Stripe",
  "version": "0.0.8",
  "manifest_version": 2,
  "icons": {
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "browser_action": {
    "default_title": "sss-autofill",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "inject/all.js"
      ],
      "all_frames": true
    },
    {
      "matches": [
        "https://js.stripe.com/*"
      ],
      "js": [
        "inject/stripe.js"
      ],
      "all_frames": true
    },
    {
      "matches": [
        "https://www.supremenewyork.com/checkout"
      ],
      "js": [
        "inject/supreme.js"
      ]
    },
    {
      "matches": [
        "https://*/*/checkouts/*"
      ],
      "js": [
        "inject/shopify.js"
      ]
    },
    {
      "matches": [
        "https://*.shopifycs.com/*"
      ],
      "js": [
        "inject/shopify-card.js"
      ],
      "all_frames": true
    }
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}