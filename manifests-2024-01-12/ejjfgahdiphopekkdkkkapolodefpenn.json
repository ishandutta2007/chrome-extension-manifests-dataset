        "128": "img/logo.png"
    },
    "permissions": [
        "*://youtube.com/*"
    ],
    "background": {
        "scripts": ["thirdParty/jquery-3.3.1.min.js", "src/background.js"],
        "persistent": false
    },
    "default_locale": "en",
    "content_scripts": [ {
        "js": [ "src/content-script.js" ],
        "all_frames": true,
        "run_at": "document_end",
        "matches": ["*://www.youtube.com/*"]
    }]
