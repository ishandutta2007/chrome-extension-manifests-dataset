{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 2,
    "name": "Dotorage",
    "description": "URL들을 체계적으로 관리해주는 앱입니다.",
    "version": "1.12",
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_title": "Dotorage",
        "default_icon": "assets/dotori.png",
        "default_popup": "popup.html"
    },
    "content_scripts": [{
        "matches": ["*://localhost/*", "https://dotori.ga/"],
        "js": ["content_script.js"]
    }],
    "icons": {
        "16": "assets/dotori.png",
        "48": "assets/dotori.png",
        "128": "assets/dotori.png"
    },
    "permissions": [
        "tabs",
        "storage",
        "contextMenus",
        "bookmarks",
        "http://dotori.ga/*",
        "https://dotori.ga/*"
    ],
    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "default": "Ctrl+Shift+A"
            }
        },
        "show_add_popup": {
            "suggested_key": {
                "default": "Ctrl+Shift+S"
            },
            "description": "show add popup"
        }
    },
    "default_locale": "en"
}