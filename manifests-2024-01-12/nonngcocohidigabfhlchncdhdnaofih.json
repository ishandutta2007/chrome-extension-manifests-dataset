{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "PROMAN extension",
  "description": "Set timer, blacklist browser/urls, alarm and make Todos for your tasks and be a PROMAN.",
  "version": "0.0.2",
  "manifest_version": 3,

  "background": {
      "service_worker": "background.js"
  },

  "content_scripts": [
    {
      "matches": ["https://*/*","http://*/*"] ,
      "css": ["/css/all.min.css"],
      "js": ["/scripts/content/jquery-3.6.0.js", "/scripts/content/content.js"]
    }
  ],

  "permissions": [
      "storage",
      "notifications",
      "alarms", 
      "declarativeNetRequest"
  ],

  "action": {
    "default_popup": "/popup/popup.html",
    "default_icon": {
      "16": "/images/logo5.png",
      "24": "/images/logo4.png",
      "32": "/images/logo3.png",
      "64": "/images/logo2.png",
      "128": "/images/logo1.png"
    }
  },

  "icons": {
    "16": "/images/logo5.png",
    "24": "/images/logo4.png",
    "32": "/images/logo3.png",
    "64": "/images/logo2.png",
    "128": "/images/logo1.png"
  },

  "options_ui": {
      "page": "/options/options.html",
      "open_in_tab": false
    },

  "web_accessible_resources": [{
      "resources": [
          "/options/options.html" ,
          "/options/options.css" ,
          "/css/all.min.css" , 
          "/options/options.js" ,
          "webfonts/*"
      ],
      "matches": [
        "<all_urls>"
      ]
  }]
}