{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "Pin-The-Look Extension",
  "version": "0.1.31",
  "icons": {
    "16": "assets/logo.png",
    "32": "assets/logo.png",
    "48": "assets/logo.png"
  },
  "description": "This extension pins inspiration room images and parses e-commerce product to use on the Pin the Look platform.",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "scripts/content.js",
        "scripts/parser.js",
        "settings/options.js",
        "templates/renderModal.js"
      ],
      "css": [
        "assets/styles.css"
      ]
    }
  ],
  "options_ui": {
    "page": "settings/options.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "storage",
    "<all_urls>"
  ],
  "browser_action": {
    "default_icon": {
      "16": "assets/logo.png",
      "24": "assets/logo.png",
      "32": "assets/logo.png"
    },
    "default_title": "Click to open modal"
  },
  "web_accessible_resources": [
    "templates/pin-modal-template.html",
    "templates/select-modal-template.html",
    "assets/font/walkwayUltraBold.ttf"
  ]
}
