{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "ChatGPT for all sites",
    "description": "基于ChatGPT版本的chrome插件",
    "version": "1.0.0",
    "action": {
        "default_title": "my-vue3-plugin",
        "default_icon": "assets/images/icon_1x.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "storage",
        "contextMenus"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "commands": {
        "open_content_page": {
            "suggested_key": {
                "windows": "Ctrl+M",
                "mac": "Command+M"
            },
            "description": "open content"
        }
    },
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "16": "assets/images/icon_1x.png",
        "48": "assets/images/icon_3x.png",
        "128": "assets/images/icon_8x.png"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "css": [
                "css/content.css",
                "css/element-ui.css"
            ],
            "js": [
                "chunk-vendors.js",
                "content.js",
                "js/vue.js"
            ],
            "run_at": "document_idle"
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "js/inject.js"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ],
    "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'self'"
    }
}