  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["main.js"],
      "css": ["style.css"]
    }
  ],
  "action": {
    "default_popup": "popup/popup.html"
  },
  "permissions": ["storage", "scripting", "tabs"],
  "host_permissions": ["*://*/*"]
}
