  "manifest_version": 3,
  "name": "WordPicss",
  "description": "Learn English words with images & sound in 3 fun interactive modes with this Chrome extension.",
  "version": "0.0.5",
  "web_accessible_resources": [
    {
        "resources": [ "data/icons/*", "data/sound/*", "data/database.json", "style.css", "scan.js" ],
