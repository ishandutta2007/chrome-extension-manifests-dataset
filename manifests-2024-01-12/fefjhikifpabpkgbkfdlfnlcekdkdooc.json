  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Taglayer Website Editor",
    "default_icon": {
      "19": "icons/19.png",
      "38": "icons/38.png"
    }
  },
  "version": "0.1.5",
  "content_security_policy": "script-src 'self' ; object-src 'self'"
