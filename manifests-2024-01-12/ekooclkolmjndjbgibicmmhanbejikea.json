{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "Redbubble Tag Generator",
  "version": "0.1",
  "permissions": [
    "tabs",
    "https://bubblesear.ch/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://www.redbubble.com/*",
        "*://redbubble.com/*"
      ],
      "js": [
        "clipboard.min.js",
        "content.js"
      ]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": [
      "jquery-3.5.1.min.js",
      "background.js"
    ]
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
