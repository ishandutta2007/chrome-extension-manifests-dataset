  "service_worker": "./background/background.js"
 },
 "content_scripts": [
  {
   "matches": ["<all_urls>"],
   "js": ["./content/content.js"],
   "css": ["./content/content.css"]
  }
 ],
 "web_accessible_resources": [
  {
    "resources": [ "content/content.css" ],
    "matches": ["<all_urls>"]
  }
 ]
