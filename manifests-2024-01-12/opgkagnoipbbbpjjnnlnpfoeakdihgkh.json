"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "GPT Theme",
    "version": "1.1",
    "description": "A Chrome extension to change the Theme ChatGPT",
    "permissions": [
        "storage",
        "activeTab"
