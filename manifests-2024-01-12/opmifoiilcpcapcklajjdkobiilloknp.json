 {
"update_url": "https://clients2.google.com/service/update2/crx",

    "name": "SoundFilter",
    "description" : "Remove reposts from soundcloud feed.",
    "version": "1.5",
    "manifest_version": 2,
    "permissions": [
     "activeTab",
     "*://*.soundcloud.com/*",
     "storage"
    ],
    "background": {
     "scripts" : ["background.js"],
     "persistent": false
    },
    "browser_action": {
      "default_title": "SoundFilter",
      "default_icon": "icon_16.png"
    },
    "icons": { 
            "16": "icon_16.png",
            "48": "icon_48.png",
            "128": "icon_128.png" 
    },
    "content_scripts": [
     {
      "matches": ["*://*.soundcloud.com/*"],
      "js": ["removeReposts.js"]
     }
    ]
  }