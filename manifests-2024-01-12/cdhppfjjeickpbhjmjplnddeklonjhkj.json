  "web_accessible_resources": [
    "assets/*"
  ],

  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "css": [
        "src/inject/styles.css"
      ],
      "js": [
        "src/inject/scripts.js"
      ]
    }
  ]
