"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 2,
    "name": "Google Meet Word Alert",
    "version": "0.2",
    "permissions": [
        "storage",
        "notifications"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://meet.google.com/*"
            ],
