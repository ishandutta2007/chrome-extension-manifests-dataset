  "version": "1.0.10",
  "manifest_version": 3,
  "name": "homebound",
  "description": "Use the Homebound property tool to save, organise and share properties",
  "action": {
    "default_popup": "/js/popup.html",
    "default_title": "Homebound"
  },
  "icons": {
    "128": "assets/logo128.png",
