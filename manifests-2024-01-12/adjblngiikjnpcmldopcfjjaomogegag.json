{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "Grannify",
    "version": "1.3",

    "description": "Provides a custom new tab page of mostly-used sites and  a custom web page zoomer for the older folks.",
    "icons": {
        "16": "/images/icon-20.png",
        "48": "/images/icon-48.png",
        "128": "/images/icon-128.png"
    },
    "action": {
      "default_icon": {           
      "16": "images/green.png",  
      "24": "images/green.png",   
      "32": "images/green.png"   
      },
      "default_title": "Click to refresh page zoom."

    },
    "background": {
      "service_worker": "background.js"
    },
    "chrome_url_overrides" : {
      "newtab": "main.html"
    },
    
    "permissions": [
      "storage",
      "scripting"
      
      
    ],
    "host_permissions": [
 
      "<all_urls>"
    ]

  
  }
