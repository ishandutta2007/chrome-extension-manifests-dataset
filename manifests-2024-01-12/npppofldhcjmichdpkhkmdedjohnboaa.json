  "matches": [
   "*://outlook.office.com/*",
   "*://outlook.office365.com/*",
   "*://mail.google.com/*"
  ]
 },
 "web_accessible_resources": [
  {
   "matches": [
    "<all_urls>"
   ],
   "resources": [
    "profile.json",
    "Injection/*"
   ]
  }
 ],
 "action": {
  "default_icon": {
   "32": "Injection/Resources/favicon.png"
  }
 },
 "icons": {
  "128": "Injection/Resources/logo128.png"
 },
