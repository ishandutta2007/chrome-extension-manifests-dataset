  "background": {
  "service_worker": "background.js"
 },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": [
        "./jquery-3.6.0.min.js",
        "./jquery-ui-1.13.1/jquery-ui.min.js",
        "./pet.js"
      ],
      "css": ["content.css"],
      "run_at": "document_start",
      "all_frames": false
    }
  ],

  "web_accessible_resources": [
    {
      "matches": ["<all_urls>"],
