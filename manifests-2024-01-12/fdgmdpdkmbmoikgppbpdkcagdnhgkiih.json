  "page_action": {
    "browser_style": false,
    "default_popup": "pages/popup.html",
    "default_icon": "icons/icon_48.png"
  },
  "web_accessible_resources": [
    "assets/css/ui.css",
    "src/observer.js"
  ],
  "icons": {
    "16": "icons/icon_16.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png",
    "256": "icons/icon_256.png"
  },
  "manifest_version": 2
