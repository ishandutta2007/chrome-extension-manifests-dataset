{
"update_url": "https://clients2.google.com/service/update2/crx",

    "name": "__MSG_extName__",
    "description": "__MSG_extDesc__",
    "short_name": "__MSG_extName__",
    "version": "1.0",
    "manifest_version": 3,
    "default_locale": "en",
    "permissions": [
        "tabs",
        "storage",
        "declarativeNetRequest",
        "gcm",
        "scripting"
    ],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ],
    "icons": {
        "16": "icons/16.png",
        "48": "icons/48.png",
        "128": "icons/128.png"
    },
    "declarative_net_request": {
        "rule_resources": [{
            "id": "rules",
            "enabled": true,
            "path": "rules.json"
        }]
    },
    "action": {
        "default_icon": "assets/day-icon.png",
        "default_popup": "popup/popup.html"
    },
    "content_scripts": [
        {
            "js": [
                "js/content.js"
            ],
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "all_frames": true,
            "run_at": "document_start"
        }
    ],
    "background": {
        "service_worker": "js/bg.js"
    },
    "web_accessible_resources": [
        {
            "resources": [
                "assets/*",
                "js/*"
            ],
            "matches": [
                "http://*/*",
                "https://*/*"
            ]
        }
    ]
