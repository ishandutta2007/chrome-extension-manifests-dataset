{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "View Spec file",
  "short_name": "View Spec File",
  "version": "1.1",
  "description": "An extension for showing 'Go to spec file' on code file diff and 'Go to code file' on spec file diff for Ruby On Rails project",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["https://*.github.com/*/*/pull/*/files"],
      "run_at": "document_idle",
      "js": ["contentScript.js"]
    }
  ],
  "page_action": {
    "default_icon": {
      "16": "images/icon1.png",
      "32": "images/icon1.png",
      "48": "images/icon1.png",
      "128": "images/icon1.png"
    }
  },
  "icons": {
    "16": "images/icon1.png",
    "32": "images/icon1.png",
    "48": "images/icon1.png",
    "128": "images/icon1.png"
  },
  "web_accessible_resources": [
    "src/js/*.js",
    "src/js/**/*.js"
  ],
  "manifest_version": 2
}
