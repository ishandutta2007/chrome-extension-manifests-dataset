"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "血庫小精靈 Blood Order Genius",
  "version": "1.0.1",
  "description": "血液基金會醫院網路作業平台(https://dh.blood.org.tw/hospital/)之 Chrome 套件。",
  "icons": {
    "128": "icons/icon.png"
