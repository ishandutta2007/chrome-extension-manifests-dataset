{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "Block Page Assistant",
  "description": "If using DNS based web filtering, this extension will improve the user experience for block pages when using HTTPS.",
  "version": "2.0.5",
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png",
    "16": "icon16.png"
  },
  "background": {
    "scripts": [
      "bg.js"
    ]
  },
  "browser_action": {
  },
  "options_ui": {
    "page": "options.html"
  },
  "content_scripts": [
    {
      "matches": [
        "*://mytools.management/*",
        "*://*/*"
      ],
      "js": [
        "cs.js"
      ]
    }
  ],
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "recaptcha",
        "enabled": false,
        "path": "recaptcha-ruleset.json"
      }
    ]
  },
  "permissions": [
    "<all_urls>",
    "declarativeNetRequest",
    "storage",
    "tabs",
    "webNavigation",
    "webRequest"
  ]
}
