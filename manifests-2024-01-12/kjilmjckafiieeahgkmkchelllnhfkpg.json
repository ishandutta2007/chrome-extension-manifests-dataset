{
"update_url": "https://clients2.google.com/service/update2/crx",

 "manifest_version": 3,
 "name": "ChatLock GPT",
 "version": "1.0",
 "description": "Lock individual chats in ChatGPT chat log with a password.",
 "permissions": ["storage", "activeTab", "tabs", "webNavigation"],
 "background": {
  "service_worker": "background.js"
 },
 "content_scripts": [
  {
   "matches": ["https://chat.openai.com/*"],
   "js": ["contentScript.js"]
  }
 ],
 "action": {
  "default_popup": "popup.html",
  "default_icon": {
   "16": "icon16.png",
   "48": "icon48.png",
   "128": "icon128.png"
  }
 },
 "icons": {
  "16": "icon16.png",
  "48": "icon48.png",
  "128": "icon128.png"
 }
}
