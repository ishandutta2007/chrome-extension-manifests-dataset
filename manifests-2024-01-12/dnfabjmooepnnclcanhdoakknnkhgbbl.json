      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["d3.min.js","d3.layout.cloud.min.js"],
      "matches": ["<all_urls>"]
    }
