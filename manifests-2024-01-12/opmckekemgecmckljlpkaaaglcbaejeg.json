"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 2,
    "name": "Malicious Content Observer",
    "version": "0.0.0.2",
    "content_security_policy": "script-src 'self' https://www.google-analytics.com; object-src 'self'",
