    "background": {
        "scripts": [
            "eventPage.js"],
        "persistent": true
    },
    "content_scripts": [
        {
            "matches": ["https://*.youtube.com/*"],
            "js": ["content.js",
                "eventPage.js"
            ],
            "css": ["./style.css"]
        }
    ],
    "permissions": [
        "tabs",
      "clipboardRead",
      "storage",
    "https://adobservatory.com/ads",
    "https://adobservatory.com/users",
