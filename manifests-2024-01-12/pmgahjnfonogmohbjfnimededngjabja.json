{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "TrustBuilder",
  "description": "TrustBuilder Shopify Application Installer",
  "version": "1.0.7",
  "permissions" : [
    "tabs",
 "activeTab",
    "http://*.myshopify.com/*",
    "https://*.myshopify.com/*",
    "https://trustbuilder.limitlessmarketer.com/*"
  ],
 "externally_connectable" : {
      "matches": [
        "https://trustbuilder.limitlessmarketer.com/*"
      ]
  },  
  "content_scripts": [
    {
      "matches": [
        "http://trustbuilder.dev/*",
        "https://trustbuilder.limitlessmarketer.com/*"
      ],
      "js": ["contentscript.js"],
      "run_at": "document_start"
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },

  "browser_action" :
  {
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_title" : "TrustBuilder is active!"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "manifest_version": 2
}

