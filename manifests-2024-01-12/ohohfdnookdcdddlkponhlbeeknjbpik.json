    },
  "default_width": 300, 
  "default_height": 400 
  },
  "background": {
    "service_worker": "background.js"
  },
  "options_page": "settings.html",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": ["contentScript.js"],
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  }
