  "manifest_version": 3,
  "name": "Video Speed Controller",
  "version": "1.0.2",
  "description": "Control the playback speed of HTML5 videos.",
  "permissions": [
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "48": "icon48.png",
