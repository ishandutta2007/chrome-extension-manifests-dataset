{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Youtube auto bookmark",
  "version": "0.0.3",
  "description": "Make bookmark for youtube automatically",
  "options_ui": {
    "page": "src/pages/options/index.html",
    "open_in_tab": false
