{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "__MSG_name__",
  "version": "1.0.8",
  "manifest_version": 3,
  "description": "__MSG_description__",
  "default_locale": "en",
  "background": {
    "service_worker": "js/background.js"
  },
  "action": {
    "default_icon": {
      "48": "images/48.png",
      "16": "images/48.png",
      "32": "images/48.png",
      "128": "images/48.png",
      "300": "images/48.png",
