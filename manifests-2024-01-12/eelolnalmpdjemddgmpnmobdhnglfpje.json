    },
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "tabs",
    "webRequest"
  ],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/contentscript.js"
      ],
      "css": [
        "css/youtube_summary.css"
      ],
      "all_frames": false,
      "run_at": "document_end"
    },
    {
      "matches": [
        "https://*.openai.com/*"
      ],
      "js": [
        "js/OpenAiCopy.js"
      ],
      "css": [
        "css/youtube_summary.css"
      ],
      "run_at": "document_end"
    }
  ],
  "host_permissions": [
    "https://*.openai.com/*",
    "https://*.youtube.com/*"
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "images/*"
      ],
      "matches": [
        "<all_urls>"
