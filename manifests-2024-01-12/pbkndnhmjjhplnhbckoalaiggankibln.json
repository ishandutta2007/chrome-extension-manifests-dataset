{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Command YouTube Controller",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "This is made for me, listnening musics on Youtube in the office.",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "commands": {
    "play-stop": {
      "suggested_key": {
        "default": "Ctrl+Shift+Space",
        "mac": "Command+Shift+Space"
      },
      "description": "play or stop youtube video"
    },
    "previous-video": {
      "suggested_key": {
        "default": "Ctrl+Shift+Left",
        "mac": "Command+Shift+Left"
      },
      "description": "go previous youtube video"
    },
    "next-video": {
      "suggested_key": {
        "default": "Ctrl+Shift+Right",
        "mac": "Command+Shift+Right"
      },
      "description": "go next youtube video"
    },
    "show-title": {
      "suggested_key": {
        "default": "Ctrl+Shift+Up",
        "mac": "Command+Shift+Up"
      },
      "description": "show title on notification"
    }

  },
  "permissions": ["commands", "notifications", "tabs", "https://www.youtube.com/*"]
}
