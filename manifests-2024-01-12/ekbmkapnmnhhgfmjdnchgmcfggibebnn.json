    "service_worker": "background.bundle.js"
  },
  "action": {
    "default_popup": "popup/public/index.html",
    "default_title": "GenAlt"
  },
  "web_accessible_resources": [
    {
      "resources": ["usage.html"],
      "matches": ["<all_urls>"]
    }
  ]
