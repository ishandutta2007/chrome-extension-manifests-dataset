      ],
      "js": ["content.bundle.js"],
      "run_at": "document_end",
      "all_frames": true
    }
  ],
  "permissions": [
    "activeTab",
    "contextMenus",
    "scripting"
  ],
    "host_permissions": ["https://*/"],
    "content_security_policy": {
    "extension_pages": "default-src 'self'; style-src 'self' data: 'unsafe-inline'; script-src 'self';"
  }
