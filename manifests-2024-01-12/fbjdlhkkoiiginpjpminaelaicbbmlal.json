{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "Mood Picker",
  "description": "Chrome Extension for moodof",
  "version": "1.6",
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "browser_action": {
    "default_icon": "pickerIcon-default.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/vendor.js",
        "js/content_script.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "js/vendor.js",
      "js/background.js"
    ]
  },
  "commands": {
    "pick": {
      "suggested_key": {
        "default": "Ctrl+Shift+S",
        "mac": "Command+Shift+S",
        "windows": "Ctrl+Shift+S"
      },
      "description": "Toggle pick"
    }
  },
  "permissions": [
    "storage",
    "<all_urls>",
    "contextMenus"
  ],
  "web_accessible_resources": [
    "redirect_uri.html"
  ]
}