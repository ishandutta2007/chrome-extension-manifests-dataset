  }],
  "background": {
    "persistent": false,
    "scripts": ["js/image.js"]
  },
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_title": "Pic info"
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "permissions": [
    "tabs",
    "activeTab",
    "<all_urls>"
  ]
