  "manifest_version": 2,
  "name": "Pic info",
  "description": "Gives a detailed info about the image like URL, alt text, file size and dimension",
  "version": "1.5",
  "icons": {
    "16": "img/icon.png",
    "32": "img/icon-32.png",
