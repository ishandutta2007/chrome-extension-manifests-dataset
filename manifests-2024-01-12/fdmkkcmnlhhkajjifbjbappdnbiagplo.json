{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "Thankd",
  "version": "1.012",
  "permissions": [
      "identity",
      "tabs",
      "activeTab",
      "*://*/*",
   "webNavigation"
  ],
  "background": {
    "scripts": ["background.js"],
 "persistent": true
  },
  "icons": { "16": "logo_16.png",
          "30": "logo_30.png",
             "48": "logo_48.png",
       "128": "logo_128.png" },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-3.2.1.min.js",  "content.js"]

    }
  ],
  "browser_action": {
    "default_icon": "logo_16.png"
  },
  "web_accessible_resources": [
      "logo_16.png",
   "logo_48.png"
  ]
}