{
   "manifest_version": 3,
   "name": "__MSG_appName__",
   "short_name": "__MSG_appName__",
   "author": "The QR Code Generator",
   "default_locale": "en",
   "description": "__MSG_appDesc__",
   "permissions": [ "activeTab", "clipboardWrite", "contextMenus", "storage", "downloads"],
   "host_permissions": [
      "<all_urls>"
    ],
   "offline_enabled": true,
   "icons": {
      "256": "images/256.png",
      "128": "images/128.png",
      "96": "images/96.png",
      "64": "images/64.png",
      "48": "images/48.png",
      "32": "images/32.png",
      "16": "images/16.png"   
   },
   "action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html",
      "default_title": "__MSG_appTitle__"
   },
   "options_ui":{
      "page": "options.html",
      "open_in_tab": true
   },
   "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": [
          "js/lib/qrcode.js", "js/content.js"
        ],
        "css": [
          "css/content.css"
        ]
      }
    ],
   "background": {
      "service_worker": "background.js",
      "type":"module"
   },
   "content_security_policy": {
      "extension_pages": "default-src 'self'; img-src 'self' https://www.paypalobjects.com data:; script-src 'self'; object-src 'self'"
   },
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0.3"
}
