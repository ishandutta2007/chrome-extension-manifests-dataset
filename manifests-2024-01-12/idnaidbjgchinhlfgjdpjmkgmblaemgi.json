        "./static/js/content.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["inject.js"],
      "matches": ["https://*.qbo.intuit.com/*"]
    },
    {
      "resources": ["icons/*"],
      "matches": ["<all_urls>"]  
