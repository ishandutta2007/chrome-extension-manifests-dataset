{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Curb Page",
  "version": "1.01",
  "description": "Navigate to specific parts of the current domain. It's like making a custom navigation menu for yourself on any website",
  "permissions": [
    "storage",
    "activeTab",
    "tabs"
