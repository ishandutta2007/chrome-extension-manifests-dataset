{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Youtube ads skip",
  "version": "0.2.5",
  "description": "The extension skips youtube ads automatically",
  "host_permissions": [
    "*://www.youtube.com/*",
    "*://www.youtu.be/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://www.youtube.com/*",
        "*://www.youtu.be/*"
      ],
      "js": [
        "import-contentScript.js"
      ]
    }
  ],
  "icons": {
    "16": "assets/skip.png",
    "48": "assets/skip.png",
    "128": "assets/skip.png"
  },
  "action": {
    "default_icon": {
      "16": "assets/skip.png",
      "24": "assets/skip.png",
      "32": "assets/skip.png"
    },
    "default_title": "Youtube ads skip"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "chunks/*-*.js",
        "contentScript.js"
      ],
      "matches": [
        "*://www.youtube.com/*",
        "*://www.youtu.be/*"
      ]
    }
  ]
}