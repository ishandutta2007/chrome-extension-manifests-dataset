  "name": "Hippoc Web Extension",
  "version": "0.1.7",
  "description": "Hippoc Web Extension.",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "host_permissions": [
   "<all_urls>"
 ],
  "manifest_version": 3,
  "icons": {
    "16": "icon16.png",
    "24": "icon24.png",
    "32": "icon32.png",
    "128": "icon128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "24": "icon24.png",
      "32": "icon32.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "externally_connectable": {
    "matches": [
      "*://*.hippoc.ca/*",
      "*://*.hippoc.ai/*",
      "http://localhost:8080/*"
    ]
  }
