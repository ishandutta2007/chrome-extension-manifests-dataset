            ],
            "js": [
                "js/injector.js"
            ]
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "js/injected.js"
            ],
            "matches": [
                "http://10.0.100.5/*",
                "http://localhost/*"
            ]
        }
    ],
    "icons": {
        "48": "img/logo48.png",
        "128": "img/logo128.png"
    },
    "action": {
        "default_popup": "menu.html",
        "default_icon": "img/logo.png"
    },
    "host_permissions": [
        "http://10.0.100.5/FieldMonitor",
        "http://localhost/FieldMonitor/"
    ]
