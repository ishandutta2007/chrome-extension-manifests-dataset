{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Youtube中文配音",
  "description": "在youtube看英文视频的时候，点击插件图标,会用自动进行翻译，然后用中文进行朗读。",
  "version": "1.1.3",
  "icons": {
    "16": "images/icon.png",
    "32": "images/icon.png",
    "48": "images/icon.png",
    "128": "images/icon.png"
  },
  "background": {
    "service_worker": "background.bundle.js"
  },
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["BeforeContent.bundle.js"],
      "run_at" : "document_start"
    },
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["content.bundle.js"],
      "css": ["css/loading.css","css/loading.min.css", "css/introjs.min.css"],
      "run_at" : "document_end"
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon.png",
      "32": "images/icon.png",
      "48": "images/icon.png",
      "128": "images/icon.png"
    }
  },
  "permissions": ["tabs", "storage", "cookies", "contextMenus"],
  "host_permissions": [
    "https://yd.transduck.com/"
  ],
  "web_accessible_resources": [
    {
      "resources": ["images/*"],
      "matches": ["https://www.youtube.com/*"]
    }
  ]
}