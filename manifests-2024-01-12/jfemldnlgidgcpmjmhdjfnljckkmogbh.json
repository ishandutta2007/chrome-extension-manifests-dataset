            ]
        },
        {
            "matches": [
                "https://secure.tickets/*"
            ],
            "run_at": "document_start",
            "js": [
                "assets/barcode_secure_tickets.js"
            ]
        },
        {
            "matches": [
                "https://presto.ticketmaster.com/*"
            ],
