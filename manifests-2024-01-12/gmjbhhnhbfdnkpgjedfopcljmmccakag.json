    "name": "Just Check",
    "description": "Displays food safety inspection scores against take-aways on just-eat.co.uk and hungryhouse.co.uk",
    "version": "1.4",
    "permissions": ["http://ratings.food.gov.uk/*"],
    "content_scripts": [
        {
            "matches": [
                "*://*.just-eat.co.uk/area/*",
                "*://*.hungryhouse.co.uk/*",
                "*://*.fillmybelly.com/*"
            ],
            "run_at": "document_end",
            "css": [
                "justeat.css"
            ],
            "js": [
                "jquery.js",
                "utils.js",
                "justeat.js"
            ]
        }
    ],
    "web_accessible_resources": [
      "images/*.jpg"
    ]
