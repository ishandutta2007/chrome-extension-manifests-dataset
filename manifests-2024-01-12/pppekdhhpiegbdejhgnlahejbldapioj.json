{
"update_url": "https://clients2.google.com/service/update2/crx",

   "action": {   
   },
   "content_scripts": [
   {
      "all_frames": true,
      "js": [ "contentScript.js" ],
      "matches": [ "https://www.montana.edu/*" ],
      "run_at": "document_idle"
