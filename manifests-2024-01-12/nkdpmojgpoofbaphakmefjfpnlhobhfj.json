  "permissions": ["storage","tabs","activeTab","scripting"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "/js/background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "css": [
      ],
      "js": [
        "/js/pagecolor.js"
      ],
      "run_at": "document_idle",
      "all_frames": false
    }
  ]
