  "manifest_version": 3,
  "name": "Easy Screen Shader",
  "description": "Tint Chrome with a soothing hue to ease eye strain and fatigue.",
  "version": "0.1.2",
  "action":{
    "default_icon": "images/logo.png",
    "default_popup": "setting.html"
  },
  "icons": {
    "16": "images/logo.png",
    "48": "images/logo.png",
    "128": "images/logo.png"
