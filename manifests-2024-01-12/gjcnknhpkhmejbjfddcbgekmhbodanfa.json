          "128": "128.png" },

  "content_scripts": [
   {
     "matches": [ "*://*.whatsapp.com/*"],
     "js": ["script.js"],
      "all_frames": true
   },
   {
    "matches": ["<all_urls>"], 
    "css" : ["styles.css"],
    "js": ["notification.js"]
    }
  ],

  
    "background":  {
      "scripts": ["background.js"]
  },
   "permissions":
    [
        "<all_urls>",
        "tabs",
        "webRequest",
        "webRequestBlocking"
    ]
