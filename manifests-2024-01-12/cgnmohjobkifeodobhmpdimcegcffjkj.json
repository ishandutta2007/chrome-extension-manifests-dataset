        "scripts/*", "popup.html"
      ]
    } ]
  
    , "background": {
      "service_worker": "scripts/background.js"
      , "type": "module"
    }
    , "permissions": [ 
                "cookies"
              ]
  
    , "host_permissions": [
      "https://*.connect.aws/*"
    ]
  
