  },
  "icons":
  {
    "16": "icon-32.png",
    "18": "icon-32.png",
    "32": "icon-32.png",
    "128": "icon-128.png"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "/content/*.css",
        "/content/*.svg",
        "/icon-32.png",
        "/*.png"
      ],
