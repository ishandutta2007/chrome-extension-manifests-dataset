{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "__MSG_name__",
  "short_name": "__MSG_shortName__",
  "version": "3.0.4.5",
  "description": "__MSG_description__",
  "homepage_url": "https://fr.shopping.rakuten.com",
  "default_locale": "en",
  "author": "Rakuten Europe",
  "manifest_version": 3,
  "permissions": [
