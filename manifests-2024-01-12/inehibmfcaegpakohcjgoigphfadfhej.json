{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Pin QR",
  "version": "1.3",
  "author": "Inal Karov",
  "manifest_version": 2,
  "description": "QR code generator",
  "icons": {
    "16": "images/16.png",
    "32": "images/32.png",
    "48": "images/48.png",
    "128": "images/128.png"
  },
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "permissions": ["tabs", "storage", "activeTab"],
  "browser_action": {
    "default_title": "Pin QR",
    "default_popup": "popup/popup.html",
    "default_icon": {
      "16": "images/16.png",
      "32": "images/32.png",
      "48": "images/48.png",
      "128": "images/128.png"
    }
  },
  "content_scripts": [
    {
      "run_at": "document_end",
      "matches": ["<all_urls>"],
      "css": ["pin/pin-style.css"],
      "js": [
        "lib/easy.qrcode.min.js",
        "content.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "pin/pin.html",
    "lib/*",
    "css/*"
  ]
}
