    "background": {
      "persistent": false,
      "scripts": ["background.js"]
    },
  
    "content_scripts": [{
      "matches": ["https://*.wixsite.com/*"],
      "js": ["content.js"]
    }],
  
    "page_action": {
      "default_title": "Wix Image Downloader", 
      "default_popup": "popup.html"
    }
 
