      ]
    },
    {
      "matches": [
        "https://www.linkedin.com/sales/*"
      ],
      "run_at": "document_idle",
      "js": [
        "build/webLinkedin/sales-nav-init.js"
      ]
    }

  ],
  "web_accessible_resources": [{
    "resources": ["smartreach.html"],
    "matches": ["<all_urls>"]
