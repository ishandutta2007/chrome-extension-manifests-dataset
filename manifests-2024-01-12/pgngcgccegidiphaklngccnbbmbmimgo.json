{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 2,
    "name": "TheShoppersClub",
    "description": "Shop from your favourite stores",
    "version": "2.0",
    "browser_action": {
        "default_title": "TheShoppersClubs - Shop from your favourite stores",
        "default_icon": "logo.png"
    },
    "permissions": [
        "activeTab"
    ],
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": true
    },
    "content_scripts": [{
        "matches": [
            "https://*.amazon.com/*",
            "https://*.ebay.com/*",
            "https://*.walmart.com/*",
            "https://*.alibaba.com/*",
            "http://*.alibaba.com/*",
            "https://theshoppersclubs.com/*"
        ],
        "js": [
            "speakingurl.min.js",
            "content.js"
        ],
        "run_at": "document_end"
    }]
}
