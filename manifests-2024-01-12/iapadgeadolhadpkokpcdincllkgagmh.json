  "version": "1.1.32",
  "icons": { "32": "icons/logo32.png",
           "48": "icons/logo48.png",
           "96": "icons/logo96.png",
          "128": "icons/logo128.png" },
  "background":{
    "page":"pages/background.html"
  },

  "web_accessible_resources": [
