      "js": ["dist/frame.js"], 
      "matches": ["https://mail.google.com/*",
                  "http://mail.google.com/*"], 
      "all_frames":true,
      "run_at":"document_idle"
    }
  ],
  "permissions": [
    "activeTab",
    "https://www.facebook.com/",
    "https://*.mycontacts.io/",
    "https://*.google.com/",
