  "content_scripts": [
    {
      "matches": ["https://*.aws.amazon.com/*"],
      "js": ["bundle.js"],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["icons/*"],
      "matches": ["https://*.aws.amazon.com/*"]
    }
  ]
}
