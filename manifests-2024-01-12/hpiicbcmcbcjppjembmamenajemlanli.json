  "manifest_version": 3,
  "name": "AWS Favicons",
  "description": "Favicons for all AWS services",
  "version": "0.2.0",
  "author": "Maddison Hellstrom",
  "icons": {
    "32": "assets/extension-icon-32.png",
    "64": "assets/extension-icon-64.png",
    "128": "assets/extension-icon-128.png"
