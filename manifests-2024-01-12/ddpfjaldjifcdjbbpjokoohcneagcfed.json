{
"update_url": "https://clients2.google.com/service/update2/crx",

 "description": "An extension to track how you feel on Twitter.",
 "version": "1.0",
 "name": "Twitter Feelings",
 "author": "Uğur Kellecioğlu",
 "manifest_version": 3,
 "options_page": "options.html",
 "action": {
  "default_popup": "popup.html",
  "default_icon": "icon-34.png"
 },
 "icons": {
  "128": "icon-128.png"
 },
 "web_accessible_resources": [
  {
   "resources": [
    "happy.png",
    "sad.png",
    "neutral.png"
   ],
   "matches": [
    "<all_urls>"
   ]
  }
 ],
 "content_scripts": [
  {
   "matches": [
    "http://twitter.com/*",
    "https://twitter.com/*"
   ],
   "js": [
    "content.bundle.js"
   ]
  }
 ],
 "background": {
  "service_worker": "background.bundle.js"
 },
 "content_security_policy": {
  "extension_pages": "script-src 'self' ; object-src 'self'"
 },
 "permissions": [
  "storage",
  "tabs",
  "webRequest",
  "alarms"
 ]
}