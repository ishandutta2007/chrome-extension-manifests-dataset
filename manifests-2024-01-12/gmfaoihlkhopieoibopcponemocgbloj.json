{
"update_url": "https://clients2.google.com/service/update2/crx",

  "version": "2.0.0",
  "manifest_version": 3,
  "name": "__MSG_extName__",
  "description": "__MSG_extDescription__",
  "action": {
    "default_title": "__MSG_extName__"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self';"
  },
  "background": {
    "service_worker": "js/background.js",
    "type": "module"
  },
  "default_locale": "en",
  "permissions": [
    "identity",
    "storage",
    "tabs",
    "activeTab",
    "contextMenus",
    "scripting",
    "sidePanel"
  ],
  "externally_connectable": {
    "matches": ["<all_urls>"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/index.js"],
      "all_frames": false
    }
  ],
  "side_panel": {
    "default_path": "js/sidebar.html"
  },
  "commands": {
    "saveCurrentUrl": {
      "suggested_key": {
        "default": "Alt+S",
        "mac": "Alt+S"
      },
      "description": "Bookmark current URL"
    },
    "createStickyNote": {
      "suggested_key": {
        "default": "Alt+W",
        "mac": "Alt+W"
      },
      "description": "Create sticky note"
    }
  },
  "host_permissions": ["<all_urls>"],
  "icons": {
    "16": "images/saveday_square_logo_16x16.png",
    "32": "images/saveday_square_logo_16x16.png",
    "48": "images/saveday_square_logo_16x16.png",
    "128": "images/saveday_square_logo_16x16.png"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "js/index.html",
        "js/connectTele.html",
        "js/manageContent.html",
        "js/sidebar.html",
        "js/config.html",
        "images/*",
        "fonts/*",
        "js/*.map"
      ],
      "matches": ["<all_urls>"]
    }
  ],
  "options_page": "js/config.html",
  "key": ""
}
