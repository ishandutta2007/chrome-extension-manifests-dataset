  "manifest_version": 2,

  "name": "Spotify: ask before closing",
  "description": "This extension ask the user to confirm his exit from play.spotify.com",
  "version": "1.0",

  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "storage",
    "tabs"
  ],
"content_scripts": [
    {
      "matches": ["https://play.spotify.com/*"],
      "js": ["script.js"],
      "run_at": "document_end"
    }
  ]
