 "version" : "1.0.0",
 "manifest_version" : 3,
  "host_permissions": ["*://www.tiktok.com/*"],
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "base.js",
        "jquery-3.6.0.min.js",
        "inject.js"
      ],
      "matches": [
    "https://www.tiktok.com/*"]
    }
  ],
  "content_scripts": [
      {
        "matches": ["https://www.tiktok.com/*"],
        "js": ["base.js", "jquery-3.6.0.min.js", "contentscript.js"]
      }
    ],
  "permissions": [
      "webRequest",
      "storage",
      "declarativeNetRequest",
      "browsingData"
    ] 
}