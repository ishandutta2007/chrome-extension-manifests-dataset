      "css": ["content/contentKey.css", "content/contenKeyUi.css"],
      "run_at": "document_start"
    }
  ],
  "action": {
    "default_popup": "popup/popup.html"
  },
  "options_page": "options/options.html",
  "web_accessible_resources": [
    {
      "resources": ["/*"],
      "matches": ["<all_urls>"]
    }
