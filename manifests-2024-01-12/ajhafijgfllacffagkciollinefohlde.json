      "matches": ["*://world.hey.com/*"],
      "run_at": "document_end",
      "js": ["./contentScript/index.js"],
      "css": ["./style.css"]
    }
  ],
  "background": {
    "service_worker": "./background/background.js"
  },
  "permissions": ["storage"],
  "icons": {
    "72": "icon-72x72.png",
    "96": "icon-96x96.png",
    "128": "icon-128x128.png",
    "144": "icon-144x144.png",
    "152": "icon-152x152.png"
  }
