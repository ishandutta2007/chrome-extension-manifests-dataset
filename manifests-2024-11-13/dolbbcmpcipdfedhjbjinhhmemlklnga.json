{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "BlueNote Highlighter",
    "description": "Track, Structure and Collaborate your note making with ease!",
    "version": "1.0.4",
    "icons": {
        "16": "images/post (1).png",
        "48": "images/post (5).png",
        "128": "images/post (5).png"
    },
    "action": {
        "default_icon": {
            "16": "images/post (1).png",
            "48": "images/post (5).png",
            "128": "images/post (5).png"
        },
        "default_popup": "src/popup/index.html"
    },
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": [
                "lib/jquery-2.1.3.min.js",
                "contentScript.js"
            ],
            "css": [
                "src/contentScripts/hoverTools/index.css"
            ],
            "all_frames": true
        }
    ],
    "background": {
        "service_worker": "background.js",
        "matches": [
            "http://*/*",
            "https://*/*"
        ],
        "type": "module",
        "script": "lib/aws-sdk.js"
    },
    "commands": {
        "execute-highlight": {
            "suggested_key": {
                "default": "Ctrl+H",
                "mac": "MacCtrl+H"
            },
            "description": "Highlight selected text"
        },
        "toggle-highlighter-cursor": {
            "suggested_key": {
                "default": "Ctrl+Shift+T",
                "mac": "MacCtrl+Shift+T"
            },
            "description": "Toggle the highlighter cursor"
        }
    },    
    "permissions": [
        "activeTab",
        "contextMenus",
        "scripting",
        "storage",
        "notifications"
    ],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "images/*.png",
                "images/*.svg",
                "src/contentScripts/*",
                "storage/*",
                "webSocket/*",
                "protocol/*",
                "dataTransmitter/*",
                "dataReceiver/*",
                "authentication/*",
                "constant.js",
                "lib/aws-sdk.js"
            ],
            "matches": [
                "http://*/*",
                "https://*/*"
            ]
        }
        
    ],
    "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'self'"   
    }
}
