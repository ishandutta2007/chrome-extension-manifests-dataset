  "action": {
    "default_title": "Go2Mail",
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }    
  },
  "content_scripts": [
    {
      "js": ["./content.js"],
      "run_at": "document_end",
      "matches": ["https://*/*"]
    }
  ],  
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+B",
        "mac": "Command+Shift+B"
