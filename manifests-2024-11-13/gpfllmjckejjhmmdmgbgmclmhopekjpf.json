"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Xperience Browser Extension",
  "version": "1.0.13",
  "description": "Xperience Browser Extension for managing your XPLL",
  "icons": {
    "16": "assets/icon-16.png",
    "32": "assets/icon-32.png",
