  "name": "Letterboxd Searcher",
  "description": "Let's you search the title of the current Netflix, Disney Plus or Amazon Prime movie or series on letterboxd.com.",
  "version": "0.1.3",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "tabs",
    "cookies"
  ],
