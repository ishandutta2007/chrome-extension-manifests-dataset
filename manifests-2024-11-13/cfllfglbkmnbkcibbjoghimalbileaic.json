{
"update_url": "https://clients2.google.com/service/update2/crx",

 "manifest_version": 3,
 "name": "__MSG_extName__",
 "default_locale": "en",
 "short_name": "BTRoblox",
 "description": "__MSG_extDescription__",
 "version": "1.1.2",
 "incognito": "split",
 "options_page": "settings.html",
 "action": {
  "default_title": "Open BTRoblox Settings"
 },

 "icons": {
  "128": "assets/img/icon_128.png"
 },

 "background": {
  "service_worker": "background/bg.js"
 },

 "host_permissions": [
  "<all_urls>"
 ],

 "permissions": [
  "declarativeNetRequestWithHostAccess",
  "notifications",
  "contextMenus",
  "scripting",
  "storage",
  "alarms"
 ],
 "content_scripts": [
  {
   "matches": [
    "*://www.roblox.com/*",
    "*://web.roblox.com/*",
    "*://create.roblox.com/marketplace/*"
   ],
   "exclude_matches": [
    "*://*/userads/*",
    "*://*/user-sponsorship/*",
    "*://*/build/upload"
   ],
   "js": [
    "app/roblox/Constants.js",

    "app/shared/common.js",
    "app/shared/utility.js",

    "app/shared/sharedData.js",
    "app/shared/messaging.js",
    "app/shared/settings.js",

    "app/inject/injectJS.js",
    "app/inject/injectCSS.js",
    "app/inject/modifyTemplate.js",
    "app/inject/loadOptionalLibrary.js",

    "app/main/themes.js",
    "app/main/adblock.js",
    "app/main/fastSearch.js",
    "app/main/navigation.js",
    "app/main/robuxToCash.js",

    "app/roblox/FileFormat/ByteReader.js",
    "app/roblox/FileFormat/Instance.js",
    "app/roblox/FileFormat/XmlParser.js",
    "app/roblox/FileFormat/BinaryParser.js",
    "app/roblox/FileFormat/MeshParser.js",

    "app/roblox/AnimationParser.js",
    "app/roblox/Parser.js",
    "app/roblox/AssetCache.js",
    "app/roblox/RobloxApi.js",

    "app/website/common.js",
    "app/website/avatar.js",
    "app/website/catalog.js",
    "app/website/develop.js",
    "app/website/friends.js",
    "app/website/gamedetails.js",
    "app/website/groups.js",
    "app/website/groupadmin.js",
    "app/website/home.js",
    "app/website/inventory.js",
    "app/website/itemdetails.js",
    "app/website/marketplace.js",
    "app/website/messages.js",
    "app/website/money.js",
    "app/website/placeconfig.js",
    "app/website/profile.js",
    "inject.js",
    "main.js"
   ],
   "run_at": "document_start",
   "all_frames": true
  },
  {
   "matches": [
    "<all_urls>"
   ],
   "js": [
    "cursor/content.js"
   ],
   "css": [
    "assets/cursor.css"
   ],
   "run_at": "document_idle",
   "all_frames": true
  }
 ],
 "web_accessible_resources": [
  {
   "resources": ["/assets/img/*", "/assets/*"],
   "matches": ["<all_urls>"]
  }
 ]
