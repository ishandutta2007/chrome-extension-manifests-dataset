{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Fugo Digital Signage Player",
  "short_name": "Fugo player",
  "version": "1.3",
  "description": "Digital Signage for Chrome",
  "icons": {
    "192": "logo.png",
    "512": "logo512.png"
  },
  "author": "Fugo team",
  "offline_enabled": true,
  "background": {
    "service_worker": "starter.js"
  },
  "permissions": [
    "power",
    "system.memory",
    "system.storage",
    "system.cpu",
    "tabs",
    "declarativeNetRequest",
    "windows"
  ],
  "host_permissions": ["<all_urls>"],
  "content_scripts": [
    {
      "all_frames": true,
      "js": ["content.js"],
      "matches": [
        "http://*.localhost.localhost/*",
        "https://player.fugo.ai/*",
        "https://www.fugo.ai/*",
        "https://fugo.ai/*",
        "https://www.youtube.com/*"
      ],
      "run_at": "document_start"
    }
  ],

  "action": {
    "default_icon": "logo.png",
    "default_title": "Fugo Player"
  },
  "web_accessible_resources": [
    {
      "resources": ["logo.png", "logo512.png"],
      "matches": ["<all_urls>"]
    }
  ],
  "externally_connectable": {
    "matches": ["http://*.localhost.localhost/*", "https://player.fugo.ai/*"]
  },
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "blockRuleset",
        "enabled": true,
        "path": "rules.json"
      }
    ]
  }
}
