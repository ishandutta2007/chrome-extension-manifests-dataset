  "manifest_version": 3,
  "name": "Formula Explorer",
  "version": "1.1.1",
  "description": "Inspect Google Sheets formula structures in a user-friendly pop-up window. Enhance your workflow with precision.",
  "background": {
    "service_worker": "background.js"
  },
  "key": "",
  "action": {
    "default_title" : "Open Formula Explorer (Ctrl-Q)",
    "default_icon" : {
      "16": "assets/icon16.png",
      "32": "assets/icon32.png"
    }
  },
  "icons": {
    "48": "assets/icon48.png",
    "128": "assets/icon128.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["index.html", "assets/logo.svg"],
      "matches": ["<all_urls>"]
    }
  ],
  "content_scripts": [
    {
      "matches": ["https://docs.google.com/spreadsheets/*"],
      "js": ["content.js"]
    }
  ],
  "permissions": [
    "activeTab",
    "scripting",
    "identity",
    "storage"
  ],
  "host_permissions": [
    "https://docs.google.com/spreadsheets/*",
    "https://formula-explorer.cognisheets.com/api/*",
    "https://accounts.google.com/o/oauth2/revoke/*"
  ],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Q",
        "mac": "Command+Q"
      }
    }
  },
  "oauth2": {
    "client_id": "241014300902-e4mr4t7dpves5e7ga4777c4364fk1idu.apps.googleusercontent.com",
