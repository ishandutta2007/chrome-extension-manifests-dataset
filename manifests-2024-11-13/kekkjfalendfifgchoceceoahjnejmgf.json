{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Bulk Image Downloader From Url List",
  "version": "2.3",
  "description": "A Chrome extension to bulk download images from a list of image URLs. Schedule downloads, filter by image type, save in folder etc",
  "permissions": ["downloads", "storage", "alarms"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "images/icon.jpg"
    }
  },
  "icons": {
    "128": "images/icon.jpg"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["popup.js"]
    }
  ],
  "options_page": "options.html",
  "web_accessible_resources": [
    {
      "resources": ["redirection/redirect-checker.html", "redirection/redirect-checker.js"],
      "matches": ["<all_urls>"]
    }
  ]
}
