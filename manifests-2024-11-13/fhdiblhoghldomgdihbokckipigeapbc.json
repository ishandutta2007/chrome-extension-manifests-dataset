{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Tiny Astro",
  "description": "Enhance your NFT trading with the Tiny Astro Extension",
  "version": "1.0",
  "permissions": ["tabs","background"],
  "host_permissions": ["https://blur.io/*","https://*.tinyastro.io/*"],
  "author": "Tiny Astro",
  "web_accessible_resources" : [{
    "resources": [ "execute_purchase.js","ethers-6.6.js","buffer.js" ],
    "matches": [ "https://blur.io/*" ]
  }],
  "background": {
    "service_worker": "background.js"
  },
  "externally_connectable": {
    "matches": [
      "https://*.tinyastro.io/*",
      "https://blur.io/*"
    ]
  },
  "content_scripts": [
    {
      "matches": ["https://blur.io/*","https://*.tinyastro.io/*"],
      "js": ["contentscript.js"]
    }
  ],
  "icons": {
    "16": "icon-16x16.png",
    "32": "icon-32x32.png",
    "192": "icon-192x192.png"
  }
}
