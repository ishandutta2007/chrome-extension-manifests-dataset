{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "OMD Wordstat Cookie Grabber",
  "description": "Сборщик cookies с сайта Yandex Wordstat",
  "version": "1.1",
  "action": {
    "default_popup": "popup.html",
    "default_title": "OMD Wordstat Cookie Grabber"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
   "16": "img/icon-16x16.png",
   "32": "img/icon-32x32.png",
   "48": "img/icon-48x48.png",
   "128": "img/icon-128x128.png"
  },
  "permissions": [
    "activeTab",
    "cookies"
  ],
  "host_permissions": [
    "https://*.yandex.ru/*",
    "https://*.bi.omd.ru/*",
    "http://93.170.244.89:8090/*",
    "http://localhost:8091/*",
    "http://127.0.0.1:8091/*"
  ],
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "include_globs": [
        "https://*.yandex.ru/*"
      ],
      "js": ["wordstat_content.js"]
    },
    {
      "matches": ["*://*/*"],
      "include_globs": [
        "https://at.bi.omd.ru/*",
        "http://localhost:8091/*",
        "http://127.0.0.1:8091/*"
      ],
      "js": ["toolkit_content.js"]
    }
  ]
}