  "host_permissions": [
    "https://*.socialsense.ai/*",
    "https://*.linkedin.com/*"
  ],
  "web_accessible_resources": [
    {
      "resources": ["popup.html", "img/*.png", "img/*.svg", "popup.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://*.linkedin.com/*"],
      "exclude_matches": [
        "https://*.linkedin.com/*/overlay*",
        "https://*.linkedin.com/home*",
        "https://*.linkedin.com/logout*",
        "https://www.linkedin.com/uas/login",
        "https://www.linkedin.com/oauth/*"
      ],
      "js": ["content.js"]
    }
