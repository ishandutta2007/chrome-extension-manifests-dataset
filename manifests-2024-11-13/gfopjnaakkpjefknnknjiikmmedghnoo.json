         "RecordRTC/StereoAudioRecorder.js",
         "RecordRTC/DiskStorage.js",
         "RecordRTC/EBML.js"
      ],
      "persistent":false
   },
   "content_scripts":[  
      {  
        "matches":[  
          "https://unikclassroom.prepaze.com/*",
          "https://webrtc-staging.prepaze.com/*"
