{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "ChatGPT Consolidator",
  "version": "1.0",
  "description": "Consolidate ChatGPT conversation information",
  "permissions": ["storage", "activeTab", "tabs"],
  "host_permissions": [
    "https://chat.openai.com/*",
    "https://chatgpt.com/*"
  ],
  "web_accessible_resources": [
    {
      "resources": ["export.html"],
      "matches": ["<all_urls>"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://chat.openai.com/*",
        "https://chatgpt.com/*"
      ],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
  "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
  "uninstall_url": "https://forms.gle/3HXbLdqP8BPRng7L7"
}
