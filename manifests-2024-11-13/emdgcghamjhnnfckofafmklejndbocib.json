{
"update_url": "https://clients2.google.com/service/update2/crx",

  "description": "Export your Discord and Instagram messages",
  "manifest_version": 3,
  "name": "message-exporter",
  "version": "1.0",

  "permissions": [
    "webRequest",
    "downloads"
  ],

  "host_permissions": [
    "https://*.discord.com/*",
    "https://*.instagram.com/*"
