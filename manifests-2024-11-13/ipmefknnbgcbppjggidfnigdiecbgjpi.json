  "permissions": [
    "storage",
    "tabs"
  ],
  "manifest_version": 3,
  "name": "banabo-utm-manager",
  "version": "1.0",
  "description": "UTM Manager for Banabo's Cookiesless Marketing Attribution Platform",
  "options_page": "src/pages/options/index.html",
  "background": {
    "service_worker": "src/pages/background/index.js"
  },
  "action": {
    "default_popup": "src/pages/popup/index.html",
    "default_icon": "waveLogo.png"
  },
