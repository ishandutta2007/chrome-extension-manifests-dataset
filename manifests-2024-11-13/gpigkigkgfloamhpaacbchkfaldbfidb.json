{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Menufy",
  "version": "2.0.0",
  "description": "Manage Shopify navigation with importing, exporting, and copy-pasting of menus, streamlining customization and efficiency.",
  "permissions": ["storage"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "host_permissions": [
    "*://online-store-web.shopifyapps.com/*",
    "https://api.getmenufy.com/*",
    "http://localhost:3000/*"
  ],
  "content_scripts": [
    {
      "matches": ["*://*.admin.shopify.com/*"],
      "js": ["content.js"]
    }
  ]
}
