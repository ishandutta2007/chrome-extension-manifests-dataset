  "manifest_version": 3,
  "name": "Bulk invitation withdrawal for LinkedIn™",
  "version": "1.3.2",
  "description": "Withdraw your sent LinkedIn invitations in bulk",
  "permissions": ["scripting", "tabs"],
  "host_permissions": ["https://www.linkedin.com/*"],
  "action": {
    "default_popup": "popup.html"
