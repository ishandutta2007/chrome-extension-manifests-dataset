  "manifest_version": 3,
  "name": "zenzen - Productivity Guardian: Site Blocker",
  "version": "1.3",
  "description": "Boost productivity and minimize distractions by easily blocking time-wasting websites.",
  "permissions": [
    "storage",
    "declarativeNetRequest",
    "browsingData",
    "activeTab",
    "contextMenus"
