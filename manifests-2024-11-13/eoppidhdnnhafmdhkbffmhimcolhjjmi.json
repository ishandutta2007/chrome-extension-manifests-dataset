  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Delete On Exit",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
