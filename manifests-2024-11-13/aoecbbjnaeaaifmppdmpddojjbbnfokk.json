{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Tomo AI",
  "version": "0.0.0.15",
  "key": "",
  "description": "Interact with your Tomos across the entire web",
  "permissions": [
    "tabs",
    "sidePanel",
    "audioCapture",
    "storage",
    "unlimitedStorage",
    "windows"
  ],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+K",
        "mac": "Command+Shift+K"
      },
      "description": "Activate extension"
    },
    "toggle-sidepanel": {
      "suggested_key": {
        "default": "Ctrl+Shift+L",
        "mac": "Command+Shift+L"
      },
      "description": "Open/Close Sidepanel"
    },
    "change-tomo-left": {
      "suggested_key": {
        "default": "Ctrl+Shift+Left",
        "mac": "Command+Shift+Left"
      },
      "description": "Change to left Tomo"
    },
    "change-tomo-right": {
      "suggested_key": {
        "default": "Ctrl+Shift+Right",
        "mac": "Command+Shift+Right"
      },
      "description": "Change to right Tomo"
    }
  },
  "action": {
    "default_icon": "icon.png"
  },
  "side_panel": {
    "default_path": "sidepanel.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],

  "web_accessible_resources": [
    {
      "resources": [
        "Resources/*",
        "Core/*",
        "styleXStyles.css",
        "voice-activity-detector.js",
        "icon_expanded.png"
      ],
      "matches": ["<all_urls>"]
    }
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self'"
  },
  "background": {
    "service_worker": "background.js"
  }
}
