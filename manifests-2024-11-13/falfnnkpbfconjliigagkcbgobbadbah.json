      "matches": ["https://*/*", "http://*/*"],
      "js": ["bundle.js"]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["index.html", "popup.js", "popup.css"],
