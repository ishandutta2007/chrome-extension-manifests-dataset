  "manifest_version": 3,
  "name": "Galxe Quest Assistant",
  "version": "2.0.2",
  "description": "Galxe Quest Assistant is a free Chrome extension designed specifically for the Galxe community",
  "permissions": ["storage", "notifications", "alarms"],
