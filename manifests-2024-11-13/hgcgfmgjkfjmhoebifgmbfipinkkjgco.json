{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "version": "1.0.0",
    "name": "Codeforces Lite",
    "side_panel": {
        "default_path": "index.html"
    },
    "permissions": ["tabs", "storage", "unlimitedStorage", "activeTab", "scripting", "sidePanel", "idle"],
    "host_permissions": ["http://*/*", "https://*/*"],
    "icons": {
        "32": "assets/icons/codeforces.png"
    },
    "web_accessible_resources": [
        {
            "resources": ["assets/scripts/ftaa-script.js"],
            "matches": ["<all_urls>"]
        }
    ],
    "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'self';"
    },
    "background": {
        "service_worker": "assets/scripts/background.js"
    },
    "content_scripts": [
        {
            "matches": ["https://codeforces.com/*"],
            "css": ["assets/styles/inject-styles.css"],
            "js": [
                "assets/scripts/content.js",
                "assets/scripts/changeUIFunctions/loginPageChanges.js",
                "assets/scripts/changeUIFunctions/problemSetPageChanges.js",
                "assets/scripts/darkModeFunctions/darkMode.js"
            ],
            "run_at": "document_start"
        }
    ]
