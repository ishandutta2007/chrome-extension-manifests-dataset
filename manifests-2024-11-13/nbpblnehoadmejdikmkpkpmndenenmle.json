{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Shein Product Finder",
  "version": "1.0",
  "description": "Find products on Shein based on current product page",
  "permissions": [
    "activeTab"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.amazon.com/*",
        "https://*.walmart.com/*",
        "https://*.ebay.com/*",
        "https://*.target.com/*"
      ],
      "js": [
        "content_script.js",
        "analytics.js"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "materialize.min.css"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}