{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "eOcontent",
  "description": "Your AI assistant",
  "version": "1.2.0",
  "action": {
    "default_title": "Click Me",
    "default_popup": "index.html"
  },
  "icons": {
    "16": "image/logo.png",
    "48": "image/logo.png",
    "128": "image/logo.png"
  },
  "permissions": [
    "storage",
    "tabs",
    "scripting",
    "activeTab"
  ],
   "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "externally_connectable": {
  "matches": ["*://eocontent.com/*"]
  },
  "content_scripts": [
    {
     "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
