    "permissions": [
        "storage"
      ],
    "icons": {
        "48": "icon.png"
      },
    "action": {
        "default_icon": {
          "128": "icon.png"
        },
        "default_title": "Filtered Links",
        "default_popup": "popup.html"
      }
  }
  