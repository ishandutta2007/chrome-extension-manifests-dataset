    "manifest_version": 3,
    "name": "Hacker News Filter",
    "version": "1.1.0",
    "description": "Filter out AI and GPT submissions from Hacker News",
    "content_scripts": [
      {
        "matches": ["*://news.ycombinator.com/*"],
        "js": ["contentScript.js"]
      }
