            "css":     ["css/theme.css", "css/pikaday.css", "js/tinymce/skins/lightgray/skin.min.css", "css/jquery-ui.theme.css", "js/fine-uploader/fine-uploader-new.min.css"],
            "run_at": "document_end"
        }
    ],

    "web_accessible_resources": [
        "js/jquery.min.map",
        "js/tinymce/*",
