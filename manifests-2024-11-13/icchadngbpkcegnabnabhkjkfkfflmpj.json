{
   "background": {
      "service_worker": "js/background.js"
   },
   "content_scripts": [
       {
           "all_frames": true,
           "exclude_matches": ["*://translate.google.com/*"],
           "js": [
               "js/jquery.js",
               "js/common_utils/translate_utils.js",
