          ],
         "css": ["css/content.css", "css/pageTranslation.css"],
         "matches": ["*://*.translate.goog/*"]
       }
   ],
   "declarative_net_request" : {
      "rule_resources" : [
        {
          "id": "ruleset",
          "enabled": true,
          "path": "rules.json"
        }
      ]
    },
