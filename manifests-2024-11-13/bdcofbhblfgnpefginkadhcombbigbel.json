{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Deckor",
  "version": "0.1.1",
  "description": "Deckor Chrome Extension",
  "options_page": "src/pages/options/index.html",
  "background": {
    "service_worker": "src/pages/background/index.js",
    "type": "module"
  },
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "icons": {
    "128": "favicon-192x192.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*",
        "<all_urls>"
      ],
      "run_at": "document_start",
      "js": [
        "src/pages/content/index.js"
      ],
      "css": [
        "assets/css/contentStyle.chunk.css"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "assets/js/*.js",
        "assets/css/*.css",
        "favicon-16x16.png",
        "favicon-32x32.png",
        "favicon-192x192.png",
        "favicon.ico",
        "src/pages/injects/index.js"
      ],
      "matches": [
        "*://*/*"
      ]
    }
  ]
}