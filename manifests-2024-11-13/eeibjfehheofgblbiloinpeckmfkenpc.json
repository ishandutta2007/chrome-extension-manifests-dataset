      ],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "icons": {
    "128": "src/asset/images/icon_128x128_활성.png"
  },
  "web_accessible_resources": [
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
