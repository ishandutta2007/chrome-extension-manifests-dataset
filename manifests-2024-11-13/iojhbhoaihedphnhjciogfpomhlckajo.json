{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Code Smart",
  "version": "0.14",
  "description": "Unlock your coding potential with CodeSmart: hints, examples, solutions, advanced analysis, and personalized mentorship.",
  "homepage_url": "https://www.codesmart.in/login",
  "permissions": ["storage", "activeTab"],
  "host_permissions": ["https://www.codesmart.in/*", "https://leetcode.com/*"],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "assets/icon16.png",
      "32": "assets/icon32.png",
      "128": "assets/icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources": [{
    "resources": ["index.css"],
     "matches":["<all_urls>"]
  },
  {
    "resources": ["tailwind.css"],
     "matches":["<all_urls>"]
  },
  {
    "resources": ["assets/*"],
     "matches":["<all_urls>"]
  }

],
  "content_scripts": [
    {
      "matches": ["https://leetcode.com/problems/*"],
      "run_at": "document_idle",
      "js": ["content2.js"]
    }
  ]
}