{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "韩国电商采集插件",
  "version": "1.0.4",
  "description": "本插件是一款强大的商品信息采集工具,可以从Naver、Coupang和Poizon等热门电商平台快速获取商品数据。它能够采集商品的价格、主图、详情图等关键信息,为您的商品分析和决策提供有力支持。",
  "permissions": [
    "activeTab"
  ],
  "host_permissions": [
    "http://localhost:17788/"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://smartstore.naver.com/*",
        "https://brand.naver.com/*",
        "https://www.coupang.com/vp/products*",
        "https://www.poizon.com/product/*"
      ],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ],
  "background": {
    "service_worker": "background.js"
  }
}
