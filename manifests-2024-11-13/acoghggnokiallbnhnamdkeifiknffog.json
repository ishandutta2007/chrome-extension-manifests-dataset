{
"update_url": "https://clients2.google.com/service/update2/crx",

 "manifest_version": 2,

 "name": "BlackMuffin Single-video Album Controls",
 "description": "Control single-video albums on Youtube",
 "version": "1.1",

 "page_action": {
  "default_icon": "images/icon-32.png",
  "default_popup": "popup.html",
  "default_title" : "BlackMuffin Single-video Album Controls"
 },
 "background": {
  "persistent": true,
  "scripts": ["js/background.min.js"]
 },
 "content_scripts": [{
  "matches" : ["https://www.youtube.com/*"],
  "js": [
   "js/lib/jquery-3.2.1.min.js",
   "js/controls.min.js"
  ],
  "css": [
   "css/controls.min.css"
  ]
 }],
 "permissions": [
  "activeTab",
  "tabs",
  "storage",
  "https://www.youtube.com/"
 ],
 "icons": {
  "16": "images/icon-16.png",
  "32": "images/icon-32.png",
  "64": "images/icon-64.png"
 },
 "web_accessible_resources": [
  "images/*.png"
 ]
}