{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "Databroker Chrome Extension",
  "description": "This extension insert Ad div to the top of the html body for correct service provider",
  "version": "1.0.1",
  "browser_action": {
    "default_icon": "Letter-D-grey-icon-32.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "cookies",              
    "activeTab",
    "http://localhost:3003/",
    "http://www.betacitizens.eu/"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "contentscript.js"
      ],
      "run_at": "document_end",
      "all_frames": false
    }
  ],
  "icons": {
    "16": "Letter-D-blue-icon-16.png",
    "32": "Letter-D-blue-icon-32.png",
    "48": "Letter-D-blue-icon-48.png",
    "128": "Letter-D-blue-icon-128.png"
  },
  "web_accessible_resources": [
     "images/*.png"
  ]
}
