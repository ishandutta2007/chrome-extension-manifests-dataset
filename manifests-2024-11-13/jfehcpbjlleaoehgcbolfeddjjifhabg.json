{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Tabs manager",
  "description": "An application for managing web tabs",
  "version": "1.4",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "64": "icon64.png",
    "128": "icon128.png"
  },
  "action": {
    "default_popup": "index.html?render_type=popup",
    "default_icon": "icon128.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html?render_type=full"
  },
  "background": {
    "service_worker": "chrome/background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://*/*", "http://*/*"],
      "js": ["chrome/http-intercept.js"],
      "all_frames": true,
      "run_at": "document_start",
      "world": "MAIN",
      "exclude_matches": ["http://*/*.csv", "http://*/*.json", "http://*/*.xml", "https://*/*.csv", "https://*/*.json", "https://*/*.xml"]
    },
    {
      "matches": ["https://*/*", "http://*/*"],
      "js": ["chrome/inject.js"],
      "all_frames": true,
      "run_at": "document_start",
      "world": "ISOLATED",
      "exclude_matches": ["http://*/*.csv", "http://*/*.json", "http://*/*.xml", "https://*/*.csv", "https://*/*.json", "https://*/*.xml"]
    }
  ],
  "permissions": ["activeTab", "declarativeNetRequest"],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "declarative_net_request": {
    "rule_resources": [{
      "id": "ruleset_1",
      "enabled": true,
      "path": "rules_1.json"
    }]
  }
}
