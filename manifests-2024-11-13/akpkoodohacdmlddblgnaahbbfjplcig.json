      ],
      "js": [
        "control-panel.js",
        "dark-mode-content.js"
      ],
      "css": [
        "assets/style-injector.css"
      ],
      "run_at": "document_start"
    }
  ],
  "action": {
    "default_title": "__MSG_title__"
  },
  "background": {
    "service_worker": "background.js"
  },
  "options_page": "./options.html",
  "web_accessible_resources": [
    {
      "resources": [
        "static/*",
        "assets/*.svg",
        "assets/*.png",
        "assets/*.css",
        "assets/*.otf"
      ],
      "matches": ["<all_urls>"]
    }
  ]
