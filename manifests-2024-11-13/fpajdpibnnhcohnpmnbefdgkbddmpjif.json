{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 3,
  "name": "Image Link Grabber",
  "default_locale": "en",
  "description": "Grab all image links from webpage.",
  "version": "1.0",
  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
    "content_scripts":[
    {
      "run_at": "document_end",
      "all_frames": false,
      "js": ["content.js"],
      "matches": [          
      "<all_urls>"]
    }
  ]
}