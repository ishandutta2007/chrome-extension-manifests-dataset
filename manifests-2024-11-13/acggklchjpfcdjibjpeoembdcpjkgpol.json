{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "ChatNoti",
  "version": "1.3.4",
  "description": "Add sound alarms and Table of Contents to ChatGPT",
  "icons": {
    "128": "icons/128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "icons/128.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["https://chatgpt.com/*"],
      "js": ["translations.js", "content.js"]
    }
