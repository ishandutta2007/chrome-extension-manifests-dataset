  "permissions": ["storage"],
  "host_permissions": ["https://chatgpt.com/*"],
  "web_accessible_resources": [
    {
      "resources": ["sounds/*.wav"],
      "matches": ["https://chatgpt.com/*"]
    }
  ]
}
