{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "My Efforts",
    "version": "1.2",
    "description": "Extracts job info and fills out forms automatically",
    "permissions": ["activeTab", "scripting", "storage", "webNavigation", "contextMenus"],
    "host_permissions": [
        "https://*.job-room.ch/*"
    ],
    "background": {
        "service_worker": "background.js",
        "type": "module"
    },
    "action": {
        "default_title": "Click to start extraction",
        "default_icon": {
            "16": "icon16.png",
            "48": "icon48.png",
            "128": "icon128.png"
        }
    },
    "content_scripts": [
        {
            "matches": ["*://*.job-room.ch/work-efforts*"],
            "js": ["content.js"]
        }
    ]
}
  