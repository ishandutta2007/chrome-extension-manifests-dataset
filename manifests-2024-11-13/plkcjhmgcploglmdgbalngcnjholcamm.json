rating: 5
rating_count: 1
user_count: 6
release_date: '2024-06-10T10:15:32.000Z'
size: 483KiB
languages:
  - English
description: >-
  Customize your Netflix experience with Custom Tudum! Replace the Netflix tudum
  with your own sound.
publisher_account: Rajveer Sodhi
extension_website: rajveersodhi.com
---

{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "Custom Tudum",
    "description": "Customize your Netflix experience with Custom Tudum! Replace the Netflix tudum with your own sound.",
    "version": "1.0.0",
    "action": {
        "default_popup": "templates/popup.html",
        "default_icon": "images/logo.png"
    },
    "background": {
        "service_worker": "scripts/background.js"
    },
    "icons": {
        "16": "images/logo.png",
        "32": "images/logo.png",
        "48": "images/logo.png",
        "128": "images/logo.png"
    },
    "permissions": [
        "storage",
        "activeTab",
        "scripting",
        "offscreen"
    ],
    "host_permissions": [
        "https://www.netflix.com/watch/*"
    ],
    "offscreen_documents": [
        {
            "matches": ["*://*/*"],
            "url": "templates/offscreen.html"
        }
    ],
    "content_scripts": [
        {
            "matches": [
                "https://www.netflix.com/watch/*"
            ],
            "js": [
                "scripts/content.js"
            ]
        }
    ]
}