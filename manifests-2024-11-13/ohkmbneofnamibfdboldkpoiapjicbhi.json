{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "RapidQ",
  "description": "Runs a script automation!",
  "version": "1.0",
  "default_locale": "en",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/logo-small-16.png",
      "48": "images/logo-small-48.png",
      "128": "images/logo-small-128.png"
    }
  },
  "icons": {
    "16": "images/logo-small-16.png",
    "32": "images/logo-small-32.png",
    "48": "images/logo-small-48.png",
    "128": "images/logo-small-128.png"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "scripting",
    "storage",
    "cookies"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "js": [
        "content-script.js",
        "lib/jquery.min.js",
        "lib/jquery-ui.min.js",
        "auto-manager.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "all_frames": true,
      "run_at": "document_idle",
      "match_about_blank": true
    }
  ],
  "host_permissions": [
    "<all_urls>",
    "https://*.google.com/",
    "http://*/",
    "https://*/"
  ],
  "externally_connectable": {
    "matches": [
      "<all_urls>"
    ]
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'; connect-src *; frame-src *"
  }
}