        {
            "matches": ["http://*/*","https://*/*"],
            "js": ["js/script.js"]
        },
        {
            "matches": ["*://grepless.com/new-story"],
            "js": ["js/grepless.js"]
        }
    ],
    
    "permissions": ["storage","activeTab"]
