{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "V-Day Blocker",
  "description": "Block any Valentine related content seamlessly",
  "version": "1.3.2",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "webNavigation"
  ],
  "background": {
    "service_worker": "./static/js/background.js"
  },
  "host_permissions": [
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "./static/js/content.js"
      ],
      "all_frames": true
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "/model/*"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  }
}