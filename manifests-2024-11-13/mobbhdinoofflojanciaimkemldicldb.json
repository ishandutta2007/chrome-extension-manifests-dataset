{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Social Media Muffler",
  "version": "2.1",
  "description": "Overlay dog images on social media content.",
  "permissions": ["storage", "activeTab"],
  "host_permissions": [
    "*://*.x.com/*",
    "*://*.twitter.com/*",
    "*://*.instagram.com/*",
    "*://*.youtube.com/*",
    "https://docs.google.com/*"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["*://*.x.com/*", "*://*.twitter.com/*", "*://*.instagram.com/*", "*://*.youtube.com/*"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ]
}