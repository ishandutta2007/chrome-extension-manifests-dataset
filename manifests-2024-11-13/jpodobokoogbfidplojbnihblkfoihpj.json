{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "default_locale": "en",

  "name": "__MSG_extension_name__",
  "description": "__MSG_extension_description__",
  "version": "2.0.1",


  "icons": {
      "48": "images/idside-icon-48.png",
      "96": "images/idside-icon-96.png",
      "128": "images/idside-icon-128.png",
      "256": "images/idside-icon-256.png",
      "512": "images/idside-icon-512.png"
  },

  "background": {
      "service_worker": "background.js",
      "type": "module"
  },
  "externally_connectable": {
      "matches": ["*://*/*"]
  },

  "content_scripts": [{
      "js": [
          "content.js"],
      "css": [
          "content.css"
      ],
      "all_frames": true,
      "match_about_blank": true,
      "matches": ["<all_urls>"],
      "run_at": "document_start"
  }],
  "web_accessible_resources": [{
     "matches": ["<all_urls>"],
     "resources": [
       "Tools.js",
       "CookieManager.js",
       "Matcher.js",
       "Consent.js",
       "Action.js",
       "Detector.js",
       "CMP.js",
       "ConsentEngine.js",
       "contentScript.js",
       "GDPRConfig.js",
       "UserManager.js"
     ]
   }],
  "action": {
      "default_title": "IDSide",
      "default_popup": "popup.html",
      "default_icon": {
          "16": "images/idside_toolbar-icon-16.png",
          "19": "images/idside_toolbar-icon-19.png",
          "32": "images/idside_toolbar-icon-32.png",
          "38": "images/idside_toolbar-icon-38.png",
          "48": "images/idside_toolbar-icon-48.png",
          "72": "images/idside_toolbar-icon-72.png"
      }
  },
  "host_permissions": [
    "<all_urls>"
  ],
  "permissions": [
      "activeTab",
      "tabs",
      "storage",
      "scripting"]
}
