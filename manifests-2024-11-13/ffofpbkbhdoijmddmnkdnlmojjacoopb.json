    "web_accessible_resources": [
      {
        "resources": ["images/*"],
        "matches": ["<all_urls>"]
      }
    ],
    "action": {
      "default_popup": "./popup.html",
      "default_icon": {
        "16": "images/perceptive-logo_16.png",
        "48": "images/perceptive-logo_48.png",
        "128": "images/perceptive-logo_128.png"
      }
    }
