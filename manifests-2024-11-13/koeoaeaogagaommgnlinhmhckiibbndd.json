  "permissions": ["activeTab", "storage", "tabs", "scripting", "cookies"],
  "host_permissions": ["*://*/*"],
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Nachonacho Web Extension",
