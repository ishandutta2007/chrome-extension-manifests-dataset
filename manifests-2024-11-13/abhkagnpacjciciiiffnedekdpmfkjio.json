    "description": "Support for enhanced Chrome interactions within CRA Health.",
    "page_action": {
        "default_icon": "cra48.png"
    },
    "permissions": [
     "activeTab","<all_urls>"
    ],
    "background": {
        "scripts": ["jquery-2.2.4.js", "background.js"]
    },
    "content_scripts": [{
        "matches": ["http://*/*", "https://*/*"],
        "js": ["jquery-2.2.4.js","contentscript.js"],
        "run_at": "document_idle",
        "all_frames": false
    }],
