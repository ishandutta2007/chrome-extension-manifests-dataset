{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "AdsyBoost - enhanced Etsy Ads management",
  "version": "1.2.5",
  "description": "Boost your Etsy Ads success with optimized features and insights.",
  "permissions": ["tabs", "scripting", "webNavigation", "storage"],
  "host_permissions": ["https://www.etsy.com/*"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://www.etsy.com/your/shops/me/advertising?*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    },
    {
      "matches": ["https://www.etsy.com/your/shops/me/advertising/listings/*"],
      "js": ["detailed-listing-page.js"],
      "run_at": "document_idle"
    },
    {
      "matches": ["https://www.etsy.com/your/shops/me/listing-editor/edit/*"],
      "js": ["edit-listing-page.js"],
      "run_at": "document_idle"
    },
    {
      "matches": ["https://www.etsy.com/*"],
      "css": ["styles.css"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "icons/icon128.png"
      ],
      "matches": ["<all_urls>"]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
