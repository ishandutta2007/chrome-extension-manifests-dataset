{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,

  "name": "__MSG_appName__",
  "description": "__MSG_appDesc__",
  "version": "1.0.0",

  "default_locale": "en",

  "icons": {
    "48": "icon.png",
