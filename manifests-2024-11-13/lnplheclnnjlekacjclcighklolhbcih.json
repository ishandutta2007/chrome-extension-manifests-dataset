{
"update_url": "https://clients2.google.com/service/update2/crx",

    "name": "TopBraid Browser Extension",
    "version": "0.3.1",
    "description": "Interact from your current web page with TopBraid EDG knowledge graphs.",
    "manifest_version": 2,
    "permissions": [
        "activeTab",
        "contextMenus",
        "history",
        "storage",
        "tabs",
        "windows",
        "http://*/",
        "https://*/"
    ],
    "background": {
        "scripts": [
            "jquery.min.js",
            "core.js", 
            "background.js"
        ],
        "persistent": false
    },
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "css": [
                "core.css"
            ],
            "js": [
                "jquery.min.js",
                "core.js",
                "contentScript.js"
            ],
            "run_at": "document_idle"
        }
    ],
    "content_security_policy": "script-src 'self' blob: filesystem: http://localhost; object-src 'self'",
    "page_action": {
        "default_title": "TopBraid Browser Extension",
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/logo-16.png",
            "32": "images/logo-32.png"
        }
    },
    "icons": {
        "16": "images/logo-16.png",
        "32": "images/logo-32.png"
    }
}