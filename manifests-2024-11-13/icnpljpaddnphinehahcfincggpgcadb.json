{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Jisho Kioku",
  "manifest_version": 2,
  "version": "0.3.5",
  "description": "Remembers kanji looked up in Jisho.org and other stuff.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "options_page": "options/options.html",
  "permissions": [
    "storage", "unlimitedStorage"
  ],
  "background": {
    "scripts": ["event-page.js"],
    "persistent": false
  },
  "content_scripts": [{
    "matches": ["*://classic.jisho.org/kanji/radicals"],
    "js": ["classic-jisho-kioku.js"],
    "css": ["classic-jisho-kioku.css"]
  }, {
    "matches": ["*://beta.jisho.org/*", "*://jisho.org/*"],
    "js": [
      "kioku-data.js",
      "kana-table.js",
      "radical-table.js",
      "new-jisho-radicals.js",
      "new-jisho-search.js",
      "new-jisho-kanji.js",
      "new-jisho-deck.js",
      "new-jisho-kioku.js"
    ],
    "css": ["new-jisho-kioku.css", "new-jisho-radicals.css"]
  }]
}