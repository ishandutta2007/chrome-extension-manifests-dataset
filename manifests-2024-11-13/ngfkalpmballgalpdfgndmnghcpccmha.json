      "js": [
        "lib/hh.js",
        "scripts/pin_store.js",
        "scripts/jira_pin_store.js",
        "scripts/confluence_pin_store.js",
        "scripts/decorator.js",
        "scripts/jira_decorator.js",
        "scripts/confluence_decorator.js",
        "scripts/index.js"
      ],
      "css": [
        "styles/index.css"
      ],
      "matches": [
        "https://*.atlassian.net/*"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "images/icon-*.png"
      ],
      "matches": [
        "https://*/*"
      ]
    }
