    "manifest_version": 3,
    "name": "Lyricity",
    "version": "1.0",
    "description": "Lyrics for every song on YouTube",
    "icons": {
        "16": "images/favicon.png",
        "24": "images/favicon.png",
        "32": "images/favicon.png",
        "48": "images/favicon.png",
