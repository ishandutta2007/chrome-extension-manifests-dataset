    "background": {
        "service_worker": "scripts/src/service-worker.js"
    },
    "action": {
        "default_title": "Click to open Lyricity"
    },
    "permissions": [
        "sidePanel",
        "tabs",
        "activeTab",
        "scripting"
    ],
    "commands": {
        "_execute_action": {
            "suggested_key": {
                "default": "Ctrl+K",
                "mac": "Command+K"
            }
        }
    },
    "host_permissions": [
        "https://*.genius.com/*",
        "https://*.youtube.com/*"
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "pages/popup.html",
                "styles/popup.css",
                "scripts/src/popup.js"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ]
