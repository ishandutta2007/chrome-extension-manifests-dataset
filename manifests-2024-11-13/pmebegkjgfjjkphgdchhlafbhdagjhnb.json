{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "Job Application Tracker",
    "version": "1.2",
    "description": "Track your job applications manually",
    "permissions": [
      "activeTab"
     
      
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_icon": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
      }
    },
    "content_scripts": [
      {
        "matches": ["https://www.indeed.com/*","https://www.monster.com/*","https://www.dice.com/*","https://www.linkedin.com/*","https://www.glassdoor.com/*"],
        "js": ["contentScript.js"],
        "css": ["contentStyle.css"]
      }
    ],
    "icons": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    },
    "web_accessible_resources": [
      {
        "resources": ["sidebar.html", "sidebar.js", "contentStyle.css", "firebase/*"],
        "matches": ["https://www.indeed.com/*","https://www.monster.com/*","https://www.dice.com/*","https://www.linkedin.com/*","https://www.glassdoor.com/*"]
      }
    ],
    "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self'"
      
    }
  }
  