                "*://superuser.com/questions/*",
                "*://serverfault.com/questions/*",
                "*://askubuntu.com/questions/*"
            ],
            "css": ["content_style.css"],
            "js": ["content_script.js"],
            "run_at": "document_start"
        }
    ]
