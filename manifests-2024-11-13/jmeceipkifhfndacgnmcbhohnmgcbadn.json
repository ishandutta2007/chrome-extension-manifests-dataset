{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "BookmarkNavigator_书签搜索器",
  "version": "1.5",
  "description": "A tool to search and manage bookmarks.",
  "default_locale": "en",
  "permissions": [
    "bookmarks",
    "history",
    "storage",
    "activeTab",
    "favicon"
  ],
  "host_permissions": [
    "https://www.google.com/",
    "https://ssl.gstatic.com/"
  ],
  "web_accessible_resources": [
    {
      "resources": ["images/*", "_favicon/*"],
      "matches": ["<all_urls>"]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'; img-src 'self' data: https:;"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+B",
        "mac": "Command+B"
      },
      "description": "Open the BookmarkNavigator extension popup"
    }
  }
}