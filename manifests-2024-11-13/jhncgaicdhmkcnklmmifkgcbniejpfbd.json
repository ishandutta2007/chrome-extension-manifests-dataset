{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,

  "name": "No News Feed (Free Beta)",
  "short_name": "No News Feed",
  "description": "Hides the news feed on facebook.com & the new item indicator on twitter.",
  "version": "0.2.0",
 
  "background" : {
    "scripts" : [ "bg.js" ],
    "persistent" : false
  },

  "content_scripts": [
    {
      "matches": ["https://*.facebook.com/*"],
      "js": ["newsFeed.js"],
      "run_at": "document_end"
    },
    {
      "matches": ["https://twitter.com/*"],
      "js": ["twtr.js"],
      "run_at": "document_end"
    }
  ],

  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "64": "icons/icon64.png",
    "128": "icons/icon128.png"
  },

  "page_action": {
    "default_icon": {
      "19": "icons/icon19.png",
      "38": "icons/icon38.png"
    }
  },
  
  "permissions": [
    "webNavigation"
  ]
}
