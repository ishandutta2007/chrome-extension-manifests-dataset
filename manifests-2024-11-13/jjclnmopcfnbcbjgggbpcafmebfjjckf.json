 "version": "0.0.1",
 "manifest_version": 3,
 "description": "The only personalized career-building destination that lets you learn skills and earn money at the same time.",
    "action": {
  "default_icon": "icon-48x48.png",
  "default_title": "Audo - Chrome Extension"
 },
 "icons": { 
