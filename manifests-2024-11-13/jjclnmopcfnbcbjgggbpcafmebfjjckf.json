  "type": "module"
 },
    "content_scripts": [
      {
       "all_frames": true,
        "matches": [
         "https://*/*", 
         "http://*/*"
        ],
        "js": [
         "lib/jquery-3.2.1.min.js", 
         "Status.js", 
         "Credentials.js",
            "Parameters.js",
            "Notice.js",
   "content.js"
        ],
        "run_at": "document_end"
      }
    ],
    "web_accessible_resources": [{
        "resources": [
   "manifest.json",
   "lib/jquery-3.2.1.min.js",
   "style.css",
   "frame.html",
   "notice.html",
   "sidebar-toggle.png",
   "transparent.png"
  ],
        "matches": ["<all_urls>"],
  "use_dynamic_url": true
 }],
 "externally_connectable": {
  "matches": ["*://*.audo.com/*"]
 },
 "permissions": [
  "storage",
  "notifications",
        "tabs",
        "activeTab",
  "scripting"
 ],
 "host_permissions": [
