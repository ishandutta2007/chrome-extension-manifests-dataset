  },
  "web_accessible_resources": [
    {
      "resources": ["*.svg", "*.js",
        "mapShowTraffic.js", "mapHideTraffic.js",
        "mapShowTransit.js", "mapHideTransit.js",
        "mapShowBike.js", "mapHideBike.js",
