{
"update_url": "https://clients2.google.com/service/update2/crx",

    "name": "PreVeil for Gmail",
    "version": "1.3.14",
    "description": "PreVeil browser extension for Gmail",
    "author": "PreVeil",
    "homepage_url": "https://github.com/PreVeil/gmail-plugin#readme",
    "manifest_version": 3,
    "default_locale": "en",
    "icons": {
        "16": "images/icon_encryption_16.png",
        "48": "images/icon_encryption_48.png",
        "128": "images/icon_encryption_128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://mail.google.com/*"
            ],
            "js": [
                "gmail.js",
                "inject.js"
            ],
            "run_at": "document_end"
        }
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_icon": "images/icon_encryption_32.png",
        "default_title": "PreVeil for Gmail",
        "default_popup": "views/popup.html"
    },
    "devtools_page": "views/devtools.html",
    "options_page": "views/options.html",
    "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'self';"
    },
    "permissions": [
        "activeTab",
        "background",
        "scripting"
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "pageWorld.js",
                "images/*",
                "vendor/handle-xhr.js"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ],
    "host_permissions": [
        "http://127.0.0.1:4002/",
        "https://collections.preveil.com/",
        "https://mail-attachment.googleusercontent.com/",
        "https://mail.google.com/*"
    ]
}
