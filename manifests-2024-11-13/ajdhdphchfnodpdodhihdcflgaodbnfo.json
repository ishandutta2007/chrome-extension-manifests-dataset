        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "commands": {
        "simulate-execute-button-click": {
            "suggested_key": {
                "default": "Ctrl+Shift+X",
                "mac": "Command+Shift+X"
            },
            "description": "Simulate the execute button click"
        },
        "simulate-submit-button-click": {
            "suggested_key": {
                "default": "Ctrl+Shift+F",
