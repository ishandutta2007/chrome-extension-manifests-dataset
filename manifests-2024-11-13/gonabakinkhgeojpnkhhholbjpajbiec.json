    "matches": [
        "<all_urls>"
    ],
    "js": ["content.js"]
  }],
  "icons": {
    "16": "/assets/icon_128.png",
    "32": "/assets/icon_128.png",
    "48": "/assets/icon_128.png",
    "128": "/assets/icon_128.png"
  },
  "web_accessible_resources": [{
    "resources": ["/assets/*"],
    "matches": ["<all_urls>"]
  }],
  "permissions": ["storage", "tabs", "activeTab"]
