"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Hidden Eye",
  "description": "hide/view HTML elements on a web page.",
  "version": "0.1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "./views/index.html",
    "default_icon": {
      "16": "/assets/icon_128.png",
