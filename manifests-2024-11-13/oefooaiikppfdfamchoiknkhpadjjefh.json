{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Download Defender",
  "version": "1.2",
  "description": "Download manager to block downloads based on a blocked term, file name, or extension.",
  "permissions": [
    "downloads",
    "storage",
    "notifications",
    "windows"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon481.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon481.png",
    "128": "icons/icon128.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["block_notification.html"],
      "matches": ["<all_urls>"]
    }
  ]
}
