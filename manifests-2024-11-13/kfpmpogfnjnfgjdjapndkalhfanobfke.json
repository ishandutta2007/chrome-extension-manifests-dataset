      "resources": [
        "*.json",
        "favicon.ico"
      ]
    }
  ],
  "content_security_policy": {
    "extension_pages": "default-src 'self' 'unsafe-inline'; connect-src 'self' https://trackyourorders.net/; script-src 'self'; object-src 'self'; img-src 'self' ;"
  }
