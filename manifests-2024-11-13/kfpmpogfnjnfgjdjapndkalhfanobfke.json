{
"update_url": "https://clients2.google.com/service/update2/crx",

  "version": "1.0.4",
  "manifest_version": 3,
  "short_name": "Search Manualy",
  "name": "Search Manualy",
  "background": {
    "service_worker": "install_service.js"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": [
    "search",
    "storage"
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "<all_urls>"
