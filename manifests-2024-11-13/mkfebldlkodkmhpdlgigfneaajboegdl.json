    "name": "froglogic Squish Integration",
    "description": "Enables Qt QA Squish GUI Tester to drive Chrome for performing automated GUI tests (Manifest V2 version).",
    "version": "2.1.9",
    "manifest_version": 2,
    "permissions": [ "tabs", "browsingData", "http://*/*", "https://*/*", "webNavigation", "<all_urls>", "storage", "nativeMessaging", "webRequest", "webRequestBlocking" ],
    "icons" : {
        "16": "webext_icon16.png",
        "32": "webext_icon32.png",
        "48": "webext_icon48.png",
        "64": "webext_icon64.png",
        "128": "webext_icon128.png"
    },
    "content_scripts": [
        {
            "matches": ["http://*/*", "https://*/*"],
            "js": ["ispagehooked.js", "runinwebpage.js"]
        }, {
            "matches": ["http://*/*", "https://*/*"],
            "all_frames": true,
            "run_at": "document_start",
            "js": [
                "wasmarguments.js"
            ]
        }
      ],
    "options_ui": {
        "page": "options.html",
        "chrome_style": true
      },
    "background": { "scripts": [
            "jsonipc.js",
            "sharedfunctions.js",
            "sharedrequests.js",
            "background.js"
        ]
    }
