  "manifest_version": 3,
  "name": "aside",
  "version": "0.0.14",
  "description": "A modular browser extension for developers",
  "background": {
    "service_worker": "./dist/background/index.global.js",
    "type": "module"
  },
  "action": {
    "default_icon": {
      "16": "./assets/logo-16.png",
      "48": "./assets/logo-48.png",
      "128": "./assets/logo-128.png"
