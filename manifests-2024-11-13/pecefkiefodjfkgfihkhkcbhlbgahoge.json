  },
  "icons": {
    "16": "./assets/logo-16.png",
    "48": "./assets/logo-48.png",
    "128": "./assets/logo-128.png"
  },
  "permissions": [
    "storage",
    "unlimitedStorage",
    "webNavigation"
  ],
  "host_permissions": [
    "http://*/",
    "https://*/"
  ],
  "content_scripts": [
    {
      "run_at": "document_start",
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "./dist/contentScripts/index.global.js"
      ]
    }
  ],
  "devtools_page": "./dist/pages/devtools/index.html"
