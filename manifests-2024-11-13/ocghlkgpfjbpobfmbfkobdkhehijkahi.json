{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Scalboost Extension",
  "version": "1.0.8",
  "description": "A basic Chrome extension to communicate with a front-end",
  "permissions": [
    "activeTab",
    "scripting",
    "tabs",
    "storage",
    "tabs"
  ],
  "background": {
    "service_worker": "service-worker.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": [
        "https://adsparo.com/*",
        "https://app.scalboost.com/*",
        "http://localhost:4200/*"
      ],
      "js": [
        "content/content.js"
      ],
      "type": "module"
    }
  ],
  "action": {
    "default_popup": "popup/index.html",
    "default_icon": {
      "16": "icon/logo2.png",
      "32": "icon/logo2.png",
      "48": "icon/logo2.png",
      "128": "icon/logo2.png"
    }
  },
  "host_permissions": [
    "https://adsparo.com/*",
    "https://nichescraper.com/*",
    "https://app.dropkiller.com/*",
    "https://app.minea.com/*",
    "https://app.adspy.com/*",
    "https://elevenlabs.io/*",
    "https://app.dropship.io/*",
    "https://adsparo.com/*",
    "https://my.brain.fm/*",
    "https://app.shophunter.io/*",
    "https://app.ecomhunt.com/*",
    "https://www.pipiads.com/es/*",
    "https://www.freepik.es/*",
    "https://nichescraper.com/*",
    "https://artlist.io/*",
    "https://login.junglescout.com/*",
    "https://app.afterlib.com/*"
  ],
  "externally_connectable": {
    "matches": [
      "https://adsparo.com/*",
      "https://app.scalboost.com/*",
      "http://localhost:4200/*"
    ]
  },
  "web_accessible_resources": [
    {
      "resources": ["/icon/logo.png"],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
