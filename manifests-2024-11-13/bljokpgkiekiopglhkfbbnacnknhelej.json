  "manifest_version": 3,
  "name": "Contexto Answer Button",
  "version": "1.0",
  "description": "Injects a button to the contexto.me page to display the answer to the current contexto.",
  "permissions": ["activeTab"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://contexto.me/*"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  },
