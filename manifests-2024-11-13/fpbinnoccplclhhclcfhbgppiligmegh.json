  "permissions": [
    "tabs",
    "storage",
    "activeTab"
  ],
  "host_permissions": [
    "*://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://portal.connect247.vn/*",
        "*://uat.basebs.vn/*",
        "*://lab.connect247.vn/*",
        "*://app.hubspot.com/*",
        "*://ucrm.connect247.vn/*"
      ],
      "js": [
        "dist/contentScripts/index.global.js"
      ]
    }
