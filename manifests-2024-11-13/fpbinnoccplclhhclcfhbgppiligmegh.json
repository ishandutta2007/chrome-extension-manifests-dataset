  "web_accessible_resources": [
    {
      "resources": [
        "dist/contentScripts/style.css"
      ],
      "matches": [
        "*://portal.connect247.vn/*",
        "*://uat.basebs.vn/*",
        "*://lab.connect247.vn/*",
        "*://app.hubspot.com/*",
        "*://ucrm.connect247.vn/*"
