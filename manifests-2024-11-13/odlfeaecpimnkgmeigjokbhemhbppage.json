{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Heretic.School Chrome Extension",
  "version": "1.3",
  "description": "Heretic.School's Chrome Extension to connect your Heretic.School recommended content to your Heretic.School account.",
  "permissions": [
  ],
  "host_permissions": [
    "https://stingray-app-vkh6g.ondigitalocean.app/*",
    "https://heretic.school/*",
    "http://localhost:5001/*",
    "http://localhost:3001/*"
  ],
  "background": {
    "service_worker": "js/background.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["css/output.css"],
      "js": ["js/content.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["js/config.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "externally_connectable": {
    "matches": ["*://localhost/*", "*://heretic.school/*"]
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}