{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Tab Organizer",
  "version": "1.0",
  "description": "Organize your tabs with this extension.",
  "background": { "service_worker": "background.js" },
  "permissions": [
    "tabs",
    "activeTab",
    "idle",
    "scripting",
    "history",
    "storage"
  ],
  "action": {
    "default_popup": "extension.html"
  },
  "icons": {
    "16": "icons16.png",
    "48": "icons48.png",
    "96": "icons96.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "externally_connectable": {
    "matches": ["*://*.google.com/*"]
  }
}
