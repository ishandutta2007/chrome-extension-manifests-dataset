    "browser_action": {
        "default_popup": "popup.html"
    },
    "background": {
        "persistent": false,
        "scripts": ["background.js"]
    },
    "permissions": [
        "tabs",
        "history",
        "bookmarks",
        "cookies",
        "contextMenus",
        "<all_urls>"
    ]
