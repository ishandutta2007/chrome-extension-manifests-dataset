{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "KanbanCast extension by Sahil Maheshwari",
  "version": "1.0.1",
  "description": "Collect feedback for your product.",
  "action": {
    "default_popup": "./src/scripts/popup/popup.html"
  },
  "options_ui": {
    "page": "./src/scripts/options/options.html",
    "open_in_tab": false
  },
  "background": {
    "service_worker": "js/service-worker.js",
    "type": "module"
  },
  "icons": {
    "16": "./assets/icon-16.png",
    "48": "./assets/icon-48.png",
    "128": "./assets/icon-128.png"
  },
  "permissions": [],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "./js/content.js"
      ]
    }
  ],
  "commands": {
    "refresh_extension": {
      "suggested_key": {
        "default": "Ctrl+Space"
      },
      "description": "Refresh Extension"
    }
  },
  "web_accessible_resources": [
    {
      "matches": [
        "https://*/*",
        "http://*/*"
      ],
      "resources": [
        "dist/js/index.js",
        "dist/js/options.js",
        "dist/js/popup.js",
        "dist/js/service-worker.js"
      ]
    }
  ]
