{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Search Console Enhancer",
  "version": "1.2",
  "description": "Enhances the comparison view in Google Search Console by adding calculated differences.",
  
  "permissions": [
    "scripting",
    "activeTab"
  ],
  "host_permissions": [
    "https://search.google.com/*"  
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Run Script"
  },
  "content_scripts": [
    {
      "matches": ["https://search.google.com/*"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
