{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "YT Studio Theme Changer",
    "version": "1.0",
    "description": "Change the background color of the header and sidebar in YouTube Studio.",
    "permissions": ["storage", "activeTab", "scripting"],
    "host_permissions": ["https://studio.youtube.com/*"],
    "action": {
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["https://studio.youtube.com/*"],
        "js": ["content.js"]
      }
    ],
    "icons": {
          "16": "icons/icon16.png",
          "32": "icons/icon32.png",
          "48": "icons/icon48.png",
          "128": "icons/icon128.png"
        }
  }
  