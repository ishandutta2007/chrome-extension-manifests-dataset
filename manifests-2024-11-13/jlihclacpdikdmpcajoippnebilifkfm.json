        "scripts": [
            "options/defaultOptions.js",
            "scripts/background/onRefreshPage.js",
            "scripts/background/onShowIcon.js",
            "scripts/background/onContextMenus.js",
            "scripts/background/onExpandedQueues.js"
        ],
        "persistent": false
    }
