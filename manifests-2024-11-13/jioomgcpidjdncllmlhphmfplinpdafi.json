{
"update_url": "https://clients2.google.com/service/update2/crx",

 "manifest_version": 2,

 "name": "pydocs version loader",
 "description": "An extension to load your prefered version of the python docs",
 "version": "2.0.0",
 "permissions": [
  "storage",
  "webRequest",
  "*://docs.python.org/",
  "webRequestBlocking"
 ],

 "icons": { "16": "icon16.png",
     "48": "icon48.png",
     "128": "icon128.png"
 },

 "browser_action": {
  "default_popup": "popup.html"
 },

 "options_ui": {
  "page": "options.html",
  "open_in_tab": false
 },

 "background": {
  "scripts": ["background.js"],
  "persistent": true
 }
}
