{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Page Content Editor (Ctrl+Q)",
  "version": "1.0",
  "description": "Uma extensão para ativar o modo de design, by: @gabriel_torresr",
  "permissions": ["activeTab"],
  "action": {
    "default_icon": {
      "16": "icon_off_128.png",
      "48": "icon_off_128.png",
      "128": "icon_off_128.png"
    }
  },
  "icons": {
    "16": "icon_off_128.png",
    "48": "icon_off_128.png",
    "128": "icon_off_128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "commands": {
    "toggleDesignMode": {
      "suggested_key": {
        "default": "Ctrl+Q"
      },
      "description": "Toggle Design Mode"
    }
  }
}
