        "https://*.target.com/*",
        "https://target.com/*",
        "https://*.walmart.com/orders*",
        "https://walmart.com/orders*",
        "https://*.lowes.com/*",
        "https://*.lowescdn.com/*",
        "https://*.costco.com/*"
