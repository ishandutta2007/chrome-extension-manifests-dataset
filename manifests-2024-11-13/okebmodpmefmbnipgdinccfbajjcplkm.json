  "manifest_version": 3,
  "name": "linely",
  "$schema": "https://json.schemastore.org/chrome-manifest.json",
  "version": "0.1.2",
  "description": "A happy helper to aggregate your order details from online vendors.",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
