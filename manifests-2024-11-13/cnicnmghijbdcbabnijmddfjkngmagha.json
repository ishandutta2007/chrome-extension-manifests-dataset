{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 2,

    "name": "GF改 Extension",
    "short_name": "GF改",
    "description": "ガールフレンド(仮)用Chrome Extension。小窓プレイ・アイテムログ・ガールメモなどガルフレをより快適にプレイするための拡張です。",
    "version": "0.0.8.2",

    "homepage_url": "http://tools.gfdb.info/",

    "permissions": [
        "tabs",
        "webNavigation",
        "storage",
        "unlimitedStorage",
        "http://vcard.ameba.jp/*"
    ],

    "page_action": {
        "default_icon": {
            "19": "icon_19.png"
        },
        "default_title": "MENU",
        "default_popup": "popup.html"
    },

    "content_scripts": [
        {
            "matches": [
                "http://vcard.ameba.jp/*"
            ],
            "css": ["gf_default.css", "gf_skip.css"],
            "js": ["gf_main.js"],
            "run_at": "document_end"
        },
        {
            "matches": [
                "http://vcard.ameba.jp/item"
            ],
            "js": ["gf_log_bag.js"],
            "run_at": "document_end"
        },
        {
            "matches": [
                "http://vcard.ameba.jp/card"
            ],
            "js": ["gf_girls_memo.js"],
            "run_at": "document_end"
        }
    ],

    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },

    "web_accessible_resources": [
        "gf_skip.js",
        "gf_kill_alert.js"
    ]
}
