{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Side Panel for Gemini",
  "version": "0.5.5",
  "manifest_version": 3,
  "description": "Opens Gemini AI in Google Chrome's side panel",
  "host_permissions": ["*://*.google.com/*", "*://*.googleusercontent.com/*", "*://*.googletagmanager.com/*"],
  "action": {
      "default_icon": {
        "16": "images/icon-16-1.png",
        "128": "images/icon-128-1.png"
      }
    },
  "commands": {
  "_execute_action": {
    "suggested_key": {
        "default": "Alt+G"
      },
    "description" : "Opens the 'Side Panel for Gemini' extension"
    }
  },
  "background": {
    "service_worker": "service-worker.js"
  },
  "icons": {
    "128": "images/icon-128-1.png",
    "16": "images/icon-16-1.png"
  },
  "side_panel": {
    "default_path": "sidepanel.html"
  },
  "permissions": [
    "declarativeNetRequest",
    "sidePanel",
    "storage"
  ],
  "web_accessible_resources": [
    {
      "resources": ["service-worker.js","sidepanel.html"],
      "matches": ["<all_urls>"],
      "use_dynamic_url": true
    }
  ],
  "options_page": "options.html",
  "content_security_policy": {
    "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self'; style-src * 'unsafe-inline' 'unsafe-hashes' 'self'; frame-src 'self' https://gemini.google.com/; connect-src 'self' https://gemini.google.com/;"
  }

}
