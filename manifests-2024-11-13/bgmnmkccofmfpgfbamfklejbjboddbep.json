{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "AstroSafe",
  "version": "1.1.6",
  "description": "Stay safe exploring the internet!",
  "manifest_version": 3,
  "action": {
      "default_popup": "index.html",
      "default_title": "ASTRO"
  },
  "icons": {
      "16": "logo192.png",
      "48": "logo192.png",
      "128": "logo192.png"
  },
  "permissions": [
      "tabs",
      "storage",
      "webNavigation",
      "cookies",
      "idle",
      "http://*/*",
      "https://*/*"
  ],
  "host_permissions": [
      "http://localhost:3000/*",
      "https://*.astrosafe.co/*"
  ],
  "content_scripts": [
      {
          "matches": [
              "<all_urls>"
          ],
          "js": [
              "./static/js/content.js"
          ],
          "css": [
              "overrides.css",
              "font.css"
          ]
      },
      {
          "matches": [
              "*://*.duckduckgo.com/*"
          ],
          "js": [
              "./static/js/duckduckgoHiding.js"
          ],
          "css": [
              "duckduckgoHiding.css"
          ]
      },
      {
          "matches": [
              "*://*.google.com/*"
          ],
          "css": [
              "googleHiding.css"
          ]
      },
      {
        "matches": ["<all_urls>"],
        "js": ["./static/js/pageHiding.js"],
        "run_at": "document_start"
      }
  ],
  "background": {
      "service_worker": "./static/js/service_worker.js",
      "type": "module"
  },
  "web_accessible_resources": [
      {
          "resources": [
              "*.ttf",
              "font.css"
          ],
          "matches": [
              "<all_urls>"
          ]
      }
  ],
  "externally_connectable": {
      "ids": [
          "bgmnmkccofmfpgfbamfklejbjboddbep"
      ],
      "matches": [
          "https://browser.astrosafe.co/*"
      ],
      "accepts_tls_channel_id": false,
      "host_permissions": [
          "<all_urls>"
      ]
  }
}