{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Storyline Course Inspector",
  "description": "A DevTools extension for inspecting storyline courses",
  "version": "0.0.5",
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'none'"
  },
  "devtools_page": "devtools.html",
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources": [
    {
      "resources": ["script.js", "bootstrap.min.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*/*index_lms.html",
        "*://*/*story.html",
        "file:///*/index_lms.html",
        "file:///*/story.html"
      ],
      "js": ["content-script.js"],
      "run_at": "document_end",
      "all_frames": true
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
