    "icons": {
        "48": "icon.png"
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "background": {
        "persistent": false,
        "scripts": ["bg.js", "jquery-1.10.2.min.js"]
    }

