    "manifest_version": 2,

    "name": "Made by Humans",
    "description": "This extension lets you know if your active tab has a humans.txt",
    "version": "1.0",

    "permissions": [
        "tabs",
        "http://*/humans.txt*",
        "https://*/humans.txt*"
