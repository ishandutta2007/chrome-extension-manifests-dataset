{
"update_url": "https://clients2.google.com/service/update2/crx",

  "version": "1.0.2",
  "manifest_version": 3,
  "name": "Learnify",
  "homepage_url": "https://learnfy.world",
  "description": "Uncover Your Hidden Progress. This AI-powered Chrome extension transforms your self-directed online learning into verifiable skills",
  "action": {
    "default_title": "Learnify"
  },
  "background": {
    "service_worker": "content/service-worker.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "js": ["content/parse-2.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Comma",
        "mac": "Command+Comma",
        "windows": "Ctrl+Comma"
      }
    }
  },
  "web_accessible_resources": [
    {
      "resources": ["index.html", "main.js", "main.css"],
      "matches": ["<all_urls>"]
    }
  ],
  "permissions": ["tabs", "activeTab", "storage"],
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  }
}
