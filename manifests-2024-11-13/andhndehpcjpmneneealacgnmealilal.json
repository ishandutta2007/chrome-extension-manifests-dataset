    "content_security_policy": {
        "extension_pages": "frame-ancestors 'none'; script-src 'self' 'wasm-unsafe-eval'; object-src 'none'"
    },
    "name": "HaHa Wallet",
    "description": "HaHa isn't just a regular wallet. We provide the tools that help you stay informed and degen on the go.",
    "version": "1.0.6",
    "manifest_version": 3,
    "permissions": ["storage", "unlimitedStorage", "clipboardWrite", "activeTab", "notifications"],
    "host_permissions": ["file://*/*", "http://*/*", "https://*/*"],
    "action": {
        "default_popup": "/popup.html",
        "default_icon": {
            "16": "images/icon-16.png",
            "32": "images/icon-32.png",
            "48": "images/icon-48.png",
            "128": "images/icon-128.png"
        }
    },
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    },
    "content_scripts": [
        {
            "matches": ["file://*/*", "http://*/*", "https://*/*"],
            "js": ["./scripts/contentscript.js"],
            "run_at": "document_start",
            "all_frames": true
        },
        {
            "matches": ["file://*/*", "http://*/*", "https://*/*"],
            "js": ["./scripts/inpage.js"],
            "run_at": "document_start",
            "all_frames": true,
            "world": "MAIN"
        }
    ],
    "web_accessible_resources": [
        {
            "resources": ["/home.html"],
            "matches": ["<all_urls>"]
        }
    ],
    "background": {
        "service_worker": "./scripts/background.js"
    }
}
