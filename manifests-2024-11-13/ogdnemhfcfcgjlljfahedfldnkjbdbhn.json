            "128": "/images/which-social-icon-128.png"
        }
    },
    "icons": {
        "16": "/images/which-social-icon-16.png",
        "32": "/images/which-social-icon-32.png",
        "48": "/images/which-social-icon-48.png",
        "128": "/images/which-social-icon-128.png"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "css": ["styles.css"],
            "js": ["content-script.js"],
            "all_frames": true
        }
