        "js/content-script.js"
      ],
      "match_about_blank": false,
      "all_frames": true
    }
  ],
  "browser_action": {
    "default_title": "SWATHub Inspector",
    "default_icon": {
      "19": "icons/19.png",
      "38": "icons/38.png"
