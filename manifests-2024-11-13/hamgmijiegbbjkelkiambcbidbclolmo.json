{
"update_url": "https://clients2.google.com/service/update2/crx",

    "name": "wykop blacklist",
    "version": "0.0.0.7",
    "manifest_version": 3,
    "description": "A Chromium extension that expands blocking users on wykop.pl",
    "action": {
        "default_popup": "popup/popup.html"
    },
    "permissions": [
        "storage"
    ],
    "host_permissions": [
        "*://*.wykop.pl/*"
    ],
    "content_scripts": [
        {
            "matches": [
                "*://*.wykop.pl/*"
            ],
            "js": [
                "utils.js",
                "comments.js",
                "overlay.js",
                "blocker.js"
            ]
        },
        {
            "matches": [
                "*://*.wykop.pl/ludzie/*"
            ],
            "js": [
                "profile_overlay.js"
            ]
        }
    ]
}