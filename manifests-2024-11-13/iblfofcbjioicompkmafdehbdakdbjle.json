support_website: https://github.com/astiskala/psp-detector/issues
privacy_policy: https://adamstiskala.com/psp-detector/privacy-policy.html
---

{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "PSP Detector",
  "version": "1.7",
  "description": "Detects which Payment Service Provider (PSP) is used on the webpage and displays its logo.",
  "action": {
    "default_icon": {
      "16": "images/default_16.png",
      "48": "images/default_48.png",
      "128": "images/default_128.png"
    },
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "content.js"
      ],
      "matches": [
        "https://*/*"
      ]
    }
  ],
  "host_permissions": [
    "*://*/*"
  ],
  "icons": {
    "16": "images/default_16.png",
    "48": "images/default_48.png",
    "128": "images/default_128.png"
  },
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "psp-config.json"
      ]
    }
  ]
}