{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Platform RPA",
  "version": "1.0.7",
  "description": "Browser Automation Chrome extension for Platform RPA",
  "permissions": [
    "tabs",
    "nativeMessaging",
    "scripting",
    "webNavigation",
    "downloads",
    "activeTab"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": [ "<all_urls>" ],
      "js": [ "content.js" ],
      "all_frames": true,
      "run_at": "document_start"
    }
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "icons": {
    "16": "Platform16.png",
    "48": "Platform48.png",
    "128": "Platform128.png"
  },
  "web_accessible_resources": [
    {
      "resources": [ "/*" ],
      "matches": [ "https://*/*", "http://*/*" ]
    }
  ],
  "action": {
    "default_icon": {
      "16": "Platform16.png",
      "48": "Platform48.png",
      "128": "Platform128.png"
    }
  }
}
