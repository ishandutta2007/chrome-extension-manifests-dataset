{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Mailshunt",
  "version": "1.0.2",
  "manifest_version": 2,
  "description": "An extension for finding any email address on the web",
  "icons": {
    "192": "logo192.png",
    "512": "logo512.png"
  },
  "permissions": [
    "storage",
    "tabs",
    "webNavigation",
    "http://localhost:3000/*",
    "https://mailshunt.com/*",
    "*://*.mailshunt.com/*"
  ],
  "background": {
    "scripts": [
      "utils/Helper.js",
      "utils/Search.js",
      "background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
    "matches": [
      "https://mailshunt.vercel.app/app",  
      "*://*.mailshunt.com/*", 
      "http://localhost:3000/*"
    ],
    "js": ["utils/Account.js","utils/Search.js","mailshunt-content.js"]
  }
],
  "browser_action": {
    "default_icon": "logo.png",
    "default_popup": "popup.html"
  }
}
