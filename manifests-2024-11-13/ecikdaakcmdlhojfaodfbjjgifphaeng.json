{
  "version": "1.4",
  "background": {
    "service_worker": "js/background.js",
    "type": "module"
  },
  "description": "This extension enables ClickOnce apps in Google Chrome.",
  "content_security_policy": {
    "extension_pages": "default-src 'self'"
  },
  "icons": {
    "16": "icons/e2open-logo-16.png",
    "24": "icons/e2open-logo-24.png",
    "32": "icons/e2open-logo-32.png",
    "48": "icons/e2open-logo-48.png",
    "64": "icons/e2open-logo-64.png",
    "96": "icons/e2open-logo-96.png",
    "128": "icons/e2open-logo-128.png"
  },
  "manifest_version": 3,
  "name": "E2open ClickOnce launcher",
  "permissions": ["declarativeNetRequestWithHostAccess", "nativeMessaging", "webRequest"],
  "host_permissions": ["https://*.e2open.com/*", "https://*.ecvision.com/*"],
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "ruleset_1",
        "enabled": true,
        "path": "./rules/rule_1.json"
      }
    ]
  },
  "content_scripts": [
    {
      "matches": ["https://*.e2open.com/*", "https://*.ecvision.com/*"],
      "js": ["js/content.js"]
    }
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "key": "",
  "web_accessible_resources": [
    {
      "resources": ["images/*"],
      "matches": ["*://*/*"]
    },
    {
      "resources": ["nativeinstall.html", "js/nativeinstall.js", "e2openclickoncehelper.dat"],
      "matches": ["*://*/*"]
    }
  ]
}
