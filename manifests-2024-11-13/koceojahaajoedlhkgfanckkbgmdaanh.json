{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Window Closer for Asana Desktop Links",
  "description": "This extension automatically closes the launched window shared Asana links.",
  "version": "1.1",
  "manifest_version": 3,
  "action": {},
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "run_at": "document_idle",
      "matches": ["https://app.asana.com/-/desktop_app_link*"],
      "js": ["content.js"],
      "css": ["content.css"]
    }
  ],
  "icons": {
    "16": "assets/icon-16.png",
    "32": "assets/icon-32.png",
    "48": "assets/icon-48.png",
    "128": "assets/icon-128.png"
  }
}
