  "manifest_version": 3,
  "name": "Blixem Tab Manager",
  "homepage_url": "https://blixem.io/",
  "description": "Organize, save & share your tabs for easy collaboration with Blixem.",
  "version": "1.0.2",
  "default_locale": "en",
  "permissions": [
    "tabs",
    "sessions",
    "topSites",
    "search",
    "bookmarks",
    "commands",
    "tabGroups",
    "storage",
    "sidePanel"
