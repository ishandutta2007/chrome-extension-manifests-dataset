{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 2,
    "name": "YouTubePlayer",
    "version": "1.0.0",
    "description": "Control YouTube videos across all tabs and windows from extension bar",
    "icons": {
        "16": "images/16.png",
        "48":"images/48.png",
        "128":"images/128.png"
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "background": {
      "scripts": ["background.js"],
      "persistent": true
    },
    "content_scripts": [
        {
          "matches": [
            "*://www.youtube.com/*"
          ],
          "js": ["content.js"],
          "run_at":"document_start"
        }
      ],
    "permissions":[
        "tabs"
    ]
}