{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Quick Mock with MIR",
  "manifest_version": 3,
  "version": "0.1.0",
  "description": "Extract job descriptions from webpages.",
  "permissions": [
    "tabs",
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "https://www.linkedin.com/jobs/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://www.linkedin.com/jobs/*"],
      "js": ["contentScript.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "assets/mirwork.png"
      ],
      "matches": ["*://*/*"]
    }
  ],
  "action": {
    "default_title": "Mirwork quickmock plugin",
    "default_icon": {
      "16": "assets/icon16.png",
      "24": "assets/icon48.png",
      "32": "assets/icon128.png"
    }
  }
}
