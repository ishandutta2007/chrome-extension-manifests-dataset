{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "RideNeutral",
  "description": "Offset carbon emissions for every Uber ride you've taken.",
  "version": "1.0.1",
  "permissions": [
    "*://uber.com/*",
    "*://*.uber.com/*"
  ],
  "background": {
    "persistent": true,
    "scripts": [
      "js/libs/jquery.js",
      "js/background.js",
      "js/common.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "*://uber.com/*",
        "*://*.uber.com/*",
        "https://riders.uber.com/*"
      ],
      "js": [
        "js/libs/jquery.js",
        "js/libs/sweetalert2.all.min.js"
      ]
    }
  ],
  "icons": {
    "64": "images/64.png",
    "32": "images/32.png",
    "38": "images/38.png",
    "128": "images/128.png"
  },
  "browser_action": {
    "default_icon": "images/38.png"
  },
  "homepage_url": "https://jonlu.ca",
  "web_accessible_resources": [
    "js/*",
    "html/*",
    "css/*",
    "fonts/*",
    "images/*"
  ]
}
