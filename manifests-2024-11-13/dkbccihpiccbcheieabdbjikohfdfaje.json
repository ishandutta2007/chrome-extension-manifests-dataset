  },
  "icons" : {
    "128" : "assets/icons/app/128.png",
    "64" : "assets/icons/app/64.png",
    "32" : "assets/icons/app/32.png"
  },
  "web_accessible_resources": [{
    "resources": [
      "*"
    ],
