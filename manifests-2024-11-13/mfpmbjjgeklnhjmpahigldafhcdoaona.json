  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_title": "Taaabs",
    "default_popup": "popup.html"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Alt+S"
      }
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": ["storage", "activeTab", "tabs", "webNavigation", "alarms"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://taaabs.com/*"],
      "js": ["get-auth-data-content-script.js"]
    },
    {
      "matches": ["<all_urls>"],
      "js": [
        "send-prompt-content-script.js",
        "get-parsed-html-content-script.js",
        "get-upsert-bookmark-params-content-script.js",
        "get-window-dimensions-content-script.js",
        "get-selected-text-content-script.js"
      ]
    }
  ],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  }
