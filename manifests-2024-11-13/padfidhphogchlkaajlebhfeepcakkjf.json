{
  "name": "MovaADB for Chrome",
  "version": "1.0.0.0",
  "manifest_version": 2,
  "minimum_chrome_version": "33.0.1715.0",
  "description": "Access local or remote Android device from browser, eclipse, android studio. ",
  "icons": {
    "128": "icon.png",
    "16": "icon.png",
    "48": "icon.png"
  },
  "app": {
    "background": {
      "persistent": true,
      "scripts": [
        "utils/utils.js",
        "net/Socket.js",
        "net/Server.js",
        "adb/Adb.js",
        "adb/AdbClient.js",
        "adb/AdbDevice.js",
        "adb/AdbPull.js",
        "adb/AdbServer.js",
        "adb/AdbSocket.js",
        "adb/initdevice.js",
        "adb/AdbBridge.js"
      ]
    }
  },
  "permissions": [ {
    "fileSystem": [ "write", "retainEntries", "directory" ]
  }, "storage", "system.network", "usb", {
    "usbDevices": [ {
      "productId": 20894,
      "vendorId": 4046
    } ]
  } ],
  "sockets": {
    "tcp": {
      "connect": "*"
    },
    "tcpServer": {
      "listen": "*"
    }
  },
  "externally_connectable": {
    "matches": [
      "https://www.movatesting.com/*",
      "*://localhost:*/*"
    ]
  },
  "update_url": "https://clients2.google.com/service/update2/crx"
}
