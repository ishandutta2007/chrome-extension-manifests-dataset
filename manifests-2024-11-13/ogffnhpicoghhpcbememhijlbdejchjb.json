        "storage"
    ],

    "content_scripts": [
        {
            "js": [
                "js/library/jquery3.7.1.js",
                "js/library/showdown.js",

                "js/dashblox.js",

                "js/pages.js",

                "js/pages/assets.js",
                "js/pages/avatar.js",
                "js/pages/catalog.js",
                "js/pages/discover.js",
                "js/pages/games.js",
                "js/pages/groups.js",
                "js/pages/home.js",
                "js/pages/profile.js",
                "js/pages/settings.js",
                "js/pages/accountsettings.js",
                "js/pages/universal.js",

                "js/util.js"
            ],

            "run_at": "document_start",
            "all_frames": true,
            "matches": ["*://*.roblox.com/*"]
        }
    ]
