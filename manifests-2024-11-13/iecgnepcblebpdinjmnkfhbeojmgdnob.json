{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "精卫拓客助手",
  "description": "精卫拓客助力客户开发",
  "version": "1.3",
  "manifest_version": 3,
  "permissions": [
    "tabs","activeTab", "scripting", "sidePanel",  "debugger", "cookies",   "storage",
    "storage",
    "webNavigation",
    "downloads",
    "declarativeNetRequest",
    "declarativeNetRequestWithHostAccess",
    "alarms"],

  "action": {
    "default_title": "Click to open panel",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon1.png",
      "48": "icon1.png",
      "128": "icon1.png"
    }
  },


  "background": {
    "service_worker": "background.js"
  },
  "host_permissions":["<all_urls>"],
  "content_scripts": [
    {
      "matches": ["*://web.whatsapp.com/*","https://branddb.wipo.int/*"],
      "match_origin_as_fallback": true,
      "css": [
        "css/lib/jquery.modal.min.css",
        "css/content-css.css"
      ],
      "js": [
        "scripts/lib/jquery-3.5.1.slim.min.js",
        "scripts/lib/jquery.modal.min.js",
        "scripts/init.js",
        "scripts/helper.js",
        "scripts/event.js",
        "scripts/business.js",
        "scripts/render.js",
    "scripts/branddbFetch.js"
      ],
      "run_at": "document_end",
      "all_frames": true
    },
    {
      "matches": ["*://www.google.com/search*", "*://www.google.com.hk/search*"],
      "js": [
        "scripts/init.js",
        "scripts/lib/jquery-3.5.1.slim.min.js",
        "scripts/fetch.js"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "matches": ["https://web.whatsapp.com/*"],
      "resources": ["import_tpl.xlsx", "scripts/lib/w.js", "scripts/cs.js"]
    }
  ],
  "externally_connectable": {
    "matches":["<all_urls>"]
  },
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "ruleset_1",
        "enabled": true,
        "path": "rules.json"
      }
    ]
  }
}
