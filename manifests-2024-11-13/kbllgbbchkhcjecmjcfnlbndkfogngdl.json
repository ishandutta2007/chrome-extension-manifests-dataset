  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage"],
  "web_accessible_resources": [{
    "resources": ["injected.js","index.html"],
    "matches": ["*://twitter.com/*","*://x.com/*"]
  }],
  "externally_connectable": {
    "matches": ["*://app.rocketbird.io/*", "*://rocketbird.io/*", "*://twitter.com/*", "*://x.com/*"]
  },
  "action": {}
