      "matches": [
        "*://*.wikipedia.org/*",
        "*://*.wiktionary.org/*",
        "*://*.wikibooks.org/*",
        "*://*.wiktionary.org/*"
      ],
      "js": ["scripts.js"]
    }
  ],
  "default_icon": {
    "16": "/images/wicon16.png",
    "32": "/images/wicon32.png",
    "48": "/images/wicon48.png",
