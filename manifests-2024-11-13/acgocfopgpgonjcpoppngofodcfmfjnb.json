{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "DG Edge - Claim your profile",
    "version": "1.0",
    "icons": {
        "16": "icon-16.png",
        "32": "icon-32.png",
        "48": "icon-48.png",
        "128": "icon-128.png"
    },
    "description": "Claim your GT7 Sport profile automatically with this extension",
    "content_scripts": [
        {
            "matches": [
                "https://www.gran-turismo.com/",
                "https://www.gran-turismo.com/*"
            ],
            "js": [
                "gt7.js"
            ],
            "world": "MAIN"
        },
        {
            "matches": [
                "https://dg-edge.com/",
                "https://dg-edge.com/*",
                "https://www.dg-edge.com/",
                "https://www.dg-edge.com/*",
                "https://dev.hub.dg-edge.com/",
                "https://dev.hub.dg-edge.com/*",
                "https://dev.hub.dg-edge.com:3000/",
                "https://dev.hub.dg-edge.com:3000/*"
            ],
            "js": [
                "edge.js"
            ],
            "world": "MAIN"
        },
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": "icon-128.png"
    },
    "permissions": [
        "activeTab",
        "storage",
        "https://www.gran-turismo.com/",
        "https://www.gran-turismo.com/*",
        "https://dg-edge.com/",
        "https://dg-edge.com/*",
        "https://www.dg-edge.com/",
        "https://www.dg-edge.com/*",
        "https://dev.hub.dg-edge.com/",
        "https://dev.hub.dg-edge.com/*",
        "https://dev.hub.dg-edge.com:3000/",
        "https://dev.hub.dg-edge.com:3000/*"
    ]
}