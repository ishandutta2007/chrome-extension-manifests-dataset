{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "Twitter Follow Tools",
    "description": "a useful extension helping mass follow or unfollow ",
    "version": "0.0.2",
    "icons": {
        "16": "/icons/16.png",
        "32": "/icons/32.png",
        "48": "/icons/48.png",
        "128": "/icons/128.png"
    },
    "background": {
        "service_worker": "/background.js"
    },
    "content_scripts": [
        {
            "matches": [
                "https://*.twitter.com/*"
            ],
            "js": [
                "/content.js"
            ]
        }
    ],
    "web_accessible_resources": [
        {
            "matches": ["<all_urls>"],
            "resources": [
                "/icons/16.png",
                "/icons/32.png",
                "/icons/48.png",
                "/icons/128.png"
            ]
        }
    ],
    "options_page": "options.html"
}