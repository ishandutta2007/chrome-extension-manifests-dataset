{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "Quick FLS",
    "description": "This chrome extension helps in setting object/field level permissions in a salesforce org.",
    "version": "1.3",
    "host_permissions": [
        "https://*.force.com/",
        "https://*.salesforce.com/",
        "https://*.Salesforce-setup.com/*"
      ],
    "permissions": [
        "cookies"
    ],
    "content_scripts": [
        {
          "matches": [
            "https://*.force.com/*",
            "https://*.salesforce.com/*",
            "https://*.Salesforce-setup.com/*"
          ],
          "js": ["content.js"],
          "run_at": "document_end"
        }
    ],
    "background": {
        "service_worker": "/js/service-worker.js"
      }
  }