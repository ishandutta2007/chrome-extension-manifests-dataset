{
"update_url": "https://clients2.google.com/service/update2/crx",

  "short_name": "tab管理",
  "name": "tab manager",
  "manifest_version": 3,
  "version": "1.0",
  "description": "tab管理",
  "permissions": [
    "tabs","storage","notifications"
  ],
  "action": {  
    "default_icon": {  
      "16": "logo_16.png",
      "32": "logo_32.png",
      "48": "logo_48.png",
      "128": "logo_128.png"
    },
    "default_popup": "index.html",  
    "default_title": "Click Me"  
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./content.js"]
    }
  ],
  "background": {
    "service_worker":"background.js",
    "type": "module" 
  },
  "commands": {
    "save_wait": {
      "suggested_key": {
        "default": "Ctrl+Shift+S",
        "mac": "Command+Shift+S"
      },
      "description": "保存到待阅读"
    },
    "tab_rename": {
      "suggested_key": {
        "default": "Ctrl+Shift+R",
        "mac": "Command+Shift+R"
      },
      "description": "重命名"
    },
    "tab_pin":{
      "suggested_key": {
        "default": "Ctrl+Shift+P",
        "mac": "Command+Shift+P"
      },
      "description": "PIN置顶当前tab"
    }
  },
  "start_url": ".",
  "display": "standalone",
  "theme_color": "#000000",
  "background_color": "#ffffff"
}

