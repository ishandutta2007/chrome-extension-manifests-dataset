{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Rekroo",
  "description": "Добавление кандидатов в ваш аккаунт Rekroo с сайтов резюме, таких как hh.ru, career.habr.com, stackoverflow.com, github.com и др.",
  "version": "0.0.1.11",
  "manifest_version": 3,
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/logo16.png",
      "32": "/images/logo32.png",
      "48": "/images/logo48.png",
      "128": "/images/logo128.png"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.hh.ru/*",
        "https://hh.ru/*",
        "https://career.habr.ru/*",
        "https://stackoverflow.com/*",
        "https://github.com/*",
        "https://superjob.ru/*",
        "https://avito.ru/*",
        "https://linkedin.com/*",
        "https://*.rekroo.org/*",
        "https://rekroo.org/*",
        "https://*.rekroo.ru/*",
        "https://rekroo.ru/*",
        "https://facebook.com/*"
      ],
      "js": ["parser.js", "content-script.js"],
      "css": ["content-script.css"]
    }
  ],
  "icons": {
    "16": "/images/logo16.png",
    "32": "/images/logo32.png",
    "48": "/images/logo48.png",
    "128": "/images/logo128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "commands": {
    "Ctrl+M": {
      "suggested_key": {
        "default": "Ctrl+M",
        "mac": "Command+M"
      },
      "description": "Ctrl+M."
    }
  }
}
