{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Aupa Athletic Helper",
  "version": "1.2",
  "description": "Permite que la creación de posts en Aupa Athletic sea más sencilla.",
  "permissions": [
    "activeTab",
    "scripting"
