    "default_popup": "popup.html",
    "default_icon": {
      "256": "icons/icon256.png"
    }
  },
  "web_accessible_resources": [
    {
      "resources": ["images/campo.png"],
      "matches": ["*://www.aupaathletic.com/*"]
    }
  ]
