    },
    {
      "matches": ["https://b.linkskill.nl/*", "https://linkskill.nl/*"],
      "js": ["scripts/jquery-3.7.1.min.js", "scripts/linkskill_content.js"]
    }
  ],
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
