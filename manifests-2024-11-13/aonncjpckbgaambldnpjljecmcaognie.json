{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Search Results Analyzer",
  "version": "1.1",
  "description": "Extracts product data from Walmart, Target, and Amazon search results and provides summaries and CSV downloads.",
  "permissions": [
    "activeTab",
    "scripting",
    "downloads"
  ],
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "48": "icons/icon48.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
          "https://www.walmart.com/search?q*",
          "https://www.amazon.com/s?*",
          "https://www.target.com/s?searchTerm*"
      ],
      "js": ["content.js"]
  }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }  
}
