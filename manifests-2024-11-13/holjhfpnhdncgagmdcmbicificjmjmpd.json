{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "TeleDisMed Chrome Extension",
  "description": "A simple extension that allows you to stream your desktop into TeleDisMed.",
  "version": "1.4.0",
  "minimum_chrome_version": "34",
  "icons": {
    "16": "dismed-logo-16x16.png",
    "48": "dismed-logo-48x48.png",
    "128": "dismed-logo-128x128.png"
  },
  "background": {
        "scripts": [ "background.js" ],
        "persistent": true
  },
  "permissions": [
      "desktopCapture", "https://calendar.google.com/*"
  ],
  "externally_connectable": {
      "matches": [
        "*://teledismed.fr/*"
      ]
  },
  "content_scripts": [
    {
      "matches": ["https://calendar.google.com/calendar/*", "https://outlook.live.com/owa/*"],
      "js": ["jquery.js", "RandomUtil.js", "RoomnameGenerator.js", "meet-calendar.js"],
      "css": ["/css/all.css"],
      "all_frames" : false,
      "run_at" : "document_end"
    }
  ],
  "web_accessible_resources": [
    "dismed-logo-48x48.png",
    "dismed-logo-white-48x48.png",
    "dismed-logo-blue.svg",
    "dismed-logo-grey.svg"
  ],
  "browser_action": {
    "default_title": "Create Scheduled Teleconsultation",
    "default_popup": "popup.html"
  }
}
