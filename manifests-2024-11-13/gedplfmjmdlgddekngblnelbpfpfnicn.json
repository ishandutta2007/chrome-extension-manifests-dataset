{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Clever Youtube Bookmarker",
  "version": "1.1.0",  
  "icons": {
    "16": "src/img/icon-16.png",    
    "32": "src/img/icon-32.png",
    "48": "src/img/icon-48.png",
    "128": "src/img/icon-128.png"
  },
  "description": "Creating bookmarks in Youtube videos",
  "permissions": ["storage", "tabs", "webNavigation"],
  "host_permissions": ["https://*.youtube.com/*"],
  "background": {
    "service_worker": "src/background.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": ["https://*.youtube.com/*"],      
      "js": ["src/lib/jquery/jquery-3.6.3.min.js", "src/lib/toastify.js", "src/content.js"],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "src/img/add-bookmark.png",
        "src/img/icon-32.png",
        "src/lib/toastify.js",
        "src/lib/toastify.min.css"
      ],
      "matches": ["https://*.youtube.com/*"]
    }
  ],
  "action": {
    "default_icon": {
      "16": "src/img/icon-16.png",
      "24": "src/img/icon-24.png",
      "32": "src/img/icon-32.png"
    },
    "default_title": "Clever Youtube Bookmarker",
    "default_popup": "src/popup.html"
  },
  "options_page": "src/options.html",
  "manifest_version": 3
}
