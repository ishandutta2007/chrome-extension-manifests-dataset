      "run_at": "document_start"
    },
    {
      "all_frames": false,
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "ping.js"
      ],
      "run_at": "document_start"
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "storage",
