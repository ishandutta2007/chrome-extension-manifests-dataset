  "manifest_version": 2,
  "name": "Turtle",
  "short_name": "Turtle",
  "description": "Share links on Turtle with one click.",
  "author": "Turtle",
  "version": "1.0.2",
  "permissions": [
    "https://turtle.community/*",
    "tabs",
    "activeTab",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://turtle.community/*"
      ],
      "js": [
        "get-turtle-token.js"
      ],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "Turtle",
    "default_popup": "index.html",
    "browser_style": true
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Space",
        "windows": "Ctrl+Space",
        "mac": "Ctrl+Space",
        "linux": "Ctrl+Space"
      }
    }
  },
  "icons": {
    "19": "assets/favicon.ico"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
