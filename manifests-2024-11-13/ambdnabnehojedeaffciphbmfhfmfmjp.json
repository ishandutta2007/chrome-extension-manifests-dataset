{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "YouTube Live Stream Latency Mitigator",
    "version": "1.14.2",
    "description": "__MSG_appDesc__",
    "default_locale": "en",
    "icons": {
        "16": "icons/16.png",
        "48": "icons/48.png",
