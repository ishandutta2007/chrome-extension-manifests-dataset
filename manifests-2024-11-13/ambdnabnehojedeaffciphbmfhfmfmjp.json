    },
    "content_scripts": [
        {
            "js": [
                "content.js"
            ],
            "run_at": "document_end",
            "matches": [
                "https://www.youtube.com/*"
            ],
            "all_frames": true
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "common.js",
                "inject.js"
            ],
            "matches": [
                "https://www.youtube.com/*"
            ]
        }
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "options_ui": {
        "page": "popup.html",
        "open_in_tab": true
    },
    "permissions": [
        "storage"
    ]
