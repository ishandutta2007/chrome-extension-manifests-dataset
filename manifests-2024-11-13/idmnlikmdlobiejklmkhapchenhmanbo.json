"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Notion Enhancer for Google Search",
  "description": "Display Notion search results alongside Google search results.",
  "version": "1.1.1",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
