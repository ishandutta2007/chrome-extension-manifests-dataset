    },
    {
      "resources": [
        "chunks/*-*.js",
        "content.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*",
        "https://.wisiwig.co/*",
        "https://*.wisiwig.co/*"
      ]
    }
  ],
  "commands": {
    "toggle": {
      "suggested_key": {
