  "host_permissions": [
    "<all_urls>"
  ],
  "icons": {
    "128": "/assets/logo.png"
  },
  "action": {
    "default_icon": {
      "128": "/assets/logo.png"
    },
    "default_title": "__MSG_name__"
  },
  "content_scripts": [
    {
      "match_about_blank": true,
      "all_frames": false,
      "js": [
        "./js/app.js"
      ],
      "run_at": "document_idle",
      "matches": [
        "*://*/*"
      ]
    }
  ],
  "background": {
    "service_worker": "./js/bg.js"
  }
