        "16": "/icons/icon_16.png",
        "32": "/icons/icon_32.png",
        "48": "/icons/icon_48.png",
        "128": "/icons/icon_128.png"
    },
    "permissions": [
        "storage",
        "tabs",
        "scripting",
        "clipboardWrite",
        "clipboardRead",
        "downloads"
    ],
    "host_permissions": [
        "*://*.facebook.com/*",
        "*://*.messenger.com/*"
    ],
    "content_scripts": [
        {
            "matches": [
                "*://*.facebook.com/*",
                "*://*.messenger.com/*"
