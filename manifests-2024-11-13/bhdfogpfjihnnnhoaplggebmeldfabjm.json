{
"update_url": "https://clients2.google.com/service/update2/crx",

    "name": "Note Mennpin",
    "version": "0.1.1",
    "description": "Note Mennpin extension helps the user to set any note on any position on the screen",
    "permissions": ["storage", "tabs","unlimitedStorage"],
    "host_permissions": ["<all_urls>"],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["contentScript.js"]
      }
    ],
    "web_accessible_resources": [
      {
        "resources": [
          "assets/remove.svg"
        ],
        "matches": ["<all_urls>"]
      }
    ],
    "action": {
      "default_icon": {
        "16": "assets/ext-icon.png",
        "24": "assets/ext-icon.png",
        "32": "assets/ext-icon.png"
      },
      "default_title": "Note Mennpin",
      "default_popup": "popup.html"
    },
    "manifest_version": 3
}
