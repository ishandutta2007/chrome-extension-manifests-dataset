{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "YTQ - YouTube Quality Selector",
  "description": "A chrome plugin to automatically set the quality of YouTube videos",
  "version": "2",
  "browser_action": {
    "default_title": "YTQ - YouTube Quality Selector",
    "default_icon": "icons/logo.png",
    "default_popup": "YTQ.html"
  },
  "icons": {
    "16": "icons/logo16.png",
    "48": "icons/logo.png",
    "128": "icons/logo128.png"
  },
  "permissions": [
    "activeTab",
    "storage",
    "http://youtube.com/*",
    "https://youtube.com/*",
    "http://*.youtube.com/*",
    "https://*.youtube.com/*"
  ],
  "background": {
    "scripts": [
      "popup.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "http://youtube.com/*",
        "https://youtube.com/*",
        "http://*.youtube.com/*",
        "https://*.youtube.com/*"
      ],
      "js": [
        "popup.js",
        "YTQ.js"
      ]
    }
  ],
  "manifest_version": 2
