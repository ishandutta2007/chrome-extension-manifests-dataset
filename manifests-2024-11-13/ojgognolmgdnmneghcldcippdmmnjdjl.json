{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "noAd for myself",
  "version": "1.88.2",
  "version_name": "2.0 beta (1.88.2)",
  "manifest_version": 3,
  "minimum_chrome_version": "129.0",
  "description": "This extension removes some Ads on Youtube, X(Twitter) and more.",
  "permissions": ["declarativeNetRequest"],
  "background": {
    "service_worker": "background/background.js",
    "type": "module"
  },
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "common",
        "enabled": true,
        "path": "static/rules/common.json"
      },
      {
        "id": "block ads",
        "enabled": true,
        "path": "static/rules/block_ads.json"
      },
      {
        "id": "google",
        "enabled": true,
        "path": "static/rules/google.json"
      },
      {
        "id": "youtube",
        "enabled": true,
        "path": "static/rules/youtube.json"
      },
      {
        "id": "twitter",
        "enabled": true,
        "path": "static/rules/twitter.json"
      },
      {
        "id": "spotify",
        "enabled": true,
        "path": "static/rules/spotify.json"
      }
    ]
  },
  "host_permissions": ["http://*/*", "https://*/*"],
  "icons": {
    "48": "static/icons/icon48.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["static/css/noad.css"],
      "matches": [
        "*://twitter.com/*",
        "*://*.twitter.com/*",
        "*://x.com/*",
        "*://*.x.com/*"
      ]
    },
    {
      "resources": ["static/css/noad.css"],
      "matches": ["*://*.youtube.com/*"]
    },
    { "resources": ["/static/audio/1.mp3"], "matches": ["*://*.spotify.com/*"] }
  ],
  "content_scripts": [
    {
      "run_at": "document_start",
      "matches": ["<all_urls>"],
      "match_about_blank": true,
      "js": ["all_urls/entrypoint.js"],
      "all_frames": true,
      "world": "MAIN"
    },
    {
      "run_at": "document_start",
      "matches": ["*://*.youtube.com/*"],
      "js": ["youtube_main/entrypoint.js"],
      "all_frames": true,
      "world": "MAIN"
    },
    {
      "run_at": "document_start",
      "matches": ["*://*.youtube.com/*"],
      "js": ["youtube_isolated/entrypoint.js"],
      "css": ["static/css/noad.css"],
      "world": "ISOLATED"
    },
    {
      "run_at": "document_start",
      "matches": [
        "*://twitter.com/*",
        "*://*.twitter.com/*",
        "*://x.com/*",
        "*://*.x.com/*"
      ],
      "js": ["twitter_main/entrypoint.js"],
      "all_frames": true,
      "world": "MAIN"
    },
    {
      "run_at": "document_start",
      "matches": [
        "*://twitter.com/*",
        "*://*.twitter.com/*",
        "*://x.com/*",
        "*://*.x.com/*"
      ],
      "js": ["twitter_isolated/entrypoint.js"],
      "css": ["static/css/noad.css"],
      "world": "ISOLATED"
    },
    {
      "run_at": "document_start",
      "matches": ["*://*.spotify.com/*"],
      "js": ["spotify_main/entrypoint.js"],
      "all_frames": true,
      "world": "MAIN"
    },
    {
      "run_at": "document_start",
      "matches": ["*://*.spotify.com/*"],
      "js": ["spotify_isolated/entrypoint.js"],
      "all_frames": true,
      "world": "ISOLATED"
    }
  ]
}
