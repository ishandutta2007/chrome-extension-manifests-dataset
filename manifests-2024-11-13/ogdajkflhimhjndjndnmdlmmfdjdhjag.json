    "permissions": [
        "activeTab",
        "storage"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://colab.research.google.com/*"
            ],
            "js": [
                "getRevisions.js"
            ]
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "script.js"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ],
    "optional_host_permissions": [
        "https://dev.processfeedback.org/api/colabsecured/v1/*",
        "https://app.processfeedback.org/api/colabsecured/v1/*"
    ],
    "background": {
        "service_worker": "background.js"
    }
