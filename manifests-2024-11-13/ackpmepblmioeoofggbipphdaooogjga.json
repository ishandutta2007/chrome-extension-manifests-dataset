{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "Google UDM=14",
    "version": "1.0",
    "description": "Appends 'udm=14' to the end of Google search queries immediately after the first '&'.",
    "permissions": [
      "webNavigation"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["https://www.google.com/search*"],
        "js": ["content.js"],
        "run_at": "document_end"
      }
    ],
    "host_permissions": [
      "https://www.google.com/search*"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    }
  }
  