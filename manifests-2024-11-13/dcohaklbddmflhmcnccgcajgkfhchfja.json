      "js": [
        "load.js",
        "content-script.js"
      ],
      "run_at": "document_end"
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "icons/128.png"
    }
  },
  "web_accessible_resources": [
    {
      "resources": [
        "images/*",
        "css/*",
        "fonts/*",
        "img/*",
        "icons/*",
        "inject-scripts/*"
      ],
      "matches": [
        "*://*.whatsapp.com/*"
      ]
    },
