      "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self' ;",
      "sandbox": "sandbox allow-scripts"
    },   
    "host_permissions": [
      "https://my.vartopia.com/*",
      "https://my.vartopia.com/VartopiaV2/Login",
