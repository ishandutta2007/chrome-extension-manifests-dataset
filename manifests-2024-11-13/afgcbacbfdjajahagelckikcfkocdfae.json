      "https://my.vartopia.com/VartopiaV2/AdminHome"
    ],
    "web_accessible_resources": [{ 
      "resources": [
        "/src/assets/js/cus.js",
        "/src/assets/js/cus.wasm"
      ],
      "matches": ["<all_urls>"]
    } ],
    "permissions": [
        "storage",
        "tabs",
        "scripting",
        "activeTab",
        "cookies"
        
    ]
