{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "LinkScalper - Advanced Link Grabber",
  "version": "1.0",
  "description": "Extracts all links from a webpage and displays them on a new page with options.",
  "permissions": [
    "activeTab",
    "storage",
    "contextMenus",
    "tabs",
    "scripting",
    "system.display"
  ],
  "key": "",
  "content_scripts": [
    {
      "matches": ["https://codingdir.com/link-extractor/results*"],
      "js": ["results-content-script.js"],
      "run_at": "document_end"
    }
  ],
  "host_permissions": [
    "<all_urls>",
    "https://codingdir.com/"
  ],
  "action": {
    "default_title": "LinkScalper - Advanced Link Grabber",
    "default_popup": "popup.html",
    "default_icon": "icons/icon16.png"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "options_page": "options.html",
  "web_accessible_resources": [{
    "resources": [
      "results.html",
      "results.js",
      "vendor/jszip.min.js",
      "vendor/xlsx.full.min.js",
      "loading.html",
      "icons/icon16.png",
      "selector-overlay.html",
      "error-popup.html",
      "loading.gif"
    ],
    "matches": ["<all_urls>"]
  }]
}