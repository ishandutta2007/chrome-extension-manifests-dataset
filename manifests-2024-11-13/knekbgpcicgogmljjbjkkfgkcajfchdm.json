{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "Huntica: приглашение кандидатов",
  "description": "Расширение позволяет приглашать кандидатов на онлайн-собеседование в сервис Huntica прямо с сайта HeadHunter",
  "permissions": [
    "storage",
    "identity",
    "http://*.hh.ru/*",
    "https://*.hh.ru/*"
  ],
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Huntica Toolbar для HH.ru",
    "default_icon": {
      "19": "icons/19.png",
      "38": "icons/38.png"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "http://*.hh.ru/*",
        "https://*.hh.ru/*"
      ],
      "js": [
        "js/content-script.js"
      ],
      "css": [
        "css/style.css"
      ]
    }
  ],
  "web_accessible_resources": [
    "js/page-script.js",
    "img/*.*",
    "css/*.*"
  ],
  "externally_connectable": {
    "matches": [
      "*://*.huntica.works/*"
    ]
  },
  "version": "1.2.0",
  "content_security_policy": "script-src 'self' ; object-src 'self'"
}