    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"],
      "match_origin_as_fallback": true,
      "run_at": "document_idle"
    }
  ],
  "host_permissions": ["https://flozable.com/*", "http://localhost:3000/*"]
