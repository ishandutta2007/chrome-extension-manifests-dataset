{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "Floqer",
    "description": "Free extension to scrape leads data from the web to and import into Floqer or Clay.",
    "version": "1.0.1",
    "action": {
        "default_title": "Floqer Utility",
        "default_icon": {
            "16": "icon-16.png",
            "48": "icon-48.png",
            "128": "icon-128.png"
        }
    },
    "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'self'; script-src-elem 'self' 'unsafe-inline';"
    },
    "background": {
        "service_worker": "js/background.js"
    },
    "icons": {
        "16": "icon-16.png",
        "48": "icon-48.png",
        "128": "icon-128.png"
    },
    "host_permissions": [
        "*://*.linkedin.com/*",
        "*://*.salesnavigator.linkedin.com/*",
        "*://dev-workflows.netlify.app/*",
        "*://*.google.com/*",
        "*://app.floqer.com/*",
        "*://localhost/*"
    ],
    "web_accessible_resources": [
        {
            "resources": ["*.css", "*/*.otf", "*.html"],
            "matches": ["https://*/*", "http://*/*"]
        }
    ],
    "externally_connectable": {
        "matches": ["http://*/*", "https://*/*"]
    },
    "permissions": [
        "webNavigation",
        "tabs",
        "storage",
        "cookies",
        "unlimitedStorage"
    ],
    "content_scripts": [
        {
            "matches": ["http://*/*", "https://*/*"],
            "js": ["js/content-script.js"],
            "run_at": "document_idle"
        }
    ]
}
