{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Dyslexic font",
  "version": "1.0.0",
  "description": "Activate this extension to change font styles for better readability with just one click.",
  "action": {
    "default_title": "Dyslexic font",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "Icon 16.png",
      "32": "Icon 32.png",
      "48": "Icon 48.png"
    }
  },
  "icons": {
    "64": "Icon 64.png",
    "128": "Icon 128.png",
    "2000": "Icon 2000.png"
  },
  "host_permissions": [
    "*://*/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content.js"
      ],
      "css": [
        "content.css"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["open_dyslexic/*"],
      "matches": ["<all_urls>"]
    }
],
  "permissions": [
    "storage",
    "notifications",
    "activeTab"
  ]
}