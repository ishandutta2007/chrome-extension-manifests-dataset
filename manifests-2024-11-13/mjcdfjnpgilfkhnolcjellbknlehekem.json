{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Sophistree",
  "version": "0.1.5",
  "description": "A browser extension for mapping arguments appearing in web pages",
  "permissions": [
    "contextMenus",
    "scripting",
    "sidePanel",
    "webNavigation"
  ],
  "action": {
    "default_title": "Sophistree"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["content.css"]
    }
  ],
  "host_permissions": ["<all_urls>"],
  "side_panel": {
    "default_path": "sidebar.html"
  },
  "icons": {
    "32": "logo-32.png",
    "512": "logo-512.png"
  }
}
