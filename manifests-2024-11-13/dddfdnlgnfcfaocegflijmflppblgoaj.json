{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "All Images Downloader",
  "version": "1.0",
  "description": "Download all images from the current webpage, including lazy-loaded and original images.",
  "permissions": [
    "downloads",
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["jszip.min.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "icons" : {
    "16" : "images/icon16.png",
    "32" : "images/icon32.png",
    "48" : "images/icon48.png",
    "128" : "images/icon128.png"
  }
}
