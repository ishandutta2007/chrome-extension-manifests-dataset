{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Wikéo - Your Wikipedia-compatible Podcasts",
  "version": "1.0.1",
  "manifest_version": 3,
  "description": "Rediscover Wikipedia in audio. This extension allows you to browse and listen to short podcasts from any Wikipedia article.",
  "icons": {
    "128": "icon.png"
  },
  "permissions": [],
  "background": {
    "service_worker": "service.js"
  },
  "host_permissions": [
    "https://wikipedia.org/*",
    "https://*.wikipedia.org/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://*.wikipedia.org/*"],
      "css": ["wikeo-extension.css"],
      "js": [
        "common.js",
        "wikeo-extension.js"
      ],
      "run_at":"document_end"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "common.js",
        "wikeo-extension.html" 
      ],
      "matches": [ "https://*.wikipedia.org/*"]
    }
  ],
  "action": {}
}
