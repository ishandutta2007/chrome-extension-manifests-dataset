{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "AN Border Price",
  "version": "1.12",
  "description": "AUCNETオークションページに推定最低落札金額を表示します。ログイン機能付き。",
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "https://bid.brand-auc.com/*",
    "https://u.brand-auc.com/*",
    "https://identitytoolkit.googleapis.com/*",
    "https://securetoken.googleapis.com/*"
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/an_border_icon16.png",
      "48": "images/an_border_icon48.png",
      "128": "images/an_border_icon128.png"
    }
  },
  "icons": {
    "16": "images/an_border_icon16.png",
    "48": "images/an_border_icon48.png",
    "128": "images/an_border_icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://bid.brand-auc.com/*"
      ],
      "js": ["content.js"]
    },
    {
      "matches": [
        "https://bid.brand-auc.com/items/detail*"
      ],
      "js": ["content_detail.js"]
    },
    {
      "matches": [
        "https://u.brand-auc.com/previewSearch",
        "https://u.brand-auc.com/",
        "https://u.brand-auc.com/main",
        "https://u.brand-auc.com/previewFavoriteItems*"
      ],
      "js": ["content_preview.js"]
    },
    {
      "matches": [
        "https://u.brand-auc.com/previewDetailItem?*"
      ],
      "js": ["content_preview_detail.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "aucnet_price_sheet.json",
        "estimated-style.css"
      ],
      "matches": [
        "https://bid.brand-auc.com/*",
        "https://u.brand-auc.com/*"
      ]
    }
  ]
}