"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Skip-n-Watch",
  "description": "Skips every video ad on YouTube automatically",
  "version": "1.0",
  "icons": {
    "128": "icon.png"
  },
  "background": {
    "service_worker": "script/background.js"
