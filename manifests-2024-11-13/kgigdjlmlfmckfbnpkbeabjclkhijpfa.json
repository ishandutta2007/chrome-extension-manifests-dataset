{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "YouTube Bookmarks",
    "version": "1.0",
    "description": "Organize your YouTube bookmarks into folders.",
    "permissions": [
        "storage",
        "activeTab"
    ],
    "host_permissions": [
        "https://www.youtube.com/*"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "assets/bookmark.png",
        "48": "assets/bookmark.png",
        "128": "assets/bookmark.png"
      }
    },
    "icons": {
      "16": "assets/bookmark.png",
      "48": "assets/bookmark.png",
      "128": "assets/bookmark.png"
    },
    "content_scripts": [
      {
        "matches": ["https://www.youtube.com/*"],
        "js": ["content.js"],
        "run_at": "document_end"
      }
    ],
    "web_accessible_resources": [
      {
        "resources": [
          "assets/bookmark.png",
          "assets/notes.png", 
          "folder-icon.png",
          "default-channel-icon.png",
          "delete.png",
          "new-folder.png",
          "delete-folder.png",
          "share.png",
          "copy-link-icon.png",
          "facebook-icon.png",
          "twitter-icon.png",
          "email-icon.png",
          "whatsapp-icon.png",
          "expand.png",
          "collapse.png",
          "coffee.png",
          "share_SM.png",
          "rate.png",
          "reddit-icon.png"
        ],
        "matches": ["https://www.youtube.com/*"]
      }
    ]
  }
