{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "Saywhat",
    "version": "0.0.0.4",
    "author": "Saywhat LLC",
    "description": "LinkedIn Community Management Tool",
    "host_permissions": [
        "*://linkedin.com/*",
        "*://*.linkedin.com/*",
        "http://localhost:5006/*",
        "https://localhost:5006/*",
        "http://amplify.monkeysee.ai/*",
        "https://amplify.monkeysee.ai/*",
        "http://saywhat.ai/*",
        "https://saywhat.ai/*",
        "http://app.saywhat.ai/*",
        "https://app.saywhat.ai/*"
    ],
    "permissions": ["cookies", "storage"],
    "action": {},
    "background": {
        "service_worker": "build/background.js"
    },
    "icons": {
        "16": "images/icon@16.png",
        "32": "images/icon@32.png",
        "48": "images/icon@48.png",
        "128": "images/icon@128.png"
    },
    "web_accessible_resources": [
        {
            "resources": [
                "build/content_external/views/**/*.js",
                "build/content_external/views/**/*.html",
                "build/content_external/views/**/*.css"
            ],
            "matches": ["https://linkedin.com/*", "https://www.linkedin.com/*"]
        },
        {
            "resources": ["build/content_external/injected.js"],
            "matches": ["*://linkedin.com/*", "*://*.linkedin.com/*"]
        }
    ],
    "content_scripts": [
        {
            "matches": [
                "http://localhost:5006/*",
                "https://localhost:5006/*",
                "http://amplify.monkeysee.ai/*",
                "https://amplify.monkeysee.ai/*",
                "http://saywhat.ai/*",
                "https://saywhat.ai/*",
                "http://app.saywhat.ai/*",
                "https://app.saywhat.ai/*"
            ],
            "js": ["build/content_internal/content_internal.js"],
            "run_at": "document_end"
        },
        {
            "matches": ["https://linkedin.com/*", "https://www.linkedin.com/*"],
            "js": ["build/content_external/content_external.js"],
            "run_at": "document_end"
        }
    ],
    "externally_connectable": {
        "matches": [
            "*://linkedin.com/*",
            "*://*.linkedin.com/*",
            "http://localhost:5006/*",
            "https://localhost:5006/*",
            "http://amplify.monkeysee.ai/*",
            "https://amplify.monkeysee.ai/*",
            "http://saywhat.ai/*",
            "https://saywhat.ai/*",
            "http://app.saywhat.ai/*",
            "https://app.saywhat.ai/*"
        ]
    }
}
