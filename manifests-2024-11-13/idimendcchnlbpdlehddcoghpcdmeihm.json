{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "MentorRank",
  "description": "Extract public data from Etsy pages",
  "version": "3.2",
  "permissions": [
    "activeTab",          
    "scripting",                         
    "storage"             
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "icon.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["*://*.etsy.com/*"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "externally_connectable": {
    "matches": [
      
      "https://mentorrank.app/*"       
    ]
  },
  "host_permissions": [
    "https://*.firebaseapp.com/*",        
    "https://firestore.googleapis.com/*",
    "https://identitytoolkit.googleapis.com/*" 
  ]
}