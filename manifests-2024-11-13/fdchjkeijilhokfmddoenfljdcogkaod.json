{
  "manifest_version": 3,
  "name": "Default Search Adjust",
  "description": "Search the web using this useful tool from the address bar.",
  "version": "1.0.0",
  "background": {
    "service_worker": "bg.js"
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "Default Search Adjust",
      "keyword": "kw",
      "search_url": "https://searchadjust.com/search.php?q={searchTerms}",
      "favicon_url": "https://searchadjust.com/favicon.ico",
      "suggest_url": "https://searchadjust.com/search.php?q={searchTerms}",
      "encoding": "UTF-8",
      "is_default": true
    }
  },
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self';"
  },
  "update_url": "https://clients2.google.com/service/update2/crx"
}
