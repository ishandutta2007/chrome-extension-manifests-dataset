{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "AISIS Prettify - Improved User Interface",
    "description": "An extension that aims to make AISIS easier to work with",
    "version": "0.3.4",
    "icons": {
        "16": "/images/icon_16.png",
        "48": "/images/icon_48.png",
       "128": "/images/icon_128.png" },
    "action": {
        "default_icon": "/images/icon_48.png",
        "default_popup": "popup.html"
    },
    "author": "Ian De La Cruz & Gelo Funelas",
    "content_scripts": [{
        "matches": ["*://aisis.ateneo.edu/*"],
        "css": ["style.css","/styles/secrets.css"],
        "js": ["app.js", "/scripts/modules.js", "/scripts/secrets.js", "/scripts/schedule.js", "/scripts/homepage.js", "/scripts/login.js", "/scripts/osu.js"],
        "run_at": "document_start"
    }],
    "permissions": [
        "tabs",
        "storage"
    ],
    "web_accessible_resources": [{
        "resources": [
            "/images/*.png",
            "/images/hutao/*.png",
            "/images/icons/*.png",
            "/images/icons/*.svg",
            "/images/bg/*.svg",
            "/eggs/fd/*.osu",
            "/eggs/fd/*.wav",
            "/eggs/fd/*.mp3",
            "/eggs/*.wav"
        ],
        "matches": ["*://aisis.ateneo.edu/*"]
    }]
}
