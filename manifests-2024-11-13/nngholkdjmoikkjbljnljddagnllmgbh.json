{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Fezzant Accessibility Scanner",
  "version": "1.1",
  "description": "Scans web pages for accessibility issues",
  "icons": {
    "16": "./icon.png",
    "18": "./icon.png",
    "128": "./icon.png"
  },
  "permissions": ["scripting", "activeTab"],
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": ["http://*/*", "https://*/*"]
    }
  ],
  "action": {
    "default_popup": "index.html"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "fullResults.html",
        "downloadResult.html",
        "static/js/main.js"
      ],
      "matches": ["<all_urls>"]
    }
  ]
}
