{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "StyleShift",
    "version": "1.2",
    "description": "An extension that allows you to restyle images on some property websites.",
    "permissions": [
        "storage"
    ],
    "host_permissions": [
        "https://styleshift.pro/*"
    ],
    "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'self'; frame-src https://styleshift.pro http://localhost:*;"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icon16.png",
            "48": "icon48.png",
            "128": "icon128.png"
        }
    },
    "content_scripts": [
        {
            "matches": [
                "https://www.rightmove.co.uk/*",
                "https://www.cliveemson.co.uk/*"
            ],
            "js": [
                "bundle-bundled.js"
            ],
            "type": "module",
            "css": [
                "styles.css"
            ]
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "icon.png"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ]
}