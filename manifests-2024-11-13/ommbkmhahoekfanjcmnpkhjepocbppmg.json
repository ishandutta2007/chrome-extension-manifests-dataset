{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "GE Driving Tickets Manager",
  "version": "1.3.1",
  "icons": {
    "128": "icon-128.png"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage"
  ],
  "background": {
    "service_worker": "service-worker.js",
    "type": "module"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "*://teoria.on.ge/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "popup.js",
        "store.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}
