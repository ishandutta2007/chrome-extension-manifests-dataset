{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Soundcloud Comment Track IDs",
  "description": "Extracts track IDs from SoundCloud comments. This will help you grab the tracks you love!",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "storage",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["*://*.soundcloud.com/*"],
      "js": ["content.js"]
    }
  ],
  "host_permissions": [
    "https://*.soundcloud.com/*"
  ]
}