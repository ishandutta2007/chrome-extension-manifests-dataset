{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "DoaToa - Chrome Extension",
  "version": "0.1.1",
  "description": "This plugin allows you to interact with your DoaToa account.",
  "permissions": [
    "contextMenus",
    "activeTab",
    "storage",
    "scripting"
  ],
  "background": {
    "service_worker": "src/background/background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.doatoa.io/*"
      ],
      "js": [
        "src/content/content.js"
      ]
    }
  ],
  "icons": {
    "16": "src/images/icon16.png",
    "48": "src/images/icon48.png",
    "128": "src/images/icon128.png"
  },
  "action": {
    "default_title": "DoaToa",
    "default_popup": "src/popup/popup.html",
    "default_icon": {
      "16": "src/images/icon16.png",
      "48": "src/images/icon48.png",
      "128": "src/images/icon128.png"
    }
  }
}
