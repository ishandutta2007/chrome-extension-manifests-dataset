{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "HTML Tags Finder",
  "version": "0.3",
  "description": "Encontre tags em todas as páginas para mapear métricas para o Google Tag Manager",
  "offline_enabled": true,

  "background": {
    "service_worker": "background.js"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["styles.css"],
      "js": ["jquery-3.2.1.js","content.js"],
      "run_at": "document_idle"
    }
  ],

  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/16.png",
      "19": "images/19.png",
      "32": "images/32.png",
      "38": "images/38.png",
      "48": "images/48.png",
      "128": "images/128.png"
    }
  },
  "icons": {
    "16": "images/16.png",
      "19": "images/19.png",
      "32": "images/32.png",
      "38": "images/38.png",
      "48": "images/48.png",
      "128": "images/128.png"
  },

  "permissions": ["tabs",
    "activeTab",
    "storage",
  "scripting"],
  "host_permissions": [
    "<all_urls>"
  ]
}
