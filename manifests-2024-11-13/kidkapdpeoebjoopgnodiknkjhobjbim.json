                "*.html",
                "*.css",
                "*.js",
                "/icons/*.png"
            ],
            "matches": [
                "<all_urls>"
            ]}],
    
    "content_scripts": [
        {
            "js": ["StickySheets.js"],
            "matches": ["<all_urls>"],
            "run_at": "document_end"
        }
    ]
