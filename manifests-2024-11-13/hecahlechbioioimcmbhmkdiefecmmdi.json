{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "ACB",
  "version": "1.0",
  "description": "Enhance functionality on websites",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon1.png",
      "48": "icons/icon2.png",
      "128": "icons/icon3.png"
    }
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'; connect-src 'self' http://127.0.0.1:8088 https://web.careerbooster.online;"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.linkedin.com/search/results/people/*",
        "*://*.linkedin.com/search/results/all/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "host_permissions": [
    "*://*.linkedin.com/*",
    "http://localhost/*",
    "http://127.0.0.1/*",
    "http://127.0.0.1:8088/*",
    "https://*.careerbooster.online/*"
  ],
  "externally_connectable": {
    "matches": [
      "*://localhost/*",
      "*://127.0.0.1/*",
      "*://*.careerbooster.online/*"
    ]
  },
  "web_accessible_resources": [
    {
      "resources": [
        "scrapePage.js"
      ],
      "matches": [
        "*://*.linkedin.com/*"
      ]
    }
  ]
}
