{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Korean to English Translation Switcher",
  "version": "1.4",
  "description": "Translate specific Korean terms to English on https://cho.elphago.work/en with a toggle switch.",
  "permissions": [
    "storage", "activeTab",
     "scripting",
    "https://cho.elphago.work/*"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://cho.elphago.work/*"],
      "js": ["translate.js"]
    }
  ],
  "icons": {
    "128": "icons/icon128.png"
  }
}
