{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "Bypass Telegram Web",
    "version": "2.3",
    "description": "Giả lập phiên bản Mobile trên Telegram Web để chạy Mini-Apps và lấy dữ liệu cần thiết giúp bạn dùng để chạy Tools.",
    "permissions": [
        "activeTab", "scripting", "webRequest",
        "declarativeNetRequest", "clipboardWrite","notifications",
        "declarativeNetRequestFeedback", "declarativeNetRequestWithHostAccess"
    ],
    "host_permissions": ["<all_urls>", "*://*/*"],
    "icons":
    {
        "16": "images/logo.png",
        "32": "images/logo.png",
        "48": "images/logo.png",
        "128": "images/logo.png"
    },
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["scripts/telegram.js"],
          "run_at": "document_end"
        }
    ],
    "background":
    {
        "service_worker": "background.js"
    },
    "action":
    {
        "default_title": "Bypass Telegram",
        "default_popup": "popup.html"
    },
    "declarative_net_request":
    {
        "rule_resources": [
        {
            "id": "ruleset_1",
            "enabled": true,
            "path": "rules.json"
        }]
    }
