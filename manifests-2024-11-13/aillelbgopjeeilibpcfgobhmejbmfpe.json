{
"update_url": "https://clients2.google.com/service/update2/crx",

 "name": "即レスbot君",
 "version": "1.4.1",
 "manifest_version": 3,
 "description": "ココナラ内でのメッセージ返信を自動化できるツールです。",
 "permissions": ["storage", "tabs", "alarms"],
 "background": {
  "service_worker": "background.js"
 },
 "content_scripts": [
  {
   "matches": ["https://coconala.com/mypage/received_orders/*"],
   "js": ["js/start_observation.js"]
  },
  {
   "matches": [
    "https://coconala.com/mypage/received_orders/open"
   ],
   "js": ["js/cue.js"]
  },
  {
   "matches": ["https://coconala.com/talkrooms/*"],
   "js": ["js/send_message.js"]
  }
 ],
 "action": {
  "default_popup": "html/popup.html"
 },
 "icons": {
  "16": "img/icon16.png",
  "48": "img/icon48.png",
  "128": "img/icon128.png"
 }
}
