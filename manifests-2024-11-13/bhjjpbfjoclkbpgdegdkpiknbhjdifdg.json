{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "Twitter Dials",
    "description": "Twitter Dials is a Chrome extension that lets you customize your Twitter experience. Control comments, trends and more soonTM.",
    "version": "1.0",
    "author": "Niklas Pferscher",
    "background": {
      "service_worker": "background.js"
