        "48": "images/icon_48.png",
        "128": "images/icon_128.png"
      }
    },
    "content_scripts": [
      {
        "matches": ["https://twitter.com/*"],
        "js": ["jquery-3.3.1.js","content.js"]
      }
    ],
    "icons": {
      "16": "images/icon_16.png",
      "32": "images/icon_32.png",
      "48": "images/icon_48.png",
      "128": "images/icon_128.png"
    },
    "permissions": ["storage","activeTab"]
