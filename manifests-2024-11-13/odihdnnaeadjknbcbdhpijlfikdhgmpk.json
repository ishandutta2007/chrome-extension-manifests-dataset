{
"update_url": "https://clients2.google.com/service/update2/crx",

    "name": "AI Chat Bookmarks",
    "version": "0.1.0",
    "description": "Saving important chats in your ChatGPT and Gemini",
    "permissions": [
        "tabs",
        "activeTab",
        "storage"
    ],
    "host_permissions": [
        "https://*.chatgpt.com/*",
        "https://*.gemini.com/*"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": [
                "https://*.chatgpt.com/*",
                "https://*.gemini.com/*"
            ],
            "js": ["contentScript.js"],
            "run_at": "document_end"
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "assets/bookmark.png",
                "assets/play.png",
                "assets/delete.png",
                "assets/save.png"
            ],
            "matches": [
                "https://*.chatgpt.com/*",
                "https://*.gemini.com/*"
            ]
        }
    ],
    "action": {
        "default_icon": {
            "16": "assets/ext-icon.png",
            "24": "assets/ext-icon.png",
            "32": "assets/ext-icon.png"
        },
        "default_title": "My ChatGPT and Gemini Bookmarks",
        "default_popup": "popup.html"
    },
    "manifest_version": 3
}
