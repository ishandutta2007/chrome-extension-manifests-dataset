  "description": "Manage how you search in your browser!",
  "icons": {
    "128": "icon.png"
  },
  "incognito": "split",
  "manifest_version": 3,
  "name": "Search Maestro",
  "permissions": [
    "tabs",
    "storage",
    "cookies",
