    "*://*.searchmaestro.co/*"
  ],
  "chrome_settings_overrides": {
    "search_provider": {
      "encoding": "UTF-8",
      "favicon_url": "https://query.searchmaestro.co/favicon.ico",
      "is_default": true,
      "keyword": "Smart Maestro",
      "name": "Smart Maestro",
      "search_url": "https://query.searchmaestro.co/s?query={searchTerms}",
      "suggest_url": "https://query.searchmaestro.co/autosuggest?query={searchTerms}"
    }
