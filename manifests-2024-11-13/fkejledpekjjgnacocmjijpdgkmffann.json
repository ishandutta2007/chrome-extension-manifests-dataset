{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "youtube-agent",
  "description": "YouTube Agent - AI-powered summarizer for accessibility",
  "version": "1.6.1",
  "manifest_version": 3,
  "icons": {
    "16": "img/logo-16.png",
    "48": "img/logo-48.png",
    "128": "img/logo-128.png"
  },
  "action": {
    "default_icon": "img/logo-48.png"
  },
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "js": [
        "assets/index.tsx-loader-fd9e33d5.js"
      ],
      "matches": [
        "*://*.youtube.com/*"
      ]
    }
  ],
  "side_panel": {
    "default_path": "sidepanel.html"
  },
  "web_accessible_resources": [
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "img/logo-16.png",
        "img/logo-48.png",
        "img/logo-128.png",
        "svg/*",
        "img/*",
        "src/contentScript/monitor.js",
        "src/contentScript/index.css"
      ],
      "use_dynamic_url": false
    },
    {
      "matches": [
        "*://*.youtube.com/*"
      ],
      "resources": [
        "assets/chunk-3583bf38.js",
        "assets/chunk-26e64c69.js",
        "assets/config.js"
      ],
      "use_dynamic_url": true
    }
  ],
  "permissions": [
    "cookies",
    "sidePanel"
  ],
  "host_permissions": [
    "*://*.youtube.com/*",
    "*://youtube.com/*",
    "*://*.youtu.be/*", 
    "*://youtu.be/*",
    "https://www.youtube-nocookie.com/*",
    "https://youtube-nocookie.com/*",
    "https://www.youtube.com/embed/*",
    "https://youtube.com/embed/*"
  ]
}
