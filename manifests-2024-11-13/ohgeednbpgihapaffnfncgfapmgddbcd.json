{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "Image Dimensions Detector",
    "version": "1.5",
    "description": "Detects all images on a website and displays their dimensions.",
    "permissions": [
      "storage"
    ],
    "background": {
      "service_worker": "javascripts/background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "48": "img/icon48.png",
        "128": "img/icon128.png"
      }
    },
    "icons": {
      "48": "img/icon48.png",
      "128": "img/icon128.png"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["javascripts/content.js"],
        "run_at": "document_end"
      }
    ]
  }
  