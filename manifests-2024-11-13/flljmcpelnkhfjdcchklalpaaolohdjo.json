{
"update_url": "https://clients2.google.com/service/update2/crx",

    "name": "BrandCycle",
    "description": "Allows BrandCycle users to easily create affiliate links.",
    "version": "1.0.0.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["contextMenus", "scripting", "storage", "system.display", "clipboardWrite"],
    "host_permissions": ["<all_urls>"],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": [
                "libraries/jquery.js",
                "libraries/bindly.js",
                "global-toasts/toastr.js",
                "global-toasts/custom-toast.js"
            ],
            "css": ["global-toasts/toastr.css", "global-toasts/custom-toast.css"],
            "run_at": "document_start"
        },
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "web_accessible_resources": [
        {
            "resources": ["global-toasts/*", "images/*", "fonts/*", "asset-page.html"],
            "matches": ["<all_urls>"]
        }
    ],
    "icons": {
        "16": "images/white brandcycle.png",
        "48": "images/brandcycle.png",
        "128": "images/brandcycle.png"
    },
    "action": {
        "default_icon": {
            "16": "images/brandcycle-16.png",
            "48": "images/brandcycle-48.png",
            "128": "images/brandcycle-128.png"
        }
    }
}
