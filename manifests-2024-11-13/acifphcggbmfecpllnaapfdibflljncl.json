{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "IntelliSecure Scanner",
  "version": "1.4",
  "description": "Automatically scans for Web3 addresses in URLs and provides token security information.",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
 "activeTab",
    "tabs"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'; img-src 'self' data:"
  },
  "web_accessible_resources": [
    {
      "resources": ["bg.jpg", "background.css", "logo.png"],
      "matches": ["<all_urls>"]
    }
  ],
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  }
}
