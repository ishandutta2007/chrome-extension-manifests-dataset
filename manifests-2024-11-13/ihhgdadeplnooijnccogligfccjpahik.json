{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "Impulse. Focus Control and Screen Time",
    "version": "1.7.1",
    "description": "Block URLs to improve your focus and productivity with Screen Time.",
    "permissions": ["storage", "tabs", "alarms", "windows", "webNavigation", "idle"],
    "host_permissions": ["http://*/*", "https://*/*", "https://api.mailjet.com/"],
    "background": {
        "service_worker": "background.js"
    },
    "options_page": "options/popup.html",
    "content_scripts": [
        {
            "matches": ["http://*/*", "https://*/*"],
            "js": ["content-scripts/content-scripts.js", "options/email-verification.js"],
            "css": ["content-scripts/content-scripts.css"]
        }
    ],
    "web_accessible_resources": [
        {
            "resources": ["onboarding.html"],
            "matches": ["http://*/*", "https://*/*"]
        }
    ],
    "action": {
        "default_popup": "options/popup.html",
        "default_icon": {
            "16": "images/icon16.png",
            "48": "images/icon48.png",
            "128": "images/icon128.png"
        },
        "default_title": "Impulse."
    },
    "icons": {
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    }
}