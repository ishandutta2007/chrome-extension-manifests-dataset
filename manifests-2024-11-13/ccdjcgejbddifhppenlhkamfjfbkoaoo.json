{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Twitter AdBlocker",
  "version": "1.0",
  "description": "Blocks ads on Twitter/X.",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "icons": {
    "256": "icon256.png"
  },
  "host_permissions": [
    "*://twitter.com/*",
    "*://x.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["*://twitter.com/*", "*://x.com/*"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  }
}
