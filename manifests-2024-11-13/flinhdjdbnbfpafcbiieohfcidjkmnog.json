{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Reddit Subreddit Remover",
  "description": "Hides specified subreddits from your reddit feed",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/logo16.png",
      "32": "/images/logo32.png",
      "48": "/images/logo48.png",
      "128": "/images/logo128.png"
    }
  },
  "icons": {
    "16": "/images/logo16.png",
    "32": "/images/logo32.png",
    "48": "/images/logo48.png",
    "128": "/images/logo128.png"
  },
  "content_scripts": [
  {
    "matches": [
      "http://www.reddit.com/",
      "https://www.reddit.com/",
      "http://www.reddit.com/*/*",
      "https://www.reddit.com/*/*"
    ],
    "js": ["script.js"]
  }]
}
