{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "ContactSwing",
    "version": "1.2",
    "description": "Contactswing browser extension for ease of accessibility.",
    "permissions": [
        "storage",
        "sidePanel"
    ],
    "icons": {
        "16": "icons/logo_16.png",
        "48": "icons/logo_48.png",
        "128": "icons/logo_128.png"
    },
    "action": {
        "default_title": "Open contactswing"
    },
    "side_panel": {
        "default_path": "sidepanel.html"
    },
    "content_scripts": [
        {
            "matches": [
                "https://mail.google.com/*"
            ],
            "js": ["./content_scripts/vue_content.js"]
        }
    ],
    "background": {
        "service_worker": "background.js",
        "type": "module"
    },
    "host_permissions": [
        "https://mail.google.com/*"
    ],
    "web_accessible_resources": [
        {
            "resources": ["assets/*", "resources/*"],
            "matches": ["https://mail.google.com/*"]
        }
    ]
}