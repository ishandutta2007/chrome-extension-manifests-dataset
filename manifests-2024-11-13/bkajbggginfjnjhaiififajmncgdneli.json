{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 3,
  "name": "SFMC Query Saver",
  "version": "0.1.2",
  "description": "This Extension saves any SQL queries executed in Query Studio.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/icon16x16.png",
      "32": "/images/icon32x32.png",
      "48": "/images/icon48x48.png",
      "192": "/images/icon128x128.png"
    }
  },
  "icons": {
    "16": "/images/icon16x16.png",
    "32": "/images/icon32x32.png",
    "48": "/images/icon48x48.png",
    "192": "/images/icon128x128.png"
  },
  "background": {
    "service_worker": "./background.js"
  },
  "host_permissions": ["https://querystudio.herokuapp.com/*"],
  "permissions": ["storage", "webRequest", "tabs", "unlimitedStorage", "activeTab"],
  "content_scripts": [
    {
      "matches": [
        "https://querystudio.herokuapp.com/*",
        "*://*.exacttarget.com/*"
      ],
      "js": ["./content.js"],
      "css": ["./styles.css"]
    }
  ],"web_accessible_resources": [
    {
      "resources": ["images/icon48x48-off.png", "images/icon48x48.png"],
      "matches": [
        "https://querystudio.herokuapp.com/*",
        "*://*.exacttarget.com/*"
      ],
      "use_dynamic_url": true
    }
  ]
}
