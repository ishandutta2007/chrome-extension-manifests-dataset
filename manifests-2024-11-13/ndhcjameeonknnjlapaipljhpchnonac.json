{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "Amazon Custom Hider",
    "version": "1.0",
    "description": "Eine Erweiterung, die von der Amazon Startseite sensible Daten entfernt",
    "action": {
        "default_icon": {
            "48": "icon.png"
        }
    },
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": [
                "https://*.amazon.com/*",
                "https://*.amazon.de/*",
                "https://*.amazon.co.uk/*",
                "https://*.amazon.co.jp/*",
                "https://*.amazon.in/*",
                "https://*.amazon.ca/*",
                "https://*.amazon.com.au/*",
                "https://*.amazon.it/*",
                "https://*.amazon.fr/*",
                "https://*.amazon.es/*"
            ],
            "css": ["styles.css"],
            "run_at": "document_idle"
        }
    ],
    "icons": {
        "48": "icon.png"
    }
}
