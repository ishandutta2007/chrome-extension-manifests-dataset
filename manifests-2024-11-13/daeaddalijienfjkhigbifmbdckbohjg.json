{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "Ask Gemini",
    "version": "1.5",
    "description": "Access Google's Gemini AI from any webpage",
    "permissions": [
        "contextMenus",
        "activeTab",
        "storage"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js",
                "libs/typewritter.js",
                "libs/html2canvas.js",
                "Popup.js",
                "utils.js",
                "icons.js"
            ],
            "run_at": "document_start"
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "libs/showdown.js",
                "Popup.js",
                "popup.css",
                "libs/microtip.css",
                "assets/fonts/*",
                "assets/fonts/*"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ],
    "content_security_policy": {
        "content_security_policy": {
            "extension_pages": "script-src 'self'; object-src 'self'; font-src 'self' https://fonts.googleapis.com; style-src 'self' 'unsafe-inline';"
        }
    },
    "options_ui": {
        "page": "options.html",
        "open_in_tab": true
    },
    "icons": {
        "16": "icon16.png"
    }
}