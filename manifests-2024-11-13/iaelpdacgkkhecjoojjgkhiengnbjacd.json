  "manifest_version": 3,
  "name": "MemoryPlugin - Long-Term Memory for AI Chats",
  "version": "1.5.0",
  "description": "Stop repeating important details in every new AI chat with shared memory across your chats on ChatGPT, Claude, Gemini, and more.",
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "https://*.memoryplugin.com/*"
  ],
  "icons": {
    "16": "public/16x16.png",
    "32": "public/32x32.png",
    "48": "public/48x48.png",
    "128": "public/128x128.png"
  },
  "background": {
    "service_worker": "src/background.js"
  },
  "key": "",
  "content_scripts": [
    {
      "exclude_matches": [
        "https://www.memoryplugin.com/*"
      ],
      "matches": [
        "https://www.anthropic.com/*",
        "https://claude.ai/*",
        "https://chat.openai.com/*",
        "https://chatgpt.com/*",
        "https://gemini.google.com/*"
      ],
      "js": [
        "src/content.js"
      ],
      "css": [
        "src/tailwind.css"
      ],
      "run_at": "document_start"
    }
  ],
  "action": {
    "default_popup": "public/index.html"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'; style-src 'self' 'unsafe-inline'"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "public/hero-img.webp",
        "public/128x128.png",
        "src/content.js",
        "src/chunks/*.js",
        "chunks/*.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
