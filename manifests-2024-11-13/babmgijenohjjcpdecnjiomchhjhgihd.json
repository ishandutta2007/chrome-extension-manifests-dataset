  "manifest_version": 3,
  "name": "myHawk",
  "version": "0.1",
  "description": "Inject rich HTML content. Interface is in the \"Developer Tools\" panel.",
  "icons": {
    "16": "icons/myHawk.16.png",
    "24": "icons/myHawk.24.png",
    "32": "icons/myHawk.32.png",
    "48": "icons/myHawk.48.png",
