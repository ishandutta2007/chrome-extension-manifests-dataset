  "applications": {
    "gecko": {
      "id": "myHawk@bacteriajoecreative.com"
    }
  },
  "devtools_page" : "devtools_page.html",
  "permissions":[
    "scripting",
    "tabs"
  ],
  "host_permissions":[
    "<all_urls>",
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts" : [{
    "matches"   : ["<all_urls>"],
    "match_about_blank" : true ,
    "js"        : [
                    "lib/content_inject.js",
                    "lib/JSML.js",
                    "lib/utilities.js"
                  ],
    "run_at"    : "document_start",
    "all_frames": true
  }]
