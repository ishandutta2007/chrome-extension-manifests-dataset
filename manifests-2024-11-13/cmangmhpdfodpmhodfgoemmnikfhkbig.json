{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "CadetNet.ext",
  "description": "A Simple CadetNet Enhancement Extension",
  "version": "1.24",
  "manifest_version": 3,
  "host_permissions": [
    "https://www.cadetnet.org.nz/*"
  ],
  "permissions": [
    "declarativeNetRequestWithHostAccess",
    "storage"
  ],
  "options_ui": {
    "page": "index.html",
    "open_in_tab": true
  },
  "web_accessible_resources": [
    {
      "resources": [
        "injected.js",
        "scripts/utils/XLSX.js",
        "scripts/global/getUnitId.js",
        "scripts/global/modal.js",
        "scripts/global/throbber.js",
        "scripts/subs/functions/exerciseSheet.js",
        "scripts/subs/functions/nokSheet.js",
        "scripts/subs/atpButtons.js",
        "scripts/subs/paperRoll.js",
        "scripts/subs/attendanceTricks.js",
        "scripts/subs/reportsIndex.js",
        "scripts/subs/utpButtons.js",
        "scripts/redirects/blocked_roll.js",
        "index.html",
        "options.html",
        "style.css",
        "resources/SheetTemplate.xlsx"
      ],
      "matches": [
        "https://*/*"
      ]
    }
  ],
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "attendanceCleanup",
        "enabled": true,
        "path": "attendanceCleanup_rules.json"
      }
    ]
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "plus_icon128.png"
  },
  "icons": {
    "16": "plus_icon16.png",
    "32": "plus_icon32.png",
    "48": "plus_icon48.png",
    "128": "plus_icon128.png"
  },
  "background": {
    "service_worker": "service-worker.js"
  },
  "content_scripts": [
    {
      "js": [
        "scripts/content.js"
      ],
      "matches": [
        "https://www.cadetnet.org.nz/*",
        "https://www.cadetnet.org.nz/*"
      ]
    }
  ]
}