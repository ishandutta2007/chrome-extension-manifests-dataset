{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "YouChrome",
  "description": "Extension to skip repeating videos for 24 hours",
  "version": "1.1",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/1.png",
      "48": "images/1.png",
      "128": "images/1.png"
    }
  },
  "permissions": [
    "activeTab",
    "scripting",
    "tabs",
    "storage",
    "background"
  ],
  "host_permissions": [
    "https://m.youtube.com/*",
    "https://www.youtube.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://m.youtube.com/*",
        "https://www.youtube.com/*"
      ],
      "js": [
        "lib/min.js",
        "content_script.js",
        "content.js"
      ]
    }
  ]
}