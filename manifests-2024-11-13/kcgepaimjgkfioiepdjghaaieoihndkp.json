---

{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "version": "1.0.0",
  "name": "ProspectDaddy Email Finder",
  "description": "Search prospects on LinkedIn, Find business emails on Gmail, & Download email-ready prospect lists",
  "homepage_url": "https://prospectdaddy.com",
  "permissions": [
    "storage",
    "webRequest",
    "tabs",
    "system.display"
  ],
  "host_permissions":[
   "*://mail.google.com/*",
   "*://contacts.google.com/*",
   "https://www.linkedin.com/*"

  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self' ; object-src 'self'",
    "service_worker": "script-src 'self'; object-src 'self'"
  },  
  "background": {
    "service_worker":  "build/background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.linkedin.com/*"
      ],
      "run_at": "document_idle",
      "js": [
        "build/inLoaders/init.js"

      ]
    },
    {
      "js": [
        "build/gmLoaders/content.js"
      ],
      "matches": [
        "*://*.mail.google.com/*"
      ]
    },
    {
      "all_frames": false,
      "js": [
        "build/gmLoaders/editor.js"
      ],
      "matches": [
        "*://*.mail.google.com/*"
      ],
      "run_at": "document_end"
    }
  ],
 "web_accessible_resources": [
  {
    "resources": [
      "prospectdaddy.html",
      "build/inLoaders/XMLHttpWatcher.js"
    ],
    "matches": ["<all_urls>"]
  }
  ],
  "icons": {
    "16": "icons/prospect-daddy-logo.png",  
    "48": "icons/prospect-daddy-logo-1.png",
    "128":"icons/prospect-daddy-logo-2.png"
  },
  "action": {
    "default_icon": {
      "16": "icons/prospect-daddy-logo.png",  
      "48": "icons/prospect-daddy-logo-1.png",
      "128":"icons/prospect-daddy-logo-2.png"
    },
    "default_title": "ProspectDaddy"
  },
  "commands": {
    "toggle-feature": {
      "suggested_key": {
        "default": "Ctrl+Shift+Y",
        "mac": "MacCtrl+Shift+Y"
      },
      "description": "Send a 'toggle-feature' event to the extension"
    },
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+L",
        "mac": "MacCtrl+Shift+L"
      }
    }
  }
}