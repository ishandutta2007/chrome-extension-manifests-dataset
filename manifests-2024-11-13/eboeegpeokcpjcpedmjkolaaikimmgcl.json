{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "wikiSd",
  "description": "This extension will change the fonts of Sindhi Wikipedia",
  "version": "1.0",
  "permissions": ["activeTab", "declarativeContent"],
  "background": {
    "background.service_worker": ["background.js"]
  },
 "icons": {
    "16": "icon/icon16.png",
    "32": "icon/icon32.png",
    "48": "icon/icon48.png",
    "128": "icon/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://sd.wikipedia.org/*"],
      "js": ["content.js"],
      "css": ["style.css"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html"
  }
}
