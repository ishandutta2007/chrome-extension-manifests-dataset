{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Upvote.club",
  "version": "1.0.2",
  "description": "Get more Twitter Followers, Likes, Reposts, Saves, Comments, not bots, no Ai, instantly, only real people.",
  "permissions": [
    "storage",
    "tabs",
    "activeTab"
  ],
  "host_permissions": [
    "https://api.upvote.club/*",
    "https://twitter.com/*",
    "https://x.com/*",
    "http://localhost:3000/*",
    "https://localhost:3000/*"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.twitter.com/*",
        "*://*.x.com/*",
        "*://*.upvote.club/*",
        "http://localhost:3000/*",
        "https://localhost:3000/*"
      ],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "externally_connectable": {
    "matches": [
      "https://*.upvote.club/*",
      "http://localhost:3000/*",
      "https://localhost:3000/*"
    ]
  }
}
