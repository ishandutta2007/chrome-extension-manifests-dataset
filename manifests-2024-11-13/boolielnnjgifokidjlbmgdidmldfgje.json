{
"update_url": "https://clients2.google.com/service/update2/crx",

    "name" : "Yaggee Screen Capturing",
    "author": "Develop Yaggee <develop@yaggee.com>",
    "version" : "1.0.0.1",
    "manifest_version" : 2,
    "minimum_chrome_version": "34",
    "description" : "This extension enables screen capturing support for https://www.yaggee.com pages.",
    "homepage_url": "https://www.yaggee.com/",
    "background": {
        "scripts": ["background-script.js"],
        "persistent": false
    },
    "content_scripts": [ {
       "js": [ "content-script.js" ],
       "all_frames": true,
       "run_at": "document_end",
       "matches": ["https://www.yaggee.com/*", "https://app.yaggee.com/*", "https://192.168.0.200/*"]
    }],
    "icons" : {
        "72" : "icon.png"
    },
    "permissions": [
        "desktopCapture"
    ],
    "web_accessible_resources": [
        "icon.png"
    ]
}
