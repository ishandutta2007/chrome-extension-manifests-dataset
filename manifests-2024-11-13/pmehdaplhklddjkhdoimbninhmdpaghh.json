{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Zalo CRM",
  "version": "2.0",
  "description": "A Chrome extension to synchronize Zalo & CRM360",
  "icons": {
    "128": "icons/logo-zalo.png"
  },
  "permissions": [
    "activeTab",
    "scripting",
    "storage",
    "windows",
    "tabs"
  ],
  "background": {
    "service_worker": "public/js/background/app.js"
  },
  "action": {
    "default_popup": "public/index.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://oa.zalo.me/*",
        "https://chat.zalo.me/*"

      ],
      "js": [
        "public/js/content/app.js"
      ]
    }
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'",
    "worker": "script-src 'self';"
  },
  "web_accessible_resources": [
    {
      "resources": ["public/js/assets/*"],
      "matches": ["<all_urls>"]
    }
  ]
}