{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "Telesur Extension",
    "description": "Extensión de navegador de Telesur para ver las noticias más recientes",
    "version": "1.0.0.0",

    "action":{
        "default_popup":"./popup/popup.html",
        "default_title": "Telesur",
        "default_icon": {
            "16": "./dist/img/16.png",
            "32": "./dist/img/32.png",
            "48": "./dist/img/48.png",
            "128": "./dist/img/128.png"
        }
    },

    "permissions":[
        "alarms"
    ],
    "content_scripts": [
        {
            "matches": ["https://www.telesurtv.net/wp-json/telesur/v1/news"],
            "css":["./popup/styles/styles.css"]
        }
    ],
    "background": {
        "service_worker": "./js/background.js"
    }
}