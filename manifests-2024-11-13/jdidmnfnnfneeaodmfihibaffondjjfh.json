{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Octavebytes- HubSpot CTI Connector",
  "version": "8.1",
  "permissions": [
    "activeTab",
    "scripting",
    "storage",
    "tabs",
    "system.display"
  ],
  "host_permissions": ["https://*.hubspot.com/*", "*://*/aws_connector.html"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Octavebytes- HubSpot CTI Connector"
  },
  "icons": {
    "16": "assets/logo-16.png",
    "24": "assets/logo-24.png",
    "32": "assets/logo-32.png",
    "48": "assets/logo-48.png",
    "128": "assets/logo.png"
  },
  "content_scripts": [
    {
      "matches": ["https://*.hubspot.com/*", "*://*/aws_connector.html"],
      "js": ["content.js"]
    }
  ],
  "options_page": "options/options.html"
}
