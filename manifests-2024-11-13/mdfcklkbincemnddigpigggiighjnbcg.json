{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "404kit",
  "version": "1.0.1",
  "description": "404kitの拡張機能です。画面上にかわいい猫が表示されるようにしたり、スクリーンショットを取ったりすることができます。created with omochi",
  "icons": {
    "36": "icons/36x36.png",
    "48": "icons/48x48.png",
    "72": "icons/72x72.png",
    "96": "icons/96x96.png",
    "144": "icons/144x144.png",
    "192": "icons/192x192.png"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "img/*",
        "screen_shot.html",
        "leave.html",
        "js/*"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/config_manager.js",
        "js/content_script.js",
        "js/neko.js"
      ]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "background": {
    "service_worker": "js/background.js"
  },
  "commands": {
    "tabLeft": {
      "suggested_key": {
        "default": "Ctrl+Left",
        "mac": "Command+Left"
      },
      "description": "アクティブなタブを左のタブにします"
    },
    "tabRight": {
      "suggested_key": {
        "default": "Ctrl+Right",
        "mac": "Command+Right"
      },
      "description": "アクティブなタブを右のタブにします"
    },
    "ScreenShot": {
      "suggested_key": {
        "default": "Ctrl+Up",
        "mac": "Command+Up"
      },
      "description": "スクリーンショットを取ります"
    }
  }
}