{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "AlignLinks",
  "version": "1.5",
  "description": "Insert links with ease, manage your insertions, find link building opportunities, and verify link placements",
  "permissions": [
    "contextMenus",
    "activeTab",
    "scripting",
    "storage",
    "tabs"
  ],
  "host_permissions": [
    "<all_urls>",
    "https://api.openai.com/"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    },
    {
      "matches": ["https://www.google.com/search?*"],
      "js": ["google-search-popup.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "insertion-popup.html",
        "parse-results.html",
        "meta-results.html",
        "google-search-popup.css",
        "sitemapParser.js",
        "urlParser.js",
        "donorMatcher.js",
        "utils.js",
        "relevancy-onpage.html",
        "relevancy-onpage.css",
        "relevancy-onpage.js"
      ],
      "matches": ["<all_urls>"]
    }
  ],
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "options_page": "settings.html"
}