    "linux": "MediaPrevTrack"
   },
   "description": "Fast Rewind",
   "global": true
  },
  "FastForwardAction": {
   "suggested_key": {
    "default": "MediaNextTrack",
    "windows": "MediaNextTrack",
    "mac": "MediaNextTrack",
    "chromeos": "MediaNextTrack",
    "linux": "MediaNextTrack"
   },
   "description": "Fast Forward",
   "global": true
  }
 }
