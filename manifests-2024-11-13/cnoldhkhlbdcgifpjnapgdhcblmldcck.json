{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "StudyLang",
  "description": "Chrome extension for studying languages!",
  "version": "1.0.0",
  "manifest_version": 3,
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "StudyLang",
    "default_icon": "icon.png"
  },
  "permissions": ["contextMenus", "storage", "unlimitedStorage", "tts"],
  "options_page": "options.html",
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "run_at": "document_end",
      "js": ["contentScript.js"]
    }
  ],
  "web_accessible_resources": [
    {
        "resources": ["icon.png", "youtubeInjection.js", "netflixInjection.js"],
        "matches": ["<all_urls>"]
    }
  ],
  "host_permissions": ["<all_urls>"],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
