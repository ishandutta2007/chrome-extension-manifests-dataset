  "manifest_version": 2,

  "name": "Voucher Genius Extension",
  "description": "This extension shows the Voucher Genius Extension",
  "version": "1.0.19",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
 "default_title": "Click for Vouchers!"
