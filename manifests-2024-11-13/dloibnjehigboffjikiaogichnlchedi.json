        "css": ["assets/css/vgstyles.css", "assets/css/vg-toaster-styles.css", "assets/css/3rd.css", "assets/css/fontawesome.min.css"]
   }
  ],
  "permissions": [
    "activeTab",
 "tabs",
 "clipboardWrite",
    "http://*/*",
 "https://*/*",
 "http://www.vouchergenius.com/*",
 "http://vouchergenius.com/*",
 "http://vgbackend.vouchergenius.com/vgbe/*"
