{
"update_url": "https://clients2.google.com/service/update2/crx",


  "manifest_version": 3,

  "name": "GitHub Repository Markdown Visualizer",

  "version": "1.0.0",

  "description": "Generate markdown view of GitHub repositories",

  "permissions": [

    "storage",

    "activeTab",

    "scripting"

  ],

  "host_permissions": [

    "https://github.com/*",

    "https://api.github.com/*"

  ],

  "action": {

    "default_popup": "popup.html",

    "default_icon": "icon/icon.png"

  },

  "background": {

    "service_worker": "background.js"

  },

  "content_scripts": [{

    "matches": ["https://github.com/*"],

    "js": ["content.js"],

    "css": ["styles.css"]

  }],

  "web_accessible_resources": [{

    "resources": ["markdown-viewer.html"],

    "matches": ["https://github.com/*"]

  }]

} 
































































