---

{
   "action": {
      "default_icon": "icons/icon-128_x_128.png",
      "default_popup": "src/layouts/popup.html"
   },
   "background": {
      "service_worker": "scripts/backgroundScript.js",
      "type": "module"
   },
   "commands": {
      "StartRecording": {
         "description": "Start capture",
         "suggested_key": {
            "default": "Alt+Shift+R",
            "mac": "Alt+Shift+R",
            "windows": "Alt+Shift+R"
         }
      },
      "StopRecording": {
         "description": "Stop capture",
         "suggested_key": {
            "default": "Alt+Shift+S",
            "mac": "Alt+Shift+S",
            "windows": "Alt+Shift+S"
         }
      }
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "scripts/content.js" ],
      "matches": [ "\u003Call_urls>" ],
      "run_at": "document_end"
   } ],
   "content_security_policy": {
      "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self';"
   },
   "default_locale": "en",
   "description": "__MSG_appDesc__",
   "icons": {
      "128": "icons/icon-128_x_128.png",
      "16": "icons/icon-16_x_16.png",
      "32": "icons/icon-32_x_32.png",
      "512": "icons/icon-512_x_512.png",
      "65": "icons/icon-64_x_64.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "__MSG_appName__",
   "permissions": [ "storage", "activeTab", "offscreen", "tabCapture", "downloads", "alarms" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "2.1.5"
}
