{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "Himate Extension",
    "version": "3.0",
    "action": {
        "default_popup": "index.html",
        "default_popup_open_in_tab": false,
        "matches": [
            "https://www.jobsearch.az/vacancies/*",
            "!https://github.com/*"
        ]
    },
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": [
                "https://www.jobsearch.az/vacancies/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "permissions": [
        "runtime"
    ],
    "icons": {
        "16": "icons/icon16.png"
    }
}