{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "YouTube Beatboxify",
    "version": "1.0.0",
    "icons": {
        "96": "icon.png"
    },
    "description": "Modify YouTube thumbnails to include Beatbox",
    "action": {
        "default_popup": "settings.html"
    },
    "permissions": [
        "storage"
    ],
    "host_permissions": ["*://*.youtube.com/*"],
    "content_scripts": [{
        "matches": ["*://*.youtube.com/*"],
        "js": ["beatboxify.js"],
        "run_at": "document_idle"
    }],
    "web_accessible_resources": [{
        "resources": [
            "images/*.png",
            "images/*.json"
        ],
        "matches": [
            "*://*/*"
        ]
    }]
