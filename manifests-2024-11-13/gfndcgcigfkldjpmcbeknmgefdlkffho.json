{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "フォームメモリー",
    "version": "1.0",
    "description": "webページのフォームに対して、入力済みの内容を保存、及び貼り付けを行います。",
    "permissions": ["storage", "activeTab", "scripting"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.PNG",
        "48": "icon.PNG",
        "128": "icon.PNG"
      }
    },
    "icons": {
      "16": "icon.PNG",
      "48": "icon.PNG",
      "128": "icon.PNG"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  