    "icons": { "16": "img/vetary-icon-16.png",
               "48": "img/vetary-icon-48.png",
               "128": "img/vetary-icon-128.png"
    },
    "permissions": [
        "tabs",
        "notifications",
        "storage",
        "background",
        "alarms",
        "cookies",
        "https://*.vetary.com/*"
    ]
