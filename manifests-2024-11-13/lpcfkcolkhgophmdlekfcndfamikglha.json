---

{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Aimfox - Linkedin Session Manager",
  "version": "1.0.1",
  "description": "Effortlessly connect your LinkedIn account to Aimfox with this handy session manager. No password required.",
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": ["cookies", "tabs"],
  "host_permissions": [
    "https://*.linkedin.com/*",
    "https://app.aimfox.com/*",
    "https://app.aimfox.dev/*"
  ],
  "background": {
    "service_worker": "service-worker.js",
    "type": "module"
  },
  "externally_connectable": {
    "matches": [
      "http://localhost:*/*",
      "https://app.aimfox.com/*",
      "https://app.aimfox.dev/*",
      "https://*.linkedin.com/*",
      "https://linkedin.com/*"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.linkedin.com/*",
        "https://linkedin.com/*"
      ],
      "js": ["hook_linkedin.js"],
      "run_at": "document_idle",
      "world": "MAIN"
    }
  ]
}
