        }
    ],
    "background": {
        "service_worker": "background.bundle.js"
    },
    "action": {
        "default_icon": {
            "128": "images/icon.png"
        },
        "default_popup": "popup.html",
        "default_title": "__MSG_AppName__"
    },
    "web_accessible_resources": [
