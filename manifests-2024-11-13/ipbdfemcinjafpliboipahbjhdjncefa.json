{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Text Copy Assistance Plugin for SAT",
  "version": "1.1.13",
  "description": "Unnecessary strings are removed, and the text is formatted while copying text from the SAT Text Database.",
  "permissions": ["activeTab", "storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icons/icon16.png",
      "48": "images/icons/icon48.png",
      "128": "images/icons/icon128.png"
    }
  },
  "background": {
    "service_worker": "js/background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://21dzk.l.u-tokyo.ac.jp/SAT2018/master30.php",
        "https://21dzk.l.u-tokyo.ac.jp/SAT/satdb2015.php",
        "https://21dzk.l.u-tokyo.ac.jp/SAT/ddb-sat2.php"
      ],
      "js": [
        "js/loadDic.js",
        "js/citation.js", 
        "js/content.js"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["resources/CharacterMap.json", "resources/tripika.json"],
      "matches": ["<all_urls>"]
    }
  ],
  "icons": {
    "16": "images/icons/icon16.png",
    "48": "images/icons/icon48.png",
    "128": "images/icons/icon128.png"
  }
}