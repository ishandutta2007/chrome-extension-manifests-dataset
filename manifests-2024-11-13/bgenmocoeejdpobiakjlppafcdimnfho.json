{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "搜索拐杖",
  "version": "3.30",
  "manifest_version": 3,
  "description": "在Google，百度，必应等等搜索引擎间进行快捷切换，搜索关键词无需重输！",
  "homepage_url": "http://xuhehuan.com/2272.html",
  "background" : {
    "service_worker": "js/background.js"
  },
  "action": { 
    "default_icon": {
      "19": "img/icon-19.png",
      "38": "img/icon-38.png"
