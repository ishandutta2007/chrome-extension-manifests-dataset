      }
    },
    "explore": {
      "description": "__MSG_command_explore__",
      "suggested_key": {
        "default": "Ctrl+J"
      }
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Kimi Popup",
