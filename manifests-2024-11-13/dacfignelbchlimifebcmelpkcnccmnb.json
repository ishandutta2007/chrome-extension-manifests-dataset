{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "Youtube Watch Later Video Remover",
    "version": "1.0",
    "description": "Removes all videos from the YouTube Watch Later playlist.",
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "action": {
        "default_popup": "index.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": ["*://www.youtube.com/*"],
            "js": ["content.js"]
        }
    ],
    "icons": {
        "128": "icon.png"
    }
}
