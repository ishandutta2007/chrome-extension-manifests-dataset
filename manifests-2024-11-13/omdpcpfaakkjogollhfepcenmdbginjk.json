{
"update_url": "https://clients2.google.com/service/update2/crx",

 "manifest_version": 3,
 "name": "Auto Points Clicker",
 "version": "1.2",
 "description": "Automatically clicks a specific link on certain websites.",
 "permissions": [
  "activeTab",
  "https://*/*"
 ],
 "background": {
  "service_worker": "background.js"
 },
 "action": {
  "default_popup": "popup.html",
  "default_icon": {
   "16": "assets/images/icon.png",
   "48": "assets/images/icon.png"
  }
 },
 "content_scripts": [
  {
   "matches": [
    "http://100percentclicks.com/*",
    "https://100percentclicks.com/*"
   ],
   "js": ["assets/js/100percentclicks.js"],
   "all_frames":true
  },{
   "matches": [
    "http://email-hog.com/*",
    "https://email-hog.com/*"
   ],
   "js": ["assets/js/emailHog.js"],
   "all_frames":true
  },{
   "matches": [
    "http://guaranteedmails.com/*",
    "https://guaranteedmails.com/*"
   ],
   "js": ["assets/js/guaranteedMails.js"],
   "all_frames":true
  },{
   "matches": [
    "http://mail.google.com/*",
    "https://mail.google.com/*"
   ],
   "js": ["assets/js/helper.js", "assets/js/gmail.js"],
   "all_frames":true
  },{
   "matches": [
    "http://*/*",
    "https://*/*"
   ],
   "js": [
    "includes/jquery.min.js",
    "includes/crypto-js.min.js",
    "assets/js/helper.js",
    "assets/js/all.js"
   ],
   "all_frames":true
  }
 ]
}