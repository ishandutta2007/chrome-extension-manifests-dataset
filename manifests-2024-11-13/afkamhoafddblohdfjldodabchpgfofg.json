{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Youtube Bookmarker",
  "description": "A bookmark extension for your videos",
  "version": "1.0.7",
  "manifest_version": 3,
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
