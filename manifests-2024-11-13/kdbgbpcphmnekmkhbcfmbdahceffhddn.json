{
  "manifest_version": 3,
  "name": "Netsweeper Client Filter",
  "version": "12.51.55.55",
  "description": "Netsweeper Client Filter",
  "icons": {
          "128": "128.png",
          "16": "small.png",
          "48": "48.png"
  },
  "permissions": [
    "identity",
    "identity.email",
    "enterprise.deviceAttributes",
    "activeTab",
    "tabs",
    "storage",
    "browsingData",
    "declarativeNetRequest",
    "declarativeNetRequestWithHostAccess",
    "declarativeNetRequestFeedback",
    "offscreen",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "all_frames": true,
      "run_at": "document_start",
      "js": ["content.js"],
      "css": ["hide_body.css"]
    }
  ],
  "background": {
    "service_worker": "service_main.js"
  },
  "storage": {
    "managed_schema": "schema.json"
  },
  "action": {

  },
  "web_accessible_resources": [{
    "resources": ["error.html", "denied.html", "disclosure.html", "nmonitor.wasm.map", "third_party_licenses.md"],
    "matches": ["<all_urls>"]
  }],
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "youtube_strict",
        "enabled": false,
        "path": "youtube_strict.json"
      },
      {
        "id": "youtube_moderate",
        "enabled": false,
        "path": "youtube_moderate.json"
      }
    ]
  },
  "update_url": "https://clients2.google.com/service/update2/crx",
  "key": "",
  "content_security_policy": {
    "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self'; connect-src 'self' https://*:*;"
  }
}
