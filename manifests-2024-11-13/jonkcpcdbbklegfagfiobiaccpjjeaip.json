{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "I don't want you!",
    "version": "0.2",
    "description": "An extension that blocks you from visiting a website and hides the website from Google search results.",
    "permissions": [
        "storage",
        "activeTab",
        "scripting"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["content.js"]
    }],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "img/img16.png",
            "48": "img/img48.png",
            "128": "img/img128.png"
        }
    },
    "web_accessible_resources": [{
        "resources": ["uhoh.html", "img/img.png", "content.js"],
        "matches": ["<all_urls>"]
    }],
    "icons": {
        "16": "img/img16.png",
        "48": "img/img48.png",
        "128": "img/img128.png"
    }
}