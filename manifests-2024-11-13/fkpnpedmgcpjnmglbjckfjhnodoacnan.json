{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "G-Switcher",
  "version": "1.1",
  "description": "Allows a user to easily switch between Google accounts",


  "icons": {
    "48": "icon.png"
  },

  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "G-Switcher"
