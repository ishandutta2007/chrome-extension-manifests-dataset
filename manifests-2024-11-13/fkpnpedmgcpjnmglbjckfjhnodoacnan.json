  "background": {
    "scripts": ["action.js", "install.js"]
  },

  "content_scripts": [
    {
      "matches": ["*://*.google.com/*"],
      "js": ["swap.js"]
    }
  ],

  "permissions": ["storage"],

  "options_ui": {
    "page": "options.html"
  }


