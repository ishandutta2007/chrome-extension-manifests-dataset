{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "Amazon 請求書ダウンローダー",
    "version": "1.0.3",
    "permissions": [
        "activeTab",
        "tabs",
        "storage",
        "scripting"
    ],
    "host_permissions": [
      "http://localhost:3000/*",
      "https://www.amazon.co.jp/*"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
        "default_icon": {
            "64": "logo.png"
         },
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": ["*://*.amazon.co.jp/*"],
            "js": ["libs/pdf.min.js", "libs/pdf.worker.min.js", "content.js"],
            "all_frames": true
        }
    ],
    "web_accessible_resources": [
        {
            "resources": ["main.html","main.js","spinner.gif", "content.js"],
            "matches": ["<all_urls>"]
        }
    ]
  }
  