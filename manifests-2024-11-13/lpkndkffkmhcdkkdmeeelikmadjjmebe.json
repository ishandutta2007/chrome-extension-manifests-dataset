    "browser_action":{
        "default_icon":{
            "19":"img/icon_19.png",
            "38":"img/icon_38.png"
        },
        "default_title": "Show Emoji Keyboard"
    },
    "background":{
        "scripts":["js/backgroundscript.js"]
    }
