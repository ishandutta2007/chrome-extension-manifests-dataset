{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Slack it!",
  "version": "1.1",
  "description": "Instant Tab Bookmarking to Slack",
  "permissions": [
    "activeTab",
    "storage",
    "notifications",
    "scripting"
  ],
  "host_permissions": [
    "https://hooks.slack.com/"
  ],
  "action": {
    "default_icon": "icon.png",
    "default_title": "Slack it!",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon_16.png",
    "32": "icon_32.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "options_page": "options.html",
  "web_accessible_resources": [
    {
      "resources": [
        "success-icon.png",
        "error-icon.png"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
