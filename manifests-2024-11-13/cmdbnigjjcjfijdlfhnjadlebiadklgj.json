{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Safe Sound for YouTube",
  "version": "1.0.1",
  "description": "Balances sound levels between YouTube videos and ads, and reduces sudden high noises.",
  "permissions": ["tabs", "activeTab"],
  "icons": {
    "16": "icons/icon.png",
    "48": "icons/icon.png",
    "128": "icons/icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/*"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icons/icon.png"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "icons/blink.png",
        "icons/active.png"
      ],
      "matches": ["*://www.youtube.com/*"]
    }
  ],
  "host_permissions": ["*://www.youtube.com/*"],
  "background": {
    "service_worker": "background.js"
  }
}
