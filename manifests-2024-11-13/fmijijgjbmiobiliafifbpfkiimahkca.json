  "background": {
  "scripts": ["event.js"]
  },

  "browser_action": {
    "default_title": "whaskins.com",
    "default_popup": "popup.html"
  },

  "permissions": [
    "https://www.tesco.com/direct",
    "https://www.amazon.co.uk/",
    "http://www.argos.co.uk/",
