{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "whaskins.com extension",
  "version": "0.1.6",
  "content_scripts": [
    {
      "matches": [
        "https://www.tesco.com/direct/*",
        "https://www.amazon.co.uk/*",
        "http://www.argos.co.uk/*",
