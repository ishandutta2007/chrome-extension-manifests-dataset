{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Message Master – AI texting generator",
  "version": "1.1.7",
  "description": "Message Master – AI texting generation tool to enhance interaction with your audience",
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon_16.png",
      "48": "icon_48.png",
      "128": "icon_128.png",
      "192": "icon_192.png"
    }
  },
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["*://*.onlyfans.com/*", "*://*.onlyfans.live/*"],
      "js": ["onlyFans.js"],
      "css": ["styles.css"]
    },
    {
      "matches": ["*://web.telegram.org/a/*"],
      "js": ["telegramA.js"],
      "css": ["styles.css"]
    },
    {
      "matches": ["*://*.telegram.org/k/*"],
      "js": ["telegramK.js"],
      "css": ["styles.css"]
    },
    {
      "matches": ["*://*.instagram.com/*"],
      "js": ["instagram.js"],
      "css": ["styles.css"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["button.svg", "button_error.svg"],
      "matches": ["<all_urls>"]
    }
  ],
  "icons": {
      "16": "icon_16.png",
      "48": "icon_48.png",
      "128": "icon_128.png",
      "192": "icon_192.png"
  }
}
