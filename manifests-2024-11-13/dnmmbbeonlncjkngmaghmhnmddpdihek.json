{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "EnTribe Extension",
  "description": "Import Instagram content to EnTribe platform",
  "version": "1.0.2",

  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "EnTribe Extension"
  },
  "icons": {
    "16": "48x48.png",
    "48": "48x48.png",
    "128": "144x144.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },

  "content_scripts": [{
    "matches": ["*://*/*"],
    "js": ["content.js"],
    "all_frames": true
  }],
  "permissions": [
    "https://*.instagram.com/*",
    "https://*.entribe.com/*",
    "tabs",
    "cookies"
  ]
}
