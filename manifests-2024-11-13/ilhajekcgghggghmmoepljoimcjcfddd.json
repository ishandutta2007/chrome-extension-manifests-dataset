"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Block Fox News",
  "description": "Blocks foxnews.com and disables links to Fox News from all sites",
  "version": "1.0.2",
  "action": {
    "default_icon": {
      "16": "icons/block-fox-news-16.png",
