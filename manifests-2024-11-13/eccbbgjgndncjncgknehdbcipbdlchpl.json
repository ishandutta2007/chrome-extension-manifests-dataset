          "js": ["content.js"]
        }
      ],
    "action": {
      "default_popup": "popup.html"
    },
    "web_accessible_resources": [
      {
        "resources": [ "logo.svg" ],
        "matches": [ "<all_urls>" ]
      }
    ]
  }
  