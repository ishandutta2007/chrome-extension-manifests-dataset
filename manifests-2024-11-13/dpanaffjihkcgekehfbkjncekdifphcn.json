"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "AutomationEdge Web Extension",
  "description": "AutomationEdge extension for web interaction",
  "version": "2.4.0",
  "manifest_version": 3,
  "permissions": [
    "activeTab",
    "tabs",
    "webNavigation",
    "scripting",
    "storage",
 "debugger"
  ],
  "host_permissions": [
    "*://*/*",
    "<all_urls>"
  ],
  "icons": {
    "16": "icons/ae16x16.png",
    "32": "icons/ae32x32.png",
    "48": "icons/ae48x48.png",
    "64": "icons/ae64x64.png",
    "128": "icons/ae128x128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "matches": ["<all_urls>"],
      "match_about_blank": true,
      "js": ["dom-to-image.min.js", "content.js"]
    }
  ]
}
