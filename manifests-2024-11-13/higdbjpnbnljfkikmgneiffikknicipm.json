"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Amax：良い商品が楽しく見つかる Chrome 拡張",
  "description": "Amax（エイマックス）は「良い商品が楽しく見つかる」をテーマに、普段の Amazon での買い物が感動的に使いやすくなる拡張機能です。",
  "version": "1.2.5",
  "action": {
    "default_popup": "",
    "default_icon": "logo.png"
  },
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
  "background": {
    "service_worker": "background.bundle.js"
  },
  "content_scripts": [
    {
      "matches": [
        "*://www.amazon.co.jp/*"
      ],
      "js": [
        "content.bundle.js"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "imgs/*.png"
      ],
      "matches": [
        "*://www.amazon.co.jp/*"
      ]
    }
  ],
  "permissions": [
    "storage",
    "bookmarks"
  ]
