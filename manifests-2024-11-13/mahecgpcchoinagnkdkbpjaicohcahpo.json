{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "Easy Pinboard",
  "description": "Save current tab to pinboard.",
  "version": "1.0",
  "permissions": [
    "tabs",
    "*://*.pinboard.in/"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "browser_action": {
    "default_title": "Easy Pinboard",
    "default_icon": "theme/img/pin_grey.png",
    "default_popup": "debug.html",
    "icons": ["theme/img/pin_grey.png", "theme/img/pin_colored.png"]
  },

  "icons": {
    "16": "theme/img/icon_16.png",
    "48": "theme/img/icon_48.png",
    "128": "theme/img/icon_128.png"
  },

  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "windows": "Ctrl+Shift+P",
        "mac": "Command+Shift+P",
        "chromeos": "Ctrl+Shift+P",
        "linux": "Ctrl+Shift+P"
      }
    }
  }
}
