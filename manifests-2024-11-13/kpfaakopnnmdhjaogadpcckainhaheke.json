{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "Script Sidekick GHL Extension",
    "version": "1.0",
    "description": "This extension adds an button to GHL opportunities to open script sidekick scripts for a given contact.",
    "action": {
      "default_popup": "index.html",
      "default_width": 400,
      "default_icon": {
        "16": "ss-phone.png",
        "48": "ss-phone.png",
        "128": "ss-phone.png"
      }
    },
    "icons": {
      "16": "ss-phone.png",
      "48": "ss-phone.png",
      "128": "ss-phone.png"
    },
    "permissions": [
      "storage"
    ],
    "host_permissions": [
      "https://app.script-sidekick.com/*"
    ],
    "content_scripts": [
      {
        "matches": [
          "https://app.gohighlevel.com/*",
          "https://developer.chrome.com/docs/webstore/*"
        ],
        "js": ["content.js"],
        "run_at": "document_idle"
      }
    ],
    "background": {
      "service_worker": "background.js"
    }
  }