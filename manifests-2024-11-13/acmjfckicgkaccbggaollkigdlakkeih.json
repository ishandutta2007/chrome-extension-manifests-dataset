name: Real facebook filters
version: '1.1'
category_slug: productivity/tools
rating: 3.6666666666666665
rating_count: 3
user_count: 25
release_date: '2024-11-07T07:29:04.000Z'
size: 1.2MiB
languages:
  - English
description: >-
  This extension enforces the location filter on Facebook Marketplace, showing
  only listings from your selected area.
---

{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "Real facebook filters",
    "version": "1.1",
    "content_scripts": [{
        "matches": ["https://*.facebook.com/*"],
        "js": ["contentScript.js"],
        "run_at": "document_start",
        "type": "module"
    }],
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["storage"],
    "web_accessible_resources": [{
    "resources": ["injectedScript.js"],
        "matches": ["https://*.facebook.com/*"],
        "type": "module"
    }],
    "icons": {
        "16": "16x16.png",
        "32": "32x32.png",
        "48": "48x48.png",
        "128": "128x128.png"
    },
    "host_permissions": [
        "https://nominatim.openstreetmap.org/*"
    ]
  }
  