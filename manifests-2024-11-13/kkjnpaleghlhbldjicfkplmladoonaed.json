{
"update_url": "https://clients2.google.com/service/update2/crx",

 "manifest_version": 3,
 "name": "UC Schedule Helper",
 "version": "1.0",
 "description": "Load RateMyProfessor ratings on the UC Davis schedule builder website! This extension works for UC Davis.",
 "permissions": [
  "https://corsanywhere.herokuapp.com/solr-aws-elb-production.ratemyprofessors.com/*",
  "storage"
 ],
 "icons": {
  "128": "icon.png",
  "32": "icon.png",
  "48": "icon.png"
 },
 "action": {
  "default_icon": {
   "16": "icon.png",
   "32": "icon.png"
  },
  "default_popup": "popup.html",
  "default_title": "Check here to see UC Schedule Helper instructions and developer credits."
 },
 "content_scripts": [
  {
   "matches": [
    "https://my.ucdavis.edu/schedulebuilder/*"
   ],
   "js": [
    "jquery-3.4.1.min.js",
    "content.js"
   ],
   "run_at": "document_end"
  }
 ]
}
