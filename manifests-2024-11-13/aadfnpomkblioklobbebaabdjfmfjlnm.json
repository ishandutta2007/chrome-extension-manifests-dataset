  "background": {
    "service_worker": "js/background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "*://www.linkedin.com/*"
      ],
      "js": [
        "js/content.js"
      ]
    }
  ],
  "icons": {
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "host_permissions": [
    "https://www.linkedin.com/*"
  ]
}