    "38": "/icons/icon_38.png",
    "48": "/icons/icon_48.png",
    "128": "/icons/icon_128.png"
  },
  "manifest_version": 3,
  "name": "YouTube Enhancer",
  "options_ui": {
    "page": "src/pages/options/index.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "tabs",
    "scripting"
  ],
