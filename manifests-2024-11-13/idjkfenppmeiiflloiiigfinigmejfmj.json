{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Subreddit Manager",
  "version": "1.0",
  "description": "Automatically bulk extract and subscribe to lists of subreddits.",
  "permissions": ["tabs", "scripting", "activeTab"],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": ["https://old.reddit.com/r/*"],
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": ["https://old.reddit.com/subreddits/"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/assets/icons/icon16.png",
      "32": "/assets/icons/icon32.png",
      "48": "/assets/icons/icon48.png",
      "128": "/assets/icons/icon128.png"
    }
  },
  "icons": {
    "16": "/assets/icons/icon16.png",
    "32": "/assets/icons/icon32.png",
    "48": "/assets/icons/icon48.png",
    "128": "/assets/icons/icon128.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["./assets/fonts/*.ttf", "assets/icons/*.svg"],
      "matches": ["<all_urls>"]
    }
  ]
}
