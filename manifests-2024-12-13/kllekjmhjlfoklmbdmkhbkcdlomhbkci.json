{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "version": "1.0.0",
  "name": "SonyLiv video watch party",
  "description": "One click is all it takes to start enjoying SonyLiv videos with your friends using this extension.",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["react-app-holder.js"],
      "all_frames": true,
      "run_at": "document_end"
    },
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "all_frames": true,
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "images/icons/grey16.png",
    "32": "images/icons/red32.png",
    "64": "images/icons/red64.png",
    "128": "images/icons/red128.png"
  },
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["tabs", "storage", "contentSettings", "notifications", "declarativeNetRequest"],
  "host_permissions": ["*://*/*"],
  "web_accessible_resources": [
    {
      "resources": ["images/*", "fonts/*", "videoScript.js", "images/icons/*","images/logos/*"],
      "matches": ["<all_urls>"]
    }
  ]
