  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "web_accessible_resources": [{
    "resources": ["auth.html", "setup.html"],
    "matches": ["<all_urls>"]
  }],
  "critical": true,
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
