{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "Title Mirror",
    "version": "1.5.2",
    "description": "Mirror any element's content to page title - 将任意元素内容同步显示到网页标题",
    "icons": {
      "32": "images/eye_icon_32.png",
      "48": "images/eye_icon_48.png",
      "128": "images/eye_icon_128.png"
    },
    "permissions": [
      "tabs",
      "activeTab",
      "storage"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "32": "images/eye_icon_32.png",
        "48": "images/eye_icon_48.png",
        "128": "images/eye_icon_128.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    },
    "options_page": "options.html",
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "run_at": "document_end"
      }
    ]
  }