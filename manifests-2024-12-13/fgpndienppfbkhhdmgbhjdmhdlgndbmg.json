    "48": "img/fi_chat_48.png",
    "128": "img/fi_chat_128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.fiverr.com/inbox/*",
        "http://www.fiverr.com/inbox/*",
        "https://www.fiverr.com/inbox/",
        "http://www.fiverr.com/inbox/"
      ],
      "js": [
        "js/jquery.min.js",
        "js/jTippy.js",
        "js/bootstrap.bundle.min.js",
        "js/constants.js",
        "js/quill.js",
        "js/jq-ui.js",
        "js/content.js",
        "js/fc_api.js"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "img/*",
        "css/*"
      ],
      "matches": [
        "<all_urls>"
