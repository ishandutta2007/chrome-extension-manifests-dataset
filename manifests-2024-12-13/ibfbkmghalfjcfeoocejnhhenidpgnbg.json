  "manifest_version": 2,
  "name": "Denote",
  "description": "Displays reblogs with comments on tumblr posts.",
  "version": "0.3.0",

  "permissions": [
    "http://*.tumblr.com/post/*"
  ],

  "browser_action": {
    "default_icon": "images/icon.png",
