"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Wide GPT",
  "version": "1.0.5",
  "description": "Expand ChatGPT's screen instantly with 'Wide GPT' Chrome extension! No clicks needed, free and easy to install. Get more space now!",
