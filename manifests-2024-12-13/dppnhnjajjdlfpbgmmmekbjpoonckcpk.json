  "permissions": [
    "activeTab",
    "scripting"
  ],
  "host_permissions": ["https://*.reversepp.com/*"],
  "content_scripts": [
    {
      "matches": [
        "https://www.rightmove.co.uk/properties/*"
      ],
      "js": ["js/popup.js"],
      "css": ["css/style.css"],
      "run_at": "document_end"
    }
  ]
