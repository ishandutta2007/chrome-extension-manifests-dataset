  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "css": [
      ],
      "js": [
        "/js/pagecolor.js"
      ],
      "run_at": "document_idle",
      "all_frames": false
    }
  ]
