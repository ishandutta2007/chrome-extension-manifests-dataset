{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Prima.Law",
  "version": "3.0.23",
  "manifest_version": 3,
  "description": "Prima.Law extension",
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "permissions": ["debugger", "background","windows","storage", "scripting", "notifications", "webNavigation","tabs","webRequest"],
  "web_accessible_resources": [
    {
      "resources": ["img/*"],
      "matches": ["<all_urls>"]
    },
    {
      "resources": ["icon/*"],
      "matches": ["<all_urls>"]
    }
  ],
  "icons": {
    "16": "icon/icon_16.png",
    "32": "icon/icon_32.png",
    "48": "icon/icon_48.png",
    "128": "icon/icon_128.png"
  },
  "action": {
    "default_popup": "./html/popup.html",
    "default_title": "Prima.Law",
    "default_icon": {
      "16": "icon/icon_16.png",
      "32": "icon/icon_32.png",
      "48": "icon/icon_48.png", 
      "128": "icon/icon_128.png"
    }
  },  
  "host_permissions": [
    "https://egov.uscis.gov/*",
    "https://i94.cbp.dhs.gov/*",
    "https://ceac.state.gov/IV/*",
    "https://acis.eoir.justice.gov/*",
    "https://portal.eoir.justice.gov/*",
    "https://ceac.state.gov/GenNIV/General/complete/*",
    "https://my.uscis.gov/*",
    "https://travel.state.gov/*",
    "https://flag.dol.gov/dashboard/application/*"
  ],
  "externally_connectable": {
    "matches": [
      "https://app.prima.law/*",
      "https://www.primafacieapp.com/*",
      "https://staging.primafacieapp.com/*",
      "https://staging2.primafacieapp.com/*",
      "https://app.staging.prima.law/*"
    ]
  },
  "content_security_policy": {
    "extension_pages": "default-src 'self'; style-src 'self' 'unsafe-inline'"
  }
}
