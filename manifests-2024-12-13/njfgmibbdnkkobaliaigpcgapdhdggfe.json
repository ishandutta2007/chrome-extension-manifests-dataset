    "create_prompt": {
      "suggested_key": {
        "default": "Ctrl+Shift+P"
      },
      "description": "Capture screenshot"
    }
  },
  "background": {
    "service_worker": "background/index.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content_scripts/index.js"],
