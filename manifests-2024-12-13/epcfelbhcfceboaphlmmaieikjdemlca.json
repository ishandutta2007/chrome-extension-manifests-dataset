{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "UNOsync",
    "version": "1.0",
    "description": "Syncs WhatsApp messages to Hubspot",
    "permissions": [
        "activeTab",
        "storage",
        "tabs",
        "identity"
    ],
    "host_permissions": [
        "https://*.google.com/*",
        "http://localhost/*",
        "https://*.hubspot.com/*",
        "http://localhost:5001/*"
    ],
    "content_scripts": [
        {
            "matches": ["https://*.hubspot.com/*"],
            "js": ["hubspot-scripts.js"]
        },
        {
            "matches": ["https://web.whatsapp.com/*"],
            "js": ["whatsapp-scripts.js"]
        }
    ],
    "action": {
        "default_icon": {
            "16": "images/icon-16.png",
            "32": "images/icon-32.png",
            "48": "images/icon-48.png",
            "128": "images/icon-128.png"
        }
    },
    "background": {
        "service_worker": "background.js"
    },
    "oauth2": {
        "client_id": "37aec8b6-57c3-4d44-8b0a-1be9c3383e7d",
        "scopes": [
            "crm.objects.contacts.read",
            "crm.objects.contacts.write",
            "crm.objects.owners.read"
        ]
    },
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    }
}
