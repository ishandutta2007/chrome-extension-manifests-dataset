{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "Salesforce Picklist Exporter",
    "version": "0.0.1",
    "description": "Export dependent picklist values from Salesforce.",
    "author": {
      "name": "Shivam Kapasia",
      "url": "https://github.com/shivamkapasia0"
    },
    "permissions": ["activeTab", "contextMenus", "tabs"],
    "content_scripts": [
      {
        "matches": [
          "https://*.force.com/*depend*",
          "https://*.salesforce.com/*depend*",
          "https://*.cloudforce.com/*depend*",
          "https://*.salesforce-setup.com/*depend*"
        ],
        "js": ["content.js", "popup.js"],
        "run_at": "document_end",
        "all_frames": true
      }
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "content_security_policy": {
      "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self'"
    }
  }
  