  "manifest_version": 3,
  "name": "Element Selector",
  "version": "1.0",
  "description": "Select and highlight elements on a webpage",
  "icons": {
    "16": "icon.svg",
    "48": "icon.svg",
    "128": "icon.svg"
  },
  "permissions": [
    "activeTab"
  ],
  "action": {
    "default_title": "Element Selector"
  },
  "background": {
