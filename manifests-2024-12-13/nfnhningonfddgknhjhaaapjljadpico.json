    "*://*.tmall.com/*",
    "*://*.aliexpress.com/*",
    "*://*.aliexpress.ru/*",
    "*://*.aliexpress.us/*",
    "*://*.aliexpress.de/*",
    "*://*.aliexpress.es/*",
    "*://*.aliexpress.it/*",
    "*://*.aliexpress.fr/*",
    "*://*.aliexpress.nl/*",
    "*://*.aliexpress.pl/*",
    "*://*.aliexpress.br/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
