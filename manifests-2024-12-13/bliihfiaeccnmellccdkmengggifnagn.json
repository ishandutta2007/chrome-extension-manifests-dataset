  "permissions": ["clipboardWrite", "storage"],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self';"
  },
  "web_accessible_resources": [
    {
      "resources": ["popup.html", "scripts/popup.js", "styles/style.css"],
      "matches": ["<all_urls>"]
    }
  ]
