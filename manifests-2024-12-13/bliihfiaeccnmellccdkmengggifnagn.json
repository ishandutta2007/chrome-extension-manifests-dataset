"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "One-Click-Copy",
  "description": "This One Click Copy extension will copy predefined text. Developed by Thomas Santiago",
  "version": "1.3.0",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
