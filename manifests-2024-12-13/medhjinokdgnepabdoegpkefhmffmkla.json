{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Slither.io With Friends",
  "version": "3.6",
  "description": "Extension to easy play Slither.io with friends",
  "manifest_version": 3,
  "permissions": [
    "activeTab",
    "scripting",
    "clipboardWrite",
    "storage"
  ],
  "action": {
    "default_title": "Open slither.io"
  },
  "background": {
    "service_worker": "extension_logic/background.js"
  },
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "content_scripts": [
    {
      "world": "MAIN",
      "matches": [
        "https://ntl-slither.com/ss/"
      ],
      "js": [
        "common/clipboard.js",
        "ntl-slither.com/better-ux/index.js"
      ],
      "css" : ["ntl-slither.com/better-ux/index.css"]
    },
    {
      "world": "MAIN",
      "matches": [
        "http://slither.io/",
        "http://slither.com/io"
      ],
      "js": [
        "common/clipboard.js",
        "slither.io/connect-block/connect-origin.js",
        "slither.io/connect-block/test-then-connect.js",
        "slither.io/connect-block/index.js",
        "slither.io/remember-nickname/index.js"
      ],
      "css" : [
        "slither.io/connect-block/index.css",
        "slither.io/remember-nickname/index.css"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "index.html"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}