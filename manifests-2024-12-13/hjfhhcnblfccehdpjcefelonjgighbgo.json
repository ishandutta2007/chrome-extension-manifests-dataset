{
"update_url": "https://clients2.google.com/service/update2/crx",

  "short_name": "WordWiz",
  "name": "WordWiz Chrome Extension",
  "description": "Learn vocabulary while watching YouTube videos",
  "version": "1.1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "serviceWorker.js"
  },
  "content_scripts": [
    {
      "matches": ["https://*.youtube.com/*"],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ],
  "permissions": ["storage", "alarms"],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Wordwiz Popup"
  },
  "host_permissions": ["https://*.youtube.com/*", "https://api.cognitive.microsofttranslator.com/*"],
  
  "web_accessible_resources": [
    {
      "resources": ["script.js", "wizardClean.png", "word-wizard-cropped.png", "warning.svg", "captionExample.jpg", "quizExample.js", "handWaving.gif", "ga.js", "passer.js"],
      "matches": ["https://*.youtube.com/*"]
    }
  ],

  "icons": {
    "256": "/icons/icon-246x.png"
  }
}
