{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "Bpmify - Add BPM to Spotify",
    "version": "1.0.5",
    "description": "Bpmify adds BPM to the Spotify web player.",
    "permissions": ["storage"],
    "host_permissions": [
      "*://*.spotify.com/*"
    ],
    "content_scripts": [
      {
        "matches": ["https://open.spotify.com/*"],
        "js": ["d3.min.js", "ExtPay.js", "content.js"],
        "css": ["styles.css"]
      },
      {
        "matches": ["https://extensionpay.com/*"],
        "js": ["ExtPay.js"],
        "run_at": "document_start"
      }
    ],
    "background": {
        "service_worker": "background.js"
    },
    "web_accessible_resources": [
      {
        "resources": ["ExtPay.js"],
        "matches": ["<all_urls>"]
      }
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }   
}