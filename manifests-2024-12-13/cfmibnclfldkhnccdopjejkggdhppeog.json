      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_start",
      "all_frames": true,
      "js": [
        "jquery-3.6.1.min.js",
        "bililiteRange.js",
        "jquery.sendkeys.js",
        "common.js",
        "operations.js",
        "content.js"
      ],
      "css": [ "style.css" ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [ "inject.js" ],
