{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "StayOnTasky",
    "version": "1.0",
  
    "description": "Block specific pages and websites that derail your focus, and customize your blocklist to fit your needs.",
    "icons": {
      "48": "icons/lamp-32.png"
    },
  
    "permissions": ["tabs", "storage"],
        
    "content_scripts": [
      {
        "matches": [
          "http://*/*",
          "https://*/*"
        ],
        "js": ["contentScript.js"]
      }
    ],

    "action": {
      "default_icon": "icons/lamp-32.png",
      "default_title": "StayOnTasky",
      "default_popup": "popup/index.html"
    },

    "background": {
      "service_worker": "background.js",
      "type": "module"
    }
  }
  