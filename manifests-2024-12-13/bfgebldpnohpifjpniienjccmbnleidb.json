{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Youtube Speed Button",
  "version": "1.0",
  "description": "A Chrome extension to add 2x speed button and set keys for toggling 2x speed.",
  "permissions": [
      "storage"
  ],
  "background": {
      "service_worker": "background.js"
  },
  "content_scripts": [
      {
          "matches": ["https://www.youtube.com/*"],
          "js": ["content.js"]
      }
  ],
  "action": {
      "default_popup": "popup.html",
      "default_icon": {
          "16": "Logo.png"
      }
  }
