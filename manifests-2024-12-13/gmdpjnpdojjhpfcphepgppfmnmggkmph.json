  "manifest_version": 3,
  "name": "IU Click 2 Dial",
  "version": "1.1",
  "description": "This extension allows you to trigger Twilio Flex Calls from any webpage.",
  "action": {
    "default_popup": "src/index.html",
    "default_icon": {}
  },
  "permissions": ["activeTab", "scripting"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["src/scripts/content_action.js"]
    }
  ],
  "externally_connectable": {
    "matches": ["http://localhost:3001/*"]
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "64": "icon64.png",
    "128": "icon128.png"
  }
