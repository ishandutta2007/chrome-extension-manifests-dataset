{
"update_url": "https://clients2.google.com/service/update2/crx",

 "manifest_version": 3,
 "default_locale": "en",
 "name": "DualTran",
 "version": "2.1.22",
 "homepage_url": "https://chrome.google.com/webstore/detail/dualtran/ihbcniigknaeillehompenjaacaocpnp",
 "description": "chatGPT-powerd browser extension that translates web pages and displays the translated text with the original text in the same page.",
 "commands": {
  "hotkey-toggle-translation": {
   "suggested_key": {
    "default": "Alt+T"
   },
   "description": "__MSG_lblSwitchTranslatedAndOriginal__"
  },
  "hotkey-translate-selected-text": {
   "suggested_key": {
    "default": "Alt+S"
   },
   "description": "__MSG_msgTranslateSelectedText__"
  },
  "hotkey-swap-page-translation-service": {
   "suggested_key": {
    "default": "Alt+Q"
   },
   "description": "__MSG_swapTranslationService__"
  },
  "hotkey-show-original": {
   "description": "__MSG_lblRestorePageToOriginal__"
  },
  "hotkey-translate-page-1": {
   "description": "__MSG_lblTranslatePageToTargetLanguage__ 1"
  },
  "hotkey-translate-page-2": {
   "description": "__MSG_lblTranslatePageToTargetLanguage__ 2"
  },
  "hotkey-translate-page-3": {
   "description": "__MSG_lblTranslatePageToTargetLanguage__ 3"
  },
  "hotkey-hot-translate-selected-text": {
   "description": "__MSG_lblHotTranslatedSelectedText__"
  }
 },
 "icons": {
  "32": "/icons/icon-32.png",
  "64": "/icons/icon-64.png",
  "128": "/icons/icon-128.png"
 },
 "permissions": [
  "storage",
  "activeTab",
  "contextMenus",
  "webRequest",
  "alarms"
 ],
 "optional_permissions": [
  "webNavigation"
 ],
 "web_accessible_resources": [
  {
   "resources": [
    "/icons/icon-16.png",
    "/icons/icon-32.png",
    "/icons/icon-64-translated.png",
    "/icons/icon-64.png",
    "/icons/icon-128.png",
    "/icons/icon-256.png",
    "/icons/google-translate-32.png",
    "/icons/yandex-translate-32.png",
    "/icons/swap-32.png",
    "/contentScript/css/*"
   ],
   "matches": [
    "*://*/*"
   ]
  }
 ],
 "options_ui": {
  "page": "/options/options.html",
  "open_in_tab": false,
  "browser_style": false
 },
 "action": {
  "browser_style": false,
  "default_icon": "/icons/icon-32.png",
  "default_title": "__MSG_pageActionTitle__",
  "default_popup": "/popup/popup.html"
 },
 "content_scripts": [
  {
   "matches": [
    "<all_urls>"
   ],
   "run_at": "document_start",
   "all_frames": false,
   "js": [
    "/contentScript/checkScriptIsInjected.js"
   ]
  },
  {
   "matches": [
    "<all_urls>"
   ],
   "run_at": "document_end",
   "all_frames": true,
   "match_about_blank": true,
   "js": [
    "/contentScript/contentScript.js"
   ]
  },
  {
   "matches": [
    "https://www.deepl.com/translator*"
   ],
   "run_at": "document_end",
   "all_frames": false,
   "js": [
    "/contentScript/deepl.js"
   ]
  }
 ],
 "background": {
  "service_worker": "/background/sw.js"
 }
}
