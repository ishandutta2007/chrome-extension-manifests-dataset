{
   "author": "ScanDis A/S",
   "background": {
      "service_worker": "background.js"
   },
   "action": {
      "default_icon": {
         "16": "vmac_16-inactive.png"
      }
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": ["content_script.js"],
      "match_about_blank": false,
      "matches": [ "<all_urls>", "*://*/*" ],
      "run_at": "document_end"
   }, {
      "all_frames": true,
      "js": [ "enable_annotations.js" ],
