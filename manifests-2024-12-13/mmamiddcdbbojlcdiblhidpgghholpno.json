{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Clean Reddit",
  "version": "1.1",
  "description": "remove user input and clutter from reddit",
  "icons": {
    "48": "icons/icon.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.reddit.com/*"],
      "js": ["entry.js"],
      "css": ["styles.css"],
      "run_at": "document_start"
    }
  ],
  "content_security_policy": {
    "extension_pages": "default-src 'self'"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "reddit@reid.systems",
      "strict_min_version": "109.0"
    }
  }
}
