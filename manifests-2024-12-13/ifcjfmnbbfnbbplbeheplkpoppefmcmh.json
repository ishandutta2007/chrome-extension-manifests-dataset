{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Maul-hnetuskoðarinn",
  "description": "Hnetu viðvörunarkerfi. This extension highlights divs containing peanuts or nuts in the Icelandic Maul menus.",
  "version": "1.2",
  "manifest_version": 3,
  "action": {
      "default_icon": "peanut.png"
  },
  "host_permissions": ["https://app.maul.is/menus/*"],
  "permissions": [
      "declarativeContent",
      "scripting"
  ],
  "background": {
      "service_worker": "background.js"
  },
  
    "content_scripts": [
        {
            "matches": ["https://app.maul.is/menus/*"],
            "js": ["content.js"],
            "run_at": "document_idle"
        }
    ]


}
