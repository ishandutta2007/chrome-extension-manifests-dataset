{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "Cato",
  "description": "Cato - a command launcher for your web browser.",
  "homepage_url": "https://github.com/cliffordfajardo/cato",
  "version": "1.1.5",

  "browser_action": {
    "default_icon": "images/cato-logo.png",
    "default_popup": "popup.html",
    "default_title": "Cato"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "background": {
    "scripts": [ "popup.js"],
    "persistent": false
  },
  "icons": {
    "16": "images/cato-logo16.png",
    "48": "images/cato-logo48.png",
    "128": "images/cato-logo128.png"
  },
  "permissions": [
    "activeTab",
    "bookmarks",
    "tabs",
    "management",
    "storage"
  ],
  "content_security_policy": "script-src 'self' https://www.google-analytics.com; object-src 'self'",
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "windows": "Ctrl+J",
        "mac": "Command+J",
        "chromeos": "Ctrl+M",
        "linux": "Ctrl+M"
      }
    }
  }
}
