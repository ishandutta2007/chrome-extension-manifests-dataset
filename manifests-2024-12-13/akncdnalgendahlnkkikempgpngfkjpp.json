{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Magic Extension",
  "description": "The extension provides a suite of time management and task management tools for personal and executive assistants in Getmagic.com",
  "version": "0.5.16",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Magic Extension",
    "default_icon": "magic.png",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": [
      "jquery.js",
      "diff-renderer.js",
      "inject.js",
      "dialer.js",
      "keycap.js",
      "banner.js"
    ],
    "css": ["dialer.css"],
    "run_at": "document_start"
  }, {
    "matches": ["*://www.google.com/_/chrome/newtab*"],
    "js": [
      "jquery.js",
      "newtab.js"
    ],
    "run_at": "document_start"
  }],
  "externally_connectable": {
    "matches": ["*://localhost/*", "*://*.bettir.com/*"]
  },
  "omnibox": { "keyword" : "m" },
  "icons": {
    "16": "magic.png",
    "48": "magic.png",
    "128": "magic.png"
  },
  "permissions": [
    "tabs",
    "storage",
    "webNavigation",
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}
