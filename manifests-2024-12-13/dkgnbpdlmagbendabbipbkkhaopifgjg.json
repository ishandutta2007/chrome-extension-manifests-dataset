{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,

  "name": "AI LinkedIn Message Generator",
  "description": "MagicSales.AI is an assistant that generates highly effective, personalized LinkedIn messages and comments with ease.",
  "version": "0.1.14",

  "options_page": "options.html",

  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_popup": "popup.html"
  },

  "content_scripts": [
    {
      "matches": [
        "https://magicsales.ai/*",
        "https://magicposting.com/*",
        "https://www.linkedin.com/*",
        "https://linkedin.com/*",
        "https://www.x.com/*",
        "https://x.com/*",
        "https://www.facebook.com/*",
        "https://facebook.com/*",
        "https://www.instagram.com/*",
        "https://instagram.com/*",
        "https://www.reddit.com/*",
        "https://reddit.com/*",
        "https://www.upwork.com/*",
        "https://upwork.com/*"
      ],
      "js": ["js/vendor.js", "js/content_script.js"]
    }
  ],

  "background": {
    "service_worker": "js/background.js"
  },

  "permissions": ["activeTab", "scripting", "clipboardWrite", "storage"],

  "host_permissions": [
    "https://www.linkedin.com/*",
    "https://x.com/*",
    "https://www.facebook.com/*",
    "https://www.instagram.com/*",
    "https://www.reddit.com/*"
  ],

  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
