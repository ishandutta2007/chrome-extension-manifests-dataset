{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Agency Access Browser Plugin",
  "version": "1.0.6",
  "description": "Agency Access: Find and Connect with Creative Industry Contacts",
  "action": {},
  "options_ui": {
    "page": "./src/scripts/options/options.html",
    "open_in_tab": false
  },
  "background": {
    "service_worker": "js/service-worker.js",
    "type": "module"
  },
  "icons": {
    "16": "./assets/logo.png",
    "48": "./assets/logo.png",
    "128": "./assets/logo.png"
  },
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "./js/content.js"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "assets/fonts/Nunito-Regular.ttf",
        "assets/fonts/Nunito-Italic.ttf",
        "assets/fonts/Nunito-Medium.ttf",
        "assets/fonts/Nunito-MediumItalic.ttf",
        "assets/fonts/Nunito-SemiBold.ttf",
        "assets/fonts/Nunito-SemiBoldItalic.ttf",
        "assets/fonts/Nunito-Bold.ttf",
        "assets/fonts/Nunito-BoldItalic.ttf"
      ]
    },
    {
      "matches": [
        "https://*/*",
        "http://*/*"
      ],
      "resources": [
        "dist\\js\\index.js",
        "dist\\js\\options.js",
        "dist\\js\\popup.js",
        "dist\\js\\service-worker.js"
      ]
    }
  ],
  "commands": {
    "refresh_extension": {
      "suggested_key": {
        "default": "Ctrl+Space"
      },
      "description": "Refresh Extension"
    }
  }
}