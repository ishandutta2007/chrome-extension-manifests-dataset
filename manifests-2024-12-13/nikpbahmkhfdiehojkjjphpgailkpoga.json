  "web_accessible_resources": [
    {
      "resources": [
        "assets/js/*.js",
        "assets/css/*.css",
        "icon-128.png",
        "icon-48.png",
        "icon-32.png",
        "icon-16.png",
        "assets/fonts/*.ttf"
      ],
      "matches": [
        "*://*/*"
      ]
    }
