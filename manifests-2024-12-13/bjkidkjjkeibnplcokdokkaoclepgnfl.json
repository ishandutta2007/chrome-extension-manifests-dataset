  "manifest_version": 2,
    "name": "VSADS FlashMax",
    "version": "1.3",
    "description": "Detects flash content so you can make classic games full screen!",

    "permissions": ["declarativeContent","storage"],
    
    "background": {
      "scripts": ["background.js"],
      "persistent": false
