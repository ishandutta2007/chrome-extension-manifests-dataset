{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "SmartTab Overlay",
  "version": "2.0.3",
  "description": "Adds a Tab Switcher, Search functionalities for Tabs, History, Bookmarks, Most Visited Websites, Reading List and Saved Tabs.",
  "icons": {
    "16": "assets/icons/icon16.png",
    "32": "assets/icons/icon32.png",
    "48": "assets/icons/icon48.png",
    "64": "assets/icons/icon64.png",
    "128": "assets/icons/icon128.png"
  },
  "action": {
    "default_icon": "assets/icons/icon128.png",
    "default_title": "SmartTab Overlay"
  },
  "permissions": [
    "activeTab",
    "storage",
    "alarms",
    "contextMenus"
  ],
  "optional_permissions": [
    "tabs",
    "search",
    "history",
    "topSites",
    "bookmarks",
    "sessions"
  ],
  "host_permissions": ["<all_urls>"],
  "options_page": "options/options.html",
  "background": {
    "service_worker": "background/background.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": [
        "libraries/jquery/jquery-3.6.0.min.js",
        "libraries/sweetalert2/sweetalert2.min.js",
        "content/overlaySearchBox.js",
        "content/overlayTabSwitch.js"
      ],
      "css": [
        "libraries/fontawesome/fontawesome.min.css",
        "libraries/fontawesome/solid.min.css",
        "libraries/fontawesome/regular.min.css",
        "libraries/sweetalert2/default.min.css",
        "content/smartTabOverlay.css"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["/assets/fonts/*.ttf"],
      "matches": ["<all_urls>"]
    },
    {
      "resources": ["/assets/fonts/*.woff2"],
      "matches": ["<all_urls>"]
    },
    {
      "resources": ["/assets/images/*.png"],
      "matches": ["<all_urls>"]
    },
    {
      "resources": ["/*.css"],
      "matches": ["<all_urls>"]
    }
  ]
}
