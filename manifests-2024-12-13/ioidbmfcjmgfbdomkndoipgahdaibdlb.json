        "js": ["content.js"]
      }
    ],
    "action": {
      "default_popup": "index.html"
    },
    "web_accessible_resources": [
      {
        "resources": ["blocked.html", "blocked.js", "sounds/start-focus.mp3", "sounds/start-rest.mp3", "sounds/button-click.mp3"],
        "matches": ["<all_urls>"]
      }
    ]
