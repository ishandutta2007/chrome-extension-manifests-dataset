        "socket.io.min.js",
        "script.js"
      ],
      "css": ["content.styles.css", "inject.styles.css"],
      "run_at": "document_end"
    }
  ],

  "web_accessible_resources": [
    {
      "resources": [
        "index.html",
        "script.js",
        "inject4Script.bundle.js",
        "assets/img/*",
        "content.styles.css",
        "inject.styles.css",
        "icon-*.png",
        "emoji.png",
        "injectScript.bundle.js",
        "inject2Script.bundle.js",
