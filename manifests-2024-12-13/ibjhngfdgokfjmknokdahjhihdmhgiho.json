  "manifest_version": 3,
  "name": "Tab Wrangler",
  "version": "1.0",
  "description": "A Chrome extension to organize your tabs",
  "permissions": ["tabs", "activeTab", "storage", "windows", "favicon"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
