{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "PS Stars Collectibles",
  "version": "0.2.1",
  "description": "This extension enables you to edit your collectibles display case.",
  "background": {
    "service_worker": "service-worker.js"
  },
  "action": {
    "default_title": "Click to open while on the PlayStation Store website"
  },
  "host_permissions": ["https://*.np.playstation.com/api/graphql/*"],
  "permissions": ["sidePanel", "tabs", "declarativeNetRequestWithHostAccess"],
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "ruleset_1",
        "enabled": true,
        "path": "rules.json"
      }
    ]
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+B",
        "mac": "Command+B"
      }
    }
  },
  "icons": {
    "16": "images/icon-16.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  }
}