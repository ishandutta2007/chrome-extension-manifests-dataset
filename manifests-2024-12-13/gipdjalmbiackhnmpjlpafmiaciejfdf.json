{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Volley extension",
  "description": "Volley lets you give fast, easy feedback for your website.",
  "version": "2.0.0",
  "permissions": [
    "tabs",
    "activeTab",
    "tabCapture",
    "cookies",
    "storage",
    "declarativeNetRequest",
    "offscreen"
  ],
  "host_permissions": [
    "*://*.meetvolley.com/"
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "static/**/*",
        "mic.html",
        "mic.js"
      ],
      "matches": [
        "*://*/*"
      ]
    }
  ],
  "action": {},
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "all_frames": true,
      "js": [
        "main.js"
      ],
      "css": [
        "static/css/main.css"
      ],
      "run_at": "document_start"
    }
  ]
}