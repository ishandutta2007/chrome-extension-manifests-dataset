    },
    "merge_all": {
      "suggested_key": {
        "default": "Alt+Shift+M",
        "mac": "Alt+Shift+M"
      },
      "description": "Merge all windows"
    },
    "shuffle": {
      "suggested_key": {
        "default": "Alt+Shift+F",
        "mac": "Alt+Shift+F"
      },
      "description": "Shuffle"
    },
    "discard": {
      "suggested_key": {
        "default": "Alt+Shift+D",
        "mac": "Alt+Shift+D"
      },
      "description": "Discard tabs"
    },
    "split_by_domains": {
      "description": "Split by domains"
    },
    "remove_duplicates": {
      "description": "Remove any duplicate urls"
    }
  }
