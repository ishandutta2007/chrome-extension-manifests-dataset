  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png", 
      "128": "icons/icon128.png"
    }
  },
  "web_accessible_resources": [
    {
      "resources": [
        "popup.css",
        "popup.html",
        "popup.js",
        "all-links.html",
        "all-links.css",
        "scripts/kaplay.js",
        "kaplay.js",
        "all-links.js",
        "icons/custom-icon.svg",
        "translations/*.json"
      ],
      "matches": ["<all_urls>"]
    }
  ]
