{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "LibrusPro - rozszerzenie do Librusa",
  "version": "3.4.0",
  "description": "Librus Helper",
  "manifest_version": 3,
  "author": "Maks Kowalski",
  "short_name": "LibrusPro",
  "permissions": [
    "storage",
    "scripting"
  ],
  "icons": {
    "512": "img/icon.png"
  },
  "background": {
    "service_worker": "service_worker.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://synergia.librus.pl/*"
      ],
      "js": [
        "scripts/jquery-3.5.1.min.js",
        "scripts/jqueryui.js",
        "librusPro.js",
        "scripts/disableAutoLogout.js",
        "scripts/printCredits.js",
        "scripts/injectjQuery.js",
        "scripts/collapseBehavior.js"
      ],
      "all_frames": true
    },
    {
      "matches": [
        "https://synergia.librus.pl/*"
      ],
      "css": [
        "librusPro.css",
        "darkTheme.css"
      ],
      "run_at": "document_start"
    },
    {
      "matches": [
        "https://portal.librus.pl/*"
      ],
      "js": [
        "portal.js"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "img/*",
        "scripts/jquery-3.5.1.min.js",
        "scripts/jqueryui.js",
        "scripts/disableAutoLogout.js",
        "scripts/printCredits.js",
        "scripts/injectjQuery.js",
        "darkTheme.css",
        "scripts/collapseBehavior.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "action": {
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  },
  "content_security_policy": {
  },
  "host_permissions": [
    "https://synergia.librus.pl/*",
    "https://portal.librus.pl/*"
  ]
}