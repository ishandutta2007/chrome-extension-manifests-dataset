{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "Layout Modifier",
    "version": "1.0",
    "description": "Modify a website's layout by dragging elements. Right click on an element to select it and all children, then drag it anywhere.",
    "permissions": ["storage", "activeTab", "scripting"],
    "host_permissions": ["<all_urls>"],
    "action": {
      "default_icon": {
        "48": "icon-off.png",
        "128": "icon-off-128.png",
        "16": "icon-off-16.png"
      },
      "default_title": "Layout Modifier"
    },
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  