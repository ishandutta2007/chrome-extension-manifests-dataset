{
    "update_url": "https://clients2.google.com/service/update2/crx",
    "manifest_version": 3,
    "name": "Wizard - AI Chrome Extension for Canvas LMS",
    "short_name": "Wizard",
    "author": "hi@canvaswizard.co",
    "homepage_url": "https://canvaswizard.co/",
    "version": "3.0.0",
    "description": "Get answers & explanations to your Canvas Quizzes, Homeworks and Tests with a single click.",
    "content_scripts": [
        {
          "matches": [
            "<all_urls>"
          ],
          "js": ["js/localstorage.js", "js/sidebar.js", "js/screenshot.js", "js/chat.js", "js/history.js", "js/answer.js", "js/notes.js", "js/selection.js"],
          "css" : ["css/main.css", "css/injected.css"]
        }
    ],
    "permissions": [
        "storage",
        "activeTab",
        "scripting"
    ],    
    "host_permissions": [
      "http://localhost:8888/*",
      "http://localhost:5000/*",
      "https://canvaswizard.co/*",
      "https://canwaswizard-api.onrender.com/*",
      "<all_urls>"
    ],
    "background": {
      "service_worker": "js/background.js"
    },  
    "icons": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "action": {
      "default_popup": "popup.html",
      "default_title": "Wizard for Canvas",
      "default_icon": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "web_accessible_resources": [
      {
        "resources": ["img/*"],
        "matches": ["<all_urls>"]
      }
    ]
  }