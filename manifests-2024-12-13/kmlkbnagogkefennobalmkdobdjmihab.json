{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Tab Search Pro",
  "version": "0.0.15",
  "description": "Search active tabs, highlight results, save search history, switch between the last two active tabs, navigate tabs chronologically.",
  "background": {
    "service_worker": "background.js"
  },
  "commands": {
    "tab_search_pro_switch_last_two_active_tabs": {
      "suggested_key": {
        "default": "Ctrl+Q",
        "mac": "Command+Q"
      },
      "description": "Switch between the last two active tabs"
    },
    "tab_search_pro_switch_to_previous_tab": {
      "suggested_key": {
        "default": "Ctrl+Shift+Space",
        "mac": "Command+Shift+Space"
      },
      "description": "Switch to the previous tab in the order they were activated, maintaining the chronological sequence of tab activations"
    },
    "tab_search_pro_search_in_tabs": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "Command+Shift+F"
      },
      "description": "Search Selected Text in Tabs"
    },
    "tab_search_pro_duplicate_current_tab": {
      "suggested_key": {
        "default": "Alt+Shift+D",
        "mac": "Alt+Shift+D"
      },
      "description": "Duplicate Current Tab and Activate It"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "permissions": [
    "alarms",
    "contextMenus",
    "scripting",
    "storage",
    "tabs"
  ],
  "host_permissions": [
    "https://*/*"
  ],
  "action": {
    "default_title": "TabSearch Pro",
    "default_popup": "popup/popup.html"
  },
  "options_ui": {
    "open_in_tab": true,
    "page": "options/options.html"
  },
  "options_page": "options/options.html",
  "homepage_url": "https://tabsearch.pro/"
}