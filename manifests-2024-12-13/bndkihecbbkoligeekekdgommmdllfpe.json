  "manifest_version": 3,
  "name": "Walmart Invoice Exporter",
  "version": "2.2",
  "description": "Download Walmart order details in xlsx format",
  "permissions": ["activeTab"],
  "host_permissions": ["https://www.walmart.com/*"],
  "background": {
    "service_worker": "background.js"
