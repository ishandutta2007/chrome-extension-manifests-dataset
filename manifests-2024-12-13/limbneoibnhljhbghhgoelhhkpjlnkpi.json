{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Site Block Checker",
  "version": "1.0",
  "description": "Check if popular websites and the current tab's website are accessible or blocked.",
  "permissions": ["activeTab"],
  "host_permissions": [
    "https://www.facebook.com/*",
    "https://www.twitter.com/*",
    "https://www.youtube.com/*",
    "https://www.instagram.com/*",
    "https://www.signal.org/*",
    "https://web.telegram.org/*",
    "https://www.google.com/*",
    "https://mail.google.com/*"
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "web_accessible_resources": [
  {
    "resources": ["css/all.min.css", "css/webfonts/*"],
    "matches": ["<all_urls>"]
  }
]

}
