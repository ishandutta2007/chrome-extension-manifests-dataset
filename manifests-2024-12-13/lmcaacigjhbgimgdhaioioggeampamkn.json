{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Threads Instagram Chat Redirect",
  "version": "1.0",
  "description": "Redirect to Instagram DM from Threads profiles",
  "host_permissions": [
    "https://www.threads.net/*",
    "https://ig.me/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://www.threads.net/*"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_title": "Threads Instagram Chat Redirect"
  },
  "author": {
    "email": "info@ivanlopezdev.es"
  }
}
