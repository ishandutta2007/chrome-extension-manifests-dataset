  "host_permissions": [
    "*://rabota.by/*",
    "*://*.hh.ru/*",
    "*://*.hh.uz/*",
    "*://*.hh.kz/*",
    "*://*.headhunter.kg/*",
    "*://*.headhunter.ge/*",
    "*://*.hh1.az/*",
    "*://www.linkedin.com/*"
