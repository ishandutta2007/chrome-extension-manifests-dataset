{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "HD Background Photo Replacer",
  "description": "Make every background photo beautiful.  For SDRs and Recruiters.  Enjoy your search.",
  "version": "1.2.1",
  "manifest_version": 2,
  "icons": {
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "browser_action": {
    "default_title": "HD Background Photo Replacer"
  },
  "background": {
    "scripts": [
      "backgroundScripts/background.js"
    ]
  },
  "externally_connectable": {
