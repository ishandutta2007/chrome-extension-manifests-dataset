  },
  "permissions": [
    "https://www.linkedin.com/*",
    "https://linkedinbackground.com/*",
    "https://*/dms/image/*",
    "https://*/sc/h/5qi38njir9vsm46ync2qjvrrs",
    "webRequest",
    "webRequestBlocking"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "web_accessible_resources": [
    "contentScripts/content.js",
    "assets/li_d_yellow_blue.svg",
    "assets/li_d_green_blue.svg",
    "assets/li_d_pink_blue.svg",
    "assets/li_d_red_blue.svg",
    "assets/li_d_skyblue_blue.svg"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.linkedin.com/*"
      ],
      "js": [
        "contentScripts/content.js"
      ]
    }
  ],
  "commands": {
    "toggle-feature": {
      "suggested_key": {
        "default": "Ctrl+Shift+Y",
        "mac": "MacCtrl+Shift+Y"
