{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "HumanAmplify: AI-powered YouTube analysis",
  "version": "0.4",
  "description": "Summarize, analyze, and understand YouTube videos with AI. Detect BS, extract business ideas, and more.",
  "icons": {
    "16": "icons/ha_16.png",
    "48": "icons/ha_48.png",
    "128": "icons/ha_128.png"
  },
  "permissions": [    
    "storage"
  ],
  "host_permissions": [
    "https://humanamplify.ai/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["contentScript.js", "marked.min.js"],
      "css": ["styles.css"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "icons/summarize.png",
        "icons/custom.png",
        "icons/extract-business-ideas.png",
        "icons/analyze-claims.png",
        "icons/analyze-debate.png",
        "icons/analyze-tech-impact.png",
        "icons/bs-detector.png",
        "icons/extract-wisdom.png",
        "icons/mind-map.png",
        "icons/human-amplify.jpg",
        "icons/ha_128.png",
        "icons/ha_48.png",
        "icons/ha_16.png",
        "setup.html"
      ],
      "matches": ["https://humanamplify.ai/*", "*://*.youtube.com/*"]
    }
  ],
  "options_page": "setup.html",
  "externally_connectable": {
    "matches": ["https://humanamplify.ai/*"],
    "ids": ["*"]
  }
}
