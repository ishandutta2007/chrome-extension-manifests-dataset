      "js": ["my2MW.js", "contentScript.js"],
      "run_at": "document_idle"
    }
  ],
  "web_accessible_resources": [{
    "resources": [
      "html/*.html",
      "html/*.css",
      "css/*.css",
      "images/*.png",
      "images/*.JPEG",
      "images/*.svg",
      "fonts/*.ttf",
