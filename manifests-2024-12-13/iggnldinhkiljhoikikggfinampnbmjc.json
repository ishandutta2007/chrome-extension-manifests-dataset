{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "TapSafe Extension",
  "description": "Manager your password easily",
  "version": "0.6.4",
  "manifest_version": 3,
  "action": {
      "default_popup": "index.html",
      "default_title": "TapSafe"
  },
  "icons": {
      "16": "16x16.png",
      "48": "48x48.png",
      "128": "128x128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
        "matches": ["http://*/*", "https://*/*"],
        "js": [
          "content.js",
          "topbarContent.js"
        ],
        "css": ["toastify.css"]
    }
 ],
 "permissions": [
  "tabs",
  "activeTab",
  "storage"
 ],
 "web_accessible_resources": [
  {
    "resources": ["notification/topbar.html"],
    "matches": ["<all_urls>"]
  }
  ]
}
