{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "TubeTamer - Youtube Activity Analytics",
  "version": "1.1.0",
  "permissions": [
    "storage",
    "history",
    "identity",
    "notifications"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://youtube.com/*",
        "https://www.youtube.com/*"
      ],
      "js": [
        "js/auth.js",
        "js/shared.js",
        "content.js"
      ]
    },
    {
      "matches": [
        "http://localhost:3000/*",
        "https://stage.tubetamer.neuraldynamics.online/*",
        "https://dashboard.tubetamer.app/*"
      ],
      "js": [
        "js/auth.js",
        "js/dashboard.js"
      ]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "web_accessible_resources": [
    {
      "matches": [
        "https://youtube.com/*",
        "https://www.youtube.com/*"
      ],
      "resources": [
        "js/auth.js",
        "js/shared.js",
        "js/tracker.js"
      ]
    }
  ]
}