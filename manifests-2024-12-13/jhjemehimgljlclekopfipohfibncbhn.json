  "background": {
    "service_worker": "worker.js"
  },
  "content_security_policy": {
    "extension_pages": "default-src 'self'; script-src 'wasm-unsafe-eval' 'self'; style-src 'unsafe-inline'; connect-src *; media-src *; img-src data:;"
