{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "YouTube Watch Later Manager",
  "version": "1.0.1",
  "description": "Easily remove videos you're no longer interested in or have already watched from your Watch Later playlist.",
   "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "activeTab"
  ],
  "host_permissions": [
    "https://*.youtube.com/playlist?list=WL"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.youtube.com/playlist?list=WL"
      ],
      "js": ["content.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  }
}