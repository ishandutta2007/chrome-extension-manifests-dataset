{
"update_url": "https://clients2.google.com/service/update2/crx",

 "name": "KauGPT",
 "short_name": "KauGPT",
 "description": "This extension gives you access to GPT-4 with access to GPTs and file ",
 "version": "1.2",
 "manifest_version": 3,

 "action": {
  "default_icon": {
   "16": "/assets/kaugpt16x16.png",
   "32": "/assets/kaugpt32x32.png",
   "48": "/assets/kaugpt48x48.png",
   "128": "/assets/kaugpt128x128.png"
  }
 },

 "permissions": [
  "storage",
  "declarativeNetRequestWithHostAccess" ],

 "host_permissions": [
  "https://*.bing.com/",
  "https://*.replicate.com/",
  "wss://*.bing.com/"
 ],
 "declarative_net_request": {
  "rule_resources": [
   {
    "enabled": true,
    "id": "ruleset_bing",
    "path": "rules/bing.json"
   },
   {
    "enabled": true,
    "id": "ruleset_replicate",
    "path": "rules/replicate.json"
   }
  ]
 },
 "web_accessible_resources": [
  {
   "resources": ["flowbite.min.css"],
   "matches": ["<all_urls>"]
  }
 ],
 "background": {
  "service_worker": "background.js"
 }
}
