      "default_icon": {
        "16": "assets/Icons-CodeZy.png",
        "48": "assets/Icons-CodeZy.png",
        "128": "assets/Icons-CodeZy.png"
      }
    },
    "icons": {
      "16": "assets/Icons-CodeZy.png",
      "48": "assets/Icons-CodeZy.png",
      "128": "assets/Icons-CodeZy.png"
    },
    "background": {
      "service_worker": "background-bookmark.js"
    },
    "web_accessible_resources": [
      {
        "resources": ["assets/bookmark.png"],
        "matches": ["https://www.youtube.com/*"]
      }
    ]
  }
  