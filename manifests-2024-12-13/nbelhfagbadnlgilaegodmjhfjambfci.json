  "manifest_version": 3,
  "name": "YouTube Filter",
  "version": "1.2",
  "description": "Filter YouTube videos based on categories",
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "https://*.youtube.com/*"
  ],
  "action": {
    "default_popup": "popup.html"
