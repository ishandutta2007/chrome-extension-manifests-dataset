{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Meetask Meeting Recorder",
  "description": "This extension is only for Meetask app!",
  "version": "3.0.0",
  "manifest_version": 3,
  "action": {
    "default_icon": "/images/MeeTask-logo-deactive.png",
    "default_popup": "popup/popup.html",
    "default_title": "Open MeeTask Meeting Recorder interface"
  },
  "icons": {
    "16": "/images/smartyx-logo-16.png",
    "32": "/images/smartyx-logo-32.png",
    "64": "/images/smartyx-logo-64.png",
    "128": "/images/smartyx-logo-128.png"
  },
  "background": {
    "service_worker": "serviceWorker/service-worker.js"
  },
  "permissions": ["tabs", "tabCapture", "offscreen"],
  "content_scripts": [
    {
      "matches": [
        "http://localhost/*",
        "https://test.smartyx.ai/*",
        "https://www.test.smartyx.ai/*",
        "https://meetask.smartyx.ai/*",
        "https://www.meetask.smartyx.ai/*"
      ],
      "js": ["scripts/recorder-content-script.js"]
    },
    {
      "matches": ["<all_urls>"],
      "js": ["scripts/meeting-content-script.js"],
      "run_at": "document_start",
      "all_frames": true,
      "exclude_matches": [
        "http://localhost/*",
        "https://test.smartyx.ai/*",
        "https://www.test.smartyx.ai/*",
        "https://meetask.smartyx.ai/*",
        "https://www.meetask.smartyx.ai/*"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["extensionWorklet/AudioRecorderProcessor.js"],
      "matches": ["<all_urls>"]
    },
    {
      "resources": [
        "streamController/streamController.html",
        "images/MeeTask-logo.png"
      ],
      "matches": ["<all_urls>"]
    }
  ]
}
