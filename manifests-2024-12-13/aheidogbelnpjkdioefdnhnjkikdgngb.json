{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "HitWit Learning Extension",
  "description": "Summarize web content, generate mindmaps, and personalize your learning experience with HitWit.",
  "manifest_version": 3,
  "version": "0.0.0.22",
  "key": "",
  "permissions": [ "activeTab", "storage", "identity", "alarms", "notifications", "desktopCapture", "tabs", "scripting"],
  "host_permissions": ["<all_urls>"],
  "default_locale": "en",
  "background": {
    "service_worker": "background.bundle.js"
  },
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "match_about_blank": true,
      "all_frames": false,
      "js": ["content_script.bundle.js"],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "<all_urls>"
      ],
      
      "resources": [
        "option.html",
        "images/*",
        "fonts/*",
        "css/*"
      ],
      "use_dynamic_url": true
    }
  ]
}
