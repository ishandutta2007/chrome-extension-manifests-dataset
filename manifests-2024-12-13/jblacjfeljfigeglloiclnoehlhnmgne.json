{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Ethos Twitter Extension",
  "description": "View Ethos credibility scores and data to enhance your Twitter experience.",
  "version": "0.2.5",
  "permissions": [
    "storage",
    "webNavigation",
    "activeTab"
  ],
  "host_permissions": [
    "https://x.com/*",
    "https://twitter.com/*",
    "https://api.testnet.ethos.network/*"
  ],
  "icons": {
    "16": "icons/prod/icon16.png",
    "48": "icons/prod/icon48.png",
    "128": "icons/prod/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://x.com/*",
        "https://twitter.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icons/prod/icon16.png",
      "48": "icons/prod/icon48.png",
      "128": "icons/prod/icon128.png"
    }
  },
  "web_accessible_resources": [
    {
      "resources": [
        "assets/*.png",
        "fonts/*.woff2"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}