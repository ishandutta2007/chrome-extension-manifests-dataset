{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,

  "name": "Markdown Timestamp from Videos",
  "description": "Capture timestamps from videos with markdown format",
  "version": "1.0",

  "action": {
    "default_icon": {
      "16": "icon-16x16.png",
      "24": "icon-24x24.png",
      "32": "icon-32x32.png"
    },
    "default_popup": "popup.html"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/vendor.js", "js/content_script.js"]
    }
  ],

  "background": {
    "service_worker": "js/background.js"
  },

  "permissions": ["storage"],

  "host_permissions": ["*://*.youtube.com/"],

  "commands": {
    "copy_timestamp": {
      "suggested_key": {
        "default": "Ctrl+Shift+M",
        "mac": "MacCtrl+Shift+M"
      },
      "description": "Copies timestamp from video"
    }
  }
}
