{
"update_url": "https://clients2.google.com/service/update2/crx",

  "author": "Shridhar Puntambekar",
  "name": "FLS Transit",
  "version": "0.10.0",
  "description": "FLS Transit automates copying and applying Field Level Security (FLS) across Salesforce orgs, with data stored securely locally.",
  "manifest_version": 3,
  "options_page": "options/options.html",
  "permissions": [
    "storage",
    "scripting",
    "activeTab",
    "webNavigation",
    "cookies"
  ],
  "host_permissions": [
    "https://*.lightning.force.com/*",
    "https://*.my.salesforce.com/*",
    "https://*.my.salesforce-setup.com/*"
  ],
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "16": "resources/icons/icon16.png",
      "48": "resources/icons/icon48.png",
      "128": "resources/icons/icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "run_at": "document_idle",
      "js": [
        "content_script.js"
      ],
      "matches": [
        "https://*.salesforce.com/*",
        "https://*.visual.force.com/*",
        "https://*.lightning.force.com/*",
        "https://*.salesforce-setup.com/*"
      ],
      "exclude_matches": [
        "https://login.salesforce.com/*",
        "https://test.salesforce.com/*",
        "https://www.salesforce.com/*",
        "https://developer.salesforce.com/*",
        "https://*.salesforce.com/_ui/system/*",
        "https://*.salesforce.com/secur/forgotpassword.jsp*",
        "https://appexchange.salesforce.com/*",
        "https://trust.salesforce.com/*"
      ],
      "all_frames": true
    }
  ],
  "icons": {
    "16": "resources/icons/icon16.png",
    "48": "resources/icons/icon48.png",
    "128": "resources/icons/icon128.png"
  }
}