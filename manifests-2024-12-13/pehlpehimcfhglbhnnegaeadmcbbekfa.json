---
name: TabDock
version: 1.2.3
category_slug: productivity/workflow
rating: 5
rating_count: 10
user_count: 101
release_date: '2024-10-11T00:35:20.000Z'
size: 1.59MiB
languages:
  - English
description: >-
  TabDock is a chrome extension that allows you to save your tabs in workspaces
  and open them later.
publisher_account: Angel Padilla
extension_website: www.tabdock.app
support_website: https://my.tabdock.app/help
privacy_policy: https://my.tabdock.app/privacy
---

{
"update_url": "https://clients2.google.com/service/update2/crx",

    "version": "1.2.3",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js",
        "type": "module"
    },
    "name": "TabDock",
    "description": "TabDock is a chrome extension that allows you to save your tabs in workspaces and open them later.",
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "permissions": [
        "tabs",
        "activeTab",
        "scripting",
        "storage",
        "tabGroups",
        "favicon",
        "alarms",
        "commands",
        "sidePanel"
    ],
    "omnibox": { "keyword" : "go" },
    "action": {
        "default_title": "TabDock",
        "default_popup": "js/popup.html"
    },
    "side_panel": {
        "default_path": "js/sidepanel_workspaces.html"
    },
    "chrome_url_overrides" : {
        "newtab": "js/index.html"
    },
    "commands": {
        "kbar": {
            "suggested_key":{
                "default": "Ctrl+K"
            },
            "description": "Open TabDock kbar"
        }
    },
    "externally_connectable": {
        "matches": ["https://my.tabdock.app/*"]
    },
    "web_accessible_resources": [
        {
            "resources": ["js/kbar.html", "js/kbar.js", "js/index.html", "icons/icon16.png", "icons/icon32.png", "icons/icon48.png", "icons/icon128.png", "*/_favicon/?*"],
            "matches": ["<all_urls>"]
        }
    ]
}
