{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "default_locale": "en",
  "name": "TLDW: AI-Powered YouTube Video Summarizer",
  "version": "1.1.0",
  "description": "Dive deeper into any YouTube video with customized AI summaries and responses designed to align with your individual goals.",
  "host_permissions": [
    "https://www.youtube.com/*",
    "https://youtube.com/*"
  ],
  "permissions": [
    "storage"
  ],
  "background": {
    "service_worker": "background.iife.js",
    "type": "module"
  },
  "action": {
    "default_popup": "popup/index.html",
    "default_icon": "icon-128.png"
  },
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.youtube.com/*",
        "https://youtube.com/*"
      ],
      "js": [
        "content-ui/index.iife.js"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "*.js",
        "*.css",
        "*.svg",
        "icon-128.png"
      ],
      "matches": [
        "*://*/*"
      ]
    }
  ]
}