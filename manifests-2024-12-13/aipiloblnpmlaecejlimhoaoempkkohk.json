{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "InEcom Auto Extension",
  "description": "Phần mềm tự động Shopee InEcom",
  "version": "1.7.3",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "webRequest",
    "background",
     "scripting",
     "tabs"
  ],"host_permissions": [
    "*://banhang.shopee.vn/*",
    "*://shopee.vn/*",
    "*://subaccount.shopee.com/*",
    "*://tiktok.com/*",
    "*://*.tiktok.com/*"
  ],
  "externally_connectable": {
    "matches": ["https://localhost:5001/*", "https://seller.inecom.club/*", "https://seller.inception.com.vn/*", "https://*.inception.com.vn/*"]
  },
  "background": {
    "service_worker": "scripts/service-worker.js"
  },
  "content_scripts": [
    {
      "js": ["scripts/banhang.js"],
      "matches": [
        "https://banhang.shopee.vn/*"
      ],
      "run_at": "document_start"

    },
    {
      "js": ["scripts/subaccount_shop.js"],
      "matches": [
        "https://subaccount.shopee.com/shops"
      ],
      "run_at": "document_start"

    },
    {
      "js": ["scripts/aff_creator.js"],
      "matches": [
        "https://banhang.shopee.vn/portal/web-seller-affiliate/*"
      ],
      "run_at": "document_end"

    },
    {
      "js": ["scripts/aff_creator_details.js"],
      "matches": [
        "https://banhang.shopee.vn/portal/web-seller-affiliate/kol_marketplace/detail*"
      ],
      "run_at": "document_end"

    },
    {
      "js": ["scripts/aff_tiktok_creator_im.js"],
      "matches": [
        "https://affiliate.tiktok.com/seller/im*"
      ],
      "world": "MAIN",
      "run_at": "document_end"

    }
  ]

}
