{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Ticket Price Modifier",
  "version": "1.2",
  "description": "Modifies StubHub URLs and VividSeats prices",
  "permissions": [
    "storage",
    "declarativeNetRequest"
  ],
  "host_permissions": [
    "https://www.stubhub.com/*",
    "https://www.vividseats.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://www.stubhub.com/*"],
      "js": ["stubhub.js"]
    },
    {
      "matches": ["https://www.vividseats.com/*"],
      "js": ["vividseats.js"]
    }
  ],
  "declarative_net_request": {
    "rule_resources": [{
      "id": "ruleset_1",
      "enabled": true,
      "path": "rules.json"
    }]
  }
}