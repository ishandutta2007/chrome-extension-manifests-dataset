  "content_scripts": [
    {
      "matches": [
        "https://www.warzone.com/*",
        "https://warzone.com/*",
        "https://warzonecdn.com/*"
      ],
      "js": [
        "js/extractor.js",
        "js/content.js"
      ]
    }
  ],
  "icons": {
    "16": "icons/icon_16.png",
    "32": "icons/icon_32.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
