  "name": "Warzone Idle Extractor",
  "version": "0.2.1",
  "description": "Extracts data from the Warzone Idle game and opens the calculator.",
  "permissions": [
    "activeTab"
  ],
  "action": {
    "default_popup": "popup.html"
