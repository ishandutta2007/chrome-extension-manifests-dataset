{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "LinkDeal",
  "version": "2.0.2",
  "description": "LinkDeal extension to automate the connection process.",
  "background": {
    "service_worker": "background/background.js"
  },
  "content_scripts": [
    {
      "js": ["assets/js/jquery.min.js", "content/background_content.js"],
      "css": ["assets/css/linkdin.css"],
      "matches": ["*://www.linkedin.com/*", "*://linkedin.com/*"],
      "run_at": "document_end"
    }
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16":  "assets/images/linkdeal.png",
    "48":  "assets/images/linkdeal.png",
    "128": "assets/images/linkdeal.png"
  },

  "permissions": ["activeTab", "tabs" , "downloads", "cookies", "storage", "alarms" ,  "scripting"],

  "host_permissions": [
    "https://app.linkdeal.io/*",  
    "https://www.linkedin.com/*"
  ],
  "web_accessible_resources": [
    {
      "resources": ["assets/images/full_logo.png"],
      "matches": ["<all_urls>"]
    }
  ]
}
