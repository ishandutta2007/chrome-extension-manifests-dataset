    "css": [ "stylesheets/content.css" ],
    "js": [ "scripts/utils.js", "scripts/blocking.js", "scripts/hiding.js", "scripts/content.js" ],
    "run_at": "document_start",
    "all_frames": true
  }],
  "browser_action": {
    "default_icon": { "19": "images/blocked/19.png", "38": "images/blocked/38.png" },
