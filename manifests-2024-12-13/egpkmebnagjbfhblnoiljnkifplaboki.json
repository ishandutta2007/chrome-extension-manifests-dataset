{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Jobsy",
  "version": "1.1",
  "description": "A Chrome extension for importing data into jobsy.works",
  "host_permissions": ["https://www.linkedin.com/*", "https://*.firebaseio.com/"],
  "permissions": ["storage", "clipboardRead"],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "icons": {
    "16": "jobsy-16.png",
    "32": "jobsy-32.png",
    "48": "jobsy-48.png",
    "128": "jobsy-128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://www.linkedin.com/*", "https://jobsy.works/*"],
      "js": ["content.js"],
      "css": ["content.css"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["content.css", "content.js", "jobsy-512.png"],
      "matches": ["https://www.linkedin.com/*", "https://jobsy.works/*"]
    }
  ],
  "externally_connectable": {
    "matches": ["https://jobsy.works/chrome-extension"]
  }
}
