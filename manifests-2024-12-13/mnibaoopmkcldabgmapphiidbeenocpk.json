{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Portfolio Keyword Checker",
  "version": "1.0",
  "description": "Checks for specific keywords in Adobe Stock assets and highlights them.",
  "host_permissions": ["https://contributor.stock.adobe.com/*"],
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://contributor.stock.adobe.com/*"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["images/img.png"],
      "matches": ["https://contributor.stock.adobe.com/*"]
    }
  ]
}
