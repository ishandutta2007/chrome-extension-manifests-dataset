{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Adadomains",
  "version": "1.1.0",
  "description": "Resolve .ada domains in your browser using the Cardano blockchain",
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icon_16.png",
      "32": "icon_32.png"
