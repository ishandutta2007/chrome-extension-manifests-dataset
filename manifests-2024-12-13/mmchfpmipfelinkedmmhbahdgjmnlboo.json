{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Samantha",
  "description": "Web Clipper to easily clip all the content you want from the internet to Samantha Notes.",
  "version": "1.0.1",
  "manifest_version": 3,
  "background": {
    "service_worker": "./background/background.js"
  },
  "content_scripts": [
    {
      "all_frames": false,
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "./assets/images/svg-consts.js",
        "./content/utils.js",
        "./content/content.js"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["assets/css/content.css"],
      "matches": ["<all_urls>"]
    }
  ],
  "action": {
    "default_popup": "./popup/index.html",
    "default_icon": "./assets/images/sam-logo-128.png"
  },
  "icons": {
    "128": "./assets/images/sam-logo-128.png"
  },
  "permissions": [
    "storage"
  ]
}