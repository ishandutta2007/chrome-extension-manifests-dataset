{
"update_url": "https://clients2.google.com/service/update2/crx",

  "short_name": "Memento",
  "name": "Memento collect",
  "description": "Capture your favorite content from the web effortlessly, and make it available on all your connected devices.",
  "icons": {
    "16": "favicon.png",
    "48": "favicon.png",
    "128": "favicon.png"
  },
  "version": "1.0.3",
  "manifest_version": 3,
  "action": {
    "default_title": "Memento"
  },
  "permissions": ["activeTab", "storage", "scripting", "tabs"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://*.twitter.com/*"],
      "js": ["content.js"]
    },
    {
      "matches": ["https://*.x.com/*"],
      "js": ["content.js"]
    }
  ],
  "externally_connectable": {
    "matches": [
      "http://localhost:3000/*",
      "http://localhost:8000/*",
      "https://api-gateway.mementocollect.com/*",
      "https://www.mementocollect.com/*",
      "https://*.twitter.com/*",
      "https://*.x.com/*"
    ]
  },
  "host_permissions": [
    "http://localhost:8000/*",
    "http://localhost:3000/*",
    "https://api-gateway.mementocollect.com/*",
    "https://www.mementocollect.com/*",
    "https://*.twitter.com/*",
    "https://*.x.com/*"
  ],

  "web_accessible_resources": [
    {
      "resources": ["index.html"],
      "matches": ["<all_urls>"]
    },{
      "resources": ["logo.192.png", "logo512.png"],
      "matches": ["<all_urls>"]
    }
  ]
}
