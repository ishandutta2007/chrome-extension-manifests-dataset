{
"update_url": "https://clients2.google.com/service/update2/crx",

  "version": "1.4.0",
  "manifest_version": 3,
  "name": "FluteSpace Radiology Co-pilot",
  "description": "This is a Chrome extension built by FluteSpace to support audio transcription for report generation.",
  "permissions": [
    "cookies",
    "offscreen",
    "activeTab",
    "tabs",
    "sidePanel",
    "storage",
    "scripting"
  ],
  "icons": {
    "16": "js/img/FluteSpace_Logo_Fit.png",
    "32": "js/img/FluteSpace_Logo_Fit.png",
    "48": "js/img/FluteSpace_Logo_Fit.png",
    "128": "js/img/FluteSpace_Logo_Fit.png"
  },
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_icon": "js/img/FluteSpace_Logo_Fit.png",
    "default_title": "Flute Audio Report Generation"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/contentScript.js"
      ]
    }
  ],
  "background": {
    "service_worker": "js/service_worker.js",
    "type": "module"
  },
  "side_panel": {
    "default_path": "js/index.html",
    "openPanelOnActionClick": true
  },
  "web_accessible_resources": [
    {
      "resources": [
        "js/requestPermissions.html",
        "js/requestPermissions.js",
        "js/img/*.png"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}