{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 2,
    "name": "TogglePassword",
    "version": "2.0.0",
    "description": "This browser extension allows you to show & hide passwords in any password entry field, using the context menu.",
    "icons": {
        "16": "img/icon.png",
        "48": "img/icon.png",
        "128": "img/icon.png"
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["js/content.js"],
        "all_frames": true,
        "match_about_blank": true
    }],
    "background": {
        "scripts": ["js/background.js"]
    },
    "permissions": [
        "contextMenus"
    ]
}