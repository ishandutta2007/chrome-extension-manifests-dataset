{
"update_url": "https://clients2.google.com/service/update2/crx",

    "name": "LMS Helper",
    "description": "코스모스 기능 확장 프로그램",
    "version": "2.0.2",
    "author": "low",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "128": "/images/helper_logo.png"
    },
    "permissions": [
        "storage"
    ],
    "host_permissions": [
        "http://*.ac.kr/*",
        "https://*.ac.kr/*"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "options_page": "popup.html",
    "content_scripts": [
        {
            "matches": [
                "http://*.ac.kr/*",
                "https://*.ac.kr/*"
            ],
            "js": [
                "helper.js",
                "shortcutHelper.js",
                "reportHelper.js"
            ],
            "run_at": "document_idle"
        },
        {
            "matches": [
                "http://*.ac.kr/mod/vod/viewer.php*",
                "https://*.ac.kr/mod/vod/viewer.php*"
            ],
            "js": [
                "videoHelper.js"
            ],
            "run_at": "document_idle"
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "js/*.js",
                "css/*.css",
                "/images/low_logo.jpg"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ]
}