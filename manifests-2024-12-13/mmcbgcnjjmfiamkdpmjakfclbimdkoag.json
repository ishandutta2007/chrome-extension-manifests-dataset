"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,

  "name": "Quick SSO Proxy",
  "description": "Reload page using a single sign-on proxy.",
  "version": "1.2",
  "background": {
    "scripts": ["main.js"],
    "persistent": false
  },
  "options_page": "options.html",
