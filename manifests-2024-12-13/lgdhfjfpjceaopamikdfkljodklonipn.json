{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Custom Dog Cursors",
  "version": "1.0",
  "description": "Custom Dog Cursor extension",
  "permissions": [
    "storage",
    "tabs"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "icons": {
   "128": "icon_128.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "all_frames": true
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["images/golden_hover.png",
      "images/golden.png",
      "images/doberman.png",
      "images/doberman_hover.png",
      "images/rottweiler.png",
      "images/rottweiler_hover.png",
      "images/german_shepherd.png",
      "images/german_shepherd_hover.png"
    ],
      "matches": ["<all_urls>"]
    }
  ]
}