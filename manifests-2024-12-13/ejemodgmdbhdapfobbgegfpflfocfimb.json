  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions":["storage"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
  {
   "matches": ["https://twitter.com/*"],
   "js": ["inject.js"],
     "run_at": "document_start"
  }
  ]
