"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "TweetCount",
  "description": "This extension counts the overall tweets you have read",
  "version": "0.1",
  "manifest_version": 3,
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png"
    }
