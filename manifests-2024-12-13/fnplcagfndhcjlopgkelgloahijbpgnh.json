        "https://ltswebapp.centralindia.cloudapp.azure.com/*"
    ],
    "background": {
        "service_worker": "scripts/background.js"
    },
    "content_scripts": [
        {
            "js": [
                "scripts/contentUtils.js",
                "scripts/content.js"
            ],
            "matches": [
                "*://*/*.pdf*"
            ],
            "css": [
                "scripts/customDivStyles.css"
            ]
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "images/*",
                "sidePanel/styles/RimouskiSb/*",
                "sidePanel/sidePanel.html",
                "sidePanel/styles/style.css"
            ],
            "matches": [
                "<all_urls>"
            ]
