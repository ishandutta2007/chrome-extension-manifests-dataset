    "manifest_version": 3,
    "name": "GmailBlur",
    "version": "1.0",
    "description": "Protect your Gmail privacy by blurring content until you hover over it.",
    "permissions": [
      "activeTab",
      "tabs",
      "storage"
    ],
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["https://mail.google.com/*"],
        "js": ["content.js"]
      }
    ],
    "icons": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    
    "background": {
      "service_worker": "background.js"
    },
    "options_page": "options.html"
  }
  