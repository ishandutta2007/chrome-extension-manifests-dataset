{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Tabito",
  "version": "1.0.0",
  "description": "Manage isolated browser sessions with visual indicators",
  "author": "Build Archetype",
  "homepage_url": "https://github.com/build-archetype/tabito-app",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": ["contextMenus", "tabs", "storage", "cookies"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_popup": "popup/popup.html"
  },
  "options_page": "settings/settings.html",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["tab-styles.css"],
      "js": ["content.js"],
      "type": "module"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["newtab.html"],
      "matches": ["<all_urls>"]
    },
    {
      "resources": ["icons/*.svg"],
      "matches": ["<all_urls>"]
    },
    {
      "resources": ["icons/reset-white.png"],
      "matches": ["<all_urls>"]
    }
  ]
}
