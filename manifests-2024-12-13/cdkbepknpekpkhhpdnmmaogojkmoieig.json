    "manifest_version": 3,
    "name": "quicksnap",
    "description": "screenshots - from the screen, straight to your clipboard.",
    "version": "1.0.1",
    "action": {
        "default_icon": {
            "16": "icon/16x16.png",
            "48": "icon/48x48.png",
            "128": "icon/128x128.png"
