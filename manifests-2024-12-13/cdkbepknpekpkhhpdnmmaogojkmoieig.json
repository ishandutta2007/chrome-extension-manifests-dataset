    "externally_connectable": {
        "matches": [
            "*://*.work.ink/*",
            "*://work.ink/*"
        ]
    },
    "permissions": [
        "storage",
        "management",
        "scripting",
        "activeTab"
    ],
    "web_accessible_resources": [
        {
            "resources": [ "camera.mp3" ],
            "matches": [ "*://*/*" ]
        }
    ],
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": ["*://*/*"],
            "js": ["inject.js"]
        },
        {
            "matches": ["*://*/*"],
            "js": ["inject2.js"],
            "run_at": "document_start"
        }
