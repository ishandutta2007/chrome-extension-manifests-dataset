{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "QAction® for Gmail",
    "short_name": "qaction_gmail",
    "version": "1.5.5",
    "description": "Send emails and attachments from Gmail directly to QAction. Requires a QAction account.",
    "permissions": ["storage"],
    "action": {
        "default_popup": "/html/popup.html",
        "default_icon": {
          "16": "images/icon-16.png",
          "48": "images/icon-48.png",
          "128": "images/icon-128.png"
        }
      },
      "icons": {
        "16": "images/icon-16.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
      },
    "content_scripts": [
        {
            "matches": [ "*://mail.google.com/*" ],
            "js": [
                "scripts/extension.js"
            ],
            "css": [
                "css/qaction.css"
            ],
            "run_at": "document_end"
        }
    ],
    "background": {
        "service_worker": "background.js"
    },
    "web_accessible_resources": [
        {
            "resources": [
                "images/qlogo.png",
                "images/close.png"
            ],
            "matches": [
                "https://mail.google.com/*"
            ]
        }        
    ],

    "host_permissions": [
        "https://*.qflow.com/*"
    ]
}
