{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "BIS Platform Document Pack Helper",
    "description": "Browser extension for assistance with document packs in BIS Platform",
    "version": "1.0",
    "icons": {
        "128": "./assets/bis-128.png",
        "48": "./assets/bis-48.png",
        "16": "./assets/bis-16.png"
    },
    "author": "david@wildcard.ie",
    "action": {
        "default_popup": "popup.html",
        "default_icon": "./assets/bis-16.png",
        "default_title": "BIS Platform Helper"
    },
    "background": {
        "service_worker": "./assets/background.js"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "css": [
                "./assets/content.css"
            ],
            "js": [
                "./assets/content.js"
            ]
        }
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "permissions": [
        "activeTab",
        "storage",
        "scripting"
    ]
}