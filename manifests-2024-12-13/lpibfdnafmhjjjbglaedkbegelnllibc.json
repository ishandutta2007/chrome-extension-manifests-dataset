{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "InSite Highlighter",
    "version": "1.0",
    "description": "The extension is designed to help you search for relevant keywords on LinkedIn, GitHub or Stack Overflow",
    "permissions": [
        "storage"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_title": "InSite Highlighter",
        "default_icon": {
            "16": "assets/img/icon16.png",
            "48": "assets/img/icon48.png",
            "128": "assets/img/icon128.png"
        }
    },
    "icons": {
        "16": "assets/img/icon16.png",
        "48": "assets/img/icon48.png",
        "128": "assets/img/icon128.png"
    },
    "content_scripts": [
        {
            "js": [
                "contentScript.js"
            ],
            "matches": [
                "https://www.linkedin.com/*",
                "https://github.com/*",
                "https://stackoverflow.com/*"
            ],
            "run_at": "document_end",
            "all_frames": true
        }
    ],
    "background": {
        "service_worker": "background.js",
        "css": [
            "background.css"
        ]
    },
    "host_permissions": [
        "https://*.linkedin.com/*",
        "https://*.github.com/*",
        "https://*.stackoverflow.com/*"
    ]
}