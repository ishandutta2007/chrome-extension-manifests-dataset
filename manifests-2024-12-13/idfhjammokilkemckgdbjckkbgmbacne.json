{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "YouTube B/W Filter",
  "version": "1.1",
  "description": "This extension lets you apply a black and white filter to YouTube videos and images, with manual or scheduled activation.",
  "permissions": ["activeTab", "storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/youtube-logo-black.png",
      "48": "images/youtube-logo-black.png",
      "128": "images/youtube-logo-black.png"
    }
  },
  "icons": {
    "16": "images/youtube-logo-black.png",
    "48": "images/youtube-logo-black.png",
    "128": "images/youtube-logo-black.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["content.js"]
    }
  ]
}
