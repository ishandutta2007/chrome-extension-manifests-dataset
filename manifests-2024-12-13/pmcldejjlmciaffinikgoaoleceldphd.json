  "manifest_version": 2,
  "name": "CleanBrowsing DNS Filtering for Schools",
  "permissions": [
    "storage",
    "webRequest",
    "webRequestBlocking",
    "https://my.cleanbrowsing.org/",
    "https://my.cleanbrowsing.org/*",
    "https://my.cleanbrowsing.org/apis/devices/debug-verify",
