    "38": "icon-128.png",
    "48": "icon-128.png",
    "128": "icon-128.png"
  },
  "version": "1.0",
  "browser_action": {
    "default_icon": {
      "16": "icon-128.png",
      "19": "icon-128.png",
      "32": "icon-128.png",
      "38": "icon-128.png",
      "48": "icon-128.png",
      "128": "icon-128.png"
    },
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_start",
      "js": [
        "./content.js"
      ]
    }
  ],
  "storage": {
    "managed_schema": "schema.json"
  },
  "background": {
    "scripts": [
      "./background.js"
    ],
    "persistent": true
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
