{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Addictive Tabs",
  "version": "0.3.1",
  "description": "Tab manager that lets you easily declutter your workspace by archiving windows to bookmarks",
  "action": {
    "default_icon": "./assets/icon-512.png",
    "default_popup": "./dist/overview/index.html?entry=popup"
  },
  "options_ui": {
    "page": "./dist/options/index.html",
    "open_in_tab": true
