{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,

  "name": "Rollick Dealer Widget - Demo",
  "description": "This extension shows a Rollick widget on a dealer website",
  "version": "3.3",

  "icons": { 
    "16":"Rollick-16.png", 
    "48":"Rollick-48.png", 
    "128":"Rollick-128.png" 
  }, 
  
  "options_page": "options.html",
  
  "background": {
 "scripts": ["rollick.js"],
 "persistent": true
  },
  
  "browser_action": {
    "default_icon": "icon.png"
  },
  
  "content_scripts": [
 { 
  "matches": ["*://*/*"], 
  "js": ["inject.js"]
 }
  ],
  
  "commands": {
   "take-screenshot": {
  "suggested_key": {
    "default": "Alt+Shift+P",
    "windows": "Alt+Shift+P",
    "mac": "Alt+Shift+P"
  },
  "description": "Take screenshot"
   }
  },

  "permissions": [
    "activeTab",
 "storage",
 "webNavigation", 
 "https://cdn.rollick.io/"
  ]
}
