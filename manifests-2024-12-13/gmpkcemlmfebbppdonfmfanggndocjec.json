{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "CRM de vendas para Whatsapp Web - via Agendor",
  "version": "1.2.1",
  "description": "Extensão do Agendor para facilitar a gestão dos seus leads e tarefas direto no Whatsapp Web",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "permissions": [
    "tabs",
    "cookies",
    "scripting"
  ],
  "host_permissions": [
    "https://*.agendor.com.br/*",
    "https://web.whatsapp.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://web.whatsapp.com/*"],
      "run_at": "document_idle",
      "js": ["content.js"]
    },
    {
      "matches": ["https://chat.agendor.com.br/*"],
      "run_at": "document_idle",
      "js": ["agendor-chat.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["/*"],
      "matches": ["https://web.whatsapp.com/*", "https://chat.agendor.com.br/*"]
    }
  ]
