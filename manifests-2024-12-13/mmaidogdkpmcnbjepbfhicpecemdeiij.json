{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "FLEXISAVES",
  "description": "Save, Share and Access Content Effortlessly Anywhere, Anytime!",
  "version": "1.5.0",
  "manifest_version": 3,
  "icons": {
    "16": "favicon.png",
    "48": "favicon.png",
    "128": "favicon.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "FLEXISAVES",
    "default_icon": "favicon.png"
  },
  "permissions": ["alarms", "contextMenus", "storage", "tabs", "activeTab"],
  "options_page": "options.html",
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "copy.svg",
        "edit.svg",
        "open-link.svg",
        "delete.svg",
        "back.svg",
        "copied.svg",
        "contentScript.css",
        "popup.css",
        "logo.svg",
        "footer-logo.svg",
        "logo-lg.png",
        "saved-icon.png",
        "favicon.png",
        "content-script.png",
        "note.png",
        "note-color.png",
        "content.png",
        "content-color.png",
        "plus.png",
        "add-note.png",
        "add-note-color.png"
      ],
      "matches": ["<all_urls>"]
    }
  ],

  "externally_connectable": {
    "matches": ["https://flexisaves.com/*"]
  },
  "commands": {
    "open-modal": {
      "suggested_key": {
        "default": "Alt+Shift+Y",
        "mac": "Command+Shift+Y"
      },
      "description": "Run \"open-modal\" on the current page."
    },
    "_execute_action": {
      "suggested_key": {
        "windows": "Alt+Shift+Y",
        "mac": "Command+Shift+Y",
        "chromeos": "Alt+Shift+U",
        "linux": "Alt+Shift+J"
      }
    }
  }
}
