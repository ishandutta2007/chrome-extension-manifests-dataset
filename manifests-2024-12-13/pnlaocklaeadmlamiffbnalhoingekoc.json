            "https://www.tatacliq.com/*",
            "https://www.fnp.com/*",
            "https://beardo.in/*",
            "https://www.healthkart.com/*",
            "https://www.samsung.com/*",
            "https://www.jackjones.in/*",
            "https://www.fastrack.in/*",
            "https://pizzaonline.dominos.co.in/*",
            "https://www.clavax.com/*"
        ],
        "css": ["style.css"],
        "js": ["content.js"]
    }
   ],

    "icons": {
        "16": "icon16.png",
        "32": "icon32.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "action": {
        "default_popup": ""
    },
    "background": {
    "service_worker": "background.js"
    },

    "content_security_policy": {
