{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "SPRL Link Shortener",
  "version": "1.0.8",
  "description": "Easy Link Shortener with advance tracking",
  "icons":{
 "16":"img/sprlicon16.png", 
 "32":"img/sprlicon32.png",
 "64":"img/sprlicon64.png",
 "128":"img/sprlicon128.png"
  },
  "content_scripts": [
    {
   "matches": ["<all_urls>"],
      "js": ["js/jquery.js","js/popup.js"],
    "css": ["css/style.css"]
    }
  ],
  "action": {
    "default_title": "Short Link with SPRL",
   "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "js/background.js",
    "type": "module"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "css/style.css"
      ],
      "matches": ["<all_urls>"]
    }
  ],
  "host_permissions": ["https://insprl.com/*"],
  "permissions": ["scripting","contextMenus","activeTab"],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self';",
    "sandbox": "sandbox allow-scripts allow-forms allow-popups allow-modals; script-src 'self' 'unsafe-inline' 'unsafe-eval'; child-src 'self';"
  }
