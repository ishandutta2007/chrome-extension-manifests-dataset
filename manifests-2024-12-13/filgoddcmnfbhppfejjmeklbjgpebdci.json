{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "KredKonnect-Shoonya",
  "description": "The KredKonnect-Shoonya Chrome Extension is a trading tool that enhances TradingView.",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": [
       "https://in.tradingview.com/chart/*",
        "https://www.tradingview.com/chart/*"
      ],
      "js": [
        "./main.js"
      ],
      "run_at": "document_end"
    }
  ],
  "background": {
    "service_worker": "./background.js"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "IronEdge128_128.png",
        "./src/components/*"
      ],
      "matches": [
        "https://*/*"
      ],
      "permissions": [
        "tabs",
        "<all_urls>"
      ]
    }
  ],
  "permissions": [
    "storage",
    "notifications",
    "unlimitedStorage"
  ],
  "host_permissions": [
    "https://in.tradingview.com/chart/*"
  ],
  "icons": {
    "16": "./publish_data/IronEdge16_16.png",
    "32": "./publish_data/IronEdge32_32.png",
    "48": "./publish_data/IronEdge48_48.png"
  },
  "action": {
    "default_icon": {
      "16": "./publish_data/IronEdge16_16.png"
    },
    "default_title": "IronEdge"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+B",
        "mac": "Command+B"
      }
    }
  }
}