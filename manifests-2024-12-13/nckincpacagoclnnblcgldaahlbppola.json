{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "ClickToCall",
  "description": "click-to-call extension is a productivity tool.",
  "version": "1.0.0",
  "manifest_version": 3,
  "background":{
    "service_worker": "background.js",
    "type": "module"
  },
  "host_permissions": ["https://*.reisift.io/*","https://*.callrail.com/*"],
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "js": ["js/application.js"],
      "css" : ["css/app.css"]
    },
    {
      "matches": ["https://*.reisift.io/*"],
      "js": ["js/reisift.js"],
      "css" : ["css/app.css"]
    },
    {
      "matches": ["https://*.callrail.com/*"],
      "js": ["js/callrail.js"],
      "css" : ["css/app.css"]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Config Click to Call Extension",
    "default_icon": {
      "16": "./images/favicon.png",
      "24": "./images/favicon.png",
      "32": "./images/favicon.png",
      "48": "./images/favicon.png"
    }
  },
  "icons": {
    "16": "./images/favicon.png",
    "24": "./images/favicon.png",
    "32": "./images/favicon.png",
    "48": "./images/favicon.png"
  },
  "permissions": ["storage","activeTab"],
  "web_accessible_resources": [
    {
      "resources": [
        "images/favicon.png",
        "images/favicon.ico"
      ],
      "matches": ["https://*.reisift.io/*"]
    }
  ]
}
