{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "BlockTwitter",
    "version": "1.0",
    "description": "Redirects Twitter and X to a customizable URL.",
    "permissions": [
        "storage",
        "tabs"
    ],
    "host_permissions": [
        "*://twitter.com/*",
        "*://www.twitter.com/*",
        "*://x.com/*",
        "*://www.x.com/*"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "options_ui": {
        "page": "options.html",
        "open_in_tab": true
    },
    "icons": {
        "48": "icon.png",
        "128": "icon.png"
    }
