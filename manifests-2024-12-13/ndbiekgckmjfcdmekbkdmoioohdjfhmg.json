  },
  "action": {
      "default_title": "Open Prompt Manager"
  },
  "side_panel": {
      "default_path": "sidepanel.html"
  },
  "content_scripts": [
      {
          "matches": ["https://claude.ai/*"],
          "js": ["content.js"]
      }
  ]
