      ]
    },
    {
      "js": ["scripts/hilton.js"],
      "matches": ["http://www.hilton.com/*/*", "https://www.hilton.com/*/*"]
    },
    {
      "js": ["scripts/jblu.js"],
      "matches": [
        "http://www.jetblue.com/booking/*",
