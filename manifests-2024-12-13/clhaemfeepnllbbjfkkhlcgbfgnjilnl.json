{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,

  "name": "__MSG_extName__",
  "description": "__MSG_extDescription__",
  "author": "__MSG_extAuthor__",
  "version": "1.2.0",

  "default_locale": "en",

  "background": {
    "page": "background.html"
  },

  "content_scripts": [{
    "matches": ["*://*/*", "ftp://*/*"],
