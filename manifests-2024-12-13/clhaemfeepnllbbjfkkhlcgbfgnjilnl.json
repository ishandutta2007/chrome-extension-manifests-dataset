  "web_accessible_resources": [
    "assets/*"
  ],

  "commands": {
    "toggle-notification": {
      "suggested_key": {
        "default": "Ctrl+M"
      },
      "description": "Minimize/restore notification"
    },
    "accept-notification": {
      "suggested_key": {
        "default": "Ctrl+Shift+F"
      },
      "description": "Found"
    },
    "next-notification": {
      "suggested_key": {
        "default": "Ctrl+Shift+K"
      },
      "description": "Go to next Suggested page"
    }
  },

  "permissions": [
    "tabs",
    "webRequest",
    "webNavigation",
    "*://*/*",
    "ftp://*/*"
