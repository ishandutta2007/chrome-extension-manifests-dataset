{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "XHumn",
  "version": "0.5.9",
  "permissions": [
    "tabs",
    "http://localhost:5500/*",
    "https://*.linkedin.com/*",
    "https://*.cypress.ai/*"
  ],
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "web_accessible_resources": [
    "overlay-app.build.js"
  ],
  "background": {
    "scripts": [
      "background.build.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.linkedin.com/*"
      ],
      "js": [
        "content-linkedin.build.js"
      ]
    },
    {
      "matches": [
        "http://localhost:5500/*",
        "https://*.cypress.ai/*"
      ],
      "js": [
        "content-cypress.build.js"
      ]
    }
  ]
