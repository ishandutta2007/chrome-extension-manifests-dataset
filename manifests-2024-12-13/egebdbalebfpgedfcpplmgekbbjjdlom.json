      "css": ["style.css"]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "ailogo.png",
      "48": "ailogo.png",
      "128": "ailogo.png"
    }
  },
  "background": {
    "service_worker": "background.bundle.js"
  },
  "icons": {
    "16": "ailogo.png",
    "48": "ailogo.png",
    "128": "ailogo.png"
  },
  "web_accessible_resources": [
    {
      "matches": ["<all_urls>"],
      "resources": ["static/js/App.bundle.js"]
    }
  ]
