{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "ChatGPT Bulk Actions",
    "version": "3.0",
    "description": "Change ChatGPT into a productivity",
    "host_permissions": [
        "https://chatgpt.com/*"
    ],
    "permissions": [
        "cookies",
        "webRequest",
        "storage"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/icon16.png",
            "48": "icons/icon48.png",
            "128": "icons/icon128.png"
        }
    },
    "content_scripts": [
        {
            "matches": [
                "https://chatgpt.com/*"
            ],
            "js": [
                "contentScript.js"
            ]
        },
        {
            "matches": [
                "<all_urls>"
            ],
            "exclude_matches": [
                "https://chatgpt.com/*"
            ],
            "js": [
                "globalContentScript.js"
            ],
            "css": [
                "globalStyle.css"
            ]
        }
    ],
    "background": {
        "service_worker": "background.js"
    }
}