{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Neztwork meeting assistant",
  "version": "1.1.5",
  "description": "Neztwork transcribes meetings in real-time. It also provides a summary and subtitles for movies, lectures",
  "permissions": ["tabCapture", "storage", "offscreen"],
  "host_permissions": ["*://*.neztwork.com/*"],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_popup": "src/popup.html"
  },
  "default_locale": "en",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self'"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "*.js",
        "*.css",
        "assets/*",
        "requestPermissions.html",
        "requestPermissions.js",
        "src/container.html",
        "container.js",
        "subtitle.js",
        "summaryRealtime.js"
      ],
      "matches": ["*://*.neztwork.com/*"]
    }
  ],
  "content_scripts": [
    {
      "matches": ["*://*.neztwork.com/*"],
      "all_frames": false,
      "js": ["contentNetzwork.js"],
      "run_at": "document_end"
    }
  ]
}
