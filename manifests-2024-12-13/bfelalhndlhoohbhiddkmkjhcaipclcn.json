    "rule_resources": [
      {
        "id": "default",
        "enabled": true,
        "path": "declarative_net_request.json"
      }
    ]
  },

  "web_accessible_resources": [
    {
        "resources": [
            "/icon.png",
            "/icon_disabled.png"
