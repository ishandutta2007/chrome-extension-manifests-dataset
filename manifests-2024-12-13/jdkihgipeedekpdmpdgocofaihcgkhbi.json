{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "gridView",
  "version": "1.0",
  "description": "A simple tool for web designers/developers to overlay a grid on any website with custom columns, colors and more.",
  "icons": {
    "128": "img/icon128.png",
    "48": "img/icon48.png",
    "16": "img/icon16.png"
  },
  "permissions": [
    "activeTab",
    "storage",
    "scripting"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "action": {
    "default_icon": "img/icon128.png",
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "css": [
        "css/grid.css"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "background": {
    "service_worker": "js/serviceWorker.js",
    "type": "module"
  }
}