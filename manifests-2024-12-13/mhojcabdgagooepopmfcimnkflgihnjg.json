    "css": ["faveit-style.css"],
    "run_at": "document_end"
  }],
  "background": {
    "persistent": true,
    "scripts": ["jquery-1.11.0.min.js","background.js"]
  },
  "icons": { 
    "128": "bookmark.png"
  }
}
