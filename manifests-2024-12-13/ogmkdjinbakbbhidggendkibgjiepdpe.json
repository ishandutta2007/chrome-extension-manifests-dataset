    "manifest_version": 3,
    "name": "YouTube Replay Extension",
    "version": "0.0.1",
    "description": "One-Click to Loop Videos & View Loop History & Retrive and Manage Videos",
    "permissions": ["tabs", "storage"],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["*://*.youtube.com/watch*"],
        "js": ["content.js"],
        "run_at":"document_idle"
      }
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
         "48": "images/icon48.png"
      }
    },
    "icons": {
      "48": "images/icon48.png",
      "16": "images/icon16.png",
      "128":"images/icon128.png"
    }
