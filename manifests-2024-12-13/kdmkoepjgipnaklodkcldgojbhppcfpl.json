{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "TurboTab",
  "version": "0.0.8",
  "description": "Spotlight search for chrome tabs, bookmarks and history!",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
     "default_popup": "popup.html"
   },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
