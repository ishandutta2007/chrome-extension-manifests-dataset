      "suggested_key": {
        "default": "Ctrl+Shift+L",
        "mac": "MacCtrl+Shift+L"
      }

    },
    "group": {
      "suggested_key": {
        "default": "Ctrl+Shift+G",
        "mac": "MacCtrl+Shift+G"
      },
      "description": "Group tabs in current window by domain"
    }
  },
  "permissions": ["tabs", "bookmarks", "history", "contextMenus", "storage"]
}
