{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Youtube Thumbnail Filter",
  "description": "An extension that filters youtube video thumbnails based on user defined criterias",
  "author": "Yicheng Gong",
  "version": "1.0.1",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "public/icon16.png",
    "48": "public/icon48.png",
    "128": "public/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.youtube.com/*"
      ],
      "js": [
        "content-script.js",
        "public/material-components-web.min.js"
      ],
      "run_at": "document_end"
    }
  ],
  "host_permissions": [
    "*://*.youtube.com/*",
    "https://www.googleapis.com/*"
  ],
  "permissions": [
    "storage"
  ],
  "action": {
    "default_popup": "popup.html"
  }
}