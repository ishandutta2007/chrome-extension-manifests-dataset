{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "PinyinTube",
  "author": "Hung Do",
  "version": "0.0.1.1",
  "icons": {
    "128": "icons/PT_128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "PinyinTube",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "tabs",
    "tabCapture",
    "offscreen"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.netflix.com/*",
        "*://*.youtube.com/*",
        "*://*.iq.com/*",
        "*://*.viki.com/*",
        "<all_urls>"
      ],
      "run_at": "document_idle",
      "js": [
        "contentScript.js"
      ],
      "css": [
        "assets/css/contentScript.css"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "assets/js/injected.js",
        "libs/socket.io.min.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}
