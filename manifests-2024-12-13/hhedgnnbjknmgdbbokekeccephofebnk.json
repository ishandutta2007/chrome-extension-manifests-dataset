{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Your Environments",
  "version": "1.0.0",
  "description": "Loads new windows with specific tabs. Clear all existing tabs and open only the new window or avoiding clearing existing tabs",
  "permissions": ["storage"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "options.html",
    "default_icon": {
      "16": "icon16.png",
      "24": "icon24.png",
      "32": "icon32.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "24": "icon24.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "commands": {
    "open_work_environment": {
      "suggested_key": {
        "windows": "Alt+Shift+1",
        "default": "Ctrl+Shift+1",
        "mac": "Command+Shift+1"
      },
      "description": "Abre las páginas configuradas en una nueva ventana"
    },
    "reset_work_environment": {
      "suggested_key": {
        "mac": "Command+Shift+0",
        "windows": "Alt+Shift+0",
        "default": "Ctrl+Shift+0"
      },
      "description": "Cierra todas las pestañas y abre una nueva ventana con las páginas configuradas"
    }
  },
  "options_page": "options.html",
  "web_accessible_resources": [
    {
      "resources": [
        "popup.html",
        "options.html",
        "popup.css",
        "options.css",
        "assets/*.js"
      ],
      "matches": ["<all_urls>"]
    }
  ]
}
