  "manifest_version": 3,
  "name": "Tab Protector",
  "version": "1.0",
  "description": "Prevents accidental tab closing and refreshing on specified websites",
  "permissions": ["storage", "tabs"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
