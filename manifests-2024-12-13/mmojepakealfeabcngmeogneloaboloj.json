{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "PopBoxGPT",
  "version": "1.21.2",
  "description": "Adds a GPT PopBox to any website.",
  "manifest_version": 3,
  "author": "Jane Goldsmith",
  "action": {
    "default_title": "PopBoxGPT",
    "default_popup": "index.html"
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "activeTab",
    "storage",
    "commands"
  ],
  "commands": {
    "openGPT": {
      "suggested_key": {
        "default": "Alt+J",
        "mac": "Alt+J"
      },
      "description": "Open the GPT Window"
    },
    "fullScreen": {
      "suggested_key": {
        "default": "Alt+K",
        "mac": "Alt+K"
      },
      "description": "Make the GPT Window Full Screen"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": ["<all_urls>"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": [
        "js/window-manager.js",
        "js/window-pane.js",
        "popbox.js"
      ],
      "css": ["css/popbox.css"],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["index.html"],
      "matches": ["<all_urls>"]
    }
  ]
}
