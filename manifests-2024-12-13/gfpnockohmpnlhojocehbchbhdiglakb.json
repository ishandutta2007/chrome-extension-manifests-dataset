---

{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "Favorite Duplicate URL Detector",
    "version": "2024.10.1",
    "permissions": [
        "bookmarks",
        "activeTab",
        "storage"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "options.html",
        "default_icon": {
            "16": "icon16.jpg",
            "48": "icon48.jpg",
            "128": "icon128.jpg"
        }
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "options_page": "options.html",
    "web_accessible_resources": [
        {
            "resources": [
                "languages.json"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ]
}