    "32": "assets/logos/ig-automator-logo.png",
    "48": "assets/logos/ig-automator-logo.png",
    "128": "assets/logos/ig-automator-logo.png"
  },
  "permissions": [
    "declarativeNetRequest",
    "declarativeNetRequestWithHostAccess",
    "storage",
    "identity"
  ],
  "host_permissions": ["*://*/*"],
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "modify-net-request",
