    "*://*.cryptocompare.com/*",
    "https://*.espn.com/*",
    "https://*.bbc.co.uk/*",
    "https://*.allorigins.win/*",
    "https://*.cors.eu.org/*",
    "https://*.corsproxy.io/*",
    "https://*.freeboard.io/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
