{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "App Store 应用信息二维码生成器",
  "version": "1.0",
  "description": "获取App Store应用下载链接生成二维码，并显示应用信息。",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "https://api.qrserver.com/*",
    "https://itunes.apple.com/*",
    "https://*.mzstatic.com/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "assets/logo.png",
      "48": "assets/logo.png",
      "128": "assets/logo.png"
    },
    "default_width": 600,
    "default_height": 600
  },
  "content_security_policy": {
    "extension_pages": "default-src 'self'; img-src 'self' data: https://*.mzstatic.com https://*.apple.com https://api.qrserver.com; connect-src 'self' https://*.apple.com https://*.mzstatic.com https://api.qrserver.com"
  }
} 