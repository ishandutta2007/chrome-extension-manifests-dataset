{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "ClipKeeper",
  "version": "1.0",
  "description": "Manages clipboard content across tabs",
  "permissions": [
    "storage",
    "clipboardWrite"
  ],
  "icons": {
    "128": "/128.png"
  },
  "background": {
    "service_worker": "/Javascript/background.js",
    "type": "module"
  },
  "action": {
    "default_popup": "/HTML/popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["/Javascript/content.js"]
    }
  ]
}