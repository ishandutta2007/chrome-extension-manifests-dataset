  },
  "icons": {
    "16": "favicon-16x16.png",
    "32": "favicon-32x32.png",
    "48": "logo192.png",
    "128": "logo192.png"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'; connect-src 'self' http://localhost:3000 https://prep5.salesably.ai"
  },
  "content_scripts": [
    {
      "matches": ["https://prep5.salesably.ai/*", "https://www.linkedin.com/*"],
      "js": ["content.js", "signInContentScript.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "index.html",
        "popup.js",
        "env.js",
        "favicon.ico",
        "logo192.png",
        "manifest.json",
        "favicon-32x32.png",
        "favicon-16x16.png"
      ],
      "matches": ["https://prep5.salesably.ai/*", "https://www.linkedin.com/*"]
    }
  ]
