{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Revscale LinkedIn Integration",
  "version": "1.5",
  "description": "Integrate Revscale features into LinkedIn Sales Navigator",
  "permissions": ["activeTab", "tabs", "storage", "webNavigation", "alarms", "scripting", "history"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
      "16": "Icons/Revscale_Icon_16.png",
      "32": "Icons/Revscale_Icon_32.png",
      "48": "Icons/Revscale_Icon_48.png",
      "128": "Icons/Revscale_Icon_128.png"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.linkedin.com/sales/*", 
        "http://*.localhost/*",
        "https://*.revscale.ai/*", 
        "https://apis.revscale.ai/*", 
        "https://www.linkedin.com/*"
      ],
      "js": ["config.js", "linkedIn.js"],
      "run_at": "document_start"
    }
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self';"
  },
  "externally_connectable": {
    "matches": [
      "http://*.localhost:3000/*",
      "https://*.revscale.ai/*",
      "https://apis.revscale.ai/*"
    ]
  },
  "icons": {
    "16": "Icons/Revscale_Icon_16.png",
    "32": "Icons/Revscale_Icon_32.png",
    "48": "Icons/Revscale_Icon_48.png",
    "128": "Icons/Revscale_Icon_128.png"
  }
}
