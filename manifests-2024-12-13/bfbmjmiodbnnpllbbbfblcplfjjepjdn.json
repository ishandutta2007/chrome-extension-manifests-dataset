  "128": "images/icon128.png"
 },
 "default_locale": "en",
 "content_security_policy": {
  "extension_pages": "default-src 'none'; style-src 'self'; media-src https://www.turnoffthelights.com; frame-src https://www.youtube.com https://www.turnoffthelights.com; connect-src https://www.turnoffthelights.com; script-src 'self'; img-src 'self' https://www.turnoffthelights.com * data:; object-src 'none'"
 },
 "content_scripts": [
  {
   "matches": [
    "<all_urls>"
   ],
   "js": [
    "scripts/constants.js",
    "scripts/content.js",
    "scripts/eco.js"
   ],
   "run_at": "document_end"
  },
  {
   "matches": [
    "<all_urls>"
   ],
   "css": [
    "styles/light.css"
   ],
   "run_at": "document_end",
   "all_frames": true,
   "match_about_blank": true
  },
  {
   "matches": [
    "*://*.youtube.com/*"
   ],
   "css": [
    "styles/youtube.css"
   ]
  }
 ],
 "options_ui": {
  "page": "options.html",
  "open_in_tab": true
 },
 "version": "4.5.1",
 "name": "__MSG_name__",
 "description": "__MSG_description__",
