    "background":
    {
        "page": "background.html",
        "persistent": true
    },
    "browser_action":
    {
        "default_title": "點我查詢GodJJ",
        "default_icon": "/img/jrobot.png",
        "default_popup": "popup.html"
    },
    "icons":
    {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
