{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "YT Tracker",
  "description": "Track youtube views",
  "version": "1.0",
  "icons": {
    "16": "img/16icon.png",
    "48": "img/48icon.png",
    "128": "img/128icon.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["src/js/refresh.js"]
    }
  ],
  "browser_action": {
    "default_icon": {
      "16": "img/16icon.png"
    }
  },
  "permissions": [
    "tabs",
    "activeTab",
    "*://*/*",
    "storage",
    "unlimitedStorage"
  ],
  "background": {
    "scripts": ["src/js/background.js"]
  }
}
