"update_url": "https://clients2.google.com/service/update2/crx",

    "name": "Twitter Blocker",
    "version": "0.0.1",
    "manifest_version": 3,
    "author": "domparso<domparso@hotmail.com>",


    "description": "推特账号屏蔽",

    "icons": {
        "16": "images/16.png",
        "32": "images/32.png",
        "48": "images/48.png",
        "128": "images/128.png"
    },

    "action": {
        "default_icon": {
                "16": "images/16.png",
                "32": "images/32.png",
                "48": "images/48.png"
            },
        "default_title": "Twitter Blocker",
        "default_popup": "popup/popup.html"
    },

    "background": {
        "service_worker": "js/background.js",
        "type": "module"
    },

    "content_scripts": [
        {
            "matches": ["https://twitter.com/*"],
            "js": [
                "js/jquery-3.5.1.min.js",
                "js/client.js",
                "js/content.js",
                "js/i18n.js"
            ],
            "css": ["css/content.css"]
        }
    ],

    "permissions": [
        "storage",
        "cookies"
    ],

    "host_permissions": [
        "*://*.twitter.com/*",
        "*://*.github.com/*",
        "*://raw.githubusercontent.com/*",
        "*://link.fakecloud.top/*"
    ]

