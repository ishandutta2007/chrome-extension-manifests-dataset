{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "Properly by Particle Space",
    "version": "1.0.4",
    "description": "Automatically pull property and property history details from your emails in Gmail.",
    "permissions": [
        "activeTab"
    ],
    "host_permissions": [
        "https://www.googleapis.com/auth/gmail.readonly"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "web_accessible_resources": [
        {
            "resources": ["*.png", "*.svg"],
            "matches": ["<all_urls>"]
        }
    ],
    "background": {
        "service_worker": "js/eventPage.js"
    },
    "content_scripts": [
        {
            "matches": ["http://mail.google.com/*", "https://mail.google.com/*"],
            "js": ["js/gmail.js"],
            "run_at": "document_idle"
        }
    ],
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    }
}
