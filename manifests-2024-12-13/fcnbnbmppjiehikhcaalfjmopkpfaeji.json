{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "DayHub",
  "version": "1.0",
  "description": "DayHub Extension for Google Chrome",
  "content_scripts": [
    {
      "matches": ["https://dayhub.co/*", "http://dayhub.co/*", "http://gokanto.com/*", "https://gokanto.com/*"],
      "js": ["content.js"],
      "css": ["css/notification.css"],
      "run_at": "document_end"
    }
  ],
  "permissions": ["cookies", "storage", "tabs", "activeTab"],
  "host_permissions": ["http://dayhub.co/*", "https://dayhub.co/*", "http://gokanto.com/*", "https://gokanto.com/*"],
  "action": {
    "default_title": "DayHub",
    "default_icon": "icons/icon-32px.png",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "background": {
    "service_worker": "service-worker.js"
  },
  "commands": {
    "execute_action": {
      "description": "Shows popup",
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon-16px.png",
        "32": "icons/icon-32px.png",
        "48": "icons/icon-48px.png",
        "128": "icons/icon-128px.png"
      }
    }
  },
  "icons": {
    "16": "icons/icon-16px.png",
    "32": "icons/icon-32px.png",
    "48": "icons/icon-48px.png",
    "128": "icons/icon-128px.png"
  },
  "web_accessible_resources": [{
    "matches": ["<all_urls>"],
    "resources": [
      "styles/base.css",
      "css/notification.css",
      "fonts/Roboto/Roboto-Regular.ttf",
      "fonts/Roboto/Roboto-Medium.ttf",
      "fonts/Roboto/Roboto-Bold.ttf"
    ]
  }],
  "externally_connectable": {
    "ids": ["*"],
    "matches": [
      "https://dayhub.co/*",
      "https://gokanto.com/*"
    ]
  },
  "manifest_version": 3
}
