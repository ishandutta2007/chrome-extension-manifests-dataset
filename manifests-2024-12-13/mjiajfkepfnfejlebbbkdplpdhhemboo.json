{
"update_url": "https://clients2.google.com/service/update2/crx",

  "version": "1.0.0",
  "manifest_version": 3,
  "name": "Instagram Downloader",
  "description": "Download photos, videos and Reels from Instagram.",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.instagram.com/*"],
      "js": ["js/extension.js"],
      "css": ["css/extension.css"]
    },
    {
      "matches": ["*://*.instagram.com/*"],
      "js": ["js/inject.js"],
      "run_at": "document_start",
      "world": "MAIN"
    },
    {
      "matches": ["*://*.instagram.com/*"],
      "js": ["js/options_listener_inject.js"],
      "run_at": "document_start"
    }
  ],
  "background": { "service_worker": "background.js" },
  "web_accessible_resources": [
    { "resources": ["/*.js"], "matches": ["<all_urls>"] },
    { "resources": ["icons/*"], "matches": ["<all_urls>"] },
    {
      "resources": ["js/options.js", "css/options.css"],
      "matches": ["*://*.instagram.com/*"]
    }
  ],
  "options_page": "options.html",
  "host_permissions": [
    "*://*.instagram.com/*",
    "*://*.cdninstagram.com/*",
    "*://*.cdninstagram.net/*",
    "*://*.fbcdn.net/*"
  ],
  "permissions": ["storage"]
}
