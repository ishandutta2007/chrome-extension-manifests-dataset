{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "Money Stuff Audio Player",
    "version": "1.1",
    "description": "Injects an audio player with bloomberg columnist Matt Levine's voice into his daily opinion piece 'Money Stuff'.",

    "host_permissions": ["*://www.bloomberg.com/opinion/*"],

    "content_scripts": [
        {
            "matches": ["*://www.bloomberg.com/opinion/*"],
            "js": ["howler.min.js", "content.js"],
            "css": ["styles.css"]
        }
    ],
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "web_accessible_resources": [
        {
            "resources": [
                "assets/play.png",
                "assets/pause.png",
                "assets/prev_ten.png",
                "assets/next_ten.png",
                "assets/speed_slow.png",
                "assets/speed_fast.png"
            ],
        "matches": ["<all_urls>"]
        }
    ]
}
