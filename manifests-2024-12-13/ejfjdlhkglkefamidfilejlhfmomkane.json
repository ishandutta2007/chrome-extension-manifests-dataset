  "name": "RedhamHR CRM Connector",
  "description": "Плагин для работы с сервисом avito.ru и CRM системой",
  "version": "1.2",
  "manifest_version": 3,
  "permissions": [
    "activeTab",
    "storage"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
