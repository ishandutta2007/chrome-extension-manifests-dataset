{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "移除搜索结果",
  "short_name": "移除搜索结果下的指定网站信息",
  "version": "1.0.0",
  "description": "输入网站名或链接，关于该网站的搜索结果就不会在搜索引擎下显示，当前只支持百度。",
  "icons": {
    "16": "icons/icon.png",
    "48": "icons/icon.png",
    "128": "icons/icon.png"
  },
  "browser_action": {
    "default_title": "Remove Search Result",
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.baidu.com/*"
      ],
      "js": [
        "contentScript.js"
      ]
    }
  ],
  "permissions": [
    "storage"
  ],
  "background": {
    "scripts": [
      "hotReload.js"
    ]
  }
}