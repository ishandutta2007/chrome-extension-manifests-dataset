{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Linkstick",
  "version": "1.0",
  "manifest_version": 3,
  "description": "Provides a structured interface for manipulating URLs. Makes the URL selecting process simple and convenient.",
  "permissions": [
    "activeTab",
    "storage",
    "history",
    "scripting"
  ], 
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.bundle.js"
  },
  "action": {
    "default_title": "Press Ctrl+Shift+X to open/close",
    "default_icon": {
      "16": "icon/logo16.png",
      "32": "icon/logo32.png"
      }
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "run_at": "document_end",
      "js": ["contentScript.bundle.js"]
    }
  ],
  "icons": {
    "16": "icon/logo16.png",
    "32": "icon/logo32.png",
    "48": "icon/logo48.png",
    "128": "icon/logo128.png"
  },
  "options_page": "options.html"
}
