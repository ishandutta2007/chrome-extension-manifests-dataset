{
"update_url": "https://clients2.google.com/service/update2/crx",

    "version": "1.0.1",
    "manifest_version": 3,
    "name": "Highperformr Chrome Extension",
    "permissions": [
        "cookies",
        "storage",
        "sidePanel",
        "alarms"
    ],
    "icons": {
        "16": "icon.png",
        "32": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "web_accessible_resources": [
        {
          "resources": ["public/*"],
          "matches": ["<all_urls>"]
        }
    ],
    "host_permissions": [
        "https://www.linkedin.com/*",
        "https://app.highperformr.ai/*",
        "http://localhost:3001/*"
    ],
    "side_panel": {
        "default_path": "js/sidepanel.html"
    },
    "background": {
        "service_worker": "js/background.js"
    },
    "action": {
        "default_popup": "js/index.html",
        "default_title": "React Chrome Extension"
    },
    "content_scripts": [
        {
          "matches": ["https://www.linkedin.com/*"],
          "js": ["js/content.js"]
        }
    ]
}