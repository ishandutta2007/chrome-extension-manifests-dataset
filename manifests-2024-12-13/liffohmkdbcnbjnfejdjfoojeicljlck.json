{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "YouTube Custom Controls",
  "version": "1.5.1",
  "description": "Adds custom controls for YouTube videos.",
  "host_permissions": [
    "https://*.youtube.com/*",
    "http://*.youtube.com/*"
  ],
  "permissions": [
    "storage",
    "scripting",
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.youtube.com/*",
        "http://*.youtube.com/*"
      ],
      "js": ["content.js"],
      "css": ["content.css"]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "icon.png"
    }
  },
  "icons": {
    "128": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}
