  "name": "Fenix-Chrome-Extension",
  "description": "Fenix extension to perform QuickTasks",
  "version": "2.8",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "background.js"
