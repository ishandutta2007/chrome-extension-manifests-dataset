{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Pin-The-Look Extension",
  "version": "0.1.37",
  "description": "This extension pins inspiration room images and parses e-commerce product to use on the Pin the Look platform.",
  "icons": {
    "16": "assets/logo.png",
    "32": "assets/logo.png",
    "48": "assets/logo.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "scripts/content.js",
        "scripts/parser.js",
        "settings/options.js",
        "templates/renderModal.js"
      ],
      "css": [
        "assets/styles.css"
      ]
    }
  ],
  "options_ui": {
    "page": "settings/options.html",
    "open_in_tab": true
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "tabs"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_icon": {
      "16": "assets/logo.png",
      "24": "assets/logo.png",
      "32": "assets/logo.png"
    },
    "default_title": "Click to open modal"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "templates/pin-modal-template.html",
        "templates/select-modal-template.html",
        "assets/font/walkwayUltraBold.ttf"
      ],
      "matches": ["<all_urls>"]
    }
  ]
