{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "Privacy Extension for WhatsApp Web",
    "version": "1.0.1",
    "description": "Protect your privacy with Cutlet's *Privacy Extension for WhatsApp Web*",
    "action": {
        "default_icon": "assets/images/icon-32.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "storage",
        "scripting",
        "tabs",
        "activeTab",
        "commands"
    ],
    "host_permissions": [
        "https://web.whatsapp.com/*"
    ],
    "background": {
        "service_worker": "assets/scripts/background.js"
    },
    "icons": {
        "16": "assets/images/icon-16.png",
        "32": "assets/images/icon-32.png",
        "48": "assets/images/icon-48.png",
        "128": "assets/images/icon-128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://web.whatsapp.com/*"
            ],
            "js": [
                "assets/scripts/content.js"
            ],
            "run_at": "document_end"
        }
    ],
    "commands": {
        "toggle-hide-all": {
            "suggested_key": {
                "default": "Alt+X",
                "mac": "Alt+X"
            },
            "description": "Toggle Hide All"
        }
    },
    "web_accessible_resources": [
        {
            "resources": [
                "assets/images/icon-16.png",
                "assets/images/icon-32.png",
                "assets/images/icon-48.png",
                "assets/images/icon-128.png"
            ],
            "matches": [
                "https://web.whatsapp.com/*"
            ]
        }
    ]
}