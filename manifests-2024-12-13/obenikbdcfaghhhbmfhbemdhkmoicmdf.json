  "minimum_chrome_version" : "35.0.0.0",
  "default_locale" : "en",
  "author" : "CopyCopy Team",
  "homepage_url" : "https://www.copycopy.com",

  "permissions" : [
    "cookies",
    "*://*.copycopy.cc/*",
    "https://www.google-analytics.com/",
