{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 3,
  "name": "Medrock Compound Extension",
  "description": "Search and manage your MedRock Pharmacy compounds",
  "version": "1.1.3",
  "permissions": [
    "activeTab"
  ],
  "host_permissions": [
    "https://*.ema.md/*",
    "https://*.ematraining.md/*"
  ],
  "action": {
    "default_popup": "start-popup.html"
  },
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.ema.md/*",
        "https://*.ematraining.md/*"
      ],
      "js": [
        "content.js",
        "progress.js"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "https://*.ema.md/*",
        "https://*.ematraining.md/*"
      ],
      "resources": [
        "compounds.json",
        "medications.json",
        "progress.js"
      ],
      "fonts": [
        "*.ttf"
      ]
    }
  ]
}