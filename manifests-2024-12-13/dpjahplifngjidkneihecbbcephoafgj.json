{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Focus Site Blocker",
  "description": "A site blocker that allows you to create a list of sites to block.",
  "version": "1.0.0",
  "manifest_version": 3,
  "icons": {
    "16": "/icons/logo16.png",
    "32": "/icons/logo32.png",
    "48": "/icons/logo48.png",
    "128": "/icons/logo128.png"
