    "<all_urls>"
  ],
  "icons": {
    "128": "/assets/icon_128.png"
  },
  "action": {
    "default_icon": {
    },
    "default_title": "__MSG_name__",
    "default_popup": "./html/popup.html"
  },
  "content_scripts": [
    {
      "match_about_blank": true,
      "all_frames": false,
      "js": [
        "./js/app.js"
      ],
      "run_at": "document_idle",
      "matches": [
        "*://*/*"
      ]
    }
  ],
  "background": {
    "service_worker": "./js/bg.js"
  }
