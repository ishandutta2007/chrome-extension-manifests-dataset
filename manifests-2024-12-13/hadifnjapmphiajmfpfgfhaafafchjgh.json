{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "__MSG_name__",
  "version": "2.1.0",
  "description": "__MSG_description__",
  "default_locale": "en",
  "permissions": [
    "declarativeNetRequest",
    "unlimitedStorage",
    "webRequest",
    "storage",
    "tabs"
