{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "LiteAPI",
  "version": "1.0.5",
  "description": "This extension compares hotel rates on Booking.com to identify and showcase the best available rates through liteAPI.",
  "host_permissions": [
    "https://api.liteapi.travel/*",
    "https://maps.googleapis.com/*",
    "https://hotel-mapping-webhook-zbcslb3kpa-ue.a.run.app/*"
  ],
  "permissions": ["webNavigation"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["*://*.booking.com/hotel*","*://*.booking.com/xapp/*/stay/details*","*://*.booking.com/xapp/stay/details*"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
