  "manifest_version": 3,
  "name": "Awesome TabMarks Extension",
  "version": "1.0.0",
  "description": "Complete Chrome companion: Session, tab & extension management, bookmarks backup, citations, page saving, DOI search & system tools.",
  "permissions": [
    "tabs",
    "tabGroups",
    "activeTab",
    "bookmarks",
    "storage",
    "system.cpu",
