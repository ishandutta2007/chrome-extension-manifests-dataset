{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "BatShield",
  "version": "1.3.1",
  "description": "Check Phishing Websites Powered By Gotham3",
  "default_locale" : "en",
  "permissions": [
    "tabs",
    "storage",
    "*://*.gotham3.io/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "512": "icon.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "512": "icon.png"
  },
  "web_accessible_resources": [{
    "resources": ["alert.html"],
    "matches": ["<all_urls>"]
}]
}
