    "https://*.amazon.co.uk/*",
    "https://*.force.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.amazon.com/*",
        "https://*.amazon.ca/*",
        "https://*.amazon.co.uk/*"
      ],
      "js": ["amazon.js"]
    },
    {
      "matches": ["https://*.force.com/*"],
      "js": ["salesforce.js"]
    },
    {
      "js": ["content.js"],
      "matches": ["https://*/*"],
      "run_at": "document_idle"
