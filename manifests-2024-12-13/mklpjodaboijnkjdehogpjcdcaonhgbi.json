        "matches": ["<all_urls>"],
        "js": ["config.js", "content.js"],
        "run_at": "document_end"
    }],
    "web_accessible_resources": [{
        "resources": ["index.html", "icon48.svg", "config.js"],
        "matches": ["<all_urls>"]
    }],
    "externally_connectable": {
        "matches": ["https://painel.digitador.app/*"],
        "accepts_tls_channel_id": true
    }   
