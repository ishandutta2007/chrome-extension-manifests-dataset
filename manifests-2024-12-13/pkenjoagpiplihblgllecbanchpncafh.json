{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "ShadowProof",
  "version": "3.1.1",
  "description": "Avoid Twitter shadowbans by preventing you from tweeting too quickly.",
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "img/icon-01.png",
    "48": "img/icon-02.png",
    "128": "img/icon-03.png"
  },
  "action": {
    "default_popup": "index.html",
      "js": ["js/App.js"],
      "css": ["css/App.css"]
    
  },
  "background": {
    "service_worker": "js/background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://twitter.com/*"],
      "js": ["js/contentScript.js"],
      "css": ["css/popup.css"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["sound/*"],
      "matches": ["https://twitter.com/*"]
    }
  ]
}
