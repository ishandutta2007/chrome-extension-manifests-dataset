{
"update_url": "https://clients2.google.com/service/update2/crx",

    "name": "Friendli",
    "description": "LinkedIn list builder",
    "version": "0.0.1.5",
    "manifest_version": 3,
    "host_permissions": ["https://www.linkedin.com/*"],
    "permissions": ["storage", "scripting"],
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": ["https://www.linkedin.com/*"],
            "js": [
                "libraries/jquery.js",
                "libraries/track-elements.js",
                "libraries/sweetalert.js",
                "libraries/popper.js",
                "libraries/tippy.js",
                "global-toasts/toastr.js",
                "global-toasts/custom-toast.js",
                "main.js"
            ],
            "css": ["global-toasts/toastr.css", "global-toasts/custom-toast.css", "main.css"]
        }
    ],
    "web_accessible_resources": [
        {
            "resources": ["images/*", "popup/*", "pages/*"],
            "matches": ["https://www.linkedin.com/*"]
        }
    ],
    "icons": {
        "16": "./images/icon.png",
        "32": "./images/icon.png",
        "48": "./images/icon.png",
        "128": "./images/icon.png"
    },
    "action": {
        "default_icon": {
            "16": "images/icon.png"
        },
        "default_popup": "popup/popup.html"
    }
}
