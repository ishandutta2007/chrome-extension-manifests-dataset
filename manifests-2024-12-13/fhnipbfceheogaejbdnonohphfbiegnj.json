{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "repmafia.net tools",
  "version": "1.1",
  "description": "Enhance your shopping experience on Kakobuy and CNFans with this helpful extension!",
  "host_permissions": [
    "https://api.finds.ly/*",
    "https://*.cnfans.com/*",
    "https://*.allchinabuy.com/*",
    "https://*.hoobuy.com/*",
    "https://*.kakobuy.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.cnfans.com/*",
        "https://*.allchinabuy.com/*",
        "https://*.hoobuy.com/*",
        "https://*.kakobuy.com/*"
      ],
      "js": ["content.js"],
      "css": ["styles.css"],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
