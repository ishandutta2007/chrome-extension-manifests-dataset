    },
    {
      "matches": ["*://*.tiktok.com/*"],
      "js": ["add-rules.js"]
    }
  ],
  "icons": {
    "16": "images/16.png",
    "32": "images/32.png",
    "48": "images/48.png",
    "128": "images/128.png"
  },
  "permissions": [
    "declarativeContent",
    "webRequest",
    "webNavigation",
    "declarativeNetRequest",
    "storage",
    "scripting"
  ],
  "host_permissions": ["*://*.tiktok.com/*"]
