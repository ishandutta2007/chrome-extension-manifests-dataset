{
"update_url": "https://clients2.google.com/service/update2/crx",

    "name": "Passport Autofill",
    "description": "Autofills passport website",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": ["storage"],    
    "host_permissions": [
        "*://portal1.passportindia.gov.in/*", "https://www.passportindia.gov.in/*"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/16.png",
            "32": "images/32.png",
            "48": "images/48.png",
            "128": "images/128.png"
        }
    },

    "content_scripts": [
        {
            "run_at": "document_end",
            "all_frames": false,
            "matches": ["*://portal1.passportindia.gov.in/*", "https://www.passportindia.gov.in/*"],
            "css": ["css/jquery-ui.min.css", "css/chosen.min.css", "css/main.css"],

            "js": [
                "js/jquery-3.6.0.min.js",
                "js/jquery-ui.min.js",
                "js/chosen.jquery.min.js",
                "common.js",
                "myscript.js"
            ]
        }
    ],

    "web_accessible_resources": [
        {
            "resources": ["popup.html"],
            "matches": ["<all_urls>"]
        }
    ]
}