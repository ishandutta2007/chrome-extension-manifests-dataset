{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,  
    "name": "Dark Mode Wizard",  
    "version": "1.0",  
    "description": "Toggle dark mode on websites that don’t support it natively.",  
    "permissions": [  
      "storage"  
    ],
    "action": {
      "default_popup": "popup.html"
    },
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "background": {
      "service_worker": "background.js"  
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],  
        "js": ["content.js"]  
      }
    ]
  }
  