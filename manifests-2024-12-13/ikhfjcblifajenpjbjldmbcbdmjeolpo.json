{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Search Twitter",
  "version": "2.0",
  "permissions": [
      "storage", 
      "activeTab", 
      "scripting"
  ],
  "action": {
      "default_popup": "popup.html",
      "default_icon": {
          "16": "search.png",
          "48": "search.png",
          "128": "search.png"
      }
  },
  "background": {
      "service_worker": "background.js"
  },
  "content_scripts": [
      {
          "matches": ["*://twitter.com/*"],
          "js": ["content.js"],
          "run_at": "document_idle"
      }
  ],
  "icons": {
      "16": "search.png",
      "48": "search.png",
      "128": "search.png"
  },
  "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self'; connect-src 'self' https://api.openai.com;"
  }
}
