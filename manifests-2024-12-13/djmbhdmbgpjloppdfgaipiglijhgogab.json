{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Buy Local, Buy Indian",
  "version": "0.1.1",
  "author": "Yatin Grover,Vaibhav Thapliyal <yatinadd@gmail.com>",
  "description": "A free chrome extension that helps you to spot Indian products easily online on shopping websites like flipkart etc.",
  "icons": {
        "16": "images/icon16.png",
        "19": "images/icon19.png",
        "38": "images/icon38.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    },
  "permissions": [],
  "background": {
    "page": "pages/background.html",
    "persistent": true
  },
  "options_page": "pages/options.html",
  "content_scripts": [
    {
      "matches": [
        "https://www.flipkart.com/*",
        "https://*.myntra.com/*",
        "https://myntra.com/*",
        "https://*.alibaba.com/*",
         "https://*.beardo.in/*",
        "https://www.amazon.in/*"
      ],
      "js": [
        "js/content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "js/*.js",
    "json/*.json",
    "vendor/*.js",
    "styles/*.css",
    "images/*.png",
    "pages/options.html"
  ],
  "manifest_version": 2
}