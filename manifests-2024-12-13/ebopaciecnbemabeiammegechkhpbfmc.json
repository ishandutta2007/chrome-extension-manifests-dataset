{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Job Squad Extension",
  "version": "1.20",
  "description": "Automate the easy apply process on LinkedIn job listings.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon/jobSquad.png",
      "36": "icon/jobSquad.png",
      "48": "icon/jobSquad.png",
      "64": "icon/jobSquad.png",
      "96": "icon/jobSquad.png",
      "128": "icon/jobSquad.png"
    }
  },
  "permissions": ["activeTab", "scripting", "alarms", "tabs"],
  "externally_connectable": {
    "matches": ["https://app.jobsquad.info/*"]
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "host_permissions": [
    "https://linkedin.com/*",
    "https://www.linkedin.com/jobs/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://linkedin.com/*"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["logo.png"],
      "matches": ["<all_urls>"]
    }
  ]
}
