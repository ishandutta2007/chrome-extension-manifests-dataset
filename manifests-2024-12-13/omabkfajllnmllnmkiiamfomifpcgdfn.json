{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Kwala",
  "version": "3.0.8",
  "description": "Ajoutez Kwala à votre navigateur Chrome",
  "action": {
    "default_title": "Kwala",
    "default_icon": {
      "32": "assets/icon.png"
    }
  },
  "icons": {
    "16": "assets/icon.png",
    "48": "assets/icon.png",
    "128": "assets/icon.png"
  },
  "host_permissions": [
    "https://www.linkedin.com/*",
    "https://app.kwala.fr/*"
  ],
  "permissions": [
    "tabs",
    "scripting",
    "storage",
    "cookies",
    "webRequest"
  ],
  "background": {
    "service_worker": "background-loader.js",
    "type": "module"
  },
  "externally_connectable": {
    "matches": [
      "*://*.kwala.fr/*",
      "*://localhost/*"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "scripts/content-script-loader.linkedin.js",
        "scripts/content-script-loader.iframe.js"
      ],
      "matches": [
        "https://www.linkedin.com/*"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "https://www.linkedin.com/*"
      ],
      "resources": [
        "scripts/linkedin.js"
      ],
      "use_dynamic_url": false
    },
    {
      "matches": [
        "https://*/*",
        "https://www.linkedin.com/*"
      ],
      "resources": [
        "index.html"
      ],
      "use_dynamic_url": false
    },
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "resources": [
        "scripts/iframe.js",
        "scripts/constants.js"
      ],
      "use_dynamic_url": false
    }
  ]
}