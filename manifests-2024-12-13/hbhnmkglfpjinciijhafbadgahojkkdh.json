{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "ResearchTools: Multiple Site Viewer",
  "version": "0.0.3",
  "permissions": ["contextMenus","tabs", "activeTab","storage", "alarms", "declarativeNetRequest"],
  "description": "Browse multiple websites in a single tab. Research, analyze and compare content while staying organized without switching tabs.",
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    {
      "resources": ["popup.js","script.js",  "update.html", "images/*"],
      "matches": ["<all_urls>"]
    }
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "all_frames": true
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
