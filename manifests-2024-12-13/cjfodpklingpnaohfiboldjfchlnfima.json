{
"update_url": "https://clients2.google.com/service/update2/crx",

  
  "manifest_version": 3,
  "name": "Minute – A timer to track your scrolling time",
  "version": "1.3",
  "description": "A small timer designed to keep you exploring without losing track of time.  ",
  "permissions": ["storage", "activeTab", "<all_urls>"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
