{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Neodeluxe Sidebar",
  "description": "A Neodeluxe Extension",
  "version": "1.3.3",
  "background": {
    "service_worker": "scripts/service-worker.js"
  },
  "host_permissions": [
    "*://*.fvmigration.com/*",
    "*://*.filevineapp.com/*",
    "*://app.filevine.io/*"
  ],
  "permissions": [
    "activeTab",
    "tabs",
    "sidePanel",
    "webRequest",
    "identity",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.fvmigration.com/*",
        "*://*.filevineapp.com/*",
        "*://app.filevine.io/*"
      ],
      "js": ["scripts/content.js"],
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "16": "./logo.ico",
    "48": "./logo.ico",
    "128": "./logo.ico"
  },
  "web_accessible_resources": [
    {
      "resources": ["callback.html"],
      "matches": ["<all_urls>"]
    }
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self' 'wasm-unsafe-eval' http://localhost:*; object-src 'self';"
  }
}
