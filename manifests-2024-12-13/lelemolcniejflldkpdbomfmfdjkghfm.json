{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "AGEphone Cloud",
    "version": "1.0.1",
    "description": "A SIP-based VoIP application that detects phone numbers on active web pages and allows outbound calls.",
    "permissions": [
        "activeTab",
        "scripting",
        "storage",
        "notifications"
    ],
    "host_permissions": [
        "https://*.amazonaws.com/*",
        "https://*.cognito-idp.*.amazonaws.com/*",
        "https://*.s3.*.amazonaws.com/*"
    ],
    "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'self'"
    },
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/icon16.png",
            "48": "icons/icon48.png",
            "128": "icons/icon128.png"
        }
    },
    "content_scripts": [
        {
            "matches": ["https://*/*", "https://*/"],
            "js": [
                "agephone.js"
            ],
            "css": [
                "agephone.css"
            ]
        }
    ],
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "web_accessible_resources": [
        {
            "resources": [
                "*.html",
                "*.css",
                "*.js",
                "prov.json",
                "locales/*.json",
                "agephoneimg/*"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ]
}