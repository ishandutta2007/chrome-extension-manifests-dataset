{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Blender Find Related Issues",
  "version": "0.3.3",
  "description": "On Blender's issue pages, this extension adds a button that lists the issues most similar to the one reported on the page",
  "permissions": [ "storage" ],
  "host_permissions": [
    "https://projects.blender.org/blender/blender/issues/*",
    "https://projects.blender.org/blender/blender-addons/issues/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://projects.blender.org/blender/blender/issues/*",
        "https://projects.blender.org/blender/blender-addons/issues/*"
      ],
      "exclude_matches": [
        "https://projects.blender.org/blender/blender/issues/new*",
        "https://projects.blender.org/blender/blender/issues",
        "https://projects.blender.org/blender/blender/issues?*",
        "https://projects.blender.org/blender/blender-addons/issues",
        "https://projects.blender.org/blender/blender-addons/issues?*"
      ],
      "js": [ "content_script.js" ]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Blender Find Related Issues"
  },
  "web_accessible_resources": [
    {
      "resources": [ "icon.svg" ],
      "matches": [ "<all_urls>" ]
    }
  ]
}
