{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "author": "From E",
  "name": "S.G.T.C.",
  "description": "Send \"Generated Text By UserScript\" to Clipboard.",
  "version": "0.14.3",
  "minimum_chrome_version": "114.0",
  "default_locale": "en",
  "icons": {
    "32": "src/img/icon_32.png",
    "48": "src/img/icon_48.png",
    "128": "src/img/icon_128.png"
  },
  "action": {
    "default_icon": {
      "32": "src/img/icon_32.png",
      "48": "src/img/icon_48.png",
      "128": "src/img/icon_128.png"
    },
    "default_title": "S.G.T.C.",
    "default_popup": "src/html/popup/popup.html"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "contextMenus",
    "notifications",
    "clipboardWrite",
    "offscreen"
  ],
  "content_security_policy": {
    "sandbox": "sandbox allow-scripts; script-src 'self' 'unsafe-eval';"
  },
  "sandbox": {
    "pages": [
      "src/html/sandbox/sandbox.html"
    ]
  },
  "web_accessible_resources": [
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "assets/*.js.map"
      ],
      "use_dynamic_url": false
    },
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "assets/function.js",
        "assets/PageInformation.js",
        "assets/content_scripts.js.js"
      ],
      "use_dynamic_url": false
    }
  ],
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "js": [
        "assets/content_scripts.js-loader.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "css": []
    }
  ],
  "options_page": "src/html/options/options.html"
}
