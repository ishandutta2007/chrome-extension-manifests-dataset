    }
  ],
  "background": {
    "scripts": ["background.bundle.js"],
    "persistent": false
  },
  "icons": {
    "16": "16.png",
    "32": "32.png",
    "48": "48.png",
    "128": "128.png"
  },
  "web_accessible_resources": ["*"],
  "content_security_policy": "script-src 'self'; object-src 'self'"
