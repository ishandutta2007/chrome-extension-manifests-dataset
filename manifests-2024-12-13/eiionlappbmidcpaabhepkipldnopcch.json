  "version": "1.0.3",
  "default_locale": "en",
  "manifest_version": 3,
  "icons": {
    "32": "assets/static/logo-turn-on-32.png",
    "64": "assets/static/logo-turn-on-64.png",
    "128": "assets/static/logo-turn-on-128.png"
