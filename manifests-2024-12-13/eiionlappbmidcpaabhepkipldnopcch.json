      ],
      "js": [
        "dark-mode-content.js"
      ],
      "run_at": "document_start"
    }
  ],
  "action": {
    "default_title": "__MSG_title__"
  },
  "background": {
    "service_worker": "service-worker.js"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "static/*",
        "assets/*.svg",
        "assets/*.png",
        "assets/*.css",
        "assets/*.otf",
        "*.css"
      ],
      "matches": ["<all_urls>"]
    }
  ]
