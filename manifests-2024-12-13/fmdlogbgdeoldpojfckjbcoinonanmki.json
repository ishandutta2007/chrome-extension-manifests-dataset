{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "SBT Bookmarks Pro",
  "version": "1.0",
  "description": "Transform your new tab into a powerful dashboard with visual bookmarks, themes, Google apps integration & smart search features",
  "author": "Jasmin Kodzopeljic",
  "homepage_url": "https://www.seobloggertips.com",
  "permissions": [
    "storage",
    "favicon",
    "contextMenus",
    "activeTab",
    "tabs"
  ],
  "host_permissions": [
    "https://*/*",
    "https://sbtbp.onrender.com/*"
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "action": {
    "default_popup": "settings.html"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "icons/*",
        "*.html",
        "*.js",
        "*.css"
      ],
      "matches": ["<all_urls>"]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "commands": {
    "add-to-shortcuts": {
      "suggested_key": {
        "default": "Ctrl+Shift+S",
        "mac": "Command+Shift+S"
      },
      "description": "Add current page to shortcuts"
    }
  }
}