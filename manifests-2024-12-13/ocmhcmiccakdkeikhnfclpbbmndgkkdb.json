{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "PH newtab",
  "version": "0.0.1",
  "description": "A 'new tab' extension, made by a product hunter for product hunters.",
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "scripting",
    "history"
  ],
  "chrome_url_overrides": {
    "newtab": "dist/index.html"
  },
  "host_permissions": [
    "https://www.producthunt.com/*"
  ],
  "background": {
    "service_worker": "src/utils/background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://www.producthunt.com/*"],
      "js": ["src/utils/content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  },
"icons": {
    "16": "favicon16.png",
    "32": "favicon32.png",
    "48": "favicon48.png",
    "128": "favicon128.png"
  }
}
