"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Base64 Decoder",
  "description": "This extension decodes Base64-encoded strings on right-click.",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "scripting",
    "contextMenus"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
