{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "key" : "",
  "name": "Raisly",
  "version": "1.0.7",
  "description": "Automates LinkedIn job applications by submitting to pre-set positions, saving you from manual searching and applying.",
  "content_security_policy": {
  "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "permissions": [
    "tabs",
    "cookies",
    "storage"
  ],  
  "host_permissions": [
    "https://raislyhq.com/*",
    "https://www.raislyhq.com/*"
  ],
  "background": {
    "service_worker": "js/background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.linkedin.com/*"
    ],
      "js": ["js/content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "assets/images/logo.png"
    }
  },
  "web_accessible_resources": [
      {
      "resources": ["images/*"],
      "matches": ["<all_urls>"]
      }
    ]
}
  

  