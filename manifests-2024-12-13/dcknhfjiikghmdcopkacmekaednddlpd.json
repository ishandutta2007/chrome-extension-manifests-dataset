privacy_policy: https://merchspark.net/privacy-policy
---

{
"update_url": "https://clients2.google.com/service/update2/crx",

 "manifest_version": 3,
 "name": "MerchSpark for Merch By Amazon",
 "version": "1.1.1",
 "description": "A Chrome extension which supports your tasks on Merch By Amazon (Amazon Merch on Demand)",
 "permissions": [
  "activeTab",
  "storage",
  "unlimitedStorage",
  "notifications",
  "alarms",
  "offscreen",
  "declarativeNetRequest",
  "declarativeNetRequestFeedback"
 ],
 "host_permissions": ["*://*.amazon.com/*", "https://*.merchspark.net/*"],
 "action": {
  "default_popup": "popup.html"
 },
 "icons": {
  "512": "assets/icon512.png"
 },
 "background": {
  "service_worker": "background.js"
 },
 "content_scripts": [
  {
   "all_frames": true,
   "matches": ["*://merch.amazon.com/*"],
   "js": ["navbar.js"],
   "run_at": "document_start"
  },
  {
   "css": ["app/index.css"],
   "js": ["app/index.js"],
   "matches": ["https://merch.amazon.com/resource/merchspark/*"],
   "run_at": "document_end"
  }
 ],
 "content_security_policy": {
  "extension_pages": "script-src 'self'; object-src 'self'"
 },

 "declarative_net_request": {
  "rule_resources": [
   {
    "enabled": true,
    "id": "ruleset",
    "path": "assets/rule.json"
   }
  ]
 },
 "key": "",
 "web_accessible_resources": [
  {
   "matches": ["https://merch.amazon.com/*"],
   "resources": ["app/*", "assets/*"]
  }
 ]
}
