{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Alert product domain",
  "description": "This extension helps you save URLs and alerts you whenever you visit saved URLs.",
  "version": "1.0",
  "permissions": ["storage", "tabs"],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "128": "./icon.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_icon": {
      "128": "./icon.png"
    },
    "default_popup": "popup.html"
  }
}