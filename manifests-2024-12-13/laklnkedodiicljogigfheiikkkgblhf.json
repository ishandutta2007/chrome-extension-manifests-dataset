    "name": "Alice",
    "description": "Hides the numbers of likes,shares,retweets,clap from medium/facebook/twitter",
    "version": "0.70"
    ,"content_scripts": [
      {
        "matches": ["https://*/*"]
        ,"exclude_matches":["https://twitter.com/*","https://www.facebook.com/*","https://www.youtube.com/*","https://www.quora.com/*"]
        ,"js": ["js/main.js"]
        ,"run_at":"document_start"
      }
    ]
    ,"icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon16.png",
     "128": "icons/icon16.png" 
    },"web_accessible_resources":["css/medium.css","css/twitter.css","css/facebook.css","css/youtube.css","css/linkedin.css"]
    ,"permissions": [
      "tabs","https://www.facebook.com/*","https://twitter.com/*" ,"https://www.quora.com/*","https://www.youtube.com/*","storage" , "<all_urls>"
    ]
    ,"browser_action": {
      "default_title": "Options",    
      "default_popup": "popup.html"        
    }
    ,"background": {
      "scripts": ["js/background.js"]
    }
