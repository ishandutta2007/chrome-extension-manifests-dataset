{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Amazon URL Extractor for importwoocommerce.com",
  "version": "1.6",
  "description": "A Chrome extension to extract product URLs from Amazon.com, amazon.in, amazon to woocommerce",
  "author": "Suhail Ahmad <suhail_ahmad64@yahoo.com>",
  "permissions": [
    "tabs",
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "https://www.amazon.com/*",
    "https://www.amazon.in/*",
    "https://www.amazon.co.uk/*",
    "https://www.amazon.it/*",
    "https://www.amazon.ca/*",
    "https://www.amazon.com.au/*",
    "https://www.amazon.co.jp/*",
    "https://www.amazon.de/*",
    "https://www.amazon.fr/*",
    "https://www.amazon.es/*",
    "https://www.amazon.nl/*",
    "https://www.amazon.com.br/*",
    "https://www.amazon.com.mx/*",
    "https://www.amazon.sg/*",
    "https://www.amazon.ae/*",
    "https://www.amazon.com.tr/*",
    "https://www.amazon.sa/*",
    "https://www.amazon.se/*",
    "https://www.amazon.pl/*",
    "https://importwoocommerce.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://importwoocommerce.com/*"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self';"
  }
}
