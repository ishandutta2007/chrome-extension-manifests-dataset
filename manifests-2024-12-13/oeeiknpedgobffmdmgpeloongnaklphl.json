"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Mmrz search helper",
  "version": "1.0.1",
  "manifest_version": 2,

  "description": "Search with https://mmrz.zhanglintc.co",
  "icons": {
    "16": "image/icon16.png",
    "48": "image/icon48.png"
  },

  "browser_action": {
    "default_title": "Mmrz",
    "default_icon": "image/icon16.png",
    "default_popup": "popup.html"
  },

  "background": {
    "scripts": ["menu.js"]
  }, 

  "content_scripts": [
    {
      "js": ["common.js"],

      "matches": ["<all_urls>"],

      "run_at": "document_start",

      "match_about_blank":true,
      "all_frames": true
    }
  ],

  "permissions": [
    "contextMenus",
    "https://mmrz.zhanglintc.co/*"
  ]
}