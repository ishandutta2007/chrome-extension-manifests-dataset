        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": [
                "http://*.douban.com/*",
                "https://*.douban.com/*"
            ],
            "js": ["assistant.js", "shortcuts.js"],
            "css": ["assistant.css", "shortcuts.css"],
