{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "EditThisCookies",
  "version": "1.4.1",
  "description": "EditThisCookie is a cookie manager. You can add, delete, edit, search, protect and block cookies!",
  "action": {
    "default_popup": "src/views/popup/index.html"
  },
  "incognito": "not_allowed",
  "host_permissions": [
    "*://*/*"
  ],
  "permissions": [
    "tabs",
    "storage",
    "cookies"
  ],
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "js": [
        "static/js/main.ts-loader-43tnnnaJ.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "static/*",
        "main-icon.png",
        "static/js/main.ts-G8JXnjdt.js"
      ],
      "use_dynamic_url": false
    }
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self'"
  }
}
