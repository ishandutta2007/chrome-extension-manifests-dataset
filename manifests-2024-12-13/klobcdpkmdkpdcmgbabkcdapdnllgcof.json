{
"update_url": "https://clients2.google.com/service/update2/crx",

    "name": "NoCodeFusion",
    "description": "The AI Bubble.io extension",
    "version": "0.0.7",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["webRequest","webNavigation","activeTab", "scripting", "commands"],
    "action": {
        "default_icon": {
            "16": "/icons/icon16.png",
            "48": "/icons/icon48.png",
            "128": "/icons/icon128.png"
        }
    },
    "icons": {
        "16": "/icons/icon16.png",
        "48": "/icons/icon48.png",
        "128": "/icons/icon128.png"
    },
    "host_permissions": [
        "https://*.bubble.io/*","https://*.bubble.is/*"
      ],
    "web_accessible_resources": [
        {
            "resources": [ "images/*" ],
            "matches": [ "<all_urls>" ]
        }
    ],
    "externally_connectable": {
        "matches": ["https://nocodefusion.com/*","https://bubble.io/*","https://*.bubble.is/*","https://nocodefusion.bubbleapps.io/*"]
    },
    "content_scripts":[
        {
            "matches":["https://bubble.io/*","https://*.bubble.is/*"],
            "js": [ "content_scripts.js" ],
            "css":["content_scripts.css"],
            "run_at": "document_start"
        }
    ],
   "commands": {
  "paste_with_workflows": {
    "suggested_key": {
      "default": "Ctrl+Shift+Y", 
      "mac": "Command+Shift+Y"
    },
    "description": "Triggers the paste with workflows command"
  },
  "open_popup": {
    "suggested_key": {
      "default": "Ctrl+Shift+Space",  
      "mac": "Command+Shift+Space"  
    },
    "description": "Opens the NoCodeFusion popup"
  }
}
}
