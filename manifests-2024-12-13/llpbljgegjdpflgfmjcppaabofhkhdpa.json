    "manifest_version": 3,
    "name": "My GPT bookmarker",
    "description": "A React-based bookmark sidebar for Chrome.",
    "version": "1.0",
    "permissions": [
        "sidePanel",
        "tabs",
        "activeTab",
        "bookmarks"
    ],
    "action": {
        "default_popup": "",
        "default_icon": "gpt_bookmarker.png",
        "default_title": "Click to open/close bookmark"
    },
    "side_panel": {
        "default_path": "sidepanel.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "host_permissions": [
        "https://chatgpt.com/*"
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "popup.html",
                "sidepanel.html",
                "assets/*.js",
                "assets/*.css"
            ],
            "matches": [
                "https://chatgpt.com/*"
            ]
        }
    ],
    "homepage_url": "https://hayoung0lee.github.io/gpt-bookmarker-privacy-policy"
