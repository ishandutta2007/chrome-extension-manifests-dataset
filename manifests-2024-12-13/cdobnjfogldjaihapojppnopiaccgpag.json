{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Steamworks extra reports",
  "version": "1.9.1",
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "https://store.steampowered.com/*"
