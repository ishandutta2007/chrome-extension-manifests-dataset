"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "ClickCV - Resumes Tailored for Every Job Listing",
  "description": "Instantly generate fine tuned resumes every time you click apply on your favorite job boards.",
  "version": "1.0.4",
  "permissions": [
    "storage",
    "identity",
    "identity.email"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["*://*.linkedin.com/*", "*://*.indeed.com/*", "*://*.careerbuilder.com/*", "*://*.ziprecruiter.com/*"],
      "js": [
        "libs/resume-editor-bundle.umd.js",
        "content.js"
      ]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "192": "icons/icon192.png",
      "512": "icons/icon512.png"
    }
  },
  "web_accessible_resources": [
    {
      "resources": ["success.html", "cancel.html", "loading.gif", "edit_resume.html"],
      "matches": ["<all_urls>"]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "192": "icons/icon192.png",
    "512": "icons/icon512.png"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
