{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "Auto KIT Login",
    "description": "Automatic login redirection on supported KIT pages.",
    "version": "1.2.2",
    "host_permissions": [
        "*://*.kit.edu/"
    ],
    "action": {
        "default_icon": "kit_cookie.png",
        "default_popup": "popup/popup.html"
    },
    "permissions": [
        "webNavigation",
        "webRequest",
        "cookies",
        "storage",
        "scripting"
    ],
    "background": {
        "service_worker": "background/background.js"
    },
    "content_scripts": [
        {
            "matches": ["*://idp.scc.kit.edu/*"],
            "js": ["content/auto_submit.js"]
        }
    ],
    "icons": {
        "822": "kit_cookie.png"
    }
}