{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "anchors: LinkedIn Profile Pro (for creators)",
    "version": "1.1.7",
    "description": "Your personal companion that offers smart analysis of your LinkedIn profiles & creates custom media kits",
    "action": {
        "default_popup": "popup.html",
        "default_title": "anchors: LinkedIn Profile Pro (for creators)"
    },
    "permissions": [
        "storage",
        "activeTab"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://www.linkedin.com/*"
            ],
            "js": [
                "content-script.js"
            ]
        },
        {
            "matches": [
                "https://collab.anchors.in/*"
            ],
            "js": [
                "auth-script.js"
            ]
        }
    ],
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "host_permissions": [ "https://www.linkedin.com/*", "https://collab.anchors.in/*" ],
    "externally_connectable": {
        "matches": [
            "https://collab.anchors.in/*"
        ]
    }
}