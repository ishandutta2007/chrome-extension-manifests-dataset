{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "PanzaExtension",
  "description": "A Chrome extension to connect a locally running Panza server to Gmail in the browser.",
  "version": "0.1",
  "action": {
    "default_icon": "panza128.png"
  },
  "icons": {
    "16": "panza16.png",
    "32": "panza32.png",
    "48": "panza48.png",
    "128": "panza128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://mail.google.com/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "scripting"
  ],
  "host_permissions": [
    "https://mail.google.com/"
  ],
  "manifest_version": 3,
  "web_accessible_resources": [
    {
      "resources": [
        "*.map"
      ],
      "matches": [
        "https://mail.google.com/*"
      ]
    }
  ]
}