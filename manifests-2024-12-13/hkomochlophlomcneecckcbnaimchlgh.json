{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "CMI Service Extension",
    "description": "This extension provides some enhancement to the CRM System of Carrier Midea India Private Limited website.",
    "version": "1.4.0",
    "options_page": "index.html",
    "icons": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    },
    "action": {
        "default_title": "CMI Service Extension"
    },
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": ["*://cmiservice.co.in/printServiceRequestDetails.do*"],
            "js": ["js/jobsheet.js"],
            "all_frames": true,
            "run_at": "document_end"
        },
        {
            "matches": ["*://cmiservice.co.in/printAdviceDetails.do*"],
            "js": ["js/payoutAdvice.js"],
            "all_frames": true,
            "run_at": "document_end"
        },
        {
            "matches": ["*://cmiservice.co.in/editCustDetails.do*"],
            "js": ["js/editCustDetails.js"],
            "all_frames": true,
            "run_at": "document_end"
        },
        {
            "matches": ["*://cmiservice.co.in/listPMScheduling.do*"],
            "js": ["js/pmSchedule.js"],
            "all_frames": true,
            "run_at": "document_end"
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "jobsheet.html",
                "images/*",
                "css/*",
                "js/*",
                "payoutAdvice.html",
                "index.html"
            ],
            "matches": ["*://cmiservice.co.in/*"]
        }
    ],
    "permissions": ["storage"]
}
