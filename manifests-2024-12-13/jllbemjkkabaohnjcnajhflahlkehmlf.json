    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": [
                "https://www.odoo.sh/project"
            ],
            "js": [
                "odooshScript.js"
            ]
        },
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "contentScript.js"
            ]
        }
    ],
    "offline_enabled": true,
    "options_page": "options.html",
