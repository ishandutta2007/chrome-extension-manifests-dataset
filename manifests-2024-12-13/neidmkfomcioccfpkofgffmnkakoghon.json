  "name": "School Passport SSO",
  "description": "This extension gives you seamless and secure access to all your web-based learning resources within the School Passport Platform",
  "author": "GG4L",
  "version": "1.0.9",
  "manifest_version": 3,
  "background": {
    "service_worker": "service_worker.js",
    "type": "module"
