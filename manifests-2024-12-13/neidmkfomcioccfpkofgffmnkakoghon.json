  },
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [ "inject_script.js" ],
      "matches": [ "https://*/*" ],
      "use_dynamic_url": true
    }
  ],
  "permissions": [
    "webRequest",
    "webRequestAuthProvider"
  ],
  "host_permissions": [
    "https://*/*",
    "http://*/*"
  ]
