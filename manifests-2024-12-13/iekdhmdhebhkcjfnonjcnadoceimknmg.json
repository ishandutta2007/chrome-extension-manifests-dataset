---

{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "Docmate Extension",
    "version": "0.0.2",
    "description": "Our Google Chrome extension for Docmate. Helps you easily Contribute and chat with your MedMate",
    "icons": {
        "48": "images/logo.png"
    },

    "permissions": ["activeTab", "sidePanel", "storage", "tabs", "scripting"],
    "background": {
        "service_worker": "scripts/background.js"
    },
    "action": {
        "default_popup": "pages/index.html#/popup",
        "default_icon": {
            "48": "images/logo.png"
        }
    },
    "side_panel": {
        "default_path": "pages/index.html"
    },
    "web_accessible_resources": [
        {
            "resources": ["dist/*"],
            "matches": ["*://*/*"]
        },
        {
            "resources": ["images/*"],
            "matches": ["*://*/*"]
        },
        {
            "resources": ["pages/*"],
            "matches": ["*://*/*"]
        }
    ],
    "content_scripts": [
        {
            "matches": ["*://*/*"],
            "exclude_matches": [
                "*://localhost/*",
                "https://docmate.ca/*",
                "https://staging.docmate.ca/*"
            ],

            "js": ["dist/extension-bot.js"]
        }
    ],
    "host_permissions": ["*://*/*"]
}
