  "name": "Blank-Web-Extension",
  "version": "1.1.8",
  "description": "A new reading experience",
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "scripting",
    "runtime",
    "https://app.joinblank.com/"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
