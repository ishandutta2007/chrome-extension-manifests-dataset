        "https://secure.fnac.com/orderpipe/pop/livraison/fnac-0",
        "https://shoppingcart.aliexpress.com/orders.htm",
        "https://www.leroymerlin.fr/v3/vad/v5/commande.do",
        "https://www.vinted.fr/checkout/*",
        "https://www.boulanger.com/webapp/wcs/stores/servlet/deliverySelection",
        "https://www.darty.com/tunnel_achat/opp/*",
        "https://www.zalando.fr/checkout/address",
        "https://secure.asos.com/fr/*",
        "https://www.zara.com/fr/en/shop/*",
        "https://www.etam.com/checkout",
        "https://www.ikea.com/fr/fr/order/delivery/*",
