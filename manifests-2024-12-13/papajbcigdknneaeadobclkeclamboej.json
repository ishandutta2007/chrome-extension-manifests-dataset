    }
  ],
  "action": {
    "default_title": "Boks",
    "default_popup": "src/browser_action/authentication_popup.html"
  },
  "background": {
    "service_worker": "src/background/background.js"
  },
  "permissions": [
    "activeTab",
    "scripting"
