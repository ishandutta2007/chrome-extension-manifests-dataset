{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name":           "(ORA@UMD) KR Extension for Power Users",
  "version":        "1.01",
  "description":    "A Chrome browser extension created by the Office of Research Administration at the University of Maryland.",
  "content_scripts": [
    {
      "matches": [
        "https://*.kuali.co/res/*",
        "https://*.kuali.co/dashboard/*"
      ],
      "all_frames": true,
      "js": ["detectActiveKRModuleTabContentScript.js"]
    }
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "configSettings.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "ora_icon_off_dark_128.png"
  },
  "permissions":    [
    "https://*.kuali.co/res/*",
    "https://*.kuali.co/dashboard/*",
    "activeTab"
  ],
  "icons": {
    "128": "ora_icon_128.png"
  },
  "manifest_version": 2,
  "content_security_policy": "default-src 'none'; style-src 'self'; script-src 'self'; connect-src 'none'"
}
