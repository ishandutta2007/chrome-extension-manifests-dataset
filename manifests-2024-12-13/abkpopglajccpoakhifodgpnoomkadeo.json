  "manifest_version": 3,
  "name": "YouTube Cleanup",
  "version": "1.1",
  "description": "Clean Youtube view and facilitate to focus on the video.",
  "permissions": ["storage"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
