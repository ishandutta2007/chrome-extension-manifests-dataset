    "description": "A Powerful Extraction Tool for extract emails and contact numbers from the Facebook.",
    "manifest_version": 3,
    "background": {
      "service_worker": "background.js"
    },
    "action": {
        "default_icon": {                    
            "16": "/assets/images/logo.png",           
            "24": "/assets/images/logo.png",           
