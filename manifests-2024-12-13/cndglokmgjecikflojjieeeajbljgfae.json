"update_url": "https://clients2.google.com/service/update2/crx",

  "version": "1.6.0",
  "name": "Axhub",
  "manifest_version": 3,
  "description": "Axure 辅助扩展，可用于原型发布，原型复制，图标复制等",
  "icons": {
    "16": "img/icon-16.png",
    "48": "img/icon-48.png",
    "128": "img/icon-128.png"
