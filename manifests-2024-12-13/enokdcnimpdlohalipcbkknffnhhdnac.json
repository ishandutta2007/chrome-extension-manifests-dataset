{
"update_url": "https://clients2.google.com/service/update2/crx",

  "description": "__MSG_extDesc__",
  "version": "1.0.1",
  "manifest_version": 3,
  "name": "__MSG_extName__",
  "default_locale": "en",
  "background": {
    "service_worker": "scripts/bg.js"
  },
  "icons": {
    "16": "static/icons/icon-16.png",
    "38": "static/icons/icon-38.png",
    "64": "static/icons/icon-64.png",
    "128": "static/icons/icon-128.png"
  },
  "action": {
    "default_popup": "components/index.html",
    "default_icon": "static/icons/icon-38.png"
  },
  "content_scripts": [
    {
      "js": [
        "scripts/content.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "permissions": [
    "tabs",
    "tabCapture",
    "storage",
    "unlimitedStorage"
  ],
  "host_permissions": [
    "*://*/*"
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self';"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "static/*",
        "static/icons/icon-128.png",
        "static/icons/icon-38.png",
        "scripts/libs/*.jpeg",
        "scripts/libs/*.wasm"
      ],
      "matches": [ "*://*/*"]
    }
  ]
