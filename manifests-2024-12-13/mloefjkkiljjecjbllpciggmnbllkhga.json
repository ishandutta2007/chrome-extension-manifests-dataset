{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Sudo Focus: Swap distracting sites with favorite quotes",
  "version": "1.0.2",
  "description": "Swap distracting sites with favorite quotes",
  "action": {
    "default_icon": {
      "19": "./assets/icon-19.png",
      "38": "./assets/icon-38.png"
    },
    "default_popup": "./src/scripts/popup/popup.html"
  },
  "options_ui": {
    "page": "./src/scripts/options/options.html",
    "open_in_tab": true
  },
  "background": {
    "service_worker": "js/service-worker.js",
    "type": "module"
  },
  "icons": {
    "16": "./assets/icon-16.png",
    "32": "./assets/icon-32.png",
    "48": "./assets/icon-48.png",
    "128": "./assets/icon-128.png"
  },
  "permissions": [
    "storage",
    "declarativeNetRequest",
    "declarativeNetRequestWithHostAccess",
    "tabs",
    "identity",
    "identity.email"
  ],
  "host_permissions": [
    "<all_urls>",
    "http://*/*",
    "https://*/*"
  ],
  "commands": {
    "refresh_extension": {
      "suggested_key": {
        "default": "Ctrl+Space"
      },
      "description": "Refresh Extension"
    }
  },
  "web_accessible_resources": [
    {
      "matches": [
        "https://*/*",
        "http://*/*"
      ],
      "resources": [
        "dist/js/blocked.js",
        "dist/js/index.js",
        "dist/js/options.js",
        "dist/js/popup.js",
        "dist/js/service-worker.js",
        "/src/scripts/blocked/blocked.html"
      ]
    }
  ]
}