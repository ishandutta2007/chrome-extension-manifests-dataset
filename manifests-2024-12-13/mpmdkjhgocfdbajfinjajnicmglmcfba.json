{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "ShopeeCookies",
    "description": "Mengambil cookies untuk Tool Auto Promo Shopee",
    "version": "1.3.5",
    "permissions": [
        "cookies",
        "activeTab"
    ],
    "host_permissions": [
        "https://seller.shopee.co.id/*",
        "<all_urls>"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "logo16.png",
            "48": "logo48.png",
            "128": "logo128.png"
        }
    },
    "icons": {
        "16": "logo16.png",
        "48": "logo48.png",
        "128": "logo128.png"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "all_frames": true,
            "js": ["content.js"]
        }
    ]
}

