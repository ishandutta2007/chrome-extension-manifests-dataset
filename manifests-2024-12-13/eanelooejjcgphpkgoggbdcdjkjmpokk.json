{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Lang translator for WhatsApp, Telegram, Discord",
  "version": "1.0.2",
  "description": "Translate WhatsApp, Telegram, and Discord messages with our extension. Just select text for instant translations in any language!",
  "default_locale": "en",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "default_title": "Lang translator for WhatsApp, Telegram, Discord"
  },
  "host_permissions": ["https://translate.google.so/*"],
  "permissions": ["notifications", "storage", "tabs"],
  "content_scripts": [
    {
      "matches": ["https://web.whatsapp.com/*"],
      "js": ["whatsapp_content.js"]
    },
    {
      "matches": ["https://web.telegram.org/k/*"],
      "js": ["k_telegram_content.js"]
    },
    {
      "matches": ["https://web.telegram.org/a/*"],
      "js": ["a_telegram_content.js"]
    },
    {
      "matches": ["https://discord.com/channels/*"],
      "js": ["discord_content.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  }
}
