      "css": [
        "./dist/contentScripts/style.css"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "dist/contentScripts/style.css",
        "assets/*"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
