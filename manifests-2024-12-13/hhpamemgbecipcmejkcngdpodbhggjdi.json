{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "bookmark",
  "description": "bookmark管理器",
  "version": "1.0.0",
  "permissions": [
    "tabs",
    "bookmarks",
    "storage",
    "favicon",
    "identity"
  ],

  "chrome_url_overrides": {

  },
  "options_ui": {
    "page": "popup.html" 
  },
  "omnibox": { "keyword" : "bookmark" },

  "background": {

    "service_worker": "js/background.js"
  },
  "action": {},
  "icons": {
    "16": "assets/icon.png",
    "48": "assets/icon.png",
    "128": "assets/icon.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": [],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [{
    "resources": ["js/inject.js", "chrome://favicon/", "_favicon/*", "content.html"],
    "matches": ["<all_urls>"],
    "extension_ids": ["*"]
  }],
  "commands": {
    "reload_extension": {
      "suggested_key": {
        "default": "Ctrl+Shift+K",
        "mac": "Command+Shift+K"
      },
      "description": "重新加载插件"
    }
  }
}