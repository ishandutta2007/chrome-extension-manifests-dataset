    "manifest_version": 2,
    "name": "Instapaper Notes Filter",
    "short_name": "IPaper_Filter",
    "content_scripts": [{
        "matches": ["https://www.instapaper.com/notes/*"],
        "css": ["css/bootstrap.min.css",
            "css/bootstrap-select.min.css",
            "css/main.css"
        ],
        "js": ["js/jquery.min.js",
            "js/jquery-ui.min.js",
            "js/bootstrap.min.js",
            "js/bootstrap-select.min.js",
            "/js/ip_inherit.min.js",
            "/js/mousetrap.min.js",
            "main.min.js"
        ]
    }],
    "description": "Filters Notes by article on Instapaper's Notes Page",
    "version": "0.0.0.2",
    "icons": {
        "256": "icon-256-filter-blue.png"
    },
    "web_accessible_resources": ["img/*", "templates/*", "fonts/*"],
    "browser_action": {
        "default_icon": "icon-filter-blue.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab"
    ]
