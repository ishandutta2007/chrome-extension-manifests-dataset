{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Teapot AI",
  "description": "Teapot AI Extenstion to read and interpret your current tab documents",
  "version": "0.2.0",
  "permissions": [
    "activeTab",
    "scripting"
  ],

  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "main.js"
      ]
    }
  ],
  "minimum_chrome_version": "92",
  "action": {
    "default_title": "Teapot AI",
    "default_popup": "chat.html"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self' 'wasm-unsafe-eval'"
  },
  "icons": {
    "16": "./assets/logo.png",
    "48": "./assets/logo.png" 
  }

}