{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Skimming AI",
  "version": "1.3",
  "description": "Skimming.ai Chrome extension extracts knowledge from audio, video, and text on any website.",
  "permissions": [
    "clipboardWrite",
    "activeTab",
    "storage",
    "identity",
    "scripting",
    "<all_urls>"
  ],
  "icons": {
    "16": "/public/16.png",
    "32": "/public/32.png",
    "48": "/public/48.png",
    "128": "/public/128.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>", "https://www.app.skimming.ai/*"],
      "js": ["toastify.js", "content.js"],
      "css": ["toastify.css"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["scripts/bundle.js", "public/icon.png"],
      "matches": ["<all_urls>"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "public/index.html"
  }
}
