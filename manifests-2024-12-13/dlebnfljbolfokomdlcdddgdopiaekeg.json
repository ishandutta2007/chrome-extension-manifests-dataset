{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "SalesforceToNotion",
  "description": "Import custom data queries or reports from Salesforce into Notion.",
  "version": "1.0.2",
  "action": {
    "default_title": "SalesforceToNotion",
    "default_popup": "icon-popup.html"
  },
  "icons": {
    "16": "src/assets/sn_logo_white_16.png",
    "32": "src/assets/sn_logo_white_32.png",
    "48": "src/assets/sn_logo_white_48.png",
    "128": "src/assets/sn_logo_white_128.png"
  },
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "js": [
        "assets/main.tsx-loader.30db2a39.js"
      ],
      "matches": [
        "https://www.notion.so/*"
      ],
      "css": [
        "assets/main.bf0cbba6.css"
      ]
    }
  ],
  "permissions": [
    "identity",
    "storage"
  ],
  "host_permissions": [
    "https://login.salesforce.com/*",
    "https://test.salesforce.com/*",
    "https://*/my.salesforce.com/*"
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "https://www.notion.so/*"
      ],
      "resources": [
        "assets/api.a2e8f775.js",
        "assets/main.tsx.12750979.js"
      ],
      "use_dynamic_url": true
    }
  ]
}