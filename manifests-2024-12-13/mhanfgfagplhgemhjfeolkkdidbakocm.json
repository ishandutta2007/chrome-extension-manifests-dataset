{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "LeetHub v2",
  "description": "Automatically integrate your Leetcode & GeeksforGeeks submissions to GitHub",
  "homepage_url": "https://github.com/arunbhardwaj/LeetHub-2.0",
  "version": "2.0.8",
  "author": "Arun Bhardwaj",
  "action": {
    "default_icon": "assets/thumbnail.png",
    "default_popup": "popup.html",
    "default_title": "Upload your LeetCode solutions to GitHub"
  },
  "icons": {
    "16": "assets/thumbnail.png",
    "48": "assets/thumbnail.png",
    "128": "assets/thumbnail.png"
  },
  "background": {
    "service_worker": "scripts/background.js"
  },
  "permissions": [
    "unlimitedStorage",
    "storage",
    "webNavigation"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://leetcode.com/*"
      ],
      "js": [
        "scripts/leetcode.js"
      ],
      "run_at": "document_idle"
    },
    {
      "matches": [
        "https://github.com/*"
      ],
      "js": [
        "scripts/authorize.js"
      ]
    },
    {
      "matches": [
        "https://practice.geeksforgeeks.org/*"
      ],
      "js": [
        "scripts/gfg.js"
      ]
    }
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}