{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "Sintelix Extension",
    "description": "Augment Sintelix with Extra Functionality",
    "version": "2.2.2",
    "permissions": [
        "activeTab",
        "contextMenus",
        "storage",
        "cookies"
    ],
    "host_permissions": [
        "<all_urls>",
        "http://*/*",
        "https://*/*"
    ],
    "icons": {
        "16": "images/icon_16.png",
        "19": "images/icon_19.png",
        "48": "images/icon_48.png",
        "128": "images/icon_128.png"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": "images/icon_16.png"
    },
    "commands": {
        "current-page-harvesting": {
            "suggested_key": {
                "default": "Alt+C"
            },
            "description": "Current page harvesting"
        },
        "store-url": {
            "suggested_key": {
                "default": "Alt+Z"
            },
            "description": "Store current page URL"
        }
    },
    "web_accessible_resources": [
        {
            "resources": [
                "edit_ruleset.html",
                "current_page_harvesting.html",
                "images/*"
            ],
            "matches": [
                "http://*/*",
                "https://*/*"
            ]
        }
    ],
    "background": {
        "service_worker": "script/service-worker-loader.js"
    },
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": [
                "script/lib/jquery-3.5.1.min.js",
                "script/lib/async-2.1.4.min.js",
                "script/common.js",
                "script/constants.js",
                "script/ruleset-marker-v2.js",
                "script/content-script.js"
            ],
            "css": [
                "css/content.css"
            ]
        }
    ]
}