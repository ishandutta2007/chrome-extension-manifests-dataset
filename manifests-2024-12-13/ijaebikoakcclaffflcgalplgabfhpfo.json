{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "ScrollGPT - pro GPT browsing",
    "version": "0.0.3",
    "description": "Easily navigate your GPT chats without endless scrolling.",
    "permissions": [
      "activeTab",
      "storage"
    ],
    
    "host_permissions": [
      "https://chatgpt.com/c/*",
      "https://scrollgpt.com/*"
    ],

    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },

    "background": {
      "service_worker": "background.js"
    },

    "content_scripts": [
      {
        "matches": ["*://chatgpt.com/*"],
        "js": ["utils.js", "render_nav.js", "content.js"]
      }
    ],

    "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'self'; style-src 'self' https://fonts.googleapis.com https://cdnjs.cloudflare.com; font-src 'self' https://fonts.gstatic.com https://cdnjs.cloudflare.com https://fonts.googleapis.com; connect-src 'self' https://scrollgpt.com;"
    },

    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    }
  }
  