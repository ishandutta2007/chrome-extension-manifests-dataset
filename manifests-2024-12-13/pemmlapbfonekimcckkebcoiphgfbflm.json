{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "GSTOne",
  "version": "0.0.2",
  "description": "This extension enables CA & Tax practitioners to login in their client's GST portals with one click.",
  "permissions": [
    "tabs",
    "storage",
    "webNavigation"
  ],
  "background": {
    "service_worker": "js/background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://services.gst.gov.in/services/login",
        "https://services.gst.gov.in/services/login?*"
      ],
      "css": [
        "css/content.css",
        "css/modal.css",
        "css/toastr.css",
        "css/form.css"
      ],
      "js": [
        "js/jquery.js",
        "js/toastr.min.js",
        "js/modal.js",
        "js/content.login.js"
      ]
    },
    {
      "matches": [
        "https://services.gst.gov.in/services/auth/fowelcome",
        "https://services.gst.gov.in/services/auth/fowelcome?*",
        "https://services.gst.gov.in/services/auth/dashboard",
        "https://services.gst.gov.in/services/auth/dashboard?*",
        "https://services.gst.gov.in/services/auth/changepassword",
        "https://services.gst.gov.in/services/auth/changepassword?*"
        
      ],
      "css": [
        "css/modal.css",
        "css/toastr.css",
        "css/form.css"
      ],
      "js": [
        "js/jquery.js",
        "js/toastr.min.js",
        "js/modal.js",
        "js/content.loginsuccess.js",
        "js/content.changePassword.js"
      ]
    }
  ],
  "icons": {
    "16": "images/16.png",
    "32": "images/32.png",
    "48": "images/48.png",
    "128": "images/128.png"
  }
}