{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "BLUF: Summarize PDF, YouTube & Websites",
  "description": "AI powered reading partner. Summarize & chat with any website, PDF document or YouTube video.",
  "version": "1.0.5",
  "manifest_version": 3,
  "permissions": [
    "contextMenus",
    "webRequest"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "icons": {
    "16": "assets/logo/background-square-16.png",
    "32": "assets/logo/background-square-32.png",
    "128": "assets/logo/background-square@2x.png"
  },
  "action": {
    "default_icon": {
      "16": "assets/logo/background-square-16.png",
      "32": "assets/logo/background-square-32.png"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    },
    {
      "matches": [
        "*://docs.google.com/document/*"
      ],
      "js": [
        "gdocs.js"
      ],
      "run_at": "document_start",
      "world": "MAIN"
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources": [
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "*.css",
        "*.png",
        "*.ttf",
        "*.eot",
        "*.woff",
        "*.woff2"
      ]
    }
  ],
  "externally_connectable": {
    "matches": ["https://*.bluf.ai/*"]
  },
  "commands": {
    "activate": {
      "suggested_key": {
        "default": "Shift+Alt+B"
      },
      "description": "Open/Close BLUF"
    }
  }
}