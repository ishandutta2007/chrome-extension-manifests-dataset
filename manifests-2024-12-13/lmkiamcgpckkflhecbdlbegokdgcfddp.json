  "name": "Workcal",
  "description": "Create Mac Chrome shortcut for secondary work Google calendar",
  "version": "1.3",
  "manifest_version": 2,
  "homepage_url": "http://justinshenk.com",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
