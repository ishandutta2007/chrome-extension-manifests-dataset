    "default_title": "Workcal",
    "default_icon": "images/icon128.png",
    "default_popup": "popup.html"
  },
  "web_accessible_resources": ["images/*.png"],
  "permissions": [
    "http://calendar.google.com/*",
    "https://calendar.google.com/*",
    "tabs"
  ],
  "content_scripts": [
    {
        "matches": [
          "*://calendar.google.com/*"
        ],
        "js": ["content-scripts/content.js"]
    }
  ]
