{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Documentos EBSERH",
  "description": "Extensão que facilita o preenchimento automático de documentos para usuários do sistema EBSERH",
  "version": "1.0",
  "permissions": [
      "downloads",
      "activeTab",
      "scripting"
  ],
  
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },

  "background": {
      "service_worker": "background.js"
  },
  
  "action": {
      "default_popup": "popup.html", 
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
  },
  
  "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self'"
  },
  
  "web_accessible_resources":
  [
   {
      "matches"  : ["<all_urls>"],
      "resources": ["pdf/*.pdf"]
   }
  ]
}
