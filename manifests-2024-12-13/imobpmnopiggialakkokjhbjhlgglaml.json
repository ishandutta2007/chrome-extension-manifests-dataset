      {
        "id": "ruleset_1",
        "enabled": true,
        "path": "rules.json"
      }
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "./static/js/content.js"
      ],
      "css": [
        "./static/css/content.6a1c1aab.css"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "./static/js/*",
