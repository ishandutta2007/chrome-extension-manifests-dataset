    "manifest_version": 3,
    "name": "Robinhood Portfolio Exporter",
    "version": "1.1.0",
    "description": "Export your Robinhood Stock and Crypto portfolio to CSV file.",
    "author": "BraveBinary LLC",
    "permissions": ["activeTab", "scripting", "downloads", "tabs"],
    "host_permissions": ["https://*.robinhood.com/*"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },

    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["https://*.robinhood.com/*"],
        "js": ["contentScript.js"],
        "run_at": "document_idle"
      }
    ]
  }
  