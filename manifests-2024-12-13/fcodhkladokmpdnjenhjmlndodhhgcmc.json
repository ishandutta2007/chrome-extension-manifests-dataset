{
"update_url": "https://clients2.google.com/service/update2/crx",

   "manifest_version": 2,
   "minimum_chrome_version": "52",
   "name": "Ciketi Screen Sharing Chrome Extension",
   "permissions": [ "desktopCapture" ],
   "short_name": "Ciketi ScreenShare",
   "version": "1.0.0.0",

   "background": {
      "persistent": true,
      "scripts": [ "script.js" ]
   },
   "description": "This Extension provides ability to share screen for Ciketi and hCiketi applications.",
   "externally_connectable": {
      "matches": [ "https://*.hciketi.com/*","https://*.proximie.com/*","https://*.ciketi.com/*"]
   },
   "icons": {
      "128": "hciketi_icon_128.png",
      "16": "hciketi_icon_16.png",
      "48": "hciketi_icon_48.png"
   },
   "browser_action":{
     "default_icon": "hciketi_icon_16.png"
   }
}

