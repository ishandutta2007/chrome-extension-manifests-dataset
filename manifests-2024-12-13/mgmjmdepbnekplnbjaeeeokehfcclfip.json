{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Screen recorder",
  "description": "Screen recorder extension",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "storage",
    "notifications"
  ],
  "background": {
    "service_worker": "background/index.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content/settingsModal.js"]
    },
    {
      "matches": ["<all_urls>"],
      "js": ["content/modalToggling.js"]
    },
    {
      "matches": ["<all_urls>"],
      "js": ["content/generateConstraints.js"]
    },
    {
      "matches": ["<all_urls>"],
      "js": ["content/controlsModal.js"]
    },
    {
      "matches": ["<all_urls>"],
      "js": ["content/index.js"]
    }
  ],
  "host_permissions": ["https://*.loca.lt/*", "http://localhost:5000/"],
  "externally_connectable": {
    "matches": ["https://*.loca.lt/*", "http://localhost:5000/"]
  },
  "web_accessible_resources": [
    {
      "resources": ["inject/index.html"],
      "matches": ["<all_urls>"]
    }
  ],
  "options_page": "options/options.html"
}
