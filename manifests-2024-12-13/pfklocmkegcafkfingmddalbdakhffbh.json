{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "LinkedIn Job Turbo",
  "version": "3.2",
  "description": "Streamline and accelerate your job search on LinkedIn",
  "permissions": [
    "downloads",
    "storage"
  ],
  "host_permissions": [
    "*://*.linkedin.com/*"
  ],
  "icons": {
    "16": "icons/img.png",
    "48": "icons/img.png",
    "128": "icons/img.png"
  },
  "action": {
    "default_popup": "landing/landing.html",
    "default_title": "LinkedIn Job Turbo",
    "default_icon": {
      "16": "icons/img.png",
      "48": "icons/img.png",
      "128": "icons/img.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "scripts/pdf.worker.js",
        "popup/keywords.css",
        "content/inspection_mode.css",
        "content/modal.css"
      ],
      "matches": [
        "*://*.linkedin.com/*"
      ]
    }
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self';"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.linkedin.com/*"
      ],
      "js": [
        "content/contentScript.js",
        "scripts/config.js",
        "scripts/chatGPT.js",
        "content/analyzeKeyword.js",
        "content/autoFormFill.js"
      ],
      "run_at": "document_end"
    }
  ]
}
