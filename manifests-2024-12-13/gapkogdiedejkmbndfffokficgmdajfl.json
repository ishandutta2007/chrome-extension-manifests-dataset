      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["https://www.smartrecruiters.com/app/jobs/details/*", "https://www.smartrecruiters.com/app/people/applications/*"],
        "js": ["content.js"],
        "css": ["styles.css"]
      }
    ],
    "externally_connectable": {
      "matches": ["https://cslist.belarrem.com/*"]
    },
    "icons": {
      "16": "images/icon16.png",
      "32": "images/icon32.png",
      "48": "images/icon48.png",
