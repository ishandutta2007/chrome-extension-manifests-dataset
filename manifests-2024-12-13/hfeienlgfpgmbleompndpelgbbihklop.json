{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "RigSec Web3 Extension",
  "version": "1.0.5",
  "description": "solution for Web3 access from your browser",
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "index.html",
    "default_title": "RigSec Wallet",
    "default_icon": "icons/logo_rigsec.png"
  },
  "icons": {
    "16": "icons/logo_rigsec.png",
    "48": "icons/logo_rigsec.png",
    "128": "icons/logo_rigsec.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "notifications"
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "inpage.js",
        "static/*.js"
      ],
      "matches": [
        "*://*/*"
      ]
    }
  ]
}