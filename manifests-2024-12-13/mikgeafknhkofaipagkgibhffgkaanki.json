"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "ChatGPT Chat Saver",
  "version": "1.2",
  "description": "This extension will store chats in your browser storage. Made by ChatGPTips.ai.",
  "homepage_url": "https://chatgptips.ai",
  "background": {
    "service_worker": "background.js"
  },
"content_scripts": [
  {
    "matches": ["https://chat.openai.com/*"],
    "js": ["content.js"]
  }
],  
  "permissions": [
    "storage",
    "activeTab"
  ],
  "action": {
    "default_icon": {
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_popup": "popup.html"
  }
