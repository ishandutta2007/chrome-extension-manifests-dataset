  "manifest_version": 3,
  "name": "FreeDrop",
  "version": "1.0.3",
  "description": "Make guesses on certain YouTube videos to win discount codes",
  "icons": {
    "16": "/icons/16x16-Icon.png",
    "32": "/icons/32x32-Icon.png",
    "48": "/icons/48x48-Icon.png",
    "128": "/icons/128x128-Icon.png"
