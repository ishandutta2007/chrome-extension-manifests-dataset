  },
  "web_accessible_resources": [
    {
        "resources": [
            "images/icon16.png",
            "images/icon48.png",
            "images/icon128.png"
        ],
        "matches": ["<all_urls>"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_idle"
