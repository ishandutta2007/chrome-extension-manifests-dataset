  "manifest_version": 3,
  "name": "SEO Specialist Extension",
  "version": "1.3",
  "description": "Extract the title, meta description, URL, canonical URL, meta robots, headings, hreflang, schema, and word density of a webpage.",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["<all_urls>"],
  "action": {
