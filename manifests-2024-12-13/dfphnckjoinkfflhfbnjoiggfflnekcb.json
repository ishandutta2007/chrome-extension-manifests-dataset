        "scripts": [
            "background.js"
        ],
        "persistent": true
    },
    "browser_action": {
        "default_title": "Save to Moo.do",
        "default_icon": "assets/icon-blue.png"
    },
    "web_accessible_resources": [
        "iframe.html",
        "assets/icon-128.png"
    ],
    "options_ui": {
        "page": "options.html",
        "open_in_tab": true
    },
    "content_scripts": [
        {
            "matches": [
                "https://mail.google.com/*",
                "https://inbox.google.com/*"
            ],
            "js": [
                "assets/inboxsdk.js",
                "gmail.js"
            ],
            "css": [
                "assets/styles.css"
            ],
            "run_at": "document_end"
        }
    ]
