{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Topview.ai devtool logger on/off",
  "version": "1.0.1",
  "description": "A simple Chrome extension to toggle console logs for Topview.ai",
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon.png",
    "48": "icons/icon.png",
    "128": "icons/icon.png"
  },
  "permissions": ["storage", "activeTab", "scripting", "tabs"],
  "host_permissions": [
    "https://*.topview.ai/*",
    "http://*.topview.ai/*",
    "https://aigc-web-tabcut.vercel.app/*"
  ],
  "content_scripts": [
    {
      "matches": ["*://*.topview.ai/*", "*://aigc-web-tabcut.vercel.app/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ]
}
