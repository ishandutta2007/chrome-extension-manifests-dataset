  "name": "Autobot for Alphabot",
  "description": "Automate your raffle entries on Alphabot",
  "permissions": ["activeTab", "declarativeContent", "tabs", "scripting"],
  "host_permissions": ["https://www.alphabot.app/*"],
  "background": {
    "service_worker": "background.js"
