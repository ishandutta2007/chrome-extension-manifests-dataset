      "run_at": "document_end",
      "all_frames": false
    }
  ],
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": "images/logo.png"
  },

  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
