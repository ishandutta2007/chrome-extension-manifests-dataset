{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "ShieldX",
  "version": "0.1.9",
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "clipboardRead",
    "management",
