  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "/images/128.png"
  },

  "externally_connectable": {
    "matches": [
      "http://localhost:3000/",
      "http://localhost:3000/*",
      "https://test.soulhq.ai/",
      "https://test.soulhq.ai/*",
      "https://platform.soulhq.ai/",
      "https://platform.soulhq.ai/*"
    ]
  },
  "host_permissions": ["<all_urls>"]
