---

{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "100xjobs",
    "version": "1.2",
    "description": "Scrapes job details from various job sites.",
    "permissions": [
        "scripting",
        "storage",
        "activeTab"
    ],
    "host_permissions": [
        "https://*.linkedin.com/*",
        "https://*.indeed.com/*",
        "https://*.glassdoor.co.in/*",
        "https://*.naukri.com/*",
        "https://app.100xjobs.com/*"
    ],
    "action": {
        "default_popup": "index.html",
        "default_icon": {
            "32": "icon.png",
            "48": "icon.png",
            "128": "icon.png"
        },
        "default_title": "100xjobs"
    },
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": [
                "https://*.linkedin.com/*",
                "https://*.indeed.com/*",
                "https://*.glassdoor.co.in/*",
                "https://*.naukri.com/*",
                "https://app.100xjobs.com/*",
                "http://localhost:5173/*"
            ],
            "js": ["content.js"],
            "run_at": "document_end"
        }
    ],
    "web_accessible_resources": [
        {
            "resources": ["dist/*", "index.html"],
            "matches": ["<all_urls>"]
        }
    ],
    "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'self';"
    }
}
