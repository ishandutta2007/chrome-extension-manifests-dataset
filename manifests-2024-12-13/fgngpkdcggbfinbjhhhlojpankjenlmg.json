{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "AI Email Generator - Repliio",
    "version": "1.0",
    "description": "AI-powered email generator that helps you reply to emails faster, smarter, and more effectively",
    "permissions": [
      "activeTab",
      "storage",
      "contextMenus"
    ],
    "host_permissions": [
      "*://mail.google.com/*",
      "*://mail.yandex.com/*",
      "http://localhost:5001/*"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "assets/images/favicon-16x16.png",
        "48": "assets/images/favicon-48x48.png",
        "128": "assets/images/apple-touch-icon.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": [
          "*://mail.google.com/*",
          "*://mail.yandex.com/*",
          "http://localhost:5001/*"
        ],
        "js": ["content.js"],
        "run_at": "document_idle"
      }
    ],
    "icons": {
      "16": "assets/images/favicon-16x16.png",
      "48": "assets/images/favicon-48x48.png",
      "128": "assets/images/apple-touch-icon.png"
    }
  }
  
  