{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "Odyssey Anywhere",
    "description": "Odyssey Anywhere is a browser extension that allows instant access to information from your Odyssey AI directly from any website.",
    "version": "1.0.0",
    "action": {
        "default_icon": {
            "16": "icon-16.png",
            "48": "icon-48.png",
            "128": "icon-128.png"
        }
    },
    "background": {
        "service_worker": "js/backgroundPage.js"
    },
    "icons": {
        "16": "icon-16.png",
        "48": "icon-48.png",
        "128": "icon-128.png"
    },
    "host_permissions": [
        "https://*/*",
        "http://*/*"
    ],
    "permissions": [
        "tabs",
        "activeTab",
        "storage",
        "contextMenus"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "js/contentScript.js"
            ]
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "js/contentScript.js"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ]
}