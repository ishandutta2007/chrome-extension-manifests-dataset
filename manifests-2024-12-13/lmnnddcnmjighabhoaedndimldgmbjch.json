{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "Z Themes",
    "description": "Change your search results background theme",
    "version": "0.0.1",
     "icons": {
         "16": "logo/logo-16.png",
         "24": "logo/logo-24.png",
         "32": "logo/logo-32.png",
        "48": "logo/logo-48.png",
        "128": "logo/logo-128.png"
     },
     "permissions": ["activeTab","storage"],
     "action": {
        "default_title": "Z Themes",
        "default_popup": "popup/popup.html"
    },
    "background": {
        "service_worker": "background_scripts/background_service_worker.js"
    },
    "content_scripts": [{
        "matches": ["https://www.google.com/*"],
        "run_at": "document_idle",
        "js": ["content/contentScript.js"],
        "css": ["content/contentCss.css"]
    }],
    "web_accessible_resources": [{ 
        "resources": [  "*.html","*.css","icons/*.png","patterns/*.png"],
        "matches": ["<all_urls>"]
    }]
}
