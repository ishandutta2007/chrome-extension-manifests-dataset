{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "FUT Lister",
  "version": "1.0.1",
  "description": "Keep your EA FC 25 transfer list active 24/7 with Futlister. Easy setup, safe, and reliable automation.",
  "permissions": [
    "activeTab",
    "scripting",
    "storage",
    "tabs"
  ],
  "host_permissions": [
    "*://*.ea.com/*",
    "https://futlister.com/*"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.ea.com/*",
        "https://futlister.com/*"
      ],
      "js": ["content.js"],
      "run_at": "document_end",
      "all_frames": true
    }
  ],
  "web_accessible_resources": [{
    "resources": ["content.js"],
    "matches": [
      "*://*.ea.com/*",
      "https://futlister.com/*"
    ]
  }]
}
