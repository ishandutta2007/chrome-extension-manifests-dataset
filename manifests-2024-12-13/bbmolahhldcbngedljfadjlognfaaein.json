{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "CheerpJ Applet Runner",
  "description": "Run Java applets without installing Java",
  "version": "2024.11",
  "icons": { "128":"cheerpjAR_128x128.png", "32":"cheerpjAR_32x32.png" },
  "action" : {
    "default_icon": "cheerpjAR_128x128_gray.png",
    "default_popup": "popup.html",
    "default_title": "CheerpJ Applet Runner Disabled"
  },
  "background": {
    "service_worker": "bg.js"
  },
  "permissions": [
    "scripting",
    "activeTab",
    "storage"
  ],
  "optional_host_permissions": [
    "http://*/", "https://*/"
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "web_accessible_resources": [
    {
      "resources": [ "cheerpj/*", "init_applet.js" ],
      "matches": [ "<all_urls>" ]
    }
  ],
  "storage": {
    "managed_schema": "storage_schema.json"
  }
}
