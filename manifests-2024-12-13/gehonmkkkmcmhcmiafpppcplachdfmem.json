{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Jump GPT",
  "version": "1.2",
  "description": "気になるテキストをサクッとChatGPTへ。選択してワンクリックで、あなたの疑問を解決します。",
  "permissions": ["scripting", "activeTab", "contextMenus", "system.display", "tabs"],
  "host_permissions": ["https://chat.openai.com/*", "https://chatgpt.com/*"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": [
        "errorHandler.js",
        "base.js",
        "utils.js",
        "logger.js",
        "loader.js",
        "gptHelper.js",
        "scrollBottom.js",
        "domWatcher.js",
        "checkAceHelper.js",
        "keyHelper.js",
        "js/caretposition.js",
        "positionCalculator.js",
        "selectionHelper.js",
        "tooltipHelper.js",
        "selectTextHelper.js",
        "eventState.js",
        "tooltipManager.js",
        "eventHandler.js",
        "content.js"
      ],
      "css": ["css/style.css"],
      "run_at": "document_end"
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/jump-gpt-icon-pink-16.png",
      "48": "images/jump-gpt-icon-pink-48.png",
      "128": "images/jump-gpt-icon-pink-128.png"
    }
  },
  "web_accessible_resources": [
    {
      "resources": ["images/*", "js/caretposition.js", "privacy-policy.html"],
      "matches": ["<all_urls>"]
    }
  ],
  "icons": {
    "16": "images/jump-gpt-icon-pink-16.png",
    "32": "images/jump-gpt-icon-pink-32.png",
    "48": "images/jump-gpt-icon-pink-48.png",
    "128": "images/jump-gpt-icon-pink-128.png"
  }
