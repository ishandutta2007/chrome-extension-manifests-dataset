{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "AutoLogin Extension",
  "version": "1.0",
  "permissions": [
 
    "tabs",
    "https://caterham.fireflycloud.net/login/*",
    "https://login.microsoftonline.com/*",
    "https://federation.caterhamschool.co.uk/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://caterham.fireflycloud.net/login/*",
        "https://login.microsoftonline.com/*",
        "https://federation.caterhamschool.co.uk/*"
      ],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "main_icon.png"
  }
}
