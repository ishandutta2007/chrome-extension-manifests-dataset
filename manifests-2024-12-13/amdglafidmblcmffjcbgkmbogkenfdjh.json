{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "Tab Hot Key",
    "version": "0.1.0",
    "description": "Define hotkeys to open or focus frequently used tabs.",
    "icons": { 
       "128": "icon128.png" 
    },
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ],
    "permissions": [
        "tabs", "storage"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "options.html"
    },
    "options_ui": {
        "page": "options.html",
        "open_in_tab": true
    },
    "commands": {
        "jump": {
            "suggested_key": {
                "default": "Alt+J"
            },
            "description": "Jump To Tab"
        }
    }
