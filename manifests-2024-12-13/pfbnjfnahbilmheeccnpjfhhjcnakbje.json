{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "CampEd Hire  - LinkedIn",
  "version": "1.9",
  "description": "Get job applicant data from LinkedIn job posts",
  "permissions": ["tabs", "scripting", "sidePanel"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["*://www.linkedin.com/hiring/jobs/*/applicants/*/detail/*"],
      "js": ["contentScript.js", "popup.js"],
      "run_at": "document_end"
    }
  ],
  "side_panel": {
    "default_path": "popup.html"
  },
  "action": {
    "default_title": "Click to open panel",
    "default_popup": "popup.html",

    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "host_permissions": [
    "https://www.linkedin.com/hiring/jobs/*/applicants/*/detail"
  ]
}
