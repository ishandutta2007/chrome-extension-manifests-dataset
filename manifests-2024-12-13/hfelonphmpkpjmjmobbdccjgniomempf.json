{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Amazon Price Compare for Europe",
  "description": "Find the best Amazon price in the EU markets: de, it, es, fr, uk, nl, be, se, pl",
  "version": "1.0.0",
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png"
  },
  "action": {
    "default_icon": "icon128.png",
    "default_popup": "popup.html",
    "default_title": "Amazon Price Compare Europe"
  },
  "permissions": ["activeTab", "storage"],
  "host_permissions": [
    "*://www.amazon.de/*",
    "*://www.amazon.fr/*",
    "*://www.amazon.it/*",
    "*://www.amazon.nl/*",
    "*://www.amazon.se/*",
    "*://www.amazon.es/*",
    "*://www.amazon.co.uk/*",
    "*://www.amazon.com.be/*"
  ],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "content_scripts": [
    {
      "matches": [
        "*://www.amazon.de/*",
        "*://www.amazon.fr/*",
        "*://www.amazon.it/*",
        "*://www.amazon.nl/*",
        "*://www.amazon.se/*",
        "*://www.amazon.es/*",
        "*://www.amazon.co.uk/*",
        "*://www.amazon.com.be/*"
      ],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ]
}
