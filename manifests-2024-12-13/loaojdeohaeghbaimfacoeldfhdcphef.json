  "web_accessible_resources": [
    {
      "resources": [
        "css/comment-app.css"
      ],
      "matches": [
        "*://*.reddit.com/*"
      ]
    },
    {
      "resources": [
        "css/shortcut-app.css"
      ],
      "matches": [
        "<all_urls>"
      ]
    },
    {
      "resources": [
        "fonts/*"
      ],
      "matches": [
        "*://*.reddit.com/*"
      ]
    },
    {
      "resources": [
        "img/*.png"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "options_page": "options.html",
  "version": "1.3.0"
