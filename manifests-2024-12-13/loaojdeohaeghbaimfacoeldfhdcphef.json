"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "__MSG_extName__",
  "description": "__MSG_extDes__",
  "default_locale": "en",
  "icons": {
    "128": "128.png"
  },
  "action": {
    "default_icon": {
      "128": "128.png"
