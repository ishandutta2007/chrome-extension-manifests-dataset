---

{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "BidPacer",
  "description": "Generate AI proposals and apply jobs in popular freelance platforms i.e. Upwork, Freelancer and Guru",
  "version": "0.2",
  "permissions": [],
  "content_scripts": [
    {
      "matches": ["*://*.upwork.com/nx/proposals/job/*"],
      "js": ["class-watcher.class.js", "upwork.js"],
      "css": ["content.css"],
      "run_at": "document_end"
    },
    {
      "matches": ["*://*.freelancer.com/projects/*", "*://*.freelancer.ca/projects/*", "*://*.freelancer.co.uk/projects/*"],
      "js": ["class-watcher.class.js", "freelancer.js"],
      "css": ["content.css"],
      "run_at": "document_end"
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "externally_connectable": {
    "matches": ["http://localhost:3000/*", "http://localhost:3001/*", "https://www.bidpacer.ai/*"]
  },
  "icons": {
    "16": "assets/favicon.png",
    "48": "assets/favicon.png",
    "128": "assets/favicon.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "BidPacer",
    "default_icon": "assets/favicon.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["/assets/*"],
      "matches": [ "*://*.upwork.com/*" ]
    }
 ]
}
