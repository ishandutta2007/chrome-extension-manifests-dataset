{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 2,
    "name": "ourlib",
    "description": "Save money and support your local library. This extension will find books in libraries near you when browsing online bookstores.",
    "version": "0.1.2",
    "permissions": [
        "http://www.amazon.co.uk/*",
        "http://www.waterstones.com/*",
        "https://www.waterstones.com/*",
        "http://www.whsmith.co.uk/*",
        "http://api.ourlib.co.uk/*",
        "https://fonts.gstatic.com/*",
        "storage",
        "geolocation"
    ],
    "browser_action": {
        "default_icon": "images/19.png",
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "js":["libs/jquery.js", "js/scripts.js", "libs/jquery.geolocation.min.js", "libs/handlebars-v2.0.0.js", "libs/jquery.easing.1.3.js", "libs/important.js", "libs/animate-important.js", "libs/marquee.js", "libs/jquery.resize.min.js", "libs/facebook-api.js"],
            "css": ["css/site.css", "fonts/font-awesome-4.3.0/css/font-awesome.min.css"],
            "matches":["http://www.amazon.co.uk/*", "http://www.waterstones.com/*", "http://www.whsmith.co.uk/*", "https://www.waterstones.com/*"],
            "run_at": "document_end",
            "all_frames": true
        }
    ],
    "web_accessible_resources": [
        "*.png",
        "*.jpg",
        "*.jpeg",
        "*.gif",
        "*.eot",
        "*.svg",
        "*.ttf",
        "*.woff",
        "css/*",
        "fonts/*",
        "js/*",
        "libs/*",
        "images/*",
        "options.html",
        "templates/*"
    ],
    "icons": {
        "19": "images/19.png",
        "48": "images/48.png",
        "128": "images/128.png"
    },
    "options_page": "options.html"
}