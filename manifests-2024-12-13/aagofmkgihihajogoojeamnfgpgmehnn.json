{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "GitHub Tree Map",
    "version": "1.1.0",
    "description": "Display the tree structure of a GitHub repository and export it as an image.",
    "permissions": ["activeTab", "scripting", "storage", "unlimitedStorage"],
    "host_permissions": ["https://github.com/*", "https://api.github.com/*"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/16.png",
        "48": "icons/48.png",
        "128": "icons/128.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["https://github.com/*"],
        "js": ["html2canvas.min.js", "supabase.js", "content.js"],
        "css": ["styles/treeMap.css"]
      }
    ],
    "icons": {
      "16": "icons/16.png",
      "48": "icons/48.png",
      "128": "icons/128.png"
    },
    "commands": {
      "_execute_action": {
        "suggested_key": {
          "default": "Ctrl+Shift+G",
          "mac": "Command+Shift+G"
        },
        "description": "Open GitHub Tree Map"
      }
    }
  }