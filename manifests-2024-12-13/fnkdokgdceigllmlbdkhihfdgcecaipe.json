{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "Text Humanizer Authorprivacy",
    "version": "1.0",
    "description": "Humanize text with Authorprivacy",
    "permissions": [
      "contextMenus",
      "activeTab",
      "scripting",
      "commands"
    ],
    "host_permissions": [
      "https://api.authorprivacy.com/*"
    ],
    "icons": {
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "action": {
      "default_icon": {
        "48": "icon48.png",
        "128": "icon128.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": [
          "http://*/*",
          "https://*/*"
        ],
        "js": ["content.js"],
        "css": ["styles.css"]
      }
    ],
    "commands": {
      "humanize-text": {
        "suggested_key": {
          "default": "Alt+Shift+H",
          "mac": "Alt+Shift+H"
        },
        "description": "Humanize selected text"
      }
    },
    "web_accessible_resources": [{
      "resources": ["icon128.png"],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }]
  }
  