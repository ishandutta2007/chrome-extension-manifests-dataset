{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "YouTube Theme Pro",
  "version": "1.2.1.4",
   "description": "Customize YouTube with themes, dark/light mode, stylish skins, and UI tools.Enhance appearance for sleek, premium YouTube experience",

  "permissions": ["storage", "scripting", "tabs"],
  "host_permissions": ["*://www.youtube.com/*"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/*"],
      "js": ["inject-font.js"]
    }
  ],
"web_accessible_resources": [
    {
      "resources": ["tubeSkins/style2/1.jpg","icon.png"],
      "matches": ["*://www.youtube.com/*"]
    }
  ]
}
