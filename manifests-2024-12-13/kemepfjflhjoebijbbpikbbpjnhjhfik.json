{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "HideMe. Hide YouTube End Cards",
    "version": "1.1",
    "description": "Hides YouTube's end cards to improve your video-watching experience.",
    "permissions": [
        "activeTab",
        "scripting",
        "storage"
    ],
    "host_permissions": [
        "*://*.youtube.com/*"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*.youtube.com/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "icons": {
        "48": "icons/hideme48.png",
        "128": "icons/hideme128.png"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "48": "icons/hideme48.png",
            "128": "icons/hideme128.png"
        }
    }
}