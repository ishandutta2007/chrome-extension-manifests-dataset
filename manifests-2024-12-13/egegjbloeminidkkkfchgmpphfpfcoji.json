        "resources": ["/js/axios.min.map"],
        "matches": ["file://*/*"]
    }],
    "action": {
        "default_title": "Click to open panel",
        "default_icon": {
            "16": "icons/icon16.png",
            "32": "icons/icon32.png",
            "64": "icons/icon48.png",
            "128": "icons/icon128.png"
        }
    },
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "64": "icons/icon48.png",
