{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "FAIA",
  "description": "Безкоштовна ШІ-шка для відповіді на запитання з сторінки.",
  "version": "1.0",
  "permissions": [
    "scripting",
    "activeTab"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://dl.guscoll.com/*"],
      "js": ["content.js", "set_site.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["set_site.js", "dist/bundle.js"],
      "matches": ["<all_urls>"]
    }
  ]
}