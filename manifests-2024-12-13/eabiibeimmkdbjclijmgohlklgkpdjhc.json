  "version": "0.1",
  "manifest_version": 2,
  "permissions": ["storage", "activeTab" ],
  "browser_action": {
    "default_popup": "popup/rbpPopup.html",
    "default_icon": {
      "16": "images/rbp_16.png",
      "32": "images/rbp_32.png",
      "48": "images/rbp_48.png",
