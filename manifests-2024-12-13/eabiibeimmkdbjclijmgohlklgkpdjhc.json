    }
  },
  "icons": {
    "16": "images/rbp_16.png",
    "32": "images/rbp_32.png",
    "48": "images/rbp_48.png",
    "128": "images/rbp_128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.google.com/search*"],
      "css": ["init/initPage.css"],
      "js": ["init/initPage.js"]
    }
  ],
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+B",
        "mac": "MacCtrl+B"
