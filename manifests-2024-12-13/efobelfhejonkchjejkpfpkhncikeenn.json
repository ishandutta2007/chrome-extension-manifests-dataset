{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "Watchdog - no distractions",
    "version": "1.0",
    "description": "Block websites and show a dog image with barking sound when accessed.",
    "permissions": ["storage", "webNavigation", "activeTab"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": "images/icon.png" 
    },
    "icons": {
      "128": "images/icon.png"  
    },
    "web_accessible_resources": [
      {
        "resources": ["blocked.html", "images/dog.jpg", "sounds/bark.mp3"],
        "matches": ["<all_urls>"]
      }
    ]
}