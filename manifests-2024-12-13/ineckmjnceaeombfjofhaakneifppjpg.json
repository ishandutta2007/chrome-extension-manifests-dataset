{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name":"Tuna",
    "version":"1.3.5",
    "description":"可能是最好的Chrome书签及标签页管理工具",
    "default_locale": "en",
    "permissions": ["sidePanel","bookmarks","favicon","tabs","activeTab","storage","commands","contextMenus","search","history"],
    "icons": {
      "16": "./src/icon/icon16.png",
      "32": "./src/icon/icon32.png",
      "48": "./src/icon/icon48.png",
      "128": "./src/icon/icon128.png"
    },
    "background": {
      "service_worker": "service-worker.js",
      "type": "module"
    },
    "action":{
        "default_title":"点击打开侧边栏",
        "default_popup": "side_panel.html"
    },
    "side_panel":{
        "enabled":true,
        "default_path":"side_panel.html",
        "default_width":1000
    },
    "web_accessible_resources": [
    {
      "resources": ["_favicon/*","new_popup/*","src/iconfont/*"],
      "matches": ["<all_urls>"],
      "use_dynamic_url": true
    }
  ],
  "commands": {
    "command-0": {
      "suggested_key": {
        "default": "Ctrl+Shift+T",
        "mac": "Command+Shift+T"
      },
      "description": "新建标签页"
    },
    "command-1": {
      "suggested_key": {
        "default": "Ctrl+Shift+1",
        "mac": "Command+Shift+1"
      },
      "description": "切换到第1个空间"
    },
    "command-2": {
      "description": "切换到第2个空间"
    },
    "command-3": {
      "description": "切换到第3个空间"
    },
    "command-4": {
      "description": "切换到第4个空间"
    },
    "command-5": {
      "description": "切换到第5个空间"
    },
    "command-left": {
      "suggested_key": {
        "default": "Ctrl+Shift+Left",
        "mac": "Command+Shift+Left"
      },
      "description": "切换到上个空间"
    },
    "command-right": {
      "suggested_key": {
        "default": "Ctrl+Shift+Right",
        "mac": "Command+Shift+Right"
      },
      "description": "切换到下个空间"
    }
  }
}