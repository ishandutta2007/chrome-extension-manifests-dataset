        "128": "icons/icon-128.png"
    },
    "action": {
  "default_icon": {
            "32": "icons/icon-32.png",
            "48": "icons/icon-48.png"
        },
  "default_title": "Tab Guardian",
        "default_popup": "index.html"
    },
    "background": {
        "service_worker": "assets/background.js"
    }
