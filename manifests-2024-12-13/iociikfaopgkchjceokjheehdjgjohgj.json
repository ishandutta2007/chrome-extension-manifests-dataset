  "web_accessible_resources": [
    {
      "resources": [
        "/index.html",
        "/index.html#/home",
        "/index.html#/onboarding",
        "/index.html#/no-subscription",
        "/index.html#/report",
        "/index.html#/signout",
        "/index.html#/login",
      "/assets/mindpeer/cross.svg"
      ],
      "matches": ["<all_urls>"]
