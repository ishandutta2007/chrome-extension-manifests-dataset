{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "EXPTOOL99",
  "description": "This extension allows to overwrite variation IDs with other variation IDs for any experiment.",
  "version": "1.2",
  "manifest_version": 3,
  "background": {
    "service_worker": "service-worker.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "dist/main.js"
      ],
      "all_frames": true,
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "dist/page-runtime.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "background"
  ],
  "action": {
    "default_icon": {
      "16": "/images/ex-disabled.png",
      "32": "/images/ex-disabled.png",
      "48": "/images/ex-disabled.png",
      "128": "/images/ex-disabled.png"
    }
  },
  "icons": {
    "16": "/images/experiments_tool_logo.png",
    "32": "/images/experiments_tool_logo.png",
    "48": "/images/experiments_tool_logo.png",
    "128": "/images/experiments_tool_logo.png"
  }
}