{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Leads to Notion",
  "version": "1.0.3",
  "description": "Notion CRM, Add new entries to your Notion database from LinkedIn and X(Twitter) in one click.",
  "permissions": ["storage", "tabs"],
  "host_permissions": [
    "https://www.linkedin.com/*",
    "https://twitter.com/*",
    "https://www.twitter.com/*",
    "https://x.com/*",
    "https://www.x.com/*",
    "https://api.notion.com/*",
    "https://lead-to-notion-backend.vercel.app/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.linkedin.com/*",
        "https://twitter.com/*",
        "https://www.twitter.com/*",
        "https://x.com/*",
        "https://www.x.com/*"
      ],
      "js": ["contentScript.js"],
      "css": ["content.css"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["/index.html*", "assets/*"],
      "matches": ["<all_urls>"]
    }
  ],
  "icons": {
    "16": "assets/new-logo-16.png",
    "32": "assets/new-logo-32.png",
    "48": "assets/new-logo-48.png",
    "128": "assets/new-logo.png"
  },
  "action": {
    "default_icon": {
      "16": "assets/new-logo-16.png",
      "32": "assets/new-logo-32.png",
      "48": "assets/new-logo-48.png",
      "128": "assets/new-logo.png"
    },
    "default_title": "Leads to Notion",
    "default_popup": "popup.html"
  },
  "manifest_version": 3
}
