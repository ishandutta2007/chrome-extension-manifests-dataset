{
"update_url": "https://clients2.google.com/service/update2/crx",

    "name": "Resumo extension",
    "version": "1.0",
    "description": "This extension allows you to transafer data into resumo while using other apps.",
    "manifest_version": 3,
    "permissions": [
        "storage",
        "background",
        "notifications",
        "activeTab",
        "scripting",
        "tabs"],
    "host_permissions": [
        "https://mail.google.com/mail/u/0/?tab=rm#inbox/*"
    ],
    "icons": {
        "128": "images/logo-.png"
    },

    "background": {
        "service_worker": "background.js"
    },

    "action": {
        "default_popup": "app/popup/popup.html",
        "default_icon": {
            "128": "images/logo-.png"
        }
    },
    "options_page": "app/options/options.html",
    "content_scripts": [
        {
            "matches": ["https://mail.google.com/mail/u/0/?tab=rm#inbox/*"],
            "js": ["foreground.js"]
        }
    ]

}