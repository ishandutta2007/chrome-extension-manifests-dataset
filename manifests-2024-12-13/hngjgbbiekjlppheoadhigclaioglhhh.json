  "manifest_version": 3,
  "name": "ListingSplit Connect",
  "version": "2.2",
  "description": "Integrates ListingSplit.com commission compensation details on Zillow.com, Homes.com, Realtor.com, and Redfin.com",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  },
    "content_scripts": [
      {
        "matches": [
          "*://*.homes.com/*",
          "*://*.zillow.com/*",
          "*://*.redfin.com/*",
          "*://*.realtor.com/*"
        ],
        "js": ["jquery.js", "contentScript.js"],
        "css": ["styles.css"],
        "run_at": "document_idle"
      }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/logo.png",
      "48": "images/logo.png",
      "128": "images/logo.png"
    }
  }
  
