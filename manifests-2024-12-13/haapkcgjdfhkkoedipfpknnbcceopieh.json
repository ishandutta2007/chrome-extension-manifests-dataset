{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Udemy Subtitle Reader",
  "description": "Read subtitles on Udemy in various languages.",
  "version": "1.0.8",
  "background": {
    "service_worker": "/background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "host_permissions": [
    "https://www.udemy.com/*", 
    "http://127.0.0.1:8000/*",
    "https://udemysubtitlereader.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["/content.js"]
    }
  ],
  "icons": {
    "16": "/assets/icons/logo16.png",
    "32": "/assets/icons/logo32.png",
    "48": "/assets/icons/logo48.png",
    "128": "/assets/icons/logo128.png"
  },
  "permissions": [
    "sidePanel",
    "webNavigation",
    "scripting",
    "tabs",
    "storage"
  ],
  "side_panel": {
    "default_path": "popup.html"
  },
  "options_page": "options.html"
}
