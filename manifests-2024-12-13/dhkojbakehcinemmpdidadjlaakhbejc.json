{
"update_url": "https://clients2.google.com/service/update2/crx",

 "manifest_version": 3,
 "name": "Dimmer",
 "version": "1.0",
 "description": "A Basic Tab Dimmer",
 "permissions": ["activeTab", "storage", "commands"],
 "action": {
   "default_popup": "popup.html",
   "default_icon": {
   "96": "light-bulb-96.png"
   }
 },
 "background": {
   "service_worker": "background.js"
 },
 "commands": {
    "set_opacity": {
      "suggested_key": {
        "default": "Ctrl+Shift+Down",
    "mac": "Command+Shift+Down"
      },
      "description": "Set opacity"
    },
  "reset_opacity": {
      "suggested_key": {
        "default": "Ctrl+Shift+Up",
    "mac": "Command+Shift+Up"
      },
      "description": "Reset opacity"
    }

  },
 "content_scripts": [
   {
   "matches": ["<all_urls>"],
   "js": ["content.js"]
   }
 ]
 }