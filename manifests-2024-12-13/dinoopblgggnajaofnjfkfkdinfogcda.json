      "js": [
        "app.js"
      ],
      "css": [
        "app.css"
      ]
    }
  ],
  "icons": {
    "16": "logo/logo16.png",
    "32": "logo/logo32.png",
    "48": "logo/logo48.png",
    "128": "logo/logo128.png"
