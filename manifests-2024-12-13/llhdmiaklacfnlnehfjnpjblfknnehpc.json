{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Nordium",
  "description": "Drip the droplet into your Nordium.",
  "version": "0.0.2",
  "permissions": [
    "tabs",
    "storage",
    "activeTab"
  ],
  "icons": {
    "16": "icons/@16.png",
    "32": "icons/@32.png",
    "48": "icons/@48.png",
    "96": "icons/@96.png",
    "128": "icons/@128.png"
  },
  "options_ui": {
    "page": "src/pages/settings/settings.html",
    "open_in_tab": true
  },
  "action": {
    "default_title": "Nordium",
    "default_icon": {
      "16": "icons/@16.png",
      "32": "icons/@32.png",
      "48": "icons/@48.png",
      "96": "icons/@96.png",
      "128": "icons/@128.png"
    }
  },
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "host_permissions": [
    "http://*/*",
    "https://*/*",
    "http://nordium.test/*",
    "https://nordium.test/api/*"
  ],
  "content_scripts": [
    {
      "js": [
        "assets/content.ts-loader-BlTvHLoC.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_idle"
    }
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "assets/*.*",
        "src/style.css",
        "src/tailwind.css"
      ],
      "use_dynamic_url": false
    },
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "resources": [
        "assets/runtime-dom.esm-bundler-B3zx4X1T.js",
        "assets/content.ts-Dq47jL-4.js"
      ],
      "use_dynamic_url": false
    }
  ]
}
