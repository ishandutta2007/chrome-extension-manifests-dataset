  },
  "content_scripts": [
    {
      "matches": ["https://lumeo.me/*"],
      "js": ["contentScript.js"],
      "run_at": "document_end"
    }
  ],
  "host_permissions": [
    "http://lumeo.me/*",
    "http://www.lumeo.me/*",
    "https://linkedin.com/*",
    "https://www.linkedin.com/*"
  ]
