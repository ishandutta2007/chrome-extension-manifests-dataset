{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Font Expert: Identify Fonts from Images & Websites",
  "description": "Discover what font is on images and web pages effortlessly with Font Expert!",
  "version": "1.2.1",
  "permissions": [
    "storage",
    "scripting",
    "declarativeNetRequest"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_title": "FontExpert",
    "default_popup": "popup/index.html"
  },
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "headers",
        "enabled": true,
        "path": "headers.json"
      }
    ]
  },
  "icons": {
    "128": "/assets/icon-128.png",
    "48": "/assets/icon-48.png",
    "32": "/assets/icon-32.png",
    "16": "/assets/icon-16.png"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "assets/*",
        "styles/widgetStyles.css",
        "fontIdentify/index.html"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "all_frames": true,
      "js": [
        "script/content.js"
      ],
      "run_at": "document_start"
    }
  ],
  "background": {
    "service_worker": "script/background.js"
  }
