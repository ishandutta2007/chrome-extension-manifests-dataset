{
"update_url": "https://clients2.google.com/service/update2/crx",

    "name": "Vocab Importer",
    "description": "Extension to import articles into Vocab directly from your web browser!",
    "version": "1.0.0",
    "manifest_version": 3,
    "minimum_chrome_version": "92",
    "action": {
        "default_icon": {
            "16": "/images/favicon.png",
            "32": "/images/favicon.png",
            "48": "/images/favicon.png",
            "128": "/images/favicon.png"
        }
    },
    "background": {
        "service_worker": "background.js"
    },
    "commands": {
        "_execute_action": {
            "suggested_key": {
                "default": "Ctrl+Shift+S",
                "mac": "MacCtrl+Shift+S"
            },
            "description": "Runs content script"
        }
    },
    "icons": {
        "16": "/images/favicon.png",
        "32": "/images/favicon.png",
        "48": "/images/favicon.png",
        "128": "/images/favicon.png"
    },
    "web_accessible_resources": [
        {
            "matches": [
                "*://*/*"
            ],
            "resources": [
                "tesseract/tesseract.min.js"
            ]
        }
    ],
    "content_scripts": [
        {
            "matches": [
                "https://*/*"
            ],
            "exclude_matches": [
                "*://read.amazon.com/*",
                "*://vocab.so/*",
                "*://app.vocab.so/*"
            ],
            "js": [
                "content.js"
            ]
        },
        {
            "matches": [
                "https://read.amazon.com/*"
            ],
            "js": [
                "tesseract/tesseract.min.js",
                "kindle.js"
            ]
        }
    ]
}