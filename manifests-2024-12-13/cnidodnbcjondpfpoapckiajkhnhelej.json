  "manifest_version": 3,
  "minimum_chrome_version": "120",
  "default_locale": "en",
  "name": "Starizon AI - AI Agent and Assistant for Efficient Browsing & Automation",
  "version": "2.0.0",
  "description": "__MSG_extensionDescription__",
  "permissions": [
    "storage",
    "tabs",
    "commands",
    "scripting",
    "clipboardWrite",
    "downloads",
    "offscreen",
    "alarms",
    "notifications",
    "declarativeNetRequestWithHostAccess",
    "sidePanel"
