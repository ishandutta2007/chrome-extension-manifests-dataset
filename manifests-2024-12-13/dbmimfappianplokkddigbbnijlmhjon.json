{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Raptorlink",
  "version": "1.7.4",
  "description": "Insert links with ease, manage your insertions, find link building opportunities, and verify link placements",
  "permissions": [
    "contextMenus",
    "activeTab",
    "scripting",
    "storage",
    "notifications",
    "tabs"

  ],
   "host_permissions": [
    "<all_urls>",
    "https://api.openai.com/",
    "https://script.google.com/*",
    "https://www.googleapis.com/",
    "wss://share.raptorlink.com/*",
    "https://share.raptorlink.com/*",
    "https://*/*",
    "http://*/*"
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'; connect-src 'self' https://* http://* ws://* wss://* wss://share.raptorlink.com/"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    },
    {
      "matches": ["https://www.google.com/search?*"],
      "js": ["google-search-popup.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "images/icon128.png",
        "images/reminder.png",
        "insertion-popup.html",
        "parse-results.html",
        "find-pages.html",
        "websocketClient.js",
        "popup.js", 
        "meta-results.html",
        "google-search-popup.css",
        "sitemapParser.js",
        "urlParser.js",
        "donorMatcher.js",
        "utils.js",
        "relevancy-onpage.html",
        "relevancy-onpage.css",
        "relevancy-onpage.js",
        "bulk.html",
        "bulk.css",
        "bulk.js",
        "sitemap_keywords.html",
        "sitemap_topwords.js",
        "images/icon128.png",
        "quick-match.js",
        "searchService.js"
      ],
      "matches": ["<all_urls>"]
    }
  ],
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "options_page": "settings.html"
}