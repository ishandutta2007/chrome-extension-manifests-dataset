{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Arxiv Citation Redirector",
  "description": "Redirect citation from citation section to google scholar paper search result",
    "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "version": "1.03",
  "permissions": [
    "tabs",
    "scripting",
    "webRequest"
  ],
  "host_permissions": [
    "https://arxiv.org/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "pdf-object.js",
        "pdf-lib.js",
        "pdfjs-dist/build/pdf.js",
        "pdfjs-dist/build/pdf.worker.js",
        "content.js"
      ],
      "run_at": "document_end"
    }
  ]
}
