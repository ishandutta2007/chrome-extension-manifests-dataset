  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.bundle.js"
      ],
      "css": [],
      "run_at": "document_end"
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "/icons/128.png"
    }
  },
  "web_accessible_resources": [
    {
      "resources": [
        "icons/*",
        "images/*",
        "inject.bundle.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "version": "0.0.1"
