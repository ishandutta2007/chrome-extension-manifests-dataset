{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Automateo Workflow Bookmarker",
  "version": "1.1",
  "description": "Easily bookmark and access Automateo workflow apps",
  "permissions": ["storage"],
  "host_permissions": ["https://automateo.app/*"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/automateo-16.png",
      "48": "images/automateo-48.png",
      "128": "images/automateo-128.png"
    }
  },
  "icons": {
    "16": "images/automateo-16.png",
    "48": "images/automateo-48.png",
    "128": "images/automateo-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://automateo.app/app/*",
        "https://automateo.app/apps/*"
      ],
      "js": ["content.js"],
      "css": ["content.css"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  }
}
