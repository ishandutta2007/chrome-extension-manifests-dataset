{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Chill",
  "description": "A Memecoin Trading Wallet",
  "version": "1.0.16",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "activeTab", "sidePanel", "tabs", "windows", "webNavigation"],

  "action": {
    "default_icon": {
      "16": "/images/logo16.png",
      "32": "/images/logo32.png",
      "48": "/images/logo48.png",
      "128": "/images/logo128.png"
    }
  },
  "icons": {
    "16": "/images/logo16.png",
    "32": "/images/logo32.png",
    "48": "/images/logo48.png",
    "128": "/images/logo128.png"
  },
  "side_panel": {
    "default_path": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["js/index.js", "js/component.js"],
      "css": ["css/component.css"],
      "run_at": "document_idle"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["js/component.js", "popup.html", "options.html", "images/insertToScript/*"],
      "matches": ["<all_urls>"]
    }
  ]
}
