---
name: 'Carouge | Streamline reselling: save, create, and cross-list in seconds.'
version: 1.2.5
category_slug: lifestyle/shopping
rating: 5
rating_count: 2
user_count: 20
release_date: '2024-12-02T08:11:06.000Z'
size: 1.3MiB
languages:
  - English
description: Post listings to Poshmark and other reseller platforms from Carouge
publisher_account: Carouge
extension_website: carouge.app
privacy_policy: https://www.carouge.app/privacy-policy
---

{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "default_locale": "en",
  "name": "__MSG_extensionName__",
  "version": "1.2.5",
  "description": "__MSG_extensionDescription__",
  "permissions": [
    "tabs",
    "storage",
    "activeTab"
  ],
  "options_page": "options/index.html",
  "background": {
    "service_worker": "background.iife.js",
    "type": "module"
  },
  "action": {
    "default_popup": "popup/index.html",
    "default_icon": "icon-34.png"
  },
  "icons": {
    "128": "icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://poshmark.com/create-listing",
        "https://poshmark.ca/create-listing"
      ],
      "js": [
        "content/poshmark/poshmark.iife.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "https://www.depop.com/products/create/"
      ],
      "js": [
        "content/depop/depop.iife.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "floating-button/index.iife.js"
      ],
      "css": [
        "floating-button/style.css"
      ],
      "run_at": "document_end"
    }
  ],
  "devtools_page": "devtools/index.html",
  "web_accessible_resources": [
    {
      "resources": [
        "*.js",
        "*.css",
        "*.svg",
        "icon-128.png",
        "icon-34.png"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "externally_connectable": {
    "matches": [
      "http://localhost:3000/*",
      "https://localhost:3000/*",
      "https://www.carouge.app/*",
      "https://carouge.app/*"
    ]
  },
  "side_panel": {
    "default_path": "side-panel/index.html"
  }
}