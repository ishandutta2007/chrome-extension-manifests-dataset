{
"update_url": "https://clients2.google.com/service/update2/crx",

    "name": "Search Domain Blocker™",
    "description": "Filter specific domains from google search results.",
    "version": "1.0",
    "manifest_version": 3,
    "content_scripts" : [
      {
        "matches": ["https://www.google.com/search*"],
        "js": ["content.js"],
        "css": ["style.css"],
        "run_at": "document_end"
      }
    ],
    "action": {
      "default_popup": "popup.html"
    },
    "icons": {
     "128": "icon128.png"
    },
    "permissions": ["storage", "scripting"]
  }