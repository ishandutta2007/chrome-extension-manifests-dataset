{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "PAM Message Sender",
  "version": "1.2",
  "description": "Özelleştirilmiş WhatsApp mesaj gönderim Chrome eklentisi.",
  "permissions": [
    "activeTab",
    "tabs",
    "scripting",
    "storage",
    "notifications"
  ],
  "host_permissions": [
    "https://web.whatsapp.com/*",
    "https://panelpam.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_popup_width": 400,
    "default_popup_height": 600
  },
  "content_scripts": [
    {
      "matches": [
        "https://web.whatsapp.com/*",
        "https://panelpam.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "vendor/bootstrap/dist/css/bootstrap.min.css",
        "vendor/bootstrap/dist/js/bootstrap.bundle.min.js"
      ],
      "matches": ["<all_urls>"]
    }
  ]
}
