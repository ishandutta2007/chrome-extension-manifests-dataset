{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "eesel AI: ChatGPT Sidebar for AI in Google Docs, Zendesk & more",
    "description": "Make AI bots that know your docs (like Google Docs, Confluence, Notion) and assist in your helpdesk (like Zendesk, Freshdesk).",
    "version": "2.5",
    "permissions": [
      "scripting",
      "tabs",
      "tabGroups",
      "storage",
      "sidePanel",
      "activeTab"
    ],
    "host_permissions": [
      "<all_urls>"
    ],
    "background": {
      "service_worker": "background.bundle.js"
    },
    "action": {
      "default_title":"eesel AI",
      "default_icon": {
        "16": "./assets/eesel-browser-icon-16.png",
        "24": "./assets/eesel-browser-icon-24.png",
        "32": "./assets/eesel-browser-icon-32.png",
        "48": "./assets/eesel-browser-icon-48.png",
        "96": "./assets/eesel-browser-icon-96.png"
      }
    },
    "icons": {
      "48": "./assets/eesel-browser-icon-48.png",
      "128": "./assets/eesel-browser-icon-128.png"
    },
    "side_panel": {
      "default_path": "sidepanel.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.bundle.js"]
      }
    ],
    "web_accessible_resources": [
      {
        "resources": ["*.bundle.js", "*.woff2", "assets/green_tick.ico", "assets/warning.png"],
        "matches": ["<all_urls>"]
      }
    ],
    "options_page": "onboarding.html"
}