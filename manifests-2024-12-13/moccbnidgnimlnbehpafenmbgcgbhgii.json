  "name": "Fast Unsubscribe",
  "version": "1.0.0",
  "description": "A browser extension to quickly and easily unsubscribe from YouTube channels.",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/*"],
      "js": ["content.js"]
    }
