{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "Team Bookmarks",
  "description": "Bookmark extension for you & your team.",
  "default_locale": "en",
  "permissions": [
    "storage",
    "<all_urls>",
    "*://*/*"
  ],
  "icons": {
    "16": "favicon.png",
    "48": "favicon.png",
    "128": "favicon.png"
  },
  "omnibox": {
    "keyword": "bm"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Open Team Bookmarks",
    "default_icon": {
      "19": "favicon.png",
      "38": "favicon.png"
    }
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+E",
        "mac": "Command+Shift+E"
      }
    }
  },
  "version": "0.2.7"
}