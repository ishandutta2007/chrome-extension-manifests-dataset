{
"update_url": "https://clients2.google.com/service/update2/crx",

  "icons": {
    "16": "/src/images/iconMagenta.png",
    "48": "/src/images/iconMagenta.png",
    "128": "/src/images/iconMagenta.png"
  },
  "action": {
    "default_icon": {
      "38": "/src/images/iconMagenta.png"
    },
    "default_title": "Save to iMembr"
  },
  "manifest_version": 3,
  "name": "iMembr",
  "description": "Bookmarker and highlighter for your preferred online content",
  "version": "1.22",
  "host_permissions": ["https://*/*"],
  "permissions": ["storage", "contextMenus", "tabs"],
  "externally_connectable": {
    "matches": ["*://imembr.com/*", "*://www.imembr.com/*", "*://localhost/*"]
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": [
        "custom-elements.min.js",
        "content.js",
        "highlighterContent.js",
        "highlighter.js"
      ],
      "css": ["/src/css/content.css", "styles.css"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "/src/images/favicon-16x16.png",
        "/src/images/favicon-32x32.png",
        "/src/images/android-chrome-192x192.png",
        "/src/images/android-chrome-192x192.png",
        "/src/images/iconMagentaFilled.png",
        "/src/images/iconMagenta.png"
      ],
      "matches": ["<all_urls>"]
    }
  ]
}
