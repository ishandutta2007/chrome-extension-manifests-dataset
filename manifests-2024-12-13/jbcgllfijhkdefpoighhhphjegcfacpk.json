{
"update_url": "https://clients2.google.com/service/update2/crx",

    "icons": {
        "16": "images/icon.png",
        "48": "images/icon.png",
        "128": "images/icon.png"
    },
    "manifest_version": 3,
    "name": "DevDex",
    "description": "Support Engineer for your every browser tab",
    "version": "0.0.2",
    "permissions": [
        "activeTab",
        "sidePanel",
        "storage",
        "unlimitedStorage"
    ],
    "host_permissions": [
        "https://*/*",
        "http://localhost:4000/*",
        "https://extension.devdex.ai/*"
    ],
    "background": {
        "service_worker": "/background.js"
    },
    "content_scripts": [
        {
            "matches": [
                "https://*/*",
                "http://localhost:4000/*"
            ],
            "js": [
                "/content.js"
            ]
        },
        {
            "js": [
                "/web-app-content.js"
            ],
            "matches": [
                "http://localhost:4000/*",
                "https://extension.devdex.ai/*"
            ]
        }
    ],
    "action": {
        "default_title": "Click to open Dex",
        "default_icon": {
            "16": "images/icon.png",
            "48": "images/icon.png",
            "128": "images/icon.png"
        }
    },
    "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'self'"
    },
    "side_panel": {
        "default_path": "src/popup/index.html"
    },
    "web_accessible_resources": [
        {
            "matches": [
                "<all_urls>"
            ],
            "resources": [
                "images/*"
            ]
        }
    ]
}