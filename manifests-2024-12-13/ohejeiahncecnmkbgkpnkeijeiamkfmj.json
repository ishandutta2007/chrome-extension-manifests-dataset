  "manifest_version": 3,
  "name": "__MSG_appName__",
  "description": "__MSG_appDesc__",
  "version": "0.1.3",
  "default_locale": "en",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "sidePanel",
    "storage",
    "scripting"
  ],
  "host_permissions": [
    "https://chatgpt.com/*",
    "https://eutils.ncbi.nlm.nih.gov/*",
    "https://www.ncbi.nlm.nih.gov/*",
    "https://pubmed.ncbi.nlm.nih.gov/*"
  ],
  "background": {
    "service_worker": "background/background.js"
  },
  "options_ui": {
    "page": "options/options.html",
    "open_in_tab": true,
    "css": [
      "options/options.css"
    ]
  },
  "action": {},
  "content_scripts": [
    {
      "matches": [
        "https://chatgpt.com/*",
        "https://www.ncbi.nlm.nih.gov/*"
      ],
      "js": [
        "content-scripts/mainUI.js"
      ]
    }
  ],
  "side_panel": {
    "default_path": "sidepanel-tab.html"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "content-scripts/mainUI.css",
        "icons/icon48.png",
        "leave_review.png",
        "side_panel.css",
        "scripts.js",
        "ncbi.css",
        "tailwind.min.css"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}
