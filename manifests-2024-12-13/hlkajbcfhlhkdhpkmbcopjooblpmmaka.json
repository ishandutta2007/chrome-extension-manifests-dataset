{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "ARCON Browser Extension",
  "version": "1.0.7",
  "manifest_version": 3,
  "description": "Arcon Extensions",
  "background": {
    "service_worker": "./dist/background.bundle.js"
  },
  "action": {
    "default_popup": "popups/popup.html"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "./dist/arconPlay.bundle.js"
      ],
      "match_about_blank": true,
      "matches": [
        "http://*/*",
        "https://*/*",
        "file://*/*"
      ],
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "tabs",
    "proxy",
    "webRequest",
    "webRequestAuthProvider",
    "privacy",
    "downloads",
    "storage",
    "unlimitedStorage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "icons": {
    "128": "./images/arconLogo128.png",
    "16": "./images/arconLogo16.png",
    "32": "./images/arconLogo32.png",
    "48": "./images/arconLogo48.png"
  },
  "externally_connectable": {
    "matches": [
      "<all_urls>"
    ]
  }
}