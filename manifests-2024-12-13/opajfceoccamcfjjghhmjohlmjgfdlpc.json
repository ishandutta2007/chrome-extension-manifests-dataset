{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "version": "1.0",
  "name": "Remove Moodle ForceDownloads",
  "content_scripts": [
    {
      "matches": [
        "https://*/mod*"
      ],
      "js": [
        "main.js"
      ]
    }
  ],
  "icons": {
    "128": "icon.png"
  }
}