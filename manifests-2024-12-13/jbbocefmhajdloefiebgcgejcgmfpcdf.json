  "manifest_version": 3,
  "name": "Hide Page",
  "version": "1.0",
  "description": "Blacks out the page and hides the URL when a specific key combination or panic mode is activated.",
  "permissions": ["activeTab", "storage", "commands", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "19": "icons/icon19.png",
      "32": "icons/icon32.png",
      "38": "icons/icon38.png",
      "48": "icons/icon48.png",
