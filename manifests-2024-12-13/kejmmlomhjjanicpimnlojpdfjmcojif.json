{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "Taboover",
  "version": "1.0.0",
  "description": "Taboover allows you to manage your inactive tabs and keep your browser organised!",

  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },

  "browser_action": {
    "icons": {
      "128": "./images/logo-large-128"
    },
    "default_icon": {
      "16": "./images/logo.png",
      "32": "./images/logo.png"
    },
    "default_popup": "popup.html",
    "default_title": "Check to see if our code is on this page"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": ["tabs", "storage"]
}
