{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "Risk Reminder Remover",
    "version": "1.4",
    "description": "Extension to remove Risk/Warm Reminder on various Chinese shopping agents",
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "host_permissions": [
        "https://*.cnfans.com/*",
        "https://*.mulebuy.com/*",
        "https://*.joyabuy.com/*",
        "https://*.orientdig.com/*",
        "https://*.oopbuy.com/*",
        "https://*.hoobuy.com/*"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icon16.png",
            "48": "icon48.png",
            "128": "icon128.png"
        }
    },
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://*.cnfans.com/*",
                "https://*.mulebuy.com/*",
                "https://*.joyabuy.com/*",
                "https://*.orientdig.com/*",
                "https://*.oopbuy.com/*",
                "https://*.hoobuy.com/*"               
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}