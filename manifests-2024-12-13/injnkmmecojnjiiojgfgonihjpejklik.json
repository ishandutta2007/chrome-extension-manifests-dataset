{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Zoom Contact Center CTI Extension",
  "description": "This extension resolves issues with Zoom Contact Center in multi-tab scenarios due to the browser’s third-party storage policy.",
  "version": "1.0",
  "permissions": ["tabs", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.zoom.us/cci/callbar/*",
        "https://*.zoomdev.us/cci/callbar/*",
        "https://zoom.us/cci/callbar/*",
        "https://zoomdev.us/cci/callbar/*",
        "https://zoomgovdev.com/cci/callbar/*",
        "https://zoomgov.com/cci/callbar/*",
        "https://www.zoomgovdev.com/cci/callbar/*",
        "https://www.zoomgov.com/cci/callbar/*"
      ],
      "js": ["content.js"],
      "all_frames": true,
      "run_at": "document_start"
    }
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "externally_connectable": {
    "matches": [
      "https://*.zoom.us/cci/callbar/*",
      "https://*.zoomdev.us/cci/callbar/*",
      "https://zoom.us/cci/callbar/*",
      "https://zoomdev.us/cci/callbar/*",
      "https://zoomgovdev.com/cci/callbar/*",
      "https://zoomgov.com/cci/callbar/*",
      "https://www.zoomgovdev.com/cci/callbar/*",
      "https://www.zoomgov.com/cci/callbar/*"
    ]
  },
  "web_accessible_resources": [
    {
      "resources": ["script.js"],
      "matches": ["<all_urls>"]
    }
  ]
}
