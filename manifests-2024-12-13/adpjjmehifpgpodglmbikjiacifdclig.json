    "512": "assets/icon-512.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": ["<all_urls>"],
  "content_scripts": [
    {
      "css": ["main.css"],
      "js": ["main.js", "vendor.js"],
