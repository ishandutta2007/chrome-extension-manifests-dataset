{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "GTMFixer - Google Tag Manager Enhancements",
  "description": "Google Tag Manager enhancements for easier tag, trigger and variable organization.",
  "version": "1.0.6",
  "permissions": [
    "tabs",
    "scripting",
    "storage",
    "activeTab"
  ],
  "host_permissions": [
    "https://tagmanager.google.com/*"
    ,"https://tagassistant.google.com/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/gtmfixer16.png",
      "48": "images/gtmfixer48.png",
      "128": "images/gtmfixer128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources": [
    {
      "resources": [ 
        "images/add-to-tag.png",
      "images/appendID.png",
      "images/warning-icon.png",
      "images/tick-icon.png" ],
      "matches": [ 
        "https://tagmanager.google.com/*" ]
    }
  ],
  "content_scripts": [
    {
      "matches": [
        "https://tagmanager.google.com/*"
      ,"https://tagassistant.google.com/*"],
      "css": ["style.css"],
      "js": [ "tagID.js",
      "tagErrorHighlighter.js",
      "searchBoxes.js",
      "hideBuiltInVariables.js",
      "reorderTagParameters.js",
      "filterTagTriggerVariable.js",
      "eventNameChecker.js", 
      "hideContainerQuality.js"
    ],
      "run_at": "document_idle"
    }
  ]
}