{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "SiteBlockade",
  "description": "SiteBlockade is a powerful Chrome extension that enables you to easily restrict access to specific websites.",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "options_page": "options.html",
  "permissions": [
    "storage",
    "history",
    "alarms",
    "tabs",
    "declarativeNetRequest",
    "declarativeNetRequestWithHostAccess",
    "declarativeNetRequestFeedback"
  ],
  "host_permissions": [
    "*://*/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
        "16": "icon/16x16.png",
        "32": "icon/32x32.png",
        "48": "icon/48x48.png",
        "128": "icon/128x128.png",
        "192": "icon/192x192.png"
    }
  },
  "icons": {
    "16": "icon/16x16.png",
    "32": "icon/32x32.png",
    "48": "icon/48x48.png",
    "128": "icon/128x128.png",
    "192": "icon/192x192.png"
  }
}
