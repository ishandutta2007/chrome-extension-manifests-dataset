{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Spinnin Beans™ Crates.co Track Extractor",
  "version": "1.2",
  "description": "Extract Beatport track URLs from a Crates.co crate",
  "permissions": ["activeTab"],
  "host_permissions": ["https://crates.co/*"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/logo16.png",
      "32": "images/logo32.png",
      "48": "images/logo48.png"
    }
  },
  "icons": {
    "16": "images/logo16.png",
    "32": "images/logo32.png",
    "48": "images/logo48.png",
    "128": "images/logo128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://crates.co/*"],
      "js": ["contentScript.js"]
    }
  ]
}
