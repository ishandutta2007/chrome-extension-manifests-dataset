  "manifest_version": 3,
  "name": "Amazon ASIN Extractor",
  "version": "1.0",
  "description": "Extracts ASINs from Amazon product pages and allows copying them.",
  "permissions": ["activeTab"],
  "host_permissions": ["https://www.amazon.es/*"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://www.amazon.es/*"],
      "js": ["content.js"]
    }
  ]
