  "manifest_version": 2,

  "name": "EasySpeak",
  "description": "This extension will allow users to speak into the website, a new way to interact.",
  "version": "1.5",

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"  
   ],
    "background": [
        {
            "scripts": ["background.js"]
        }
