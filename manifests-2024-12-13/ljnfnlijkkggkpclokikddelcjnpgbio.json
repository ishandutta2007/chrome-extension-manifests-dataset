{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Meet Reactions Shortcuts",
  "version": "4.1",
  "author": "Marcelo Sabino",
  "description": "Add keyboard shortcuts for Google Meet reactions.",
  "default_locale": "en",
  "content_scripts": [
    {
      "matches": ["https://meet.google.com/*"],
      "js": ["content.js"]
    }
