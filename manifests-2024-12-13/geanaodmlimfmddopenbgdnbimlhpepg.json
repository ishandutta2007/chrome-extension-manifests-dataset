  "content_scripts": [
    {
      "matches": [
        "http://localhost:8080/*",
        "http://localhost:8081/*",
        "https://*.webtraffic.exchange/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "externally_connectable": {
    "matches": [
      "*://localhost/*",
      "http://localhost:8080/*",
      "http://localhost:8081/*",
      "*://*.webtraffic.exchange/*"
    ]
  },
  "web_accessible_resources": [
