      ],
      "js": [
          "jquery.min.js", 
          "courses.js"
      ]
    }
  ],
  "web_accessible_resources": [
      "jquery-2.0.3.min.map",
      "options.html"
  ],
  "permissions": [
      "storage",
      "http://www.kurser.dtu.dk/*", 
