{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Aavaz DataConnect",
  "version": "1.4",
  "description": "Utility to ease the task of uploading contact data into Aavaz from various sites",
  "manifest_version": 3,
  "author": "Aavaz Contact Center Solutions",
  "action": {
    "default_title": "Aavaz DataConnect",
    "default_popup": "popup.html" 
  },
  "permissions": [
    "tabs",
    "storage",
    "scripting",
    "activeTab",
    "cookies"
  ],
  "host_permissions": [
    "https://recruit.naukri.com/*",
    "https://employers.indeed.com/*",
 "https://www.linkedin.com/hiring/jobs/*"
],
 "options_page": "options.html",
 "content_security_policy": {
  "directives": {
    "defaultSrc": ["'self'"],
    "scriptSrc": ["'script-src-self'", "https://code.jquery.com", "https://code.jquery.com/jquery-3.6.0.min.js"],
    "connectSrc": ["'self'", "https://cdn.datatables.net"],
    "styleSrc": ["'self'", "fonts.googleapis.com", "'unsafe-inline'"],
    "fontSrc": ["'self'", "popup.html"]
}
},
 "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["javascript/datasource/naukri.js"],
      "run_at": "document_end",
      "all_frames": true
    }
  ],  
  "icons": {
    "16": "/images/icon.png",
    "32": "/images/icon.png",
    "48": "/images/icon.png",
    "128": "/images/icon.png"
  }
}
