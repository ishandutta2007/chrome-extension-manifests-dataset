{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "__MSG_ext_name__",
  "description": "__MSG_ext_description__",
  "default_locale": "en",
  "version": "1.0.0",
  "permissions": [
    "storage",
    "tabs",
    "activeTab"
  ],
  "action": {
    "default_title": "__MSG_ext_name__",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "icons": {
    "16": "img/icon16.png",
