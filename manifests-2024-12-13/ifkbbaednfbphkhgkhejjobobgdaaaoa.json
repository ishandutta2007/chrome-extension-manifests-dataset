  },
  "web_accessible_resources": [
    {
      "resources": [
        "img/icon48.png",
        "img/icon128.png"
      ],
      "matches": [
        "*://*/*"
      ]
    }
  ],
  "content_scripts": [
    {
      "js": [
        "js/copy.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "background": {
    "service_worker": "js/background.js",
    "type": "module"
  }
