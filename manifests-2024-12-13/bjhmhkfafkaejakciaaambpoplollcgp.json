 "manifest_version": 3,
 "permissions": [
  "storage",
  "activeTab"
 ],
 "web_accessible_resources": [
  {
    "resources": [ "config.json" ],
    "matches": [ "https://*.crm4.dynamics.com/*" ]
  }
   ],
 "content_scripts": [
  {
   "run_at": "document_end",
   "matches": [ "https://*.crm4.dynamics.com/*" ],
   "js": [ "set_style.js", "popup.js" ]
  }
 ],
 "action": {
  "default_popup": "popup.html"
 }
