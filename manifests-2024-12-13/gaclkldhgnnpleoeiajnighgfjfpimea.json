{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "BrandPop",
  "description": "Automatically extract website branding",
  "version": "1.2",
  "permissions": ["scripting", "activeTab", "sidePanel", "webNavigation", "fontSettings"],
  "host_permissions": ["*://*/*"],
  "icons": {
    "128": "./images/papermillapp_logo.png"
  },
  "side_panel": {
    "default_path": "index.html"
  },
  "action": {
    "default_title": "Click to open BrandPop"
  },
  "background": {
     "service_worker": "./assets/service-worker.js"
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["./assets/content-script.js"],
    "all_frames": true,
    "run_at": "document_end"
  }]
}