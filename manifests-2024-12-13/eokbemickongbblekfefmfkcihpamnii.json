  "background": {
    "scripts": ["backgroundPage.js"],
    "persistent": false
  },
  "content_scripts": [{
      "matches": ["http://github.com/*", "https://github.com/*"],
      "js": ["jquery-2.1.0.min.js", "content.js"]
  }],
  "permissions": [
    "clipboardWrite",
    "http://github.com/",
    "https://github.com/"
  ],
  "web_accessible_resources": [
    "images/*.png"
  ]
}
