{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "PCS Screenpop for StratusLive",
  "description": "Screenpops StratusLive when a phone call is received.",
  "version": "1.1",
  "icons": {
    "16": "img/icon-16.png",
    "32": "img/icon-32.png",
    "48": "img/icon-48.png",
    "96": "img/icon-96.png"
  },

  "permissions": [
    "storage",
    "scripting"
  ],
  "host_permissions": [
    "https://*.stratuslive.com/*"
  ],

  "background": {
    "service_worker": "serviceworker.js",
    "type": "module"
  },
  "minimum_chrome_version": "92",
    
  "content_scripts": [
    {
      "matches": ["https://*.stratuslive.com/*"],
      "js": ["common/content/content.js"],
      "run_at": "document_end"
    }
  ],
  
  "action": {
    "default_icon": {
      "16": "img/button-inactive-16.png",
      "32": "img/button-inactive-32.png",
      "48": "img/button-inactive-48.png",
      "96": "img/button-inactive-96.png"
    },
    "default_title": "PCS Screenpop for StratusLive",
    "default_popup": "common/config/configuration.html#phone",
    "browser_style": true,
    "chrome_style": true
  },
  "web_accessible_resources": [
    {
      "resources": [
        "common/content/main.js",
        "common/content/node.js",
        "common/util.js"
      ],
      "matches": [ "https://*.stratuslive.com/*" ]
    }
  ]
}
