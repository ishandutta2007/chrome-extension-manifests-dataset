{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Social Souls",
  "version": "1.0",
  "description": "Souls-like notifications for your social actions",
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "*://mail.google.com/*",
    "*://www.youtube.com/*",
