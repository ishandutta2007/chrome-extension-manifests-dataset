    "*://*.reddit.com/*"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "*://mail.google.com/*",
        "*://www.youtube.com/*",
        "*://dtf.ru/*",
        "*://*.reddit.com/*"
      ],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "bridge.js", 
        "OptimusPrinceps.ttf",
        "sounds/victory.mp3",
        "sounds/death.mp3",
        "sounds/neutral.mp3"
      ],
      "matches": ["<all_urls>"]
    }
  ]
