{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Zypher",
  "description": "Your AI companion.",
  "version": "2.0.1",
  "manifest_version": 3,
  "action": {
    "default_icon": "/assets/icons/ZYPHER_ICON.png",
    "default_title": "Click me!"
  },
  "icons": {
    "16": "/assets/icons/ZYPHER_ICON.png",
    "32": "/assets/icons/ZYPHER_ICON.png",
    "48": "/assets/icons/ZYPHER_ICON.png",
    "128": "/assets/icons/ZYPHER_ICON.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["activeTab", "tabs"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "run_at": "document_end",
      "js": [
        "popper.min.js",
        "bootstrap.min.js",
        "custom-elements.min.js",
        "zypher-input-assistant-button.js",
        "zypher-summarize-button.js",
        "zypher-text-selection-assistant.js",
        "content.js"
      ],
      "css": ["content.css"]
    },
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["popper.min.js", "bootstrap.min.js", "youtube.js"]
    },
    {
      "matches": ["*://mail.google.com/*"],
      "js": ["popper.min.js", "bootstrap.min.js", "gmail.js"]
    },
    {
      "matches": ["*://outlook.office.com/mail/*", "*://outlook.office365.com/mail/*"],
      "js": ["popper.min.js", "bootstrap.min.js", "outlook.js"]
    },
    {
      "matches": ["*://*.linkedin.com/*"],
      "js": ["popper.min.js", "bootstrap.min.js", "linkedin.js"]
    },
    {
      "matches": ["*://*.facebook.com/*"],
      "js": ["popper.min.js", "bootstrap.min.js", "facebook.js"]
    }
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "popper.min.js",
        "bootstrap.min.js",
        "index.html",
        "toolbar.html",
        "toolbar.scss",
        "toolbar.js",
        "/assets/icons/ZYPHER_ICON.png",
        "/assets/icons/ZYPHER_ICON.svg",
        "main.ts",
        "linkedin.js",
        "facebook.js"
      ],
      "matches": ["<all_urls>"]
    }
  ]
}
