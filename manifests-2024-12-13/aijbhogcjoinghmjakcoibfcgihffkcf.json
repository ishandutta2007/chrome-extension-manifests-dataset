{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "1Kleek AI Assistant",
  "version": "1.2",
  "description": "A Chrome extension that'll improve your productivity using AI.",
  "permissions": ["activeTab", "scripting", "storage"],
  "background": {
    "service_worker": "assets/js/background.js"
  },
  "options_page": "options.html",
  "commands": {
    "open_ai_panel": {
      "suggested_key": {
        "default": "Ctrl+Shift+P",
        "mac": "Shift+Command+P"
      },
      "description": "Open the AI panel"
    }
  },
  "icons": {
    "48": "icon48.png"
  },
  "action": {
    "default_icon": "icon48.png",
    "default_popup": "popup.html",
    "default_title": "My AI assistant"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["assets/js/utils.js", "content.js"],
      "css": ["assets/css/custom.css"],
      "type": "module"
    }
  ],
  "host_permissions": ["https://1.kleek.fr/*"],
  "web_accessible_resources": [
    {
      "resources": ["assets/icons/*", "utils.js"],
      "matches": ["<all_urls>"]
    }
  ]
}
