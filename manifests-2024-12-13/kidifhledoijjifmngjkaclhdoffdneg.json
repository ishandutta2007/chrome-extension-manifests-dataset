{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Distill",
  "description": "Research companies and people.",
  "version": "1.2.3",
  "action": {
    "default_icon": {
      "16": "icons/16.png",
      "32": "icons/32.png"
    },
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "js/background.js"
  },
  "permissions": [
    "activeTab",
    "storage",
    "scripting",
    "cookies"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.linkedin.com/*"
      ],
      "js": [
        "js/content_script_li.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "https://mail.google.com/*"
      ],
      "js": [
        "js/content_script_gmail.js"
      ],
      "run_at": "document_end"
    }
  ],
  "host_permissions": [
    "https://*.linkedin.com/*",
    "https://*.distill.fyi/*",
    "https://*.google.com/*"
  ],
  "optional_host_permissions": [
    "*://*/*"
  ],
  "externally_connectable": {
    "matches": [
      "https://distill.fyi/*"
    ]
  },
  "options_page": "options.html",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "icons/16.png",
        "icons/32.png",
        "icons/48.png",
        "icons/128.png",
        "icons/x-thin.svg",
        "sidebar.html"
      ],
      "matches": [
        "https://*.google.com/*",
        "https://*.linkedin.com/*"
      ]
    }
  ]
}