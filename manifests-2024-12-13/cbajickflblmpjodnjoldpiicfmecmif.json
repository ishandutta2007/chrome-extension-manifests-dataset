{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "__MSG_name__",
  "description": "__MSG_desc__",
  "default_locale": "en",
  "version": "0.0.5",
  "permissions": [
    "proxy",
    "webRequest",
    "webRequestAuthProvider",
    "storage",
    "offscreen",
    "geolocation",
    "alarms"
  ],
  "host_permissions": [
    "*://*/*"
  ],
  "icons": {
    "64": "assets/icons/logo.png"
  },
  "background": {
    "service_worker": "sw.min.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content.min.js"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "js/content.min.js",
        "helper.js",
        "offscree.js",
        "offscreen.html",
        "assets/icons/*",
        "logo.png"
      ],
      "matches": [
        "*://*/*"
      ]
    }
  ]
}
