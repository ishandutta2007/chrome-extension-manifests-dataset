{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "YouTube Video Bookmarker",
  "version": "1.0.3",
  "description": "A Chrome extension to bookmark timestamps in YouTube videos.",
  "permissions": ["activeTab", "storage"],
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    },
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["load_css.js"],
      "run_at": "document_end"
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon128.png"
  },
   "icons":{
    "16": "icon128.png",
    "48": "icon128.png",
    "128": "icon128.png"
  },
  "web_accessible_resources": [
    {
        "resources": ["styles.css","icon.png"],
        "matches": ["*://*.youtube.com/*"]
    }
  ],
  "content_security_policy": {
    "extension_pages": "default-src 'none'; script-src 'self'; style-src 'self'; require-trusted-types-for 'script';base-uri 'none';"
  }

}
