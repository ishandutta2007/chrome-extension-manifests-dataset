{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Summary Master",
  "version": "0.1",
  "web_accessible_resources": [
    {
      "resources": ["content-summary/content-summary.css", "images/icon128.png", "_locales/*"],
      "matches": ["<all_urls>"]
    }
  ],
  "description": "Generate summaries of web content using GPT API.",
  "permissions": ["contextMenus", "activeTab", "storage"],
  "action": {
    "default_popup": "./popup/popup.html",
    "default_icon": {
      "24": "images/icon24.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "host_permissions": [
    "https://us-central1-summary-ext.cloudfunctions.net/*"
  ],
  "background": {
    "service_worker": "dist/background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./dist/contentSummary.js", "./dist/gpt.js"]
    }
  ],
  "icons": {
    "24": "images/icon24.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "default_locale": "en"
}
