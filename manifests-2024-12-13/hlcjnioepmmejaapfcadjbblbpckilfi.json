    "type": "module"
  },
  "web_accessible_resources": [
    {
      "matches": ["https://www.youtube.com/*"],
      "resources": ["assets/*"]
    }
  ],
  "permissions": ["activeTab", "scripting", "storage"],
  "host_permissions": ["https://www.youtube.com/*"]
