{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "YouTube Toolkit",
  "version": "2.0.3",
  "description": "Easily sync custom subtitles, manage playlists, take notes, and access transcripts.",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
