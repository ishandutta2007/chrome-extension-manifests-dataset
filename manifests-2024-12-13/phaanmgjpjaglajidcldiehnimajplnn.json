{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Digital Accessibility Report and Test Suite",
  "version": "2.16.0",
  "short_name": "DARTS",
  "author": "Accessibility Officer",
  "description": "Easier, faster, smarter WCAG accessibility testing, reporting, & ADA compliance management. Screen reader accessible. Built by PWD",
  "background": {
    "service_worker": "ext/background.js"
  },
  "action": {
    "default_popup": "popup/popup.html"
  },
  "host_permissions": ["<all_urls>"],
  "permissions": ["storage", "activeTab", "scripting", "contextMenus", "cookies"],
  "content_scripts": [
    {
      "matches": ["https://*/*", "http://*/*"],
      "js": ["ext/jquery-3.7.1.js", "ext/jquery-ui.js", "ext/content.js"],
      "run_at": "document_idle"
    },
    {
      "matches": ["https://*/*", "http://*/*"],
      "js": ["ext/axe.js", "ext/configutrAxe.js"],
      "run_at": "document_idle",
      "all_frames": true
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["index.html", "css/text-spacing.css", "ext/axe.js"],
      "matches": ["https://*/*", "http://*/*"]
    }
  ],
  "commands": {
    "open--app": {
      "suggested_key": {
        "default": "Ctrl+Shift+E",
        "mac": "MacCtrl+Shift+E"
      },
      "description": "Add Issue"
    },
    "login--app": {
      "suggested_key": {
        "default": "Ctrl+Shift+L",
        "mac": "MacCtrl+Shift+L"
      },
      "description": "Login"
    }
  },
  "icons": {
    "16": "Assets/16X16.png",
    "32": "Assets/48X48.png",
    "48": "Assets/48X48.png",
    "128": "Assets/128X128.png"
  }
