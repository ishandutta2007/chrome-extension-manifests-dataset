        }
    },
    "content_scripts": [
        {
            "matches": ["*://*.fab.com/*"],
            "js": ["content-script.js"]
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "icons/icon-48.png",
                "icons/icon-96.png",
                "content-style.css",
                "wishlist.js",
                "wishlist.html",
                "icons/loading.gif",
                "index.js",
