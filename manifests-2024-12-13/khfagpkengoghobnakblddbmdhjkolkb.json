{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "__MSG_collectDataCheck_kimi_title__",
    "description": "__MSG_collectDataCheck_kimi_description__",
    "version": "1.3.10",
    "default_locale": "zh_CN",
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    },
    "background": {
        "service_worker": "background.js",
        "type": "module"
    },
    "homepage_url": "https://note.eceg.shop/",
    "action": {
        "default_icon": {
            "16": "images/icon-16.png",
            "32": "images/icon-32.png",
            "48": "images/icon-48.png",
            "128": "images/icon-128.png"
        }
    },
    "permissions": [
        "activeTab",
        "scripting",
        "storage",
        "contextMenus",
        "tabs"
    ],
    "optional_host_permissions": [
        "<all_urls>"
    ],
    "options_ui": {
        "page": "data/options/index.html",
        "open_in_tab": true
    },
    "content_scripts": [
        {
            "js": [
                "providers/content_kimi.js"
            ],
            "matches": [
                "https://kimi.moonshot.cn/*",
                "https://chat.deepseek.com/*"
            ],
            "run_at": "document_start"
        },
        {
            "js": [
                "content_yt.js"
            ],
            "matches": [
                "*://www.youtube.com/*",
                "*://youtube.com/*"
            ],
            "run_at": "document_end"
        },
        {
            "js": [
                "providers/content_wanzhi.js"
            ],
            "matches": [
                "https://www.wanzhi.com/*",
                "https://wanzhi.com/*"
            ],
            "run_at": "document_start"
        },
        {
            "js": [
                "providers/content_chatglm.js"
            ],
            "matches": [
                "https://chatglm.cn/*",
                "https://www.chatglm.cn/*"
            ],
            "run_at": "document_start"
        },
        {
            "js": [
                "providers/content_deepSeek.js"
            ],
            "matches": [
                "https://chat.deepseek.com/*"
            ],
            "run_at": "document_start"
        },
        {
            "js": [
                "providers/content_tongyi.js"
            ],
            "matches": [
                "https://tongyi.aliyun.com/*"
            ],
            "run_at": "document_start"
        }
    ],
    "host_permissions": [
        "https://*.moonshot.cn/*",
        "https://*.volces.com/*",
        "https://*.deepseek.com/*",
        "https://*.youtube.com/*",
        "https://*.aliyun.com/*",
        "https://*.chatgpt.com/*",
        "https://*.openai.com/*",
        "https://*.chatgml.cn/*",
        "https://*.wanzhi.com/*",
        "https://*.aliyuncs.com/*"
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "content_youtube_inject.js"
            ],
            "matches": [
                "*://www.youtube.com/*",
                "*://youtube.com/*"
            ]
        },
        {
            "resources": [
                "images/loading-16x16.gif"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ],
    "commands": {
        "collectData": {
            "suggested_key": {
                "default": "Alt+N"
            },
            "description": "Note by Kimi"
        },
        "collectData2": {
            "suggested_key": {
                "default": "Shift+Alt+N"
            },
            "description": "Note by Kimi"
        }
    }
}
