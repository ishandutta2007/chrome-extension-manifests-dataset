{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Insuredmine Gmail Extension",
  "description": "The purpose of this extension is to boost the productivity of InsuredMine agents.",
  "version": "0.3",
  "content_scripts": [
    {
      "matches": [
        "https://mail.google.com/*"
      ],
      "js": ["content.js"],
      "css": ["content.css"],
      "run_at": "document_end"
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "cookies",
    "activeTab",
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "https://mail.google.com/*",
    "https://*.insuredmine.com/*",
    "https://insuredmine.com/*"
  ],
  "manifest_version": 3,
  "web_accessible_resources": [
    {
      "resources": ["*.map"],
      "matches": ["https://mail.google.com/*"]
    }
  ],
  "action": {
    "default_icon": {
      "16": "images/favicon16.png",
      "24": "images/favicon24.png",
      "32": "images/favicon32.png"
    },
    "default_title": "Click to view login Status",
    "default_popup": "popup/popup.html"
  }
}
