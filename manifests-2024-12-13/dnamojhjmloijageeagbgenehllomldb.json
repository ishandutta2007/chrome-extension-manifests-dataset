{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 2,
    "name": "Custom Code Injector",
    "version": "1.0.0",
    "minimum_chrome_version": "68",
    "description": "Run custom javascript and css in a webpage with ease",
    "author": "Charlton Cheng",
    "permissions": [
        "storage",
        "unlimitedStorage",
        "<all_urls>",
        "tabs",
