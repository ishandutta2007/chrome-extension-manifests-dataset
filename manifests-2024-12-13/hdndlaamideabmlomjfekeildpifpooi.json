{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Offboard",
  "description": "Set custom keyboard shortcuts for buttons or links on any website",
  "version": "0.0.1",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "run_at": "document_idle",
      "js": ["mousetrap.min.js", "css-selector-generator.min.js", "content.js"]
    }
  ],
  "permissions": [
    "storage", 
    "activeTab", 
    "scripting",
    "tabs"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Offboard",
    "default_icon": {
      "16": "/images/logo16.png",
      "32": "/images/logo32.png",
      "48": "/images/logo48.png",
      "128": "/images/logo128.png"
    }
  },
  "icons": {
    "16": "/images/logo16.png",
    "32": "/images/logo32.png",
    "48": "/images/logo48.png",
    "128": "/images/logo128.png"
  },
  "options_page": "options.html"
}