  "web_accessible_resources": [
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "_favicon/*",
        "index.html",
        "/assets/*",
        "sandbox.html",
        "assets/index-fe119258.js",
        "assets/contentscripts.js-594cf421.js"
      ],
      "use_dynamic_url": false
    }
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
