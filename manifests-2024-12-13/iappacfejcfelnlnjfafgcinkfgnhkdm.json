{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Valuecase Assistant",
  "version": "1.2.4",
  "permissions": [
    "activeTab",
    "storage",
    "scripting",
    "tabs",
    "sidePanel",
    "alarms"
  ],
  "action": {
    "default_path": "index.html",
    "default_icon": {
      "16": "images/16x16.png",
      "48": "images/48x48.png",
      "128": "images/128x128.png"
    }
  },
  "side_panel": {
    "default_path": "index.html"
  },
  "host_permissions": [
    "*://*.salesforce.com/*",
    "*://*.force.com/*",
    "*://*.lightning.force.com/*",
    "*://www.linkedin.com/*",
    "*://linkedin.com/*",
    "*://*.valuecase.com/*",
    "*://*.hubspot.com/*",
    "*://*/*"
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "*.html"
      ],
      "use_dynamic_url": false
    },
    {
      "matches": [
        "*://*.valuecase.com/*",
        "*://*.valuecase.com/*"
      ],
      "resources": [
        "assets/auth.tsx.js"
      ],
      "use_dynamic_url": false
    },
    {
      "matches": [
        "https://www.linkedin.com/*"
      ],
      "resources": [
        "assets/linkedin-contact-fetch.tsx.js"
      ],
      "use_dynamic_url": false
    },
    {
      "matches": [
        "*://*.force.com/*",
        "*://*.hubspot.com/*",
        "*://*.lightning.force.com/*",
        "*://*.salesforce.com/*"
      ],
      "resources": [
        "assets/integrations-fetch.tsx.js"
      ],
      "use_dynamic_url": false
    }
  ],
  "icons": {
    "16": "images/16x16.png",
    "48": "images/48x48.png",
    "128": "images/128x128.png"
  },
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "js": [
        "assets/auth.tsx-loader.js"
      ],
      "matches": [
        "*://*.valuecase.com/dashboard?extensionAuth=true",
        "*://*.valuecase.com/dashboard/?extensionAuth=true"
      ]
    },
    {
      "js": [
        "assets/linkedin-contact-fetch.tsx-loader.js"
      ],
      "matches": [
        "https://www.linkedin.com/*"
      ],
      "run_at": "document_idle"
    },
    {
      "js": [
        "assets/integrations-fetch.tsx-loader.js"
      ],
      "matches": [
        "*://*.salesforce.com/*",
        "*://*.force.com/*",
        "*://*.lightning.force.com/*",
        "*://*.hubspot.com/*"
      ]
    }
  ]
}
