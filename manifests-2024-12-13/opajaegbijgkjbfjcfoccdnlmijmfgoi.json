{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "ChatShape",
    "description": "ChatGPT for your web docs",
    "version": "0.0.0.3",
    "action": {},
    "background": {
        "service_worker": "js/backgroundPage.js"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "css": ["css/content.css"],
            "js": ["injectUI.js", "content.js"],
            "all_frames": false,
            "run_at": "document_idle"
        }
    ],
    "web_accessible_resources": [
        {
            "resources": ["content.html"],
            "matches": ["<all_urls>"]
        }
    ],
    "options_ui": { "chrome_style": false },
    "icons": {
        "16": "favicon-16.png",
        "48": "favicon-32.png",
        "128": "128.png"
    },
    "commands": {
        "_execute_action": {
            "suggested_key": {
                "default": "Ctrl+Shift+Y",
                "mac": "Command+Shift+Y",
                "linux": "Ctrl+Shift+Y"
            }
        }
    },
    "permissions": [
        "file://*/*",
        "activeTab",
        "scripting",
        "storage",
        "unlimitedStorage",
        "*://api.chatshape.com/*"
    ]
