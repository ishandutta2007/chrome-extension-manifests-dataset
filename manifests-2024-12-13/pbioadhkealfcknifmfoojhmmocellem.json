      "http://developers.facebook.com/*",
      "https://developers.facebook.com/*"
    ],
    "include_globs": [ "http://*.facebook.com/*", "https://*.facebook.com/*" ],
    "js": ["content.js" ],
    "matches": [ "http://*.facebook.com/*", "https://*.facebook.com/*" ],
    "run_at": "document_start"
   } ],
 "web_accessible_resources" : [
