{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "Viewdocs",
    "version": "1.0.7",
    "author": "erdichen@gmail.com",
    "description": "Open Google Docs in viewing mode",
    "icons": {
        "128": "images/icon_128.png"
    },
    "background": {
        "service_worker": "js/service_worker.js"
    },
    "action": {
        "default_icon": {
            "128": "images/icon_128.png"
        },
        "default_title": "Switch Docs editing/viewing mode"
    },
    "commands": {
        "toggle-edit-doc": {
            "suggested_key": {
                "default": "Ctrl+Shift+E",
                "mac": "Command+Shift+E"
            },
            "description": "Switch Docs editing/viewing mode"
        }
    },
    "content_scripts": [
        {
            "matches": [
                "https://docs.google.com/document/d/*"
            ],
            "run_at": "document_idle",
            "js": [
                "js/common.js",
                "js/content_script.js"
            ]
        }
    ],
    "host_permissions": [
        "https://docs.google.com/document/d/*"
    ],
    "permissions": [
        "contextMenus",
        "scripting",
        "storage",
        "tabs"
    ]
}
