{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Snap for Merch on Demand",
  "version": "2.4.3",
  "description": "Trimming, resizing, and converting designs into patterns, as well as transferring designs to the back of products on Merch on Demand",
  "permissions": [
    "scripting",
    "activeTab",
    "unlimitedStorage"
  ],
  "host_permissions": ["https://*.merch.amazon.com/*"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["*://merch.amazon.com/*"],
      "all_frames": true,
      "js": [
        "content.js",
        "contentcopyeditor.js",
        "contentguides.js",
        "contentnav.js",
        "contentdynamicalign.js"
        
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "Icon.png",
    "48": "Icon.png",
    "128": "Icon.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["assets/apply.svg", "assets/clear.svg", "assets/error.svg", "assets/draganddrop.svg", "assets/loaded.svg"],
      "matches": ["<all_urls>"]
    }
  ]
}
