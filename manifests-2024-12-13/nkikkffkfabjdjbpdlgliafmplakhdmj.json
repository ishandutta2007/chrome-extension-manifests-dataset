{
"update_url": "https://clients2.google.com/service/update2/crx",

    "name": "LabelColorCustomizer for Google Calendar",
    "description": "Extension to replace a default label color given by Google, replace it with pre curated color palettes",
    "version": "2.1",
    "manifest_version": 3,
    "action": {
      "default_popup": "main.html",
      "default_icon": "calendar_icon.png"
    },
    "icons": {
      "16": "calendar_icon_16.png",
      "32": "calendar_icon_32.png",
      "48": "calendar_icon_48.png",
      "128": "calendar_icon_128.png"
    },
    "permissions": [
      "storage"
    ],
    "content_scripts": [
        {
          "matches": [
            "https://www.google.com/calendar/*",
            "https://calendar.google.com/calendar/*"
        ],
          "js": ["background.js"],
          "run_at": "document_end"
        }
      ]
  }
