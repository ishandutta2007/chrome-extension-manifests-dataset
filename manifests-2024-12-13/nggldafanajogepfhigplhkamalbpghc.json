{
"update_url": "https://clients2.google.com/service/update2/crx",

  "version": "1.0.8",
  "manifest_version": 3,
  "name": "Navigate Rewards",
  "description": "Turn Your Web Browsing into Rewards!",
  "web_accessible_resources": [
    {
      "resources": [
        "images/overlay-icon.svg",
        "fonts/OldschoolGrotesk-NormalBlack.otf"
      ],
      "matches": ["<all_urls>"]
    }
  ],
  "host_permissions": [
    "<all_urls>",
    "https://dataquest.nvg8.io/*",
    "https://dev.socialsurfer.xyz/*",
    "https://api.socialsurfer.xyz/*",
    "https://api.dev.socialsurfer.xyz/*",
    "http://localhost:3000/*",
    "http://localhost:3001/*"
  ],
  "icons": {
    "128": "logo.png"
  },
  "permissions": ["tabs", "storage", "alarms", "windows"],
  "action": {
    "default_icon": {
      "16": "logo.png",
      "48": "logo.png",
      "128": "logo.png"
    },
    "default_popup": "index.html",
    "default_title": "Navigate Rewards"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["scripts/overlay.js"],
      "run_at": "document_start"
    },
    {
      "matches": ["https://dataquest.nvg8.io/*"],
      "js": ["scripts/dashboard/script.js"]
    },
    {
      "matches": ["http://localhost:3000/*"],
      "js": ["scripts/dashboard/script.js"]
    },
    {
      "matches": ["https://dev.socialsurfer.xyz/*"],
      "js": ["scripts/dashboard/script.js"]
    },
    {
      "matches": [
        "https://www.amazon.com/gp/css/order-history*",
        "https://www.amazon.com/your-orders/orders*"
      ],
      "js": ["scripts/amazon/script-a.js"],
      "run_at": "document_end"
    },
    {
      "matches": [
        "https://www.amazon.com/ap/signin*"
      ],
      "js": ["scripts/amazon/script-a-signin.js"],
      "run_at": "document_end"
    },
    {
      "matches": ["https://www.reddit.com/r/*/comments/*"],
      "js": ["scripts/reddit/script-r.js"],
      "run_at": "document_end"
    }
  ],
  "externally_connectable": {
    "matches": [
      "http://localhost:3000/*",
      "https://dataquest.nvg8.io/*",
      "https://dev.socialsurfer.xyz/*"
    ]
  }
}
