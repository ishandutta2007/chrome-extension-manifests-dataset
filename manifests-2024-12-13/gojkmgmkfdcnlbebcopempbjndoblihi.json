    "default_title": "Camo AI"
  },
  "icons": {
    "16": "images/camoai16.png",
    "48": "images/camoai48.png",
    "128": "images/camoai128.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["success.html", "signout.html", "images/camoai16.png"],
