        "scripts": [ "background.js" ],
        "persistent": true
  },
  "permissions": [
      "desktopCapture"
  ],
  "externally_connectable": {
      "matches": [
          "*://bubble.privus.network/*",
   "*://bubble.privus.io/*",
   "*://meet.privus.global/*"
      ]
