{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "InstaBoost by MyInstaFollow",
  "version": "1.0",
  "description": "Boost your Instagram followers instantly with MyInstaFollow.",
  "permissions": [
    "activeTab",      
    "storage",        
    "scripting"       
  ],
  "host_permissions": [
    "https://*.instagram.com/*", 
    "https://myinstafollow.com/*"
  ],
  "background": {
    "service_worker": "background.js" 
  },
  "content_scripts": [
    {
      "matches": ["https://*.instagram.com/*"], 
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "images/likebutton.png",
        "images/commentbutton.png",
        "images/viewsbutton.png",
        "images/impressionbutton.png",
        "images/followerbutton.png",
        "images/savebutton.png",
        "images/sharebutton.png"
      ],
      "matches": ["https://*.instagram.com/*"]
    }
  ],
  "action": {
    "default_popup": "popup.html", 
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}
