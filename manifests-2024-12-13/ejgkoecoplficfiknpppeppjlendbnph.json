{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Koala",
  "version": "0.2.0",
  "description": "Koala",
  "host_permissions": [
    "https://*.linkedin.com/*",
    "https://*.getkoala.com/*"
  ],
  "permissions": [
    "storage",
    "cookies",
    "tabs",
    "activeTab",
    "sidePanel"
  ],
  "background": {
    "service_worker": "src/pages/background/index.js",
    "type": "module"
  },
  "action": {
    "default_icon": "icon-34.png"
  },
  "icons": {
    "128": "icon-128.png"
  },
  "side_panel": {
    "default_path": "sidepanel.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.linkedin.com/*"
      ],
      "js": [
        "src/pages/content-scripts/linkedin/index.js",
        "src/pages/content-scripts/content/index.js"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "overlay.html",
        "*.js",
        "*.css",
        "*.svg",
        "icon-128.png",
        "icon-34.png"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}