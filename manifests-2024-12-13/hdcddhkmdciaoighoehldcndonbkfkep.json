{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "VoiceGPT",
  "description": "Add voice input functionality to ChatGPT, Press Ctrl+Q to open voice input",
  "version": "1.0.2",
  "permissions": ["commands"],
  "host_permissions": ["https://chatgpt.com/*"],
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "js": ["assets/content.js-loader-CN5WzRD6.js"],
      "matches": ["https://chatgpt.com/*"]
    }
  ],
  "action": {
    "default_popup": "index.html"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Q",
        "mac": "Command+K"
      },
      "description": "Open voice input"
    }
  },
  "web_accessible_resources": [
    {
      "matches": ["https://chatgpt.com/*"],
      "resources": [
        "src/permission.html",
        "src/requestPermission.js",
        "assets/content.js-x_j0QOnN.js"
      ],
      "use_dynamic_url": false
    }
  ]
}

