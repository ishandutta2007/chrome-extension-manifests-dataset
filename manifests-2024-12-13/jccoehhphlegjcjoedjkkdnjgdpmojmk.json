name: Caniuse Finder
version: 2.1.0
category_slug: productivity/developer
rating: null
rating_count: null
user_count: 15
release_date: '2024-12-10T19:41:28.000Z'
size: 16.35KiB
languages:
  - English
  - русский
  - українська
description: >-
  Extension shows info about supporting front-end web technologies in different
  browsers on caniuse.com site
publisher_account: thebestwebservices
---

{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name" : "__MSG_ext_name__",
  "description": "__MSG_ext_description__",
  "default_locale" : "en",
  "version": "2.1.0",
  "permissions": [
    "activeTab",
    "contextMenus"
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "img/icon48.png",
        "img/icon128.png"
      ],
      "matches": [
        "*://*/*"
      ]
    }
  ],

  "action": {
    "default_title": "__MSG_ext_name__"
  },

  "icons": {
    "16": "img/icon16.png",
    "32": "img/icon32.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },

  "content_scripts": [
    {
      "js": [
        "js/content.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],

  "background": {
    "service_worker": "js/background.js"
  }
}
