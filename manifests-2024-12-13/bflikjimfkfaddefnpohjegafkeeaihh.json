{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "AI Web Extractor",
    "version": "1.2",
    "description": "从任意网页中提取公司名、简介、地址、Email 和 WhatsApp，并支持批量下载到本地。",
    "permissions": ["storage"],
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["contentScript.js"],
            "run_at": "document_end"
        }
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icon16.png",
            "48": "icon48.png",
            "128": "icon128.png"
        }
    },
    "web_accessible_resources": [
        {
            "resources": ["icon16.png", "icon48.png", "icon128.png"],
            "matches": ["<all_urls>"]
        }
    ]
}
