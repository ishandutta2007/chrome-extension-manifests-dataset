{
"update_url": "https://clients2.google.com/service/update2/crx",

    "name": "Sodarr",
    "description": "Plugin to open files with xdg-open in sonrarr and radarr",
    "version": "1.0.10",
    "manifest_version": 3,
    "permissions": [],
    "host_permissions": [
      "*://*/"
    ],
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": ["contentscript.js"],
        "run_at": "document_end"
      }  
    ],
    "background": {
      "service_worker": "background.js",
      "type": "module"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "sodarr.png",
        "32": "sodarr.png",
        "48": "sodarr.png",
        "128": "sodarr.png"
      }
    },
    "icons": {
      "16": "sodarr.png",
      "32": "sodarr.png",
      "48": "sodarr.png",
      "128": "sodarr.png"
    },
    "web_accessible_resources": [
        {
          "resources": ["vlc.png", "folder.png", "loading.gif", "error.png"],
          "matches": ["<all_urls>"]
        }
    ]
  }
