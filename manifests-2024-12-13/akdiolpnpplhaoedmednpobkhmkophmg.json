      "js": ["src/content_script/gmgn.js"]
    },
    {
      "matches": ["https://dexscreener.com/*"],
      "js": ["src/content_script/dexscreener.js"]
    },
    {
      "matches": ["https://ape.pro/*"],
      "js": ["src/content_script/ape.js"]
