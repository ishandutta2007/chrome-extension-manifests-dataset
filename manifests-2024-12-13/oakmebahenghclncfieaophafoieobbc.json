{
"update_url": "https://clients2.google.com/service/update2/crx",

 "manifest_version": 2,
 "name": "gallery_plugin",
 "version": "1.0.0",
 "description": "__MSG_pluginDesc__",
 "icons": {
  "16": "img/icon.png",
  "48": "img/icon.png",
  "128": "img/icon.png"
 },
 "background": {
  "scripts": ["js/jquery-1.8.3.js", "js/background.js"]
 },
 "browser_action": {
  "default_icon": "img/icon.png",
  "default_title": "ies-hotsoon",
  "default_popup": "popup.html"
 },
 "content_scripts": [{
      "matches": [
        "http://s3.bytecdn.cn/*",
        "https://s3.bytecdn.cn/*",
        "https://hotsoon.snssdk.com/*",
        "http://hotsoon.snssdk.com/*",
        "https://i.snssdk.com/*",
        "http://i.snssdk.com/*",
        "https://ife.byted.org/*",
        "http://localhost:4000/*"
      ],
   "js": ["js/content-script.js"],
   "css": ["css/custom.css"],
      "run_at": "document_start",
      "all_frames": true
  }, {
   "matches": ["*://*/*.png", "*://*/*.jpg", "*://*/*.gif", "*://*/*.bmp"],
      "js": ["js/show-image-content-size.js"],
      "all_frames": true
  }
 ],

 "permissions": [
    "activeTab",
    "https://s3.bytecdn.cn/*",
    "https://hotsoon.snssdk.com/*",
    "https://i.snssdk.com//*",
    "cookies"
  ],

  "externally_connectable": {
    "matches": ["*://s3.bytecdn.cn/*", "*://hotsoon.snssdk.com/*", "*://i.snssdk.com/*"]
  },

 "web_accessible_resources": ["js/inject.js", "js/uamock.js", "js/inject2.js"],
 "omnibox": { "keyword" : "go" },
  "default_locale": "zh_CN"
}