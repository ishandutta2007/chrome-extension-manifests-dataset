{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "version": "1.1.0",
  "author": "Bluepixel",
  "name": "Thumbnail Boss - YouTube thumbnail previewer",
  "short_name": "Thumbnail Boss",
  "description": "Preview and test your thumbnails directly on YouTube",
  "minimum_chrome_version": "111.0",
  "icons": {
    "16": "assets/icons/16.png",
    "32": "assets/icons/32.png",
    "48": "assets/icons/48.png",
    "128": "assets/icons/128.png"
  },
  "background": {
    "service_worker": "service-worker.js"
  },
  "side_panel": {
    "default_path": "sidepanel/index.html"
  },
  "content_scripts": [
    {
      "all_frames": false,
      "js": [
        "content-scripts/inject-thumbnail.js"
      ],
      "match_about_blank": false,
      "matches": [
        "https://*.youtube.com/*"
      ],
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "tabs",
    "webNavigation",
    "storage",
    "unlimitedStorage",
    "sidePanel"
  ],
  "host_permissions": [
    "https://*.youtube.com/*"
  ],
  "web_accessible_resources": [
    {
      "resources": ["assets/icons/48.png"],
      "matches": ["https://*.youtube.com/*"]
    }
  ]
}
