{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Time Displayer",
  "version": "1.0.0",
  "manifest_version": 3,
  "description": "Displays your viewing time on websites.",
  "options_page": "options.html",
  "permissions": [
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ],
      "css": [
        "style.css"
      ]
    }
  ]
}