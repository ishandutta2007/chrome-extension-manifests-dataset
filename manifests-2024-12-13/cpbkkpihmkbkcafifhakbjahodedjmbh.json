{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name" : "Symphony for Hubspot",
  "short_name": "Symphony for Hubspot",
  "version" : "2.0.0",
  "description" : "Symphony integration with SymphonyChat and MassiveMessages Google Chrome extension for Hubspot",
  "manifest_version": 3,
  "omnibox": { "keyword" : "-call" },
  "action": {
    "default_icon": "images/Symphony16.png",
    "notificationsAction": {
      "type": "background"
    }
  },
  "background": {
    "service_worker": "SymphonyChat/background.js"
  },

  "content_scripts": [
    {
      "matches": [
        "https://*.hubspot.com/*"
      ],
      "exclude_matches": [
        "https://community.hubspot.com/*",
        "https://knowledge.hubspot.com/*",
        "https://developers.hubspot.com/*",
        "https://legacydocs.hubspot.com/*",
        "https://*.hubspot.com/login*",
        "https://*.hubspot.com/myaccounts-beta*",
        "https://*.hubspot.com/developer*",
        "https://*.hubspot.com/oauth*",
        "https://*.hubspot.com/ecosystem*",
        "https://*.hubspot.com/discover*",
        "https://*.hubspot.com/products*"
      ],
      "js": [
        "jquery.js",
        "jquery.base64.js",
        "SymphonyChat/SC_container.js",
        "SymphonyChat/SC_events_controller.js",
        "SymphonyChat/SC_button_injector.js",
        "mcm_sms/main.js",
        "SymphonyChat/font-awesome.min.js"
      ],
      "css": [
        "injected.css",
        "jquery-ui.css",
        "SymphonyChat/symphony_chat_styles.css",
        "SymphonyChat/font-awesome.min.css",
        "mcm_sms/main.css"
      ],
      "run_at": "document_idle",
      "all_frames": false
    }
  ],

  "permissions": [ "notifications", "tts" ,"identity","tabs"],
 
  "host_permissions":[ "http://*/*", "https://*/*"],
  
  "icons": {
    "16": "images/Symphony16.png",
    "48": "images/Symphony48.png",
    "128": "images/Symphony128.png"
   }
