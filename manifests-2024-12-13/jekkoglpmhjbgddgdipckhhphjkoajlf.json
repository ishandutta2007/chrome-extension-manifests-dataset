    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "options_page": "options.html",
  "web_accessible_resources": [
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "success-icon.png",
        "error-icon.png"
      ],
      "use_dynamic_url": false
