"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "CopyCat",
  "description": "Copyright protection for Etsy sellers.",
  "version": "0.0.1",
  "manifest_version": 3,
  "action": {
    "default_icon":   { 
      "16": "icon16.png",
      "24": "icon24.png",
      "32": "icon32.png"
    },
  "default_title": "Copyright protection for Etsy sellers.", 
  "default_popup": "popup.html"
  },
  "content_scripts": [
       {
         "matches": ["https://www.etsy.com/your/shops/*"],
         "js": ["etsy-shop-config.js"],
         "run_at": "document_start",
         "world": "ISOLATED"
       },
       {
         "matches": ["https://extensionpay.com/*"],
         "js": ["extension-pay.js"],
         "run_at": "document_start",
         "world": "ISOLATED"
       }
    ],
  "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
  },
  "host_permissions": ["https://www.etsy.com/your/shops/*", "https://xopcjvmxkcwubmyconte.supabase.co/*"],
  "permissions": ["storage"],
  "background": {
    "service_worker": "background.js"
  }
