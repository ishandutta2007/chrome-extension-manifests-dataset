{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Feedra",
  "version": "1.31",
  "description": "Stop scrolling on LinkedIn and proceed directly to the relevant posts.",
  "manifest_version": 3,
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "*://*.linkedin.com/*",
    "https://api.openai.com/*",
    "https://api.airtable.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.linkedin.com/feed/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["icon16.png", "icon48.png", "icon128.png", "icon.png", "logo.png", "config.js", "imagepopup.png"],
      "matches": ["<all_urls>"]
    }
  ]
}
