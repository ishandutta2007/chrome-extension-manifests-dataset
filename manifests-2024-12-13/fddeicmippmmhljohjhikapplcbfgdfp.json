{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "Frost Screen",
    "description": "Blur your active screen for privacy",
    "version": "0.0.3",
    "icons": {
        "16": "logo/logo-16.png",
        "48": "logo/logo-48.png",
        "128": "logo/logo-128.png"
    },
     "action": {
        "default_title": "Frost Screen",
        "default_popup": "popup/popup.html"
    },
    "background": {
        "service_worker": "background_scripts/background_service_worker.js"
    },
    "content_scripts": [{
        "matches": ["http://*/*", "https://*/*"],
        "exclude_matches": ["*://*/*.pdf","*://*/*.mp3","*://*/*.mp4"], 
        "run_at": "document_idle",
        "js": ["content/contentScript.js"],
        "css": ["content/contentCss.css"]
    }],
    "web_accessible_resources": [{ 
        "resources": [  "*.html","*.css","icons/*.png","patterns/*.png"],
        "matches": ["<all_urls>"]
    }]
}


