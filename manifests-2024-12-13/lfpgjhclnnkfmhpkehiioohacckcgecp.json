  "manifest_version": 3,
  "name": "uProposals Assistant",
  "version": "1.1",
  "description": "Chrome extension for ResumeAI job applications",
  "action": {
    "default_popup": "popup/popup.html"
  },
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "*://*.upwork.com/*",
