    "http://localhost/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.upwork.com/nx/proposals/*/apply*",
        "https://*.upwork.com/nx/proposals/job/*/apply*"
      ],
      "js": ["content-scripts/upwork.js"]
    },
    {
      "matches": [
        "http://localhost/*"
      ],
      "js": ["content-scripts/detector.js"],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "externally_connectable": {
    "matches": [
      "http://localhost:5173/*",
      "https://uproposals.com/*"
    ]
  },
  "background": {
    "service_worker": "background/background.js"
  }
