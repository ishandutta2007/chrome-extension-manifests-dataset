{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Balto TCN Integration",
  "version": "1.5",
  "description": "Auto start/stop via TCN",
  "permissions": [
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://agent.tcnp3.com/*"
      ],
      "js": ["TCN.js"],
      "all_frames": true
    },
    {
      "matches": [
        "https://operator.tcn.com/*"
      ],
      "js": ["TCNOperator.js"],
      "all_frames": true
    }
  ],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "action": {
    "default_icon": {
      "16": "images/balto16.png",
      "32": "images/balto32.png",
      "48": "images/balto48.png",
      "128": "images/balto128.png"
    }
  },
  "icons": {
    "16": "images/balto16.png",
    "32": "images/balto32.png",
    "48": "images/balto48.png",
    "128": "images/balto128.png"
  },
  "manifest_version": 3
}
