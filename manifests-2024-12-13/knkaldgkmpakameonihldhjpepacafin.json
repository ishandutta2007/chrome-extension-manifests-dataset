{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Shop Tach Chrome Extension",
  "version": "1.3",
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "webNavigation"
  ],
  "host_permissions": [
    "https://app.fullbay.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },

"icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon16.png",
    "128": "icons/icon16.png"
  },

  "content_scripts": [
    {
      "matches": ["https://app.fullbay.com/office/customer/viewCustomer*"],
      "js": ["customer-contact.js"],
      "run_at": "document_idle"
    },
    {
      "matches": ["https://app.fullbay.com/office/*"],
      "js": ["office-content.js"],
      "run_at": "document_idle"
    },
    {
      "matches": ["https://app.fullbay.com/office/workorder/approveRepairOrderQuote*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    },
    
    {
      "matches": ["https://app.fullbay.com/office/workorder/newRepairOrder*"],
      "js": ["create-contact.js"],
      "run_at": "document_idle"
    }
    
  ]
}
