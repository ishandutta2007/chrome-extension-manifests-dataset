{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "YouTube Categories",
    "version": "1.2",
    "description": "Chrome extension to categorize your YouTube subscriptions",
    "default_locale": "en",
