        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*.youtube.com/*"
            ],
            "css": [
                "styles.css"
            ],
            "js": [
                "utils.js",
                "storage.js",
                "subscriptions.js",
                "channels.js"
            ]
        }
    ]
