"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "Mark Maker",
    "description": "Allow MarkDown in LinkedIn About Me sections for those who also have the extension",
    "version": "0.1.2",
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
