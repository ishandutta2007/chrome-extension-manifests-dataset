    "content_scripts": [
        {
            "css": ["/styles.css"],
            "js": ["/extras/marked.js", "/main.js"],
            "run_at": "document_idle",
            "matches": ["*://www.linkedin.com/in/*", "*://linkedin.com/in/*"]
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "/extras/github-markdown-dark.css",
                "/extras/github-markdown-light.css"
            ],
            "matches": ["*://*/*"]
        }
    ]
