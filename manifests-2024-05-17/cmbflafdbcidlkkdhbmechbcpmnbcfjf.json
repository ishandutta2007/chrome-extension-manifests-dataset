  "manifest_version": 2,
  "name": "BackMark - Back up the pages you bookmark",
  "short_name": "BackMark",
  "version": "1.0.6",
  "description": "Stave off link rot. Automatically back up the pages you bookmark.",
  "icons": {
    "128": "backmark.png"
