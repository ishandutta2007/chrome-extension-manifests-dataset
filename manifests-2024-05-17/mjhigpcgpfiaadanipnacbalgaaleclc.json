{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "SimplifyNote - Your note taking assistant",
  "description": "Add text, images, and quotes to Google Docs with just one click!",
  "version": "1.5.1",
  "manifest_version": 3,
  "key": "",
  "content_scripts": [
    {
      "matches": ["<all_urls>", "file://*/*.pdf"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/logo.png",
      "32": "images/logo.png",
      "48": "images/logo.png",
      "128": "images/logo.png"
    },
    "default_title": "Click here to start creating notes"
  },
  "permissions": [
    "storage",
    "activeTab",
    "cookies",
    "scripting",
    "unlimitedStorage"
  ],
  "host_permissions": [
    "<all_urls>",
    "https://tessdata.projectnaptha.com/*.gz",
    "https://github.com/naptha/tessdata/blob/gh-pages/*.gz?raw=true"
  ],
  "icons": {
    "16": "images/logo.png",
    "32": "images/logo.png",
    "48": "images/logo.png",
    "128": "images/logo.png"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'  'wasm-unsafe-eval'; object-src 'self'"
  },
  "web_accessible_resources": [
    {
      "resources": ["/images/*", "/src/*", "/engine/index.html"],
      "matches": ["<all_urls>"]
    }
  ]
