    "default_icon": "assets/icons/icon_disabled-500.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.phind.com/*",
        "*://*.perplexity.ai/*",
        "*://*.google.com/search*"
      ],
      "js": [
        "tab.js"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "assets/*"
      ],
      "matches": [
        "<all_urls>"
