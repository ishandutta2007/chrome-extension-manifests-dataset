{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,

  "name": "Doodo Search Start With Us",
  "description": "Set Chrome's Homepage and New Tab page to Doodo.in",
  "version": "1.0.0",
  "homepage_url": "https://doodo.in/",
  "icons": {
      "16": "img/icon16.png",
      "48": "img/icon48.png",
      "128": "img/icon128.png"
  },
  "browser_action": {
    "default_icon": "img/icon16.png",
    "default_popup": "popup.html"
  },
 
 
  "chrome_settings_overrides": {
    "homepage": "https://doodo.in/",
    "startup_pages": ["https://doodo.in/"],
    "search_provider": {
      "encoding": "UTF-8",
      "favicon_url": "https://doodo.in/assets/images/logo.png",
      "is_default": true,
      "keyword": "doodo.in",
      "name": "Doodo",
      "search_url": "https://doodo.in/related/web/search?q={searchTerms}"
      }
    
  },
  "chrome_url_overrides" : {
     "newtab": "newtab.html"
  }
}
