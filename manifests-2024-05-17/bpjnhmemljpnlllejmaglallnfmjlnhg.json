  "manifest_version": 2,
  "name": "LyveBee Screen Share",
  "description": "A simple extension that allows you to stream your desktop into meetings with LyveBee users.",
  "version": "0.2.0",
  "minimum_chrome_version": "34",
  "icons": {
    "16": "lyvebee-logo-16x16.jpg",
    "48": "lyvebee-logo-48x48.jpg",
    "128": "lyvebee-logo-128x128.jpg"
  },
  "background": {
        "scripts": [ "background.js" ],
        "persistent": true
  },
  "permissions": [
      "desktopCapture"
  ],
  "externally_connectable": {
      "matches": [
          "*://meet.lyvebee.com/*",
   "*://conference-beta.lyvebee.com/*",
   "*://*.lyvebee.com/*"
      ]
  }
}
