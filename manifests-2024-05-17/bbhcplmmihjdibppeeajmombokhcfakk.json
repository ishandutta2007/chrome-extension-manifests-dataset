      ],

      "js": [
        "./dist/app.min.js"
      ],
      "run_at": "document_idle"
    }
  ],

  "background": {
    "scripts": ["./dist/background.min.js"]
  },

  "browser_action": {
    "default_title": "Github Filter Extention",
    "default_icon": "./resources/img/filter_off_128.png"
  },

