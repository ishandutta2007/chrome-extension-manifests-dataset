{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Account Tools",
  "version": "2.1",
  "description": "account manage tools",
  "permissions": [
    "tabs",
    "cookies",
    "activeTab",
    "storage",
    "webRequest",
    "browsingData",
    "webRequestBlocking",
    "downloads",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "library/js/jquery.min.js",
      "background/helper.js",
      "background/background.js"
    ],
    "persistent": true
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": [
        "library/css/theme/default/layer.css"
      ],
      "js": [
        "library/js/jquery.min.js",
        "library/js/layer.js",
        "front/helper.js"
      ],
      "all_frames": true,
      "run_at": "document_start"
    },
    {
      "matches": ["*://account.yhdjy.cn/extend*"],
      "js": ["front/iframe.js"],
      "all_frames": true,
      "run_at": "document_end"
    },
    {
      "matches": ["*://members.cj.com/*"],
      "js": ["front/cj.js"],
      "all_frames": true,
      "run_at": "document_end"
    },
    {
      "matches": ["*://www.topcashback.com/*"],
      "js": ["front/topcashback.js"],
      "all_frames": true,
      "run_at": "document_end"
    },
    {
      "matches": ["<all_urls>"],
      "js": ["front/top.js"],
      "all_frames": false,
      "run_at": "document_end"
    }
  ],
  "commands": {
    "auto-write": {
      "suggested_key": {
        "default": "Alt+W"
      },
      "description": "自动填表"
    },
    "auto-login": {
      "suggested_key": {
        "default": "Alt+L"
      },
      "description": "自动登录填充"
    },
    "auto-register": {
      "suggested_key": {
        "default": "Alt+R"
      },
      "description": "自动注册填充"
    },
    "auto-captcha": {
      "suggested_key": {
        "default": "Alt+C"
      },
      "description": "验证码"
    }
  },
  "browser_action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    }
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "manifest_version": 2
