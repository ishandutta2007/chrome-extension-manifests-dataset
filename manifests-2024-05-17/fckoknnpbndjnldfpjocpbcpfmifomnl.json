{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Unglasses.ai",
  "version": "1.0.3",
  "description": "AI-powered extension providing visually impaired users with audible web page navigation by vocalizing webpages",
  "permissions": [
    "activeTab",
    "tts",
    "offscreen",
    "storage",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "commands": {
    "take_screenshot": {
      "suggested_key": {
        "default": "Ctrl+Shift+Z",
        "mac": "Command+Shift+L"
      },
      "description": "Use AI to describe the content of the current webpage"
    },
    "sitelinks": {
      "suggested_key": {
        "default": "Ctrl+Shift+X",
        "mac": "Command+Shift+P"
      },
      "description": "Discover the other pages available within the current website"
    }
  }
}
