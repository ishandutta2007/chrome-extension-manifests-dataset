            "matches": ["<all_urls>"],
            "js": ["axios.min.js", "content.js"],
            "css": ["style.css"]
        }
    ],
    "icons": {
        "16": "icons/logo-16.png",
        "48": "icons/logo-48.png",
        "128": "icons/logo-128.png"
    },
    "web_accessible_resources": [
        {
            "resources": ["icons/*"],
            "matches": ["<all_urls>"]
        }
    ]
