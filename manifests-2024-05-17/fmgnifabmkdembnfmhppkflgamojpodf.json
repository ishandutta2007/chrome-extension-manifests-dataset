  "permissions": [
    "<all_urls>", "storage"
  ],

  "optional_permissions": [
    "tabs"
  ],

  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["content.css"],
      "all_frames": true,
      "match_about_blank": true,
      "run_at": "document_start"
    }
  ],

  "options_ui": {
    "page": "options.htm"
  }
