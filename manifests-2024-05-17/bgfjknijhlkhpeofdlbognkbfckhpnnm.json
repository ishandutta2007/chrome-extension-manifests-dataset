{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Unlock Saavn",
  "version": "2.0.0",
  "manifest_version": 2,
  "description": "Keep track of saavn easily. Remotely access music player in any tabs.",
  "homepage_url": "http://balajisankar.tech/",
  "icons": {
    "16": "icons/music-player.png",
    "48": "icons/music-player.png",
    "128": "icons/music-player.png"
  },
 
  "permissions": [
    "activeTab",
    "tabs",
    "http://*/*", "https://*/*"
  ],
  "default_locale": "en",
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": true
  },
  "commands": {
    "toggle-feature": {
      "suggested_key": {
        "default": "MediaNextTrack",
        "mac":"MediaNextTrack"
      },
      "description": "Toggle feature ",
      "global": true
    }
  },
  "browser_action": {
    "default_icon": "icons/music-player.png",
    "default_title": "Saavn",
    "default_popup": "src/browser_action/browser_action.html"
  },
  "content_scripts": [
    {
      "matches": [
        "http://www.jiosaavn.com/*",
        "https://www.jiosaavn.com/*"
      ],
      "run_at": "document_start",
      "css": [
        "src/inject/inject.css"
      ]
    }
  ]
}