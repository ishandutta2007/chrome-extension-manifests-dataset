        "run_at": "document_end"
    }],
    "action": {
        "default_icon": {
            "16": "images/logo-16.png",
            "24": "images/logo-48.png",
            "32": "images/logo-48.png"
        },
        "default_title": "__MSG_extension_name__",
        "default_popup": "./popup/popup.html"
