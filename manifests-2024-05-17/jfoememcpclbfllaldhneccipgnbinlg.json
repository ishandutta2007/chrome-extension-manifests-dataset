{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,

  "name": "InlineMedia",
  "description": "Browser Extension for the Inline Media Platform",
  "version": "0.0.0.16",
  "key": "",
  "background": {
    "service_worker": "assets/background.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": [ "assets/inlineMediaExt.js" ]
    },
    {
      "all_frames": true,
      "matches": ["<all_urls>"],
      "exclude_matches": [
        "*://*.widgets.myfinance.com/",
        "*://*.widgets.local.myfinance.com/"
      ],
      "include_globs": [
        "*/r/*",
        "*/w/*",
        "*/as/*",
        "*/c/*"
      ],
      "js": [ "assets/inlineMediaAdExt.js" ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [ "assets/inlineMediaAdMeta.js" ],
      "matches": ["<all_urls>"]
    }
  ],
  "options_page": "options.html",
  "action": {
    "default_popup": "index.html"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "windows": "Ctrl+Shift+I",
        "mac": "Command+Shift+I",
        "chromeos": "Ctrl+Shift+I",
        "linux": "Ctrl+Shift+I"
      }
    }
  },
  "icons": {
    "16": "icons/inlineMediaDisabled_16.png",
    "48": "icons/inlineMediaDisabled_48.png",
    "128": "icons/inlineMediaDisabled_128.png"
  },
  "permissions": [
    "activeTab",
    "storage",
    "unlimitedStorage",
    "tabs",
    "background",
    "cookies",
    "declarativeNetRequest"
  ],
  "host_permissions": [
    "*://*.myfinance.com/",
    "*://*.bankrate.com/",
    "*://*.cnet.com/",
    "*://*.greatist.com/",
    "*://*.nextadvisor.com/",
    "*://*.healthline.com/",
    "*://*.myslumberyard.com/",
    "*://*.medicalnewstoday.com/",
    "*://*.psychcentral.com/"
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self' http://localhost; object-src 'self'"
  }
}
