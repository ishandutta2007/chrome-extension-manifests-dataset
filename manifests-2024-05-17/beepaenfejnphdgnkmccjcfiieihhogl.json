                "https://atlasauth.b2clogin.com/*"
            ],
            "js": ["js/html2canvas.js", "js/content.js"],
            "css": ["css/content.css"],
            "run_at": "document_end"
        },
        {
            "matches": ["https://*.usvisascheduling.com/en-US/appointment-confirmation/*"],
