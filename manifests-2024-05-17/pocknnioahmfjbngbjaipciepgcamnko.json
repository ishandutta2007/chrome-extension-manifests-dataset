{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,

  "name": "imgur Timer",
  "description": "Shows time spent browsing Imgur.",
  "version": "1.2",
  "permissions": [
    "tabs", 
    "http://*/*", 
    "https://*/*",
    "background"
