    "default_popup": "timer.html"
  },
  "background": {
    "scripts": ["javascripts/background.js"]
  },
  "content_security_policy": "script-src 'self' https://apis.google.com; object-src 'self'",
  "content_scripts": [ {
    "js": [ "javascripts/imgurTimer.js" ],
    "css": [ "stylesheets/style.css" ],
    "matches": [ "http://*/*", "https://*/*" ]
  }]
