{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "MapsToSearch",
  "description": "Search maps in one second",
  "version": "1.0.2",
  "omnibox": { "keyword": "m" },

  "chrome_settings_overrides": {
     "search_provider": {
        "encoding": "UTF-8",
        "favicon_url": "https://www.mapstosearch.com/assets/mapstosearch.png",
        "is_default": true,
        "keyword": "mapstosearch",
        "name": "Mapstosearch",
        "search_url": "https://mapstosearch.com/search?q={searchTerms}"
     }
  },
  
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["mapsSearch.js"]
    }
  ],
  
  "icons": {
     "128": "images/icon128.png",
     "16": "images/icon16.png",
     "48": "images/icon48.png"
  },

  "background": {
    "service_worker": "mapsSearch.js"
  },

  "host_permissions": [
    "http://suggestqueries.google.com/"
  ]
}
