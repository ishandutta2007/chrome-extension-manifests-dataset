{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,

  "name": "__MSG_name__",
  "description": "__MSG_description__",
  "version": "1.0",
  "permissions": [
    "history",
    "bookmarks",
    "https://www.google.com/",
    "https://www.baidu.com/"
