  "icons": {
    "128": "icon_128.png",
    "256": "icon_256.png",
    "512": "icon_512.png"
  },
  "default_locale": "en",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.google.com/search*",
        "https://www.baidu.com/*"
      ],
      "js": ["content_script.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "web_accessible_resources": ["index.html"]
}