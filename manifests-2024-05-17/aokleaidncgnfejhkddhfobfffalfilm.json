{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "Extereo Audio Player",
  "version": "4.1.2",
  "short_name": "Extereo",
  "description": "An audio streaming extension that seeks for links to music and recordings on the sites you visit.",
