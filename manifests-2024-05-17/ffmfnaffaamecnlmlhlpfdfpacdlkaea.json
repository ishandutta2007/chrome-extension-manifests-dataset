    "manifest_version": 3,
    "name": "Wizymail",
    "description": "10x Faster AI assistant to write and reply email",
    "version": "1.0.8",
    "action": {
        "default_popup": "popup.html",
        "default_title": "Wizymail",
        "default_icon": "icon.png"
    },
    "permissions": [
        "scripting",
        "storage"
    ],
    "host_permissions": [
        "https://mail.google.com/",
        "http://localhost:3000/",
        "https://mail.wizymail.com/",
        "https://outlook.live.com/"
    ],
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": [
                "https://mail.google.com/*",
                "http://localhost:3000/*",
                "https://mail.wizymail.com/*",
                "https://outlook.live.com/*"
            ],
            "js": [
                "contentScript.js"
            ],
            "run_at": "document_end"
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "icon.png"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ],
    "externally_connectable": {
        "matches": [
            "http://localhost:3000/*",
            "https://mail.wizymail.com/*"
        ],
        "accept_tls_channel_id": true
    }
