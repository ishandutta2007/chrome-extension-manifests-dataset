"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "TimeSheet",
  "version": "1.4",
  "description": "This extension allows automatic reporting of working hours to the user's mytime-sheet account and create draft timesheets.",
  "permissions": [  
    "storage",
    "background",
    "idle",
    "alarms",
    "activeTab",
    "tabs",
    "scripting",
    "cookies"
  ], 
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "action": { 
    "default_popup": "popup-login.html",
    "default_icon": {
      "16": "icon16.png",
      "32": "icon32.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "externally_connectable": {
    "matches": [ "<all_urls>"]
}

