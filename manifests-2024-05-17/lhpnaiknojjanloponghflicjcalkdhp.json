{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "Focus Nazar",
    "version": "0.0.1",
    "description": "Focus Nazar is a simple, lightweight Chrome Extension designed to manage your focus.",
