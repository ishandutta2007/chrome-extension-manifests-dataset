{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "version": "1.0.4",
  "author": "rajesh@peakxv.com",
  "name": "PeakXV Talent",
  "short_name": "Peak TRM",
  "description": "",
  "icons": {
    "38": "images/icon_leaf_38.png",
    "48": "images/icon_leaf_48.png",
    "128": "images/icon_leaf_128.png"
  },
  "default_locale": "en",
  "web_accessible_resources": [
    {
      "resources": [
        "images/*.png"
