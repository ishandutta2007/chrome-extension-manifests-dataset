    {
       "js": [ "/src/js/jquery.min.js","/src/content/undistracted-instagram.js"],
       "run_at":"document_start",
       "matches": [ "*://*.instagram.com/*"  ]
    },
    {
       "js": [ "/src/js/jquery.min.js","/src/content/undistracted-reddit.js"],
       "run_at":"document_start",
