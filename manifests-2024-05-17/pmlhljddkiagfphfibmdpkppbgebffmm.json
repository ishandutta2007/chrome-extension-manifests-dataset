    "version": "1.2",
    "description": "Convert Confluence into Dark Mode",
    "permissions": ["storage"],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "include_globs": [
                "*://*.atlassian.net/*",
                "https://confluence.*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_idle"
        }
