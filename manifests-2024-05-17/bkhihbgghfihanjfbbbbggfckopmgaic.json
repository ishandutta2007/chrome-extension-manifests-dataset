  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": true
  },
  "content_scripts": [
    {
   "run_at": "document_start",
      "matches": [
        "https://www.google.com/*"
      ],
      "js": [
        "src/inject/inject.js",
        "js/jquery/jquery.js"
      ]
    }
  ],
  "options_page": "src/options/index.html"
}
