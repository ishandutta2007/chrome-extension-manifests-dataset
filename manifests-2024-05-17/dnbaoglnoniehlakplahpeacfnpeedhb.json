{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,

  "name": "VideoFetcher",
  "description": "Extension to fetch Videos on Websites",
  "version": "1.0.0",

  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon.png"
  },

  "content_scripts": [
      {
          "matches": ["<all_urls>"],
          "js": ["js/vendor.js", "js/content_script.js"]
      }
  ],

  "background": {
    "scripts": ["js/vendor.js", "js/background.js"],
    "persistent": true
  },

  "permissions": [
    "storage",
    "activeTab",
    "<all_urls>",
    "webRequest",
    "background",
    "tabs"
  ],

  "web_accessible_resources": [
    "<all_urls>"
  ]
}
