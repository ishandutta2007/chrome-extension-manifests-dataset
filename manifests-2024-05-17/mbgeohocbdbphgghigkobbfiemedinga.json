{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Poda.TV Konfigurátor programů",
  "version": "1.1.1",
  "description": "Rozšíření pro úpravu a řazení přístupných TV programů na https://live.poda.tv/",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage"
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "https://live.poda.tv/*"
      ],
      "js": [
        "js/constants.js",
        "js/init.js",
        "js/showMyChannels.js"
      ],
      "run_at": "document_start"
    }
  ],
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/tv_16.png",
      "32": "images/tv_32.png",
      "48": "images/tv_48.png",
      "128": "images/tv_128.png"
    }
  },
  "icons": {
    "16": "images/tv_16.png",
    "32": "images/tv_32.png",
    "48": "images/tv_48.png",
    "128": "images/tv_128.png"
  },
  "manifest_version": 2
