  "manifest_version": 3,

  "name": "Sitges Film Festival Plus",
  "description": "This extension improves Sitges Film Festival site.",
  "version": "2.8.4",

  "action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "storage"
  ],
