{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Hawkscale",
  "version": "1.0.0",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "host_permissions": [
    "https://*/*"
  ],
  "action": {
    "default_title": "Abra para dropar produtos!",
    "default_icon": {
      "128": "assets/images/logo128.png"
    }
  },
  "content_scripts": [
  {
   "matches": ["<all_urls>"],
   "js": ["main.js"]
  }
 ],
  "background": {
  "service_worker": "background.js",
  "type": "module"
 },
  "web_accessible_resources": [
    {
      "resources": [
        "background.js",
        "main.js",
        "assets/fonts/Urbanist/*.ttf",
        "assets/images/*.png"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "icons": {
    "16": "assets/images/logo16.png",
    "32": "assets/images/logo32.png",
    "48": "assets/images/logo48.png",
    "128": "assets/images/logo128.png"
  }
