{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "Slack Channel Data Exporter",
    "version": "2",
    "permissions": [
        "tabs",
        "downloads"
    ],
    "host_permissions": [
        "https://*.slack.com/*"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": ["https://*.slack.com/*"],
            "js": ["contentScript.js"],
            "run_at": "document_idle"
        }
    ],
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    }
}
