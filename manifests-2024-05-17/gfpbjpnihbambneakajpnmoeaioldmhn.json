{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name":"YouTube Helper",
  "short_name": "YouTube Helper",
  "description":"YouTube Helper is an extension that allows you to repeat skip etc when specific buttons are pressed.",
  "manifest_version": 2,
  "version":"0.5.0",
  "author":"codezart",
  "homepage_url": "https://github.com/codezart/youtube-helper",
  "permissions":[
    "*://*.youtube.com/*",
    "activeTab"
  ],
  "browser_action":{
      "default_icon":"icon2.png",
      "default_popup":"popup.html"
  },
  "background":
    {
      "scripts":["background.js"],
      "persistent": false
    },
  "content_scripts":[
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["witness.js"],
      "persistent": true
    }
  ],
  "icons": {
    "128": "icon2.png",
    "64":"icon2.png",
    "32":"icon2.png",
    "16":"icon2.png"
  },

  "web_accessible_resources": [
    "style.css"
  ],

  "commands": {
    "Ctrl+Shift+E": {
      "suggested_key": {
        "default": "Ctrl+Shift+E",
        "mac": "Command+Shift+E"
      },
      "description": "Plays the next video on youtube"
    },
    "Ctrl+Shift+S": {
      "suggested_key": {
        "default": "Ctrl+Shift+S",
        "mac": "Command+Shift+S"
      },
      "description": "Pauses the video on youtube"
    },
    "Ctrl+Shift+L": {
      "suggested_key": {
        "default": "Ctrl+Shift+L",
        "mac": "Command+Shift+L"
      },
      "description": "loops/unloops the video on youtube"
    },
    "Ctrl+Shift+Q": {
      "suggested_key": {
        "default": "Ctrl+Shift+Q",
        "mac": "Command+Shift+Q"
      },
      "description": "Plays the previous video on youtube"
    }
  }
}
