  "background": {
    "service_worker": "src/service.js",
    "type": "module"
  },
  "icons": {
    "16": "assets/icon-16x16.png",
    "48": "assets/icon-48x48.png",
    "128": "assets/icon-128x128.png"
  },
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ]
