{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,

  "name": "Gender Blinder",
  "version": "1.4",
  "description": "This extension attempts to suppress implicit gender identity in the pages you visit.",
  "homepage_url": "http://path/to/homepage",

  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
