  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["blind.js"]
    }
  ],

  "permissions": [
    "storage"
  ],

  "browser_action": {
    "default_icon": {
      "128": "icon128.png"
    },
    "default_title": "Gender Blinder",
    "default_popup": "popup.html"
  }

