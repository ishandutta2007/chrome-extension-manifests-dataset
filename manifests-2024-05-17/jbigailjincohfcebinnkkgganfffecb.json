{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "STAYC New Tab",
    "version": "1.0.0.1",
    "description": "Sets a custom browser home page.",
    "permissions": [
        "geolocation",
        "storage"
    ],
    "host_permissions": [
        "https://api.weatherapi.com/*"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "web_accessible_resources": [
    {
        "resources": [
            "css/*",
            "bg-jpg/*",
            "audio/*",
            "images/*",
            "*.js"
        ],
        "matches": [
            "chrome://newtab/*"
        ]
    }
    ],
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "icons": {
        "16": "images/stayc-logo.jpg",
        "48": "images/stayc-logo.jpg",
        "128": "images/stayc-logo.jpg"
    },
    "action": {
        "default_icon": "images/stayc-logo.jpg"
    },
    "content_security_policy": {
        "default-src": ["'self'", "https://api.weatherapi.com"],
        "script-src": ["'self'"],
        "script-src-elem": ["'self'", "'unsafe-inline'"],
        "connect-src": ["'self'", "https://api.weatherapi.com"],
        "object-src": ["'self'"],
        "img-src": ["'self'", "data:"],
        "style-src": ["'self'", "'unsafe-inline'"],
        "font-src": ["'self'", "fonts.googleapis.com"],
        "media-src": ["'self'"]
    }
}