  "omnibox": {
    "keyword": "b"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "content_scripts": [
    {
      "run_at": "document_start",
      "js": [
        "contentScript.js"
      ],
      "css": [
        "standby-lb-detector-css.css"
      ],
      "matches": [
        "https://*/xui/"
      ]
    },
    {
      "run_at": "document_idle",
      "js": [
        "contentScriptLoginPage.js"
      ],
      "matches": [
        "https://*/tmui/login.jsp*"
      ]
    }
