{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Standby lb detector",
  "version": "2020.3.23",
  "author": "Magnus Watn",
  "manifest_version": 2,
  "icons": {
    "64": "icon64.png",
    "128": "icon128.png"
  },
  "permissions": [
    "https://*/",
