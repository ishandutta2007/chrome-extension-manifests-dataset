{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 3,
    "name": "Disign Web Player Extension",
    "version": "0.0.0.2",
    "description": "Disign Web Player Extension enhances and develops Disign Web Player features.",
    "action": {
        "default_popup": "index.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "declarative_net_request": {
        "rule_resources": [
          {
            "id": "ruleset_1",
            "enabled": true,
            "path": "./rules.json"
          }
        ]
    },
    "content_scripts": [
        {
          "matches": ["*://localhost/*", "https://player.disign.tv/*"],
          "js": ["content-script.js"],
          "run_at": "document_start"
        }
    ],
    "externally_connectable": {
        "matches": ["*://localhost/*", "https://player.disign.tv/*"]
    },
    "permissions": [
        "declarativeNetRequest",
        "declarativeNetRequestFeedback",
        "system.memory",
        "system.storage",
        "system.display",
        "system.cpu"
    ],
    "host_permissions": [
        "*://localhost/*", 
        "https://player.disign.tv/*",
        "https://preview.disign.tv/*",
        "<all_urls>"
    ],
    "icons": {
        "16": "icons/icon-16x16.png",
        "32": "icons/icon-32x32.png",
        "48": "icons/icon-48x48.png",
        "128": "icons/icon-128x128.png"
    }
  }