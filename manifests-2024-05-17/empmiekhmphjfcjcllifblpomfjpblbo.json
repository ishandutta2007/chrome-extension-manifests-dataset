{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Pinboard++",
  "version": "1.0.1",
  "manifest_version": 2,
  "description": "This Pinboard extension provides additional features such as full-length website screenshots, thumbnails, comments...",
  "homepage_url": "https://pinboard.initech.info/",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "storage",
    "https://pinboard.in/*",
    "https://pinboard.initech.info/*",
    "https://discuss.pinboard.initech.info/*"
  ],
  "web_accessible_resources": [
    "src/window.js"
  ],
  "background": {
    "scripts": ["src/background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "https://pinboard.in/*"
      ],
      "css": [
        "src/inject.css"
      ]
    },
    {
      "matches": [
        "https://pinboard.in/*"
      ],
      "js": [
        "src/libs.min.js",
        "src/helpers.js",
        "src/inject.js"
      ]
    }
  ],
  "options_ui": {
    "page": "src/options.html",
    "open_in_tab": false
  }
}