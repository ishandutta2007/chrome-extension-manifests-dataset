name: CloudRaker Meetings
version: 0.2.9
category_slug: lifestyle/social
rating: null
rating_count: null
user_count: 0
release_date: '2019-01-03T17:16:25.000Z'
size: 104KiB
languages:
  - English
description: >-
  A simple extension that allows you to schedule CloudRaker Meetings and stream
  your desktop into conferences.
publisher_account: CloudRaker
extension_website: cloudraker.com
---

{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "CloudRaker Meetings",
  "description": "A simple extension that allows you to schedule CloudRaker Meetings and stream your desktop into conferences.",
  "version": "0.2.9",
  "minimum_chrome_version": "34",
  "icons": {
    "16": "cloudraker-logo-16x16.png",
    "48": "cloudraker-logo-48x48.png",
    "128": "cloudraker-logo-128x128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "desktopCapture",
    "https://calendar.google.com/*"
  ],
  "externally_connectable": {
    "matches": [
      "*://meet.cloudraker.com/*"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "https://calendar.google.com/calendar/*"
      ],
      "js": [
        "jquery.js",
        "RandomUtil.js",
        "RoomnameGenerator.js",
        "meet-calendar.js"
      ],
      "css": [
        "/css/all.css"
      ],
      "all_frames": false,
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "cloudraker-logo-48x48.png",
    "cloudraker-logo-grey-48x48.png",
    "cloudraker-logo-grey.svg",
    "cloudraker-logo-black.svg"
  ],
  "browser_action": {
    "default_title": "Create CloudRaker Meetings",
    "default_popup": "popup.html"
  }
}