  "manifest_version": 2,
  "name": "Jitsi desktop streamer for Beautiful Rising",
  "description": "Enable screen sharing for Beautiful Rising's jitsi meet page",
  "version": "0.1.5",
  "minimum_chrome_version": "34",
  "icons": {
    "16": "jitsi-logo-16x16.png",
