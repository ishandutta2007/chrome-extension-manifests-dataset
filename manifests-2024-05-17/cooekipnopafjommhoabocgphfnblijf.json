{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Relayz Meet",
  "description": "A smart extension that connects you to Web3 and allows you to schedule Relayz meetings",
  "version": "0.1.0",
  "minimum_chrome_version": "89",
  "icons": {
    "16": "relayz-logo-16x16.png",
    "48": "relayz-logo-48x48.png",
    "128": "relayz-logo-128x128.png"
  },
  "host_permissions": [
      "https://calendar.google.com/*"
  ],
  "externally_connectable": {
      "matches": [
        "*://meet.relayz.io/*", "*://test.meet.relayz.io/*"
      ]
  },
  "content_scripts": [
    {
      "matches": ["https://calendar.google.com/calendar/*", "https://outlook.live.com/owa/*"],
      "js": ["jquery.js", "RandomUtil.js", "RoomnameGenerator.js", "meet-calendar.js"],
      "css": ["/css/all.css"],
      "all_frames" : false,
      "run_at" : "document_end"
    }
  ],
  "web_accessible_resources": [{
    "matches": [
      "https://calendar.google.com/*",
      "https://outlook.live.com/*"
    ],
    "resources": [
      "relayz-logo-48x48.png",
      "relayz-logo-universal.svg"
    ]
  }],
  "action": {
    "default_title": "Create a Relayz Meeting",
    "default_popup": "popup.html"
  }
}
