    "manifest_version": 2,
    "version": "0.5.3",
    "name": "ABB-remover",
    "browser_action":{
        "default_icon": "icon.png"
    },
    "description": "This extension removes all traces off ABB on the webpages vg.no, Dagbladet.no, NRK.no and Aftenposten.no",
    "permissions": ["tabs",
    "<all_urls>"],
    "content_scripts":[
