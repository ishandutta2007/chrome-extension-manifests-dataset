            "matches":[
                "http://www.vg.no/",
                "http://www.dagbladet.no/",
                "http://www.dagbladet.no/?ref=inger",
                "http://www.aftenposten.no/",
                "http://www.nrk.no/"
            ],
            "js": ["abbremover.js"],
            "run_at":"document_start"
        }
    ]
}